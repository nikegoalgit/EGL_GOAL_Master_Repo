// Process valid input request
Function FQ04600()
   /* ------------------------------------------------------------*/
   /* Indicate that the map must be formatted before display.*/
   /* ------------------------------------------------------------*/
  FQ04W00.FQ04WSWF = "Y"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    set FQ04M001 initial;
    FQ04W00.XXXTSPAG = FQ04W00.XXXTSPAG - 1; /* Decrement page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    set FQ04M001 initial;
    FQ04W00.XXXTSPAG = FQ04W00.XXXTSPAG + 1; /* Increment page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If the search key was changed, save the search information.*/
   /* ------------------------------------------------------------*/
  if (FQ04M001.FY1PFYCD != FQWREC.FY1PFYCD  /* or factory changed*/
   || FQ04M001.FQ04MBDT != FQ04W00.FQ04MBYM /* or beg date changed*/
   || FQ04M001.FQ04MEDT != FQ04W00.FQ04MEYM /* or end date changed*/
   || FQ04M001.FQ04MBDF != FQWREC.FQWBDFCT  /* or beg defect changed*/
   || FQ04M001.FQ04MEDF != FQWREC.FQWEDFCT  /* or end defect changed*/
   || FQ04M001.SY1STNBR != FQWREC.SY1STNBR  /* or style nbr*/
   || FQ04M001.SY2CLRID != FQWREC.SY2CLRID  /* or color id*/
   || FQ04M001.XDMCD != FQWREC.XDMCD        /* or dimension*/
   || FQ04M001.XDVCD != FQWREC.XDVCD)       /* or division*/

    /* ------------------------------------------------------------*/
     /* Save data.*/
    /* ------------------------------------------------------------*/
    FQWREC.FY1PFYCD = FQ04M001.FY1PFYCD; /* origin factory*/
    FQWREC.SY1STNBR = FQ04M001.SY1STNBR; /* style nbr*/
    FQWREC.SY2CLRID = FQ04M001.SY2CLRID; /* color id*/
    FQWREC.XDMCD = FQ04M001.XDMCD; /* dimension*/
    FQWREC.XDVCD = FQ04M001.XDVCD; /* division*/

    FQWREC.FQWBDFCT = FQ04M001.FQ04MBDF; /* Beg defect*/
    FQWREC.FQWEDFCT = FQ04M001.FQ04MEDF; /* End defect*/
    FQWREC.YDCDESC = FQ04M001.YDCDESC; /* Defect cat desc*/

    FQWREC.FQ1IDBDT = FQZREC.FQZBCYMM; /* beg date*/
    FQWREC.FQ1IDXDT = FQZREC.FQZECYMM; /* end date*/

    FQ04W00.FQ04MBDT = FQZREC.FQZBCYMM; /* beg date*/
    FQ04W00.FQ04MEDT = FQZREC.FQZECYMM; /* end date*/

    FQ04W00.FQ04WSWR = "Y"; /* Request for data switch*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If PF9 was pressed, transfer to print screen.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf9) /* If PF9 was pressed*/

    /* ------------------------------------------------------------*/
     /* Transfer to FQ0G.*/
    /* ------------------------------------------------------------*/
    COMMAREA.CATOAP = "FQ0G"; /* TO application*/

    FQ04610(); /* Transfer to FQ0G*/

  end

   /* ------------------------------------------------------------*/
   /* If line was selected, transfer to FQ05.*/
   /* ------------------------------------------------------------*/
  if ("S" in FQ04M001.FQ04MSEL) /* Style selected*/

    COMMAREA.CATOAP = "FQ05"; /* TO application*/
    FQWREC.YDFCD = FQ04M001.YDFCD[sysVar.arrayIndex]; /* Defect code*/
    FQWREC.YDFDESC = FQ04M001.YDFDESC[sysVar.arrayIndex]; /* Defect name*/

    FQ04610(); /* Transfer to FQ05*/

  end


end // end FQ04600

