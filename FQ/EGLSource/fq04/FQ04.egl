//*** PROGRAM=FQ04 ****
// This application allows users to view a list of defects,
// descriptions, quantities, and ratio of total defects for
// a particular set of parameters.
// 
// The user must enter 'FACTORY', 'DEFECT RANGE', and 'STYLE'.
// 'PO MONTH RANGE', 'DIVISION', 'COLOR' and 'DIMENSION'
// will default if left blank.  The program will obtain a list
// of defects, their quantities, descriptions, and will also
// calculate the ratio each defect is of the total defects
// for the key.
// 
// The user may select a particular defect and be transfered
// to FQ05, FCTY PRDCTN/DEF RATIO DETAIL.  There he will be
// able to see more details concerning a defect.
// 
// The user may scroll forward and backward to view all the
// categories by pressing PF7 and PF8.  He may exit any to any
// allowed application, via FAST PATH or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      05/13/91  RHETRI  New application.
//          12/05/91  RHETRI  Chg rcv'd amt to on-order amt
//          06/24/93  MHUYNH  Regen due to change to XSFQS05
// y2kproj  12-03-97  oricha  y2k modifications:  regen to pick
//                            up xx0xs01 changes & to pick up
//                            xsfqs05 century calc changes.
// *********************
Program FQ04 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "FQ00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  FQ04W00 FQ04W00; // record
  FQ04W01 FQ04W01; // record
  FQ2REC FQ2REC; // record
  FQ6REC FQ6REC; // record
  FQAREC FQAREC; // record
  FQWREC FQWREC; // record
  FQZREC FQZREC; // record
  FY1REC FY1REC; // record
  PG2REC PG2REC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TA6REC TA6REC; // record
  TAEREC TAEREC; // record
  XX0XW01 XX0XW01; // record
  XX0XW02 XX0XW02; // record
  YDCREC YDCREC; // record
  YDFREC YDFREC; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XDMTBL; // table
  use XDVTBL; // table
  use YDFTBL; // table
  use YDCTBL; // table
  use FQ04M.FQ04M001,  // forms
      FQ04M.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPFQ001: XPFQ001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/
    set FQ04M001 initial; /* M001 input/output*/
    set FQ04W00 empty; /* Working storage*/
    set FQ04W01 empty; /* M001 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/

    FQ04W00.FQ04WSWF = "Y"; /* Format the map flag*/
    FQ04W00.FQ04WSWR = "N"; /* Data not requested*/

     /* ------------------------------------------------------------*/
     /* If defect code passed, but name missing - obtain it.*/
     /* ------------------------------------------------------------*/
    if (FQWREC.YDFCD > " " /* if defect exists*/
     && FQWREC.YDFDESC == " ") /* and defect name missing*/

      YDFREC.YDFCD = FQWREC.YDFCD; /* Format key*/
      YDFREC.XGPCD = FQWREC.XGPCD; /* Format key*/
      if (YDFREC.YDFKEY in YDFTBL.YDFKEY)
        FQWREC.YDFDESC = YDFTBL.YDFDESC[sysVar.arrayIndex];
      end
    end

     /* ------------------------------------------------------------*/
     /* If complete key is present, request data. (Defaults exist.)*/
     /* ------------------------------------------------------------*/
    if (FQWREC.FY1PFYCD > " " /* If origin factory exists*/
     && FQWREC.SY1STNBR > " ") /* and style number exists*/

      if (FQWREC.YDCCD > " ") /* If defect category exists*/
        FQ04W00.FQ04WSWR = "Y"; /* Request data*/
      else
        if (FQWREC.FQWBDFCT > " " /* If beg defect code*/
         && FQWREC.FQWEDFCT > " ") /* and end defect code*/
          FQ04W00.FQ04WSWR = "Y"; /* Request data*/
        end
      end
    end

    FQ04W00.FQ04MBDT = FQWREC.FQ1IDBDT; /* Beg date*/
    FQ04W00.FQ04MEDT = FQWREC.FQ1IDXDT; /* End date*/

     /* ------------------------------------------------------------*/
     /* Flow to check for default dates.*/
     /* ------------------------------------------------------------*/
    goto FQ04000; /* Check for default dates*/


    FQ04000: FQ04000();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FQ04001; /* Converse the main process*/


    FQ04001: FQ04001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FQ04001; /* Converse the main process*/


  end // end main
end // end FQ04

// Check for default dates
Function FQ04000()
   /* ------------------------------------------------------------*/
   /* If date range is missing, call date routine for defaults.*/
   /* ------------------------------------------------------------*/
  if (FQWREC.FQ1IDBDT == 0 /* If begin date is missing*/
   || FQWREC.FQ1IDXDT == 0) /* Or end date is missing*/

    /* ------------------------------------------------------------*/
     /* Clear copybook.*/
    /* ------------------------------------------------------------*/
    set FQZREC empty; /* clear date edit record*/

    FQZREC.FQZBEGRG = FQWREC.FQ1IDBDT; /* Beg date*/
    FQZREC.FQZENDRG = FQWREC.FQ1IDXDT; /* end date*/

    /* ------------------------------------------------------------*/
     /* Edit range.  Send YYMMMM - receive back CCYYMMMM.*/
    /* ------------------------------------------------------------*/
    XSFQS05(); /* Common date edit routine*/

    /* ------------------------------------------------------------*/
     /* Move dates into FQWREC.*/
    /* ------------------------------------------------------------*/
    FQWREC.FQ1IDBDT = FQZREC.FQZBCYMM; /* Begin date*/
    FQWREC.FQ1IDXDT = FQZREC.FQZECYMM; /* End date*/

    FQ04W00.FQ04MBDT = FQZREC.FQZBCYMM; /* Begin date*/
    FQ04W00.FQ04MEDT = FQZREC.FQZECYMM; /* End date*/

  end


end // end FQ04000

// Main process to display a map
Function FQ04001()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  FQ04100(); /* Obtain the data to display*/

  FQ04200(); /* Format and display the map*/

  FQ04300(); /* Check attention identifier*/

  FQ04400(); /* Reset fields to normal intensity*/

  FQ04500(); /* Verify the entered data*/

  FQ04600(); /* Process valid input request*/


end // end FQ04001

// Obtain the data to display
Function FQ04100()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ04W00.FQ04WSWR != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* ------------------------------------------------------------*/
  FQ04W00.XXXTSLEN = 770; /* Temporary storage page length*/

   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/
  FQ04S01(); /* Purge temporary storage*/

   /* --------------------------------------------------------*/
   /* Check for defect range codes or defect category code.*/
   /* --------------------------------------------------------*/
  FQ04110(); /* Check for defect codes*/

   /* --------------------------------------------------------*/
   /* Open DFDTL.*/
   /* --------------------------------------------------------*/
  FQ04120(); /* Open DFDTL*/

   /* ----------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ----------------------------------------------------------*/
  while (FQ04W00.FQ04WEOC == "N") /* Until end of cursor*/

    FQ04130(); /* Fetch DFDTL*/

    if (FQ04W00.FQ04WEOC == "N") /* If row found*/

      /* ----------------------------------------------------------*/
       /* Save data for display.*/
      /* ----------------------------------------------------------*/
      FQ04140(); /* Save data*/

      /* ----------------------------------------------------------*/
       /* Add quantity to defect total.*/
      /* ----------------------------------------------------------*/
      FQ04W00.FQ04WDTT = FQ04W00.FQ04WDTT + FQ2REC.FQ2DEFQT;
      /* ---------------------------------------------------------*/
       /* If the page is full, add the page to the storage queue.*/
      /* ---------------------------------------------------------*/
      if (FQ04W00.FQ04WIXM > 12) /* Page is full*/
        FQ04150(); /* Add page to temporary storage*/
      end

    end
  end

   /* ----------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ----------------------------------------------------*/
  if (FQ04W00.FQ04WIXM > 1) /* If line count greater than 1*/

    FQ04150(); /* Add page to temp storage*/

  end

   /* ------------------------------------------------------------*/
   /* Close DFDTL*/
   /* ------------------------------------------------------------*/
  FQ04160(); /* Close DFDTL*/

   /* ---------------------------------------------------------*/
   /* Select summed DFDTL for defect grand total.*/
   /* ---------------------------------------------------------*/
  FQ04170(); /* Select summed DFDTL*/

   /* ------------------------------------------------------------*/
   /* If no data to display, indicate no data to display.*/
   /* ------------------------------------------------------------*/
  if (FQ04W00.TA5TSQIX == 0) /* No pages formatted*/

    FQWREC.XXXMODID = "110"; /* Module identification*/
    converseLib.validationFailed(17); /* No data was found to display*/

    return; /* Return to previous process*/

  else
    /* ---------------------------------------------------------*/
     /* Calculate total prdctn/def ratio.*/
    /* ---------------------------------------------------------*/
    FQ04180(); /* Prdctn/def ratio*/

  end

   /* ---------------------------------------------------------*/
   /* If no message to display, reset the desired page number.*/
   /* ---------------------------------------------------------*/

  if (FQWREC.XXXMSGNO == 0) /* No message to display*/
    FQ04W00.XXXTSPAG = 1; /* Desired page number*/
  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  FQ04W00.FQ04WSWR = "N"; /* Request for data switch*/


end // end FQ04100

// Check for defect code keys
Function FQ04110()
   /* ------------------------------------------------------------*/
   /* If defect range is missing, use category to determine the*/
   /* range.*/
   /* ------------------------------------------------------------*/
  if (FQWREC.FQWBDFCT == " " /* If beg defect missing*/
   || FQWREC.FQWEDFCT == " ") /* or end defect missing*/

    /* ------------------------------------------------------------*/
     /* Determine the beginning and ending defect code for a part-*/
     /* icular defect category.*/
    /* ------------------------------------------------------------*/
    if (FQWREC.YDCCD in YDFTBL.YDCCD) /* If cat on dfct table*/
      FQ04W00.FQ04WIX2 = sysVar.arrayIndex; /* save position*/
      FQWREC.FQWBDFCT = YDFTBL.YDFCD[FQ04WIX2]; /* begin defect*/

      while (YDFTBL.YDCCD[FQ04WIX2] == FQWREC.YDCCD) /* If cat EQ cat*/
        FQWREC.FQWEDFCT = YDFTBL.YDFCD[FQ04WIX2]; /* end defect*/

        FQ04W00.FQ04WIX2 = FQ04W00.FQ04WIX2 + 1; /* add 1 to counter*/
      end

    end
  end


end // end FQ04110

// Open DFDTL
Function FQ04120()
   /* ------------------------------------------------------------*/
   /* Clear copybooks.*/
   /* ------------------------------------------------------------*/
  set FQAREC empty; /* Clear copybook*/
  set FQ2REC empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
   /* Format fqAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "S2"; /* Function code*/
  FQAREC.FQ1IDBDT = FQWREC.FQ1IDBDT; /* beg date*/
  FQAREC.FQ1IDXDT = FQWREC.FQ1IDXDT; /* ebd date*/
  FQAREC.FQWBDFCT = FQWREC.FQWBDFCT; /* beg defect*/
  FQAREC.FQWEDFCT = FQWREC.FQWEDFCT; /* end defect*/

   /* ------------------------------------------------------------*/
   /* Format key.*/
   /* ------------------------------------------------------------*/
  FQ2REC.SY1STNBR = FQWREC.SY1STNBR; /* style*/
  FQ2REC.SY2CLRID = FQWREC.SY2CLRID; /* color*/
  FQ2REC.XDMCD = FQWREC.XDMCD; /* dim*/
  FQ2REC.FY1PFYCD = FQWREC.FY1PFYCD; /* fcty origin*/
  FQ2REC.XDVCD = FQWREC.XDVCD; /* division code*/

   /* ------------------------------------------------------------*/
   /* Open DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Open cursor*/

   /* ------------------------------------------------------------*/
   /* If successful, init switches & return to previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    FQ04WEOC = "N"; /* End of cursor switch*/
    FQ04W00.FQ04WIXM = 1; /* Display line index*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "120"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    set FQ04M001 initial; /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "OPEN CURSOR";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/
  TA1REC.TA1TBLKE = FQ2REC.FQ2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ04120

// Fetch DFDTL
Function FQ04130()
   /* ------------------------------------------------------------*/
   /* Format FQAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "N2"; /* Function code*/

   /* ------------------------------------------------------------*/
   /* Fetch DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Fetch next row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == 100) /* Or row not found*/

      if (SQLCA.VAGen_SQLCODE == 100) /* If row not found*/
        FQ04W00.FQ04WEOC = "Y"; /* Indicate end of cursor*/
      end

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "130"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "FETCH NEXT TABLE ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/
  TA1REC.TA1TBLKE = FQ2REC.FQ2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ04130

// Save data
Function FQ04140()
   /* ------------------------------------------------------------*/
   /* Obtain defect description.*/
   /* ------------------------------------------------------------*/
  if (FQ2REC.YDFCD in YDFTBL.YDFCD)
    FQ04W01.YDFDESC[FQ04WIXM] = YDFTBL.YDFDESC[sysVar.arrayIndex];
  end

   /* ------------------------------------------------------------*/
   /* Save the data for display.*/
   /* ------------------------------------------------------------*/
  FQ04W01.YDFCD[FQ04WIXM] = FQ2REC.YDFCD; /* Defect category*/
  FQ04W01.FQ2DEFQT[FQ04WIXM] = FQ2REC.FQ2DEFQT; /* Defect quantity*/

   /* ------------------------------------------------------------*/
   /* Add to line counter.*/
   /* ------------------------------------------------------------*/
  FQ04W00.FQ04WIXM = FQ04W00.FQ04WIXM + 1; /* Increment line index*/


end // end FQ04140

// Add page to temporary storage
Function FQ04150()
   /* ------------------------------------------------------------*/
   /* Add the next page to temporary storage.*/
   /* ------------------------------------------------------------*/

  FQ04W01.XXXTSLEN = FQ04W00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "A"; /* Function code*/

  try
    call "TA0050" (FQ04W01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, initialize the storage area and indicators*/
   /* to be used.  Return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    FQ04W00.TA5TSQIX = TA5REC.TA5TSQIX; /* Highest page number*/
    TA5REC.TA5TSQIX = 0; /* Reset to 0*/
    FQ04W00.FQ04WIXM = 1; /* Reset to 1*/
    set FQ04W01 empty; /* M001 display storage*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  FQWREC.XXXMODID = "150"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "ADD PAGE TO TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ04150

// Close DFDTL
Function FQ04160()
   /* ------------------------------------------------------------*/
   /* Format fqAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "C2"; /* Function code*/

   /* ------------------------------------------------------------*/
   /* Close DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Close table set*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "160"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "CLOSE CURSOR";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ04160

// Select summed POSUM
Function FQ04170()
   /* ------------------------------------------------------------*/
   /* Clear copybooks.*/
   /* ------------------------------------------------------------*/
  set FQAREC empty; /* Clear copybook*/
  set FQ6REC empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
   /* Format fqAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "SS"; /* Function code*/
  FQAREC.FQ1IDBDT = FQWREC.FQ1IDBDT; /* beg date*/
  FQAREC.FQ1IDXDT = FQWREC.FQ1IDXDT; /* ebd date*/

   /* ------------------------------------------------------------*/
   /* Format key.*/
   /* ------------------------------------------------------------*/
  FQ6REC.SY1STNBR = FQWREC.SY1STNBR; /* style*/
  FQ6REC.SY2CLRID = FQWREC.SY2CLRID; /* color*/
  FQ6REC.XDMCD = FQWREC.XDMCD; /* dim*/
  FQ6REC.FY1FCTYC = FQWREC.FY1PFYCD; /* fcty*/
  FQ6REC.XDVCD = FQWREC.XDVCD; /* division code*/

   /* ------------------------------------------------------------*/
   /* Select summed POSUM.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5930" (FQAREC, SQLCA, FQ6REC) {isNoRefresh = yes, isExternal = yes};
  end /* Open cursor*/

   /* ------------------------------------------------------------*/
   /* If successful or row not found, return to previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0 /* If normal return code*/
     || SQLCA.VAGen_SQLCODE == -305) /* Or row not found*/

      /* ------------------------------------------------------------*/
       /* Save the data for display.*/
      /* ------------------------------------------------------------*/
      FQ04W00.FQ04WTOT = FQ6REC.FQ6ORDQT; /* save amount*/

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "170"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    set FQ04M001 initial; /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT SUMMED";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VPOSUM01"; /* Table view name*/
  TA1REC.TA1TBLKE = FQ6REC.FQ6KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ04170

// Calculate total ratio
Function FQ04180()
   /* ------------------------------------------------------------*/
   /* If the total is greater than zero, calculate ratio.*/
   /* ------------------------------------------------------------*/
  if (FQ04W00.FQ04WTOT > 0) /* If total greater than zero*/

    /* ------------------------------------------------------------*/
     /* Divide line quantity by total production for percent total.*/
    /* ------------------------------------------------------------*/
    FQ04W00.FQ04WFDA = mathLib.round(FQ04W00.FQ04WDTT / FQ04W00.FQ04WTOT, -mathLib.decimals(FQ04W00.FQ04WFDA));

    /* ------------------------------------------------------------*/
     /* Multiply by 100 to get nnn.nnn format.*/
    /* ------------------------------------------------------------*/
    FQ04W00.FQ04WFDA = FQ04W00.FQ04WFDA * 100; /* Times 100*/

    /* ------------------------------------------------------------*/
     /* Save percent in storage.*/
    /* ------------------------------------------------------------*/
    FQ04W00.FQ04WRTT = FQ04W00.FQ04WFDA; /* Tot ratio*/

  end


end // end FQ04180

// Format and converse the map
Function FQ04200()
   /* ------------------------------------------------------------*/
   /* If no error was found, save the error message number.*/
   /* ------------------------------------------------------------*/
  if (converseVar.validationMsgNum != 0) /* If error found*/

    FQWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/
  if (FQ04W00.FQ04WSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (FQ04W00.FQ04WSWF == "Y") /* Map must be formatted*/
      FQ04210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQ04M001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FQ04M001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FQ04M001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (FQWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (FQWREC.XXXMSGNO != 0) /* Message to display*/

      if (FQWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(FQWREC.XXXMSGNO);
      end /* Message number*/
    else
      FQ04M001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    FQ04M001.XXXMODID = FQWREC.XXXMODID; /* Module identification*/

    set FQ04M001 alarm; /* Indicate sound alarm*/

  end

  converse FQ04M001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQWREC.XXXMODID = " "; /* Module identification*/
  FQWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* message text*/

  COMMAREA.CATOAP = FQ04M001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FQ04M001.CAITEM; /* Application data*/


end // end FQ04200

// Load map fields
Function FQ04210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set FQ04M001 initial;

  FQ04W00.FQ04WSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (FQ04W00.TA5TSQIX > 0) /* Pages were formatted*/

    if (FQ04W00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/
      if (FQ04W00.XXXTSPAG > FQ04W00.TA5TSQIX) /* Desired greater highest*/
        FQ04W00.XXXTSPAG = FQ04W00.TA5TSQIX; /* Default to highest*/
      end

      FQ04S02(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/
    FQ04M001.XXXTSPAG = FQ04W00.XXXTSPAG; /* Current page*/
    FQ04M001.TA5TSQIX = FQ04W00.TA5TSQIX; /* Highest page*/

    set FQ04M001.XXXLITPG skip; /* "PAGE" literal*/
    set FQ04M001.XXXLITOF skip; /* "OF" literal*/
    set FQ04M001.XXXTSPAG skip; /* current page*/
    set FQ04M001.TA5TSQIX skip; /* max page*/

  end

   /* ------------------------------------------------------------*/
   /* Format key data in the map.*/
   /* ------------------------------------------------------------*/

  FQ04M001.FY1PFYCD = FQWREC.FY1PFYCD; /* origin factory*/
  FQ04M001.SY1STNBR = FQWREC.SY1STNBR; /* style nbr*/
  FQ04M001.SY2CLRID = FQWREC.SY2CLRID; /* color id*/
  FQ04M001.XDMCD = FQWREC.XDMCD; /* dimension*/
  FQ04M001.XDVCD = FQWREC.XDVCD; /* or division*/

  FQ04M001.FQ04MBDT = FQ04W00.FQ04MBYM; /* beg date*/
  FQ04M001.FQ04MEDT = FQ04W00.FQ04MEYM; /* end date*/

  FQ04M001.FQ04MBDF = FQWREC.FQWBDFCT; /* beg defect*/
  FQ04M001.FQ04MEDF = FQWREC.FQWEDFCT; /* end defect*/
  FQ04M001.YDCDESC = FQWREC.YDCDESC; /* defect cat name*/

  FQ04M001.FQ04WTOT = FQ04W00.FQ04WTOT; /* Total prdctn*/

   /* ------------------------------------------------------------*/
   /* If data exists, make fields visable, calc %s, & set cursor.*/
   /* ------------------------------------------------------------*/
  if (FQ04W01.YDFCD[1] > " ") /* If data exists*/

    set FQ04M001.FQ04MHDR skip; /* make visable*/
    FQ04M001.FQ04WDTT = FQ04W00.FQ04WDTT; /* Total defect*/
    FQ04M001.FQ04WRTT = FQ04W00.FQ04WRTT; /* Total ratio*/

    FQ04211(); /* Calculate ratio*/

    set FQ04M001.FQ04MSEL[1] cursor, normal; /* Position cursor*/
  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  FQ04W00.FQ04WIXM = 1; /* Initialize line index*/

  while (FQ04W00.FQ04WIXM <= 12 /* Until screen is full*/
   && FQ04W01.YDFCD[FQ04WIXM] > " ") /* and data exists*/

    FQ04M001.FQ04MSEL[FQ04WIXM] = FQ04W01.FQ04MSEL[FQ04WIXM];
    FQ04M001.YDFCD[FQ04WIXM] = FQ04W01.YDFCD[FQ04WIXM];
    FQ04M001.YDFDESC[FQ04WIXM] = FQ04W01.YDFDESC[FQ04WIXM];
    FQ04M001.FQ2DEFQT[FQ04WIXM] = FQ04W01.FQ2DEFQT[FQ04WIXM];
    FQ04M001.FQ04WPCT[FQ04WIXM] = FQ04W01.FQ04WPCT[FQ04WIXM];

    /* ------------------------------------------------------------*/
     /* Make line visable.*/
    /* ------------------------------------------------------------*/
    set FQ04M001.FQ04MSEL[FQ04WIXM] normal; /* Make visable*/

    /* ------------------------------------------------------------*/
     /* Add to counter.*/
    /* ------------------------------------------------------------*/
    FQ04W00.FQ04WIXM = FQ04W00.FQ04WIXM + 1; /* Increment line index*/

  end


end // end FQ04210

// Calculate ratio
Function FQ04211()
   /* ------------------------------------------------------------*/
   /* If the total is greater than zero, calculate ratio.*/
   /* ------------------------------------------------------------*/
  if (FQ04W00.FQ04WTOT > 0) /* If total greater than zero*/

    /* ------------------------------------------------------------*/
     /* Calculate the ratio each line is of the total production.*/
    /* ------------------------------------------------------------*/
    FQ04W00.FQ04WIXM = 1; /* Initialize line index*/

    while (FQ04W00.FQ04WIXM <= 12 /* Until screen is full*/
     && FQ04W01.YDFCD[FQ04WIXM] > " ") /* and data exists*/

      /* ------------------------------------------------------------*/
       /* Divide line quantity by total production for ratio total.*/
      /* ------------------------------------------------------------*/
      FQ04W00.FQ04WFDA = mathLib.round(FQ04W01.FQ2DEFQT[FQ04WIXM] / FQ04W00.FQ04WTOT, -mathLib.decimals(FQ04W00.FQ04WFDA));

      /* ------------------------------------------------------------*/
       /* Multiply by 100 to get nnn.nnn format.*/
      /* ------------------------------------------------------------*/
      FQ04W00.FQ04WFDA = FQ04W00.FQ04WFDA * 100; /* Times 100*/

      /* ------------------------------------------------------------*/
       /* Save ratio in storage.*/
      /* ------------------------------------------------------------*/
      FQ04W01.FQ04WPCT[FQ04WIXM] = FQ04W00.FQ04WFDA; /* percent*/

      /* ------------------------------------------------------------*/
       /* Add to counter.*/
      /* ------------------------------------------------------------*/
      FQ04W00.FQ04WIXM = FQ04W00.FQ04WIXM + 1; /* Add to counter*/

    end
  end


end // end FQ04211

// Check attention identifier
Function FQ04300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    if (FQ04W00.FQ04WSWR == "Y") /* Request for data switch*/
      set FQ04W01 empty;
    end

    FQ04W00.FQ04WSWF = "Y"; /* Format the map switch*/
    FQ04W00.FQ04WSWR = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* Fast path to application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to redisplay map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (FQ04M001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      FQ04S01(); /* Purge any temporary storage*/

      XSFQS01(); /* Exit to another application*/

      set FQ04M001.CATOAP cursor, bold; /* Highlight*/

    else
      converseLib.validationFailed(18); /* Indicate PF key not valid*/
      FQWREC.XXXMODID = "300"; /* Process name*/
    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* PF3 was pressed*/
   || converseVar.eventKey is pf4 /* PF4 was pressed*/
   || converseVar.eventKey is pf5 /* PF5 was pressed*/
   || converseVar.eventKey is pf13) /* PF13 was pressed*/
    /* OR EZEAID IS PF15             ; PF15 was pressed*/
    /* OR EZEAID IS PF16             ; PF16 was pressed*/
    /* oR EZEAID IS PF17             ; PF17 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FQ04M001"; /* Move in map name for help*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FQ0C"; /* Set to factory application*/
    end

    if (converseVar.eventKey is pf13) /* PF13 was pressed*/
      COMMAREA.CATOAP = "FQ0B"; /* Set to Nature Dfct Cat Sum*/
    end

    if (converseVar.eventKey is pf15) /* PF15 was pressed*/
      COMMAREA.CATOAP = "FQ0D"; /* Set to customer screen*/
    end

    if (converseVar.eventKey is pf16) /* PF16 was pressed*/
      COMMAREA.CATOAP = "FQ0E"; /* Set to customer ratio screen*/
    end

    if (converseVar.eventKey is pf17) /* PF17 was pressed*/
      COMMAREA.CATOAP = "FQ0F"; /* Set to warehouse screen*/
    end

    FQ04S01(); /* Purge any temporary storage*/

    XSFQS01(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf9)  /* and PF9 was not pressed*/

    FQWREC.XXXMODID = "300"; /* Process name*/
    converseLib.validationFailed(4); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end FQ04300

// Reset highlighted fields
Function FQ04400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to defined intensity.*/
   /* ------------------------------------------------------------*/
  set FQ04M001.CATOAP initialAttributes; /* TO application*/
  set FQ04M001.CAITEM initialAttributes; /* Application data*/

  set FQ04M001.SY1STNBR initialAttributes; /* style nbr*/
  set FQ04M001.SY2CLRID initialAttributes; /* color id*/
  set FQ04M001.XDMCD initialAttributes; /* dimension*/
  set FQ04M001.XDVCD initialAttributes; /* division*/
  set FQ04M001.FY1PFYCD initialAttributes; /* origin factory*/

  set FQ04M001.FQ04MBDT initialAttributes; /* beg date*/
  set FQ04M001.FQ04MEDT initialAttributes; /* end date*/

  set FQ04M001.FQ04MBDF initialAttributes; /* beg defect*/
  set FQ04M001.FQ04MEDF initialAttributes; /* end defect*/

   /* ------------------------------------------------------------*/
   /* Loop through lines, setting lines with data visable.*/
   /* ------------------------------------------------------------*/
  FQ04W00.FQ04WIXM = 1; /* Initialize line index*/

  while (FQ04W00.FQ04WIXM <= 12 /* Until end of lines*/
   && FQ04W01.YDFCD[FQ04WIXM] > " ") /* and data exists*/

    set FQ04M001.FQ04MSEL[FQ04WIXM] normal; /* Reset select flag*/

    FQ04W00.FQ04WIXM = FQ04W00.FQ04WIXM + 1; /* Increment line index*/

  end

   /* ------------------------------------------------------------*/
   /* If no lists were requested, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ04M001.XDVCD == "?"    /* division list requested*/
   || FQ04M001.FQ04MBDF == "?" /* defect list requested*/
   || FQ04M001.FQ04MEDF == "?") /* defect list requested*/

    if (converseVar.eventKey not enter) /* ENTER was not pressed*/

      FQWREC.XXXMODID = "400"; /* Module identification*/
      converseLib.validationFailed(24); /* Indicate PF key not valid*/

      exit stack; /* Exit to display map*/
    end

  else
    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of division codes.*/
   /* ------------------------------------------------------------*/
  if (FQ04M001.XDVCD == "?") /* division list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "DIVISION CODES"; /* Heading*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXDV01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FQ04M001.XDVCD = FQWREC.XDVCD; /* Restore prior code*/
    else

      FQ04M001.XDVCD = XDVTBL.XDVCD[XX0XWPIX]; /* Selected code*/
    end

    set FQ04M001.XDVCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of defect codes.*/
   /* ------------------------------------------------------------*/
  if (FQ04M001.FQ04MBDF == "?") /* defect list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "DEFECT CODES"; /* Heading*/
    XX0XW02.XGPCD = FQWREC.XGPCD; /* GPC code*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPYDF01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FQ04M001.FQ04MBDF = FQWREC.YDFCD; /* Restore prior code*/
    else

      FQ04M001.FQ04MBDF = YDFTBL.YDFCD[XX0XWPIX]; /* Selected code*/
    end

    set FQ04M001.FQ04MBDF cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of defect codes.*/
   /* ------------------------------------------------------------*/
  if (FQ04M001.FQ04MEDF == "?") /* defect list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "DEFECT CODES"; /* Heading*/
    XX0XW02.XGPCD = FQWREC.XGPCD; /* GPC code*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPYDF01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FQ04M001.FQ04MEDF = FQWREC.YDFCD; /* Restore prior code*/
    else

      FQ04M001.FQ04MEDF = YDFTBL.YDFCD[XX0XWPIX]; /* Selected code*/
    end

    set FQ04M001.FQ04MEDF cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the requested lists have been displayed and*/
   /* that ENTER must be pressed to process any changes.*/
   /* ------------------------------------------------------------*/

  converseLib.clearScreen();
   /* Clear screen before display*/

  FQWREC.XXXMODID = "400"; /* Module identification*/
  converseLib.validationFailed(25); /* Indicate press ENTER*/

  exit stack; /* Exit to display map*/


end // end FQ04400

// Verify the entered data
Function FQ04500()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (FQ04M001.CAITEM > " ") /* Application data was entered*/

    FQWREC.XXXMODID = "150"; /* Module identification*/
    set FQ04M001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid request to scroll was entered.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    FQ04510(); /* Verify scroll/add request*/

  else

    /* ------------------------------------------------------------*/
     /* Otherwise, verify data in the line.*/
    /* ------------------------------------------------------------*/

    FQ04W00.FQ04WIXM = 12; /* Initialize line index*/

    while (FQ04W00.FQ04WIXM > 0) /* Until end of lines*/

      FQ04520(); /* Verify data in the line*/

      FQ04W00.FQ04WIXM = FQ04W00.FQ04WIXM - 1; /* Decrement line number*/

    end

    /* ------------------------------------------------------------*/
     /* If the search key was changed, verify the key fields.*/
    /* ------------------------------------------------------------*/
    if (FQ04M001.SY1STNBR != FQWREC.SY1STNBR  /* Style*/
     || FQ04M001.SY2CLRID != FQWREC.SY2CLRID  /* Color*/
     || FQ04M001.XDMCD != FQWREC.XDMCD        /* Dimension*/
     || FQ04M001.XDVCD != FQWREC.XDVCD        /* Division*/
     || FQ04M001.FQ04MBDT != FQ04W00.FQ04MBYM /* Beg date*/
     || FQ04M001.FQ04MEDT != FQ04W00.FQ04MEYM /* End date*/
     || FQ04M001.FQ04MBDF != FQWREC.FQWBDFCT  /* Beg defect*/
     || FQ04M001.FQ04MEDF != FQWREC.FQWEDFCT  /* End defect*/
     || FQ04M001.FY1PFYCD != FQWREC.FY1PFYCD  /* Origin factory*/
                                              
     || FQ04M001.SY1STNBR == " "              /* Style*/
     || FQ04M001.SY2CLRID == " "              /* Color*/
     || FQ04M001.XDMCD == " "                 /* Dimension*/
     || FQ04M001.XDVCD == " "                 /* Division*/
     || FQ04M001.FQ04MBDT == 0                /* Beg date*/
     || FQ04M001.FQ04MEDT == 0                /* End date*/
     || FQ04M001.FQ04MBDF == 0                /* Beg defect*/
     || FQ04M001.FQ04MEDF == 0                /* End defect*/
     || FQ04M001.FY1PFYCD == " ")             /* Origin factory*/

      FQ04530(); /* Verify style/color/dim*/
      FQ04540(); /* Verify division*/
      FQ04550(); /* Verify dates*/
      FQ04560(); /* Verify defect codes*/
      FQ04570(); /* Verify factory*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end FQ04500

// Verify scroll
Function FQ04510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && FQ04W00.XXXTSPAG < 2)       /* and no previous pages*/

    FQWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(13); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && FQ04W00.XXXTSPAG >= FQ04W00.TA5TSQIX) /* and no more pages*/

    FQWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(14); /* No more pages to display*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate an error for any data that was modified.*/
   /* ------------------------------------------------------------*/

  FQ04W00.FQ04WIXM = 12; /* Initialize line index*/

  while (FQ04W00.FQ04WIXM > 0) /* Until end of lines*/

    if (FQ04M001.FQ04MSEL[FQ04WIXM] != " " /* If selection was made*/
     && FQ04M001.FQ04MSEL[FQ04WIXM] != "V") /* and line not reviewed*/

      FQWREC.XXXMODID = "510"; /* Module identification*/
      set FQ04M001.FQ04MSEL[FQ04WIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(9); /* Indicate data may not change*/

    end

    FQ04W00.FQ04WIXM = FQ04W00.FQ04WIXM - 1; /* Decrement line number*/

  end


end // end FQ04510

// Verify selection
Function FQ04520()
   /* ------------------------------------------------------------*/
   /* If selection was made, ensure an "S" & no prior selections.*/
   /* ------------------------------------------------------------*/

  if (FQ04M001.FQ04MSEL[FQ04WIXM] > " ") /* Selection was entered*/

    if (FQ04M001.FQ04MSEL[FQ04WIXM] != "S") /* Valid selection character*/

      FQWREC.XXXMODID = "520"; /* Module identification*/
      set FQ04M001.FQ04MSEL[FQ04WIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(12); /* Indicate only "S" may be used*/

    else
      /* ------------------------------------------------------------*/
       /* Verify that no other selection was made at the same time.*/
      /* ------------------------------------------------------------*/
      FQ04W00.FQ04WIX2 = FQ04W00.FQ04WIXM - 1; /* Set counter*/

      while (FQ04W00.FQ04WIX2 > 0) /* While counter greater than 0*/
        if (FQ04M001.FQ04MSEL[FQ04WIX2] == "S") /* Another selection made*/

          FQWREC.XXXMODID = "520"; /* Module identification*/
          set FQ04M001.FQ04MSEL[FQ04WIXM] cursor, bold; /* Highlight*/
          converseLib.validationFailed(8); /* More than one selection*/
        end

        FQ04W00.FQ04WIX2 = FQ04W00.FQ04WIX2 - 1; /* Decrement counter*/
      end

    end
  end


end // end FQ04520

// Verify style/color/dim
Function FQ04530()
   /* ------------------------------------------------------------*/
    /* If missing, default dimension.*/
   /* ------------------------------------------------------------*/
  if (FQ04M001.XDMCD == " ") /* If dim missing*/
    FQ04M001.XDMCD = "00"; /* Default to '00'*/
  end

   /* ------------------------------------------------------------*/
    /* If missing, default color.*/
   /* ------------------------------------------------------------*/
