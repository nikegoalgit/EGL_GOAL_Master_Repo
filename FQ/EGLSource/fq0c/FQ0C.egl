// Select FCTY
Function FQ0C561()
   /* ------------------------------------------------------------*/
    /* Clear copybook.*/
   /* ------------------------------------------------------------*/
  set FY1REC empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
    /* Format key.*/
   /* ------------------------------------------------------------*/
  FY1REC.XGPCD = FQWREC.XGPCD; /* GPC code*/
  FY1REC.FY1FCTYC = FQ0CM001.FY1PFYCD; /* Factory*/

   /* ------------------------------------------------------------*/
   /* Select FCTY.*/
   /* ------------------------------------------------------------*/
  try
    call "IO2810" ("S ", SQLCA, FY1REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == +0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == +100) /* row not found*/

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not fqailable, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL Commarea*/

  FQWREC.XXXMODID = "561"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not fqailable*/

    converseLib.validationFailed(2); /* Indicate not fqailable*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFCTY  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VFCTY001"; /* Table view name*/
  TA1REC.TA1TBLKE = FY1REC.FY1KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0C561

// Process valid input request
Function FQ0C600()
   /* ------------------------------------------------------------*/
   /* Indicate that the map must be formatted before display.*/
   /* ------------------------------------------------------------*/
  FQ0CW00.FQ0CWSWF = "Y"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    set FQ0CM001 initial;
    FQ0CW00.XXXTSPAG = FQ0CW00.XXXTSPAG - 1; /* Decrement page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    set FQ0CM001 initial;
    FQ0CW00.XXXTSPAG = FQ0CW00.XXXTSPAG + 1; /* Increment page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If the search key was changed, save the search information.*/
   /* ------------------------------------------------------------*/
  if (FQ0CM001.FY1PFYCD != FQWREC.FY1PFYCD  /* or factory changed*/
   || FQ0CM001.FQ0CMBDT != FQ0CW00.FQ0CMBYM /* or beg date changed*/
   || FQ0CM001.FQ0CMEDT != FQ0CW00.FQ0CMEYM /* or end date changed*/
   || FQ0CM001.SY1STNBR != FQWREC.SY1STNBR  /* or style nbr*/
   || FQ0CM001.SY2CLRID != FQWREC.SY2CLRID  /* or color id*/
   || FQ0CM001.XDMCD != FQWREC.XDMCD        /* or dimension*/
   || FQ0CM001.XDVCD != FQWREC.XDVCD)       /* or division*/

    /* ------------------------------------------------------------*/
     /* Save data.*/
    /* ------------------------------------------------------------*/
    FQWREC.FY1PFYCD = FQ0CM001.FY1PFYCD; /* origin factory*/
    FQWREC.SY1STNBR = FQ0CM001.SY1STNBR; /* style nbr*/
    FQWREC.SY2CLRID = FQ0CM001.SY2CLRID; /* color id*/
    FQWREC.XDMCD = FQ0CM001.XDMCD; /* dimension*/
    FQWREC.XDVCD = FQ0CM001.XDVCD; /* division*/

    FQWREC.FQ1IDBDT = FQZREC.FQZBCYMM; /* beg date*/
    FQWREC.FQ1IDXDT = FQZREC.FQZECYMM; /* end date*/

    FQ0CW00.FQ0CMBDT = FQZREC.FQZBCYMM; /* beg date*/
    FQ0CW00.FQ0CMEDT = FQZREC.FQZECYMM; /* end date*/

    FQ0CW00.FQ0CWSWR = "Y"; /* Request for data switch*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If PF9 was pressed, transfer to print screen.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf9) /* If PF9 was pressed*/

    /* ------------------------------------------------------------*/
     /* Transfer to FQ0G.*/
    /* ------------------------------------------------------------*/
    COMMAREA.CATOAP = "FQ0G"; /* TO application*/
    FQ0C610(); /* Transfer to FQ0G*/

  end

   /* ------------------------------------------------------------*/
   /* If line was selected, transfer to FQ04.*/
   /* ------------------------------------------------------------*/
  if ("S" in FQ0CM001.FQ0CMSEL) /* Style selected*/

    FQWREC.YDCCD = FQ0CM001.YDCCD[sysVar.arrayIndex]; /* defect category*/
    FQWREC.YDCDESC = FQ0CM001.YDCDESC[sysVar.arrayIndex]; /* def cat name*/
    COMMAREA.CATOAP = "FQ04"; /* TO application*/

    FQ0C610(); /* Transfer to FQ04*/

  end


end // end FQ0C600

// Transfer to another appl
Function FQ0C610()
   /* ------------------------------------------------------------*/
   /* Purge any temporary storage.*/
   /* ------------------------------------------------------------*/
  FQ0CS01(); /* Purge temporary storage*/

   /* ------------------------------------------------------------*/
   /* Transfer control to another application*/
   /* ------------------------------------------------------------*/
  XSFQS01(); /* Exit to another application*/

   /* ------------------------------------------------------------*/
   /* If the request failed, display the returned error message.*/
   /* ------------------------------------------------------------*/

  COMMAREA.CATOAP = " "; /* TO application*/

  exit stack; /* Exit to display map*/


end // end FQ0C610

// Purge any temporary storage
Function FQ0CS01()
   /* ------------------------------------------------------------*/
   /* Initialize the storage areas and indicators to be used.*/
   /* ------------------------------------------------------------*/

  set FQ0CW01 empty; /* M001 display storage*/

  TA5REC.TA5TSQIX = 0; /* Current page number*/
  FQ0CW00.TA5TSQIX = 0; /* Highest page number*/

  FQ0CW00.FQ0CWTOT = 0; /* Total production*/
  FQ0CW00.FQ0CWDTT = 0; /* Total defect*/
  FQ0CW00.FQ0CWRTT = 0; /* Total ratio*/

   /* ------------------------------------------------------------*/
   /* Purge any temporary storage for this application.*/
   /* ------------------------------------------------------------*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "P"; /* Function code*/

  try
    call "TA0050" (FQ0CW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, return to previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  FQWREC.XXXMODID = "S01"; /* Module identification*/
  converseLib.validationFailed(1); /* System error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "PURGE ANY TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0CS01

// Read temporary storage page
Function FQ0CS02()
   /* ------------------------------------------------------------*/
   /* Obtain the next page of data to display.*/
   /* ------------------------------------------------------------*/

  FQ0CW01.XXXTSLEN = FQ0CW00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5TSQIX = FQ0CW00.XXXTSPAG; /* Page number*/
  TA5REC.TA5FNCCD = "R"; /* Function code*/

  try
    call "TA0050" (FQ0CW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  FQWREC.XXXMODID = "S02"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "READ PAGE FROM TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0CS02

