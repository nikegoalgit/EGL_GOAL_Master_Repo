//*** PROGRAM=FQ0C ****
// This application allows users to view defect categories,
// descriptions, quantities, and ratio of total production qu-
// antity of the entered set of parameters.
// 
// The user must enter 'FACTORY' and 'STYLE'.  'COLOR', 'DIVI-
// SION', 'PO MONTH RANGE' and 'DIMENSION' will default if left
// blank.  The program will obtain a list of all defect cate-
// gories, the quantity of defects for the category, the
// description of the category, and will calculate a ratio for
// each category's total defects to all the product producted
// for the entered parameters.
// 
// The user may select a particular category and be transfered
// to FQ04, FCTY PROD/DFCT RATIO SUMMARY.  There he will be
// able to see more details concerning a category.
// 
// The user may scroll forward and backward to view all the
// categories by pressing PF7 and PF8.  He may exit any to any
// allowed application, via FAST PATH or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      05/08/91  RHETRI  New application.
//          12/05/91  RHETRI  Chg rcv'd amt to on-order amt
//          06/24/93  MHUYNH  Regen due to change to XSFQS05
// y2kproj  12-03-97  oricha  y2k modifications:  regen to pick
//                            up xx0xs01 changes & to pick up
//                            xsfqs05 century calc changes.
// *********************
Program FQ0C type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "FQ00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  FQ0CW00 FQ0CW00; // record
  FQ0CW01 FQ0CW01; // record
  FQ2REC FQ2REC; // record
  FQ6REC FQ6REC; // record
  FQAREC FQAREC; // record
  FQWREC FQWREC; // record
  FQZREC FQZREC; // record
  FY1REC FY1REC; // record
  PG2REC PG2REC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TA6REC TA6REC; // record
  TAEREC TAEREC; // record
  XX0XW01 XX0XW01; // record
  XX0XW02 XX0XW02; // record
  YDCREC YDCREC; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XDMTBL; // table
  use XDVTBL; // table
  use YDCTBL; // table
  use FQ0CM.FQ0CM001,  // forms
      FQ0CM.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPFQ001: XPFQ001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/
    set FQ0CM001 initial; /* M001 input/output*/
    set FQ0CW00 empty; /* Working storage*/
    set FQ0CW01 empty; /* M001 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/

    FQ0CW00.FQ0CWSWF = "Y"; /* Format the map flag*/

     /* ------------------------------------------------------------*/
     /* If complete key is present, request data. (Defaults exist.)*/
     /* ------------------------------------------------------------*/
    if (FQWREC.FY1PFYCD > " " /* If origin factory exists*/
     && FQWREC.SY1STNBR > " ") /* and style number exists*/

      FQ0CW00.FQ0CMBDT = FQWREC.FQ1IDBDT; /* Beg date*/
      FQ0CW00.FQ0CMEDT = FQWREC.FQ1IDXDT; /* End date*/

      FQ0CW00.FQ0CWSWR = "Y"; /* Request data*/
    else

      FQ0CW00.FQ0CWSWR = "N"; /* data not requested*/
    end

     /* ------------------------------------------------------------*/
     /* Clear out defect code range.*/
     /* ------------------------------------------------------------*/
    FQWREC.FQWBDFCT = " "; /* Begin defect code*/
    FQWREC.FQWEDFCT = " "; /* End defect code*/

     /* ------------------------------------------------------------*/
     /* Flow to check for default dates.*/
     /* ------------------------------------------------------------*/
    goto FQ0C000; /* Check for default dates*/


    FQ0C000: FQ0C000();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FQ0C001; /* Converse the main process*/


    FQ0C001: FQ0C001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FQ0C001; /* Converse the main process*/


  end // end main
end // end FQ0C

// Check for default dates
Function FQ0C000()
   /* ------------------------------------------------------------*/
   /* If date range is missing, call date routine for defaults.*/
   /* ------------------------------------------------------------*/
  if (FQWREC.FQ1IDBDT == 0 /* If begin date is missing*/
   || FQWREC.FQ1IDXDT == 0) /* Or end date is missing*/

    /* ------------------------------------------------------------*/
     /* Clear copybook.*/
    /* ------------------------------------------------------------*/
    set FQZREC empty; /* clear date edit record*/

    FQZREC.FQZBEGRG = FQWREC.FQ1IDBDT; /* Beg date*/
    FQZREC.FQZENDRG = FQWREC.FQ1IDXDT; /* end date*/

    /* ------------------------------------------------------------*/
     /* Edit range.  Send YYMMMM - receive back CCYYMMMM.*/
    /* ------------------------------------------------------------*/
    XSFQS05(); /* Common date edit routine*/

    /* ------------------------------------------------------------*/
     /* Move dates into FQWREC.*/
    /* ------------------------------------------------------------*/
    FQWREC.FQ1IDBDT = FQZREC.FQZBCYMM; /* Begin date*/
    FQWREC.FQ1IDXDT = FQZREC.FQZECYMM; /* End date*/

    FQ0CW00.FQ0CMBDT = FQZREC.FQZBCYMM; /* Begin date*/
    FQ0CW00.FQ0CMEDT = FQZREC.FQZECYMM; /* End date*/

  else

    FQ0CW00.FQ0CMBDT = FQWREC.FQ1IDBDT; /* Beg date*/
    FQ0CW00.FQ0CMEDT = FQWREC.FQ1IDXDT; /* End date*/

  end


end // end FQ0C000

// Main process to display a map
Function FQ0C001()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  FQ0C100(); /* Obtain the data to display*/

  FQ0C200(); /* Format and display the map*/

  FQ0C300(); /* Check attention identifier*/

  FQ0C400(); /* Reset fields to normal intensity*/

  FQ0C500(); /* Verify the entered data*/

  FQ0C600(); /* Process valid input request*/


end // end FQ0C001

// Obtain the data to display
Function FQ0C100()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ0CW00.FQ0CWSWR != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* ------------------------------------------------------------*/
  FQ0CW00.XXXTSLEN = 455; /* Temporary storage page length*/

   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/
  FQ0CS01(); /* Purge temporary storage*/

   /* --------------------------------------------------------*/
   /* Open DFDTL.*/
   /* --------------------------------------------------------*/
  FQ0C110(); /* Open DFDTL*/

   /* ----------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ----------------------------------------------------------*/
  while (FQ0CW00.FQ0CWEOC == "N") /* Until end of cursor*/

    FQ0C120(); /* Fetch DFDTL*/

    if (FQ0CW00.FQ0CWEOC == "N") /* If row found*/

      /* ----------------------------------------------------------*/
       /* Add quantity to defect total.*/
      /* ----------------------------------------------------------*/
      FQ0CW00.FQ0CWDTT = FQ0CW00.FQ0CWDTT + FQ2REC.FQ2DEFQT;

      /* ----------------------------------------------------------*/
       /* Save data for display.*/
      /* ----------------------------------------------------------*/
      FQ0C130(); /* Save data*/

      /* ---------------------------------------------------------*/
       /* If the page is full, add the page to the storage queue.*/
      /* ---------------------------------------------------------*/
      if (FQ0CW00.FQ0CWIXM > 13) /* Page is full*/
        FQ0C140(); /* Add page to temporary storage*/

      end
    end
  end

   /* ----------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ----------------------------------------------------*/
  if (FQ0CW00.FQ0CWIXM > 1) /* If line count greater than 1*/
    FQ0C140(); /* Add page to temp storage*/

  end

   /* ------------------------------------------------------------*/
   /* Close DFDTL*/
   /* ------------------------------------------------------------*/
  FQ0C150(); /* Close DFDTL*/

   /* ------------------------------------------------------------*/
   /* Select Sum POSUM*/
   /* ------------------------------------------------------------*/
  FQ0C160(); /* Select Sum POSUM*/

   /* ------------------------------------------------------------*/
   /* If no data to display, indicate no data to display.*/
   /* ------------------------------------------------------------*/
  if (FQ0CW00.TA5TSQIX == 0) /* No pages formatted*/

    FQWREC.XXXMODID = "100"; /* Module identification*/
    converseLib.validationFailed(17); /* No data was found to display*/

    return; /* Return to previous process*/

  else
    /* ------------------------------------------------------------*/
     /* Calculate total ratio*/
    /* ------------------------------------------------------------*/
    FQ0C170(); /* Calculate total ratio*/

  end

   /* ---------------------------------------------------------*/
   /* If no message to display, reset the desired page number.*/
   /* ---------------------------------------------------------*/
  if (FQWREC.XXXMSGNO == 0) /* No message to display*/
    FQ0CW00.XXXTSPAG = 1; /* Desired page number*/
  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/
  FQ0CW00.FQ0CWSWR = "N"; /* Request for data switch*/


end // end FQ0C100

// Open DFDTL
Function FQ0C110()
   /* ------------------------------------------------------------*/
   /* Clear copybooks.*/
   /* ------------------------------------------------------------*/
  set FQAREC empty; /* Clear copybook*/
  set FQ2REC empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
   /* Format fqAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "S1"; /* Function code*/
  FQAREC.FQ1IDBDT = FQWREC.FQ1IDBDT; /* beg date*/
  FQAREC.FQ1IDXDT = FQWREC.FQ1IDXDT; /* ebd date*/

   /* ------------------------------------------------------------*/
   /* Format key.*/
   /* ------------------------------------------------------------*/
  FQ2REC.SY1STNBR = FQWREC.SY1STNBR; /* style*/
  FQ2REC.SY2CLRID = FQWREC.SY2CLRID; /* color*/
  FQ2REC.XDMCD = FQWREC.XDMCD; /* dim*/
  FQ2REC.FY1PFYCD = FQWREC.FY1PFYCD; /* fcty origin*/
  FQ2REC.XDVCD = FQWREC.XDVCD; /* division code*/

   /* ------------------------------------------------------------*/
   /* Open DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Open cursor*/

   /* ------------------------------------------------------------*/
   /* If successful, init switches & return to previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    FQ0CWEOC = "N"; /* End of cursor switch*/
    FQ0CW00.FQ0CWIXM = 1; /* Display line index*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "110"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    set FQ0CM001 initial; /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "OPEN CURSOR";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/
  TA1REC.TA1TBLKE = FQ2REC.FQ2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0C110

// Fetch DFDTL
Function FQ0C120()
   /* ------------------------------------------------------------*/
   /* Format FQAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "N1"; /* Function code*/

   /* ------------------------------------------------------------*/
   /* Fetch DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Fetch next row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == 100) /* Or row not found*/

      if (SQLCA.VAGen_SQLCODE == 100) /* If row not found*/
        FQ0CW00.FQ0CWEOC = "Y"; /* Indicate end of cursor*/
      end

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "120"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "FETCH NEXT TABLE ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/
  TA1REC.TA1TBLKE = FQ2REC.FQ2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0C120

// Save data
Function FQ0C130()
   /* ------------------------------------------------------------*/
   /* Obtain defect category description.*/
   /* ------------------------------------------------------------*/

  if (FQ2REC.YDCCD in YDCTBL.YDCCD)
    FQ0CW01.YDCDESC[FQ0CWIXM] = YDCTBL.YDCDESC[sysVar.arrayIndex];
  end

   /* ------------------------------------------------------------*/
   /* Save the data for display.*/
   /* ------------------------------------------------------------*/
  FQ0CW01.YDCCD[FQ0CWIXM] = FQ2REC.YDCCD; /* Defect category*/
  FQ0CW01.FQ2DEFQT[FQ0CWIXM] = FQ2REC.FQ2DEFQT; /* Defect quantity*/

   /* ------------------------------------------------------------*/
   /* Add to line counter.*/
   /* ------------------------------------------------------------*/
  FQ0CW00.FQ0CWIXM = FQ0CW00.FQ0CWIXM + 1; /* Increment line index*/


end // end FQ0C130

// Add page to temporary storage
Function FQ0C140()
   /* ------------------------------------------------------------*/
   /* Add the next page to temporary storage.*/
   /* ------------------------------------------------------------*/

  FQ0CW01.XXXTSLEN = FQ0CW00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "A"; /* Function code*/

  try
    call "TA0050" (FQ0CW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, initialize the storage area and indicators*/
   /* to be used.  Return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    FQ0CW00.TA5TSQIX = TA5REC.TA5TSQIX; /* Highest page number*/
    TA5REC.TA5TSQIX = 0; /* Reset to 0*/
    FQ0CW00.FQ0CWIXM = 1; /* Reset to 1*/
    set FQ0CW01 empty; /* M001 display storage*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  FQWREC.XXXMODID = "140"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "ADD PAGE TO TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0C140

// Close DFDTL
Function FQ0C150()
   /* ------------------------------------------------------------*/
   /* Format fqAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "C1"; /* Function code*/

   /* ------------------------------------------------------------*/
   /* Close DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Close table set*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "150"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "CLOSE CURSOR";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0C150

// Select Sum POSUM
Function FQ0C160()
   /* ------------------------------------------------------------*/
   /* Clear copybooks.*/
   /* ------------------------------------------------------------*/
  set FQAREC empty; /* Clear copybook*/
  set FQ6REC empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
   /* Format fqAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "SS"; /* Function code*/
  FQAREC.FQ1IDBDT = FQWREC.FQ1IDBDT; /* beg date*/
  FQAREC.FQ1IDXDT = FQWREC.FQ1IDXDT; /* ebd date*/

   /* ------------------------------------------------------------*/
   /* Format key.*/
   /* ------------------------------------------------------------*/
  FQ6REC.SY1STNBR = FQWREC.SY1STNBR; /* style*/
  FQ6REC.SY2CLRID = FQWREC.SY2CLRID; /* color*/
  FQ6REC.XDMCD = FQWREC.XDMCD; /* dim*/
  FQ6REC.FY1FCTYC = FQWREC.FY1PFYCD; /* fcty*/
  FQ6REC.XDVCD = FQWREC.XDVCD; /* division code*/

   /* ------------------------------------------------------------*/
   /* Select summed POSUM.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5930" (FQAREC, SQLCA, FQ6REC) {isNoRefresh = yes, isExternal = yes};
  end /* Open cursor*/

   /* ------------------------------------------------------------*/
   /* If successful or row not found, return to previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0) /* If normal return code*/

      /* ------------------------------------------------------------*/
       /* Save the data for display.*/
      /* ------------------------------------------------------------*/
      FQ0CW00.FQ0CWTOT = FQ6REC.FQ6ORDQT; /* save amount*/

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "160"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    set FQ0CM001 initial; /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT SUMMED";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VPOSUM01"; /* Table view name*/
  TA1REC.TA1TBLKE = FQ6REC.FQ6KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0C160

// Calculate total ratio
Function FQ0C170()
   /* ------------------------------------------------------------*/
   /* If the total is greater than zero, calculate ratio.*/
   /* ------------------------------------------------------------*/
  if (FQ0CW00.FQ0CWTOT > 0) /* If total greater than zero*/

    /* ------------------------------------------------------------*/
     /* Divide line quantity by total production for percent total.*/
    /* ------------------------------------------------------------*/
    FQ0CW00.FQ0CWFDA = mathLib.round(FQ0CW00.FQ0CWDTT / FQ0CW00.FQ0CWTOT, -mathLib.decimals(FQ0CW00.FQ0CWFDA));

    /* ------------------------------------------------------------*/
     /* Multiply by 100 to get nnn.nnn format.*/
    /* ------------------------------------------------------------*/
    FQ0CW00.FQ0CWFDA = FQ0CW00.FQ0CWFDA * 100; /* Times 100*/

    /* ------------------------------------------------------------*/
     /* Save percent in storage.*/
    /* ------------------------------------------------------------*/
    FQ0CW00.FQ0CWRTT = FQ0CW00.FQ0CWFDA; /* Tot ratio*/

  end


end // end FQ0C170

// Format and converse the map
Function FQ0C200()
   /* ------------------------------------------------------------*/
   /* If no error was found, save the error message number.*/
   /* ------------------------------------------------------------*/
  if (converseVar.validationMsgNum != 0) /* If error found*/

    FQWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/
  if (FQ0CW00.FQ0CWSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (FQ0CW00.FQ0CWSWF == "Y") /* Map must be formatted*/
      FQ0C210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQ0CM001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FQ0CM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FQ0CM001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (FQWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (FQWREC.XXXMSGNO != 0) /* Message to display*/

      if (FQWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(FQWREC.XXXMSGNO);
      end /* Message number*/
    else
      FQ0CM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    FQ0CM001.XXXMODID = FQWREC.XXXMODID; /* Module identification*/

    set FQ0CM001 alarm; /* Indicate sound alarm*/

  end

  converse FQ0CM001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQWREC.XXXMODID = " "; /* Module identification*/
  FQWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* message text*/

  COMMAREA.CATOAP = FQ0CM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FQ0CM001.CAITEM; /* Application data*/


end // end FQ0C200

// Load map fields
Function FQ0C210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set FQ0CM001 initial;

  FQ0CW00.FQ0CWSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (FQ0CW00.TA5TSQIX > 0) /* Pages were formatted*/

    if (FQ0CW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/
      if (FQ0CW00.XXXTSPAG > FQ0CW00.TA5TSQIX) /* Desired greater highest*/
        FQ0CW00.XXXTSPAG = FQ0CW00.TA5TSQIX; /* Default to highest*/
      end

      FQ0CS02(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/
    FQ0CM001.XXXTSPAG = FQ0CW00.XXXTSPAG; /* Current page*/
    FQ0CM001.TA5TSQIX = FQ0CW00.TA5TSQIX; /* Highest page*/

    set FQ0CM001.XXXLITPG skip; /* "PAGE" literal*/
    set FQ0CM001.XXXLITOF skip; /* "OF" literal*/
    set FQ0CM001.XXXTSPAG skip; /* current page*/
    set FQ0CM001.TA5TSQIX skip; /* max page*/

  end

   /* ------------------------------------------------------------*/
   /* Format key data in the map.*/
   /* ------------------------------------------------------------*/

  FQ0CM001.FY1PFYCD = FQWREC.FY1PFYCD; /* origin factory*/
  FQ0CM001.SY1STNBR = FQWREC.SY1STNBR; /* style nbr*/
  FQ0CM001.SY2CLRID = FQWREC.SY2CLRID; /* color id*/
  FQ0CM001.XDMCD = FQWREC.XDMCD; /* dimension*/
  FQ0CM001.XDVCD = FQWREC.XDVCD; /* or division*/

  FQ0CM001.FQ0CMBDT = FQ0CW00.FQ0CMBYM; /* beg date*/
  FQ0CM001.FQ0CMEDT = FQ0CW00.FQ0CMEYM; /* end date*/

  FQ0CM001.FQ0CWTOT = FQ0CW00.FQ0CWTOT; /* Total prdctn*/

   /* ------------------------------------------------------------*/
   /* If data exists, make fields visable, calc %s, & set cursor.*/
   /* ------------------------------------------------------------*/
  if (FQ0CW01.YDCCD[1] > " ") /* If data exists*/

    set FQ0CM001.FQ0CMHDR skip; /* make visable*/
    FQ0CM001.FQ0CWDTT = FQ0CW00.FQ0CWDTT; /* Total defect*/
    FQ0CM001.FQ0CWRTT = FQ0CW00.FQ0CWRTT; /* Total ratio*/

    FQ0C211(); /* Calculate ratio*/

    set FQ0CM001.FQ0CMSEL[1] cursor, normal; /* Position cursor*/
  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  FQ0CW00.FQ0CWIXM = 1; /* Initialize line index*/

  while (FQ0CW00.FQ0CWIXM <= 13 /* Until screen is full*/
   && FQ0CW01.YDCCD[FQ0CWIXM] > " ") /* and data exists*/

    FQ0CM001.FQ0CMSEL[FQ0CWIXM] = FQ0CW01.FQ0CMSEL[FQ0CWIXM];
    FQ0CM001.YDCCD[FQ0CWIXM] = FQ0CW01.YDCCD[FQ0CWIXM];
    FQ0CM001.YDCDESC[FQ0CWIXM] = FQ0CW01.YDCDESC[FQ0CWIXM];
    FQ0CM001.FQ2DEFQT[FQ0CWIXM] = FQ0CW01.FQ2DEFQT[FQ0CWIXM];
    FQ0CM001.FQ0CWPCT[FQ0CWIXM] = FQ0CW01.FQ0CWPCT[FQ0CWIXM];

    /* ------------------------------------------------------------*/
     /* Make line visable.*/
    /* ------------------------------------------------------------*/
    set FQ0CM001.FQ0CMSEL[FQ0CWIXM] normal; /* Make visable*/

    /* ------------------------------------------------------------*/
     /* Add to counter.*/
    /* ------------------------------------------------------------*/
    FQ0CW00.FQ0CWIXM = FQ0CW00.FQ0CWIXM + 1; /* Increment line index*/

  end


end // end FQ0C210

// Calculate ratio
Function FQ0C211()
   /* ------------------------------------------------------------*/
   /* If the total is greater than zero, calculate ratio.*/
   /* ------------------------------------------------------------*/
  if (FQ0CW00.FQ0CWTOT > 0) /* If total greater than zero*/

    /* ------------------------------------------------------------*/
     /* Calculate the ratio each line is of the total production.*/
    /* ------------------------------------------------------------*/
    FQ0CW00.FQ0CWIXM = 1; /* Initialize line index*/

    while (FQ0CW00.FQ0CWIXM <= 13 /* Until screen is full*/
     && FQ0CW01.YDCCD[FQ0CWIXM] > " ") /* and data exists*/

      /* ------------------------------------------------------------*/
       /* Divide line quantity by total production for ratio total.*/
      /* ------------------------------------------------------------*/
      FQ0CW00.FQ0CWFDA = mathLib.round(FQ0CW01.FQ2DEFQT[FQ0CWIXM] / FQ0CW00.FQ0CWTOT, -mathLib.decimals(FQ0CW00.FQ0CWFDA));

      /* ------------------------------------------------------------*/
       /* Multiply by 100 to get nnn.nnn format.*/
      /* ------------------------------------------------------------*/
      FQ0CW00.FQ0CWFDA = FQ0CW00.FQ0CWFDA * 100; /* Times 100*/

      /* ------------------------------------------------------------*/
       /* Save ratio in storage.*/
      /* ------------------------------------------------------------*/
      FQ0CW01.FQ0CWPCT[FQ0CWIXM] = FQ0CW00.FQ0CWFDA; /* ratio*/

      /* ------------------------------------------------------------*/
       /* Add to counter.*/
      /* ------------------------------------------------------------*/
      FQ0CW00.FQ0CWIXM = FQ0CW00.FQ0CWIXM + 1; /* Add to counter*/

    end
  end


end // end FQ0C211

// Check attention identifier
Function FQ0C300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    if (FQ0CW00.FQ0CWSWR == "Y") /* Request for data switch*/
      set FQ0CW01 empty;
    end

    FQ0CW00.FQ0CWSWF = "Y"; /* Format the map switch*/
    FQ0CW00.FQ0CWSWR = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* Fast path to application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to redisplay map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (FQ0CM001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      FQ0CS01(); /* Purge any temporary storage*/

      XSFQS01(); /* Exit to another application*/

      set FQ0CM001.CATOAP cursor, bold; /* Highlight*/

    else
      converseLib.validationFailed(18); /* Indicate PF key not valid*/
      FQWREC.XXXMODID = "300"; /* Process name*/
    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* PF3 was pressed*/
   || converseVar.eventKey is pf4 /* PF4 was pressed*/
   || converseVar.eventKey is pf5 /* PF5 was pressed*/
   || converseVar.eventKey is pf13) /* PF13 was pressed*/
    /* OR EZEAID IS PF15             ; PF15 was pressed*/
    /* OR EZEAID IS PF16             ; PF16 was pressed*/
    /* OR EZEAID IS PF17             ; PF17 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FQ0CM001"; /* Move in map name for help*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FQ0M"; /* Set to factory application*/
    end

    if (converseVar.eventKey is pf13) /* PF13 was pressed*/
      COMMAREA.CATOAP = "FQ0B"; /* Set to factory screen*/
    end

    if (converseVar.eventKey is pf15) /* PF15 was pressed*/
      COMMAREA.CATOAP = "FQ0D"; /* Set to customer screen*/
    end

    if (converseVar.eventKey is pf16) /* PF16 was pressed*/
      COMMAREA.CATOAP = "FQ0E"; /* Set to customer ratio screen*/
    end

    if (converseVar.eventKey is pf17) /* PF17 was pressed*/
      COMMAREA.CATOAP = "FQ0F"; /* Set to warehouse screen*/
    end

    FQ0CS01(); /* Purge any temporary storage*/

    XSFQS01(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf9)  /* and PF9 was not pressed*/

    FQWREC.XXXMODID = "300"; /* Process name*/
    converseLib.validationFailed(4); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end FQ0C300

// Reset highlighted fields
Function FQ0C400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to defined intensity.*/
   /* ------------------------------------------------------------*/
  set FQ0CM001.CATOAP initialAttributes; /* TO application*/
  set FQ0CM001.CAITEM initialAttributes; /* Application data*/

  set FQ0CM001.SY1STNBR initialAttributes; /* style nbr*/
  set FQ0CM001.SY2CLRID initialAttributes; /* color id*/
  set FQ0CM001.XDMCD initialAttributes; /* dimension*/
  set FQ0CM001.XDVCD initialAttributes; /* division*/
  set FQ0CM001.FY1PFYCD initialAttributes; /* origin factory*/

  set FQ0CM001.FQ0CMBDT initialAttributes; /* beg date*/
  set FQ0CM001.FQ0CMEDT initialAttributes; /* end date*/

   /* ------------------------------------------------------------*/
   /* Loop setting lines with data visable.*/
   /* ------------------------------------------------------------*/
  FQ0CW00.FQ0CWIXM = 1; /* Initialize line index*/

  while (FQ0CW00.FQ0CWIXM <= 13 /* Until end of lines*/
   && FQ0CW01.YDCCD[FQ0CWIXM] > " ") /* and data exists*/

    set FQ0CM001.FQ0CMSEL[FQ0CWIXM] normal; /* Reset select flag*/

    FQ0CW00.FQ0CWIXM = FQ0CW00.FQ0CWIXM + 1; /* Increment line index*/

  end

   /* ------------------------------------------------------------*/
   /* If no lists were requested, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ0CM001.XDVCD == "?") /* division list requested*/

    if (converseVar.eventKey not enter) /* ENTER was not pressed*/

      FQWREC.XXXMODID = "400"; /* Module identification*/
      converseLib.validationFailed(24); /* Indicate PF key not valid*/

      exit stack; /* Exit to display map*/
    end

  else
    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of division codes.*/
   /* ------------------------------------------------------------*/
  if (FQ0CM001.XDVCD == "?") /* division list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "DIVISION CODES"; /* Heading*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXDV01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FQ0CM001.XDVCD = FQWREC.XDVCD; /* Restore prior code*/
    else

      FQ0CM001.XDVCD = XDVTBL.XDVCD[XX0XWPIX]; /* Selected code*/
    end

    set FQ0CM001.XDVCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the requested lists have been displayed and*/
   /* that ENTER must be pressed to process any changes.*/
   /* ------------------------------------------------------------*/

  converseLib.clearScreen();
   /* Clear screen before display*/

  FQWREC.XXXMODID = "400"; /* Module identification*/
  converseLib.validationFailed(25); /* Indicate press ENTER*/

  exit stack; /* Exit to display map*/


end // end FQ0C400

// Verify the entered data
Function FQ0C500()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (FQ0CM001.CAITEM > " ") /* Application data was entered*/

    FQWREC.XXXMODID = "150"; /* Module identification*/
    set FQ0CM001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid request to scroll was entered.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    FQ0C510(); /* Verify scroll/add request*/

  else

    /* ------------------------------------------------------------*/
     /* Otherwise, verify data in the line.*/
    /* ------------------------------------------------------------*/

    FQ0CW00.FQ0CWIXM = 13; /* Initialize line index*/

    while (FQ0CW00.FQ0CWIXM > 0) /* Until end of lines*/

      FQ0C520(); /* Verify data in the line*/

      FQ0CW00.FQ0CWIXM = FQ0CW00.FQ0CWIXM - 1; /* Decrement line number*/

    end

    /* ------------------------------------------------------------*/
     /* If the search key was changed, verify the key fields.*/
    /* ------------------------------------------------------------*/
    if (FQ0CM001.SY1STNBR != FQWREC.SY1STNBR  /* Style*/
     || FQ0CM001.SY2CLRID != FQWREC.SY2CLRID  /* Color*/
     || FQ0CM001.XDMCD != FQWREC.XDMCD        /* Dimension*/
     || FQ0CM001.XDVCD != FQWREC.XDVCD        /* Division*/
     || FQ0CM001.FQ0CMBDT != FQ0CW00.FQ0CMBYM /* Beg date*/
     || FQ0CM001.FQ0CMEDT != FQ0CW00.FQ0CMEYM /* End date*/
     || FQ0CM001.FY1PFYCD != FQWREC.FY1PFYCD  /* Origin factory*/
                                              
     || FQ0CM001.SY1STNBR == " "              /* Style*/
     || FQ0CM001.SY2CLRID == " "              /* Color*/
     || FQ0CM001.XDMCD == " "                 /* Dimension*/
     || FQ0CM001.XDVCD == " "                 /* Division*/
     || FQ0CM001.FQ0CMBDT == 0                /* Beg date*/
     || FQ0CM001.FQ0CMEDT == 0                /* End date*/
     || FQ0CM001.FY1PFYCD == " ")             /* Origin factory*/

      FQ0C530(); /* Verify style/color/dim*/
      FQ0C540(); /* Verify division*/
      FQ0C550(); /* Verify dates*/
      FQ0C560(); /* Verify factory*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end FQ0C500

// Verify scroll
Function FQ0C510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && FQ0CW00.XXXTSPAG < 2)       /* and no previous pages*/

    FQWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(13); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && FQ0CW00.XXXTSPAG >= FQ0CW00.TA5TSQIX) /* and no more pages*/

    FQWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(14); /* No more pages to display*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate an error for any data that was modified.*/
   /* ------------------------------------------------------------*/

  FQ0CW00.FQ0CWIXM = 13; /* Initialize line index*/

  while (FQ0CW00.FQ0CWIXM > 0) /* Until end of lines*/

    if (FQ0CM001.FQ0CMSEL[FQ0CWIXM] != " " /* If selection was made*/
     && FQ0CM001.FQ0CMSEL[FQ0CWIXM] != "V") /* and line not reviewed*/

      FQWREC.XXXMODID = "510"; /* Module identification*/
      set FQ0CM001.FQ0CMSEL[FQ0CWIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(9); /* Indicate data may not change*/

    end

    FQ0CW00.FQ0CWIXM = FQ0CW00.FQ0CWIXM - 1; /* Decrement line number*/

  end


end // end FQ0C510

// Verify selection
Function FQ0C520()
   /* ------------------------------------------------------------*/
   /* If selection was made, ensure an "S" & no prior selections.*/
   /* ------------------------------------------------------------*/

  if (FQ0CM001.FQ0CMSEL[FQ0CWIXM] > " ") /* Selection was entered*/

    if (FQ0CM001.FQ0CMSEL[FQ0CWIXM] != "S") /* Valid selection character*/

      FQWREC.XXXMODID = "520"; /* Module identification*/
      set FQ0CM001.FQ0CMSEL[FQ0CWIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(12); /* Indicate only "S" may be used*/

    else
      /* ------------------------------------------------------------*/
       /* Verify that no other selection was made at the same time.*/
      /* ------------------------------------------------------------*/
      FQ0CW00.FQ0CWIX2 = FQ0CW00.FQ0CWIXM - 1; /* Set counter*/

      while (FQ0CW00.FQ0CWIX2 > 0) /* While counter greater than 0*/
        if (FQ0CM001.FQ0CMSEL[FQ0CWIX2] == "S") /* Another selection made*/

          FQWREC.XXXMODID = "520"; /* Module identification*/
          set FQ0CM001.FQ0CMSEL[FQ0CWIXM] cursor, bold; /* Highlight*/
          converseLib.validationFailed(8); /* More than one selection*/
        end

        FQ0CW00.FQ0CWIX2 = FQ0CW00.FQ0CWIX2 - 1; /* Decrement counter*/
      end

    end
  end


end // end FQ0C520

// Verify style/color/dim
Function FQ0C530()
   /* ------------------------------------------------------------*/
    /* If missing, default dimension.*/
   /* ------------------------------------------------------------*/
  if (FQ0CM001.XDMCD == " ") /* If dim missing*/
    FQ0CM001.XDMCD = "00"; /* Default to '00'*/
  end

   /* ------------------------------------------------------------*/
    /* If missing, default color.*/
   /* ------------------------------------------------------------*/
  if (FQ0CM001.SY2CLRID == " ") /* If color missing*/
    FQ0CM001.SY2CLRID = "00"; /* Default to '00'*/
  end

   /* ------------------------------------------------------------*/
    /* Verify style/color/dimension.*/
   /* ------------------------------------------------------------*/
  if (FQ0CM001.SY1STNBR != " ") /* Style*/

    /* ------------------------------------------------------------*/
     /* Select SYCLR.*/
    /* ------------------------------------------------------------*/
    FQ0C531(); /* Select SYCLR*/

    /* ------------------------------------------------------------*/
     /* If row was not found, indicate error.*/
    /* ------------------------------------------------------------*/
    if (SQLCA.VAGen_SQLCODE != 0) /* If row was found*/

      set FQ0CM001.SY1STNBR cursor, bold; /* Highlight*/
      set FQ0CM001.SY2CLRID bold; /* Highlight*/
      set FQ0CM001.XDMCD bold; /* Highlight*/
      converseLib.validationFailed(33); /* Indicate data missing or invalid*/
      FQWREC.XXXMODID = "530"; /* Module identification*/

    end
  else

    set FQ0CM001.SY1STNBR cursor, bold; /* Highlight*/
    set FQ0CM001.SY2CLRID bold; /* Highlight*/
    set FQ0CM001.XDMCD bold; /* Highlight*/
    converseLib.validationFailed(33); /* Indicate data missing or invalid*/
    FQWREC.XXXMODID = "530"; /* Module identification*/

  end


end // end FQ0C530

// Select SYCLR
Function FQ0C531()
   /* ------------------------------------------------------------*/
    /* Clear copybook.*/
   /* ------------------------------------------------------------*/
  set PG2REC empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
    /* Format key.*/
   /* ------------------------------------------------------------*/
  PG2REC.XGPCD = FQWREC.XGPCD; /* GPC code*/
  PG2REC.SY1STNBR = FQ0CM001.SY1STNBR; /* Style number*/
  PG2REC.SY2CLRID = FQ0CM001.SY2CLRID; /* Color ID*/
  PG2REC.XDMCD = FQ0CM001.XDMCD; /* Dimension*/

   /* ------------------------------------------------------------*/
   /* Select SYCLR.*/
   /* ------------------------------------------------------------*/
  try
    call "IO3120" ("S ", SQLCA, PG2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == +0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == +100) /* row not found*/

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not fqailable, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL Commarea*/

  FQWREC.XXXMODID = "531"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not fqailable*/

    converseLib.validationFailed(2); /* Indicate not fqailable*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DPRODG  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VSYCLR01"; /* Table view name*/
  TA1REC.TA1TBLKE = PG2REC.PG2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0C531

// Verify division
Function FQ0C540()
   /* ------------------------------------------------------------*/
   /* Verify division code.*/
   /* ------------------------------------------------------------*/
  if (FQ0CM001.XDVCD == " ") /* If division missing*/
    FQ0CM001.XDVCD = "*"; /* Default to wildcard*/
  end

  if (FQ0CM001.XDVCD == "*") /* If division wildcarded*/
    return; /* Return to previous process*/
  end

  if (FQ0CM001.XDVCD != " ") /* If division entered*/

    if (FQ0CM001.XDVCD in XDVTBL.XDVCD /* If division in table*/
     && XDVTBL.XXXSTAT == "A")         /* and code active*/

       /* next sentence*/
    else

      /* ------------------------------------------------------------*/
       /* If not found, indicate missing or invalid.*/
      /* ------------------------------------------------------------*/
      set FQ0CM001.XDVCD cursor, bold; /* Highlight*/
      converseLib.validationFailed(31); /* Indicate missing or invalid*/
      FQWREC.XXXMODID = "540"; /* Module identification*/

    end
  else

    set FQ0CM001.XDVCD cursor, bold; /* Highlight*/
    converseLib.validationFailed(31); /* Indicate missing or invalid*/
    FQWREC.XXXMODID = "540"; /* Module identification*/

  end


end // end FQ0C540

// Verify dates
Function FQ0C550()
   /* ------------------------------------------------------------*/
   /* Initialize record for calls to date routine.*/
   /* ------------------------------------------------------------*/
  set FQZREC empty; /* clear date edit record*/

  FQZREC.FQZBEGRG = FQ0CM001.FQ0CMBDT; /* Beg date*/
  FQZREC.FQZENDRG = FQ0CM001.FQ0CMEDT; /* end date*/
  FQZREC.FQZCHK24 = "Y"; /* Switch for range*/
  FQZREC.FQZNUMYR = 2; /* Number of years*/

   /* ------------------------------------------------------------*/
   /* Edit range.  Send YYMMMM - receive back CCYYMMMM.*/
   /* ------------------------------------------------------------*/
  XSFQS05(); /* Common date edit routine*/

   /* ------------------------------------------------------------*/
   /* Check return codes for ending date.*/
   /* ------------------------------------------------------------*/
  if (FQZREC.FQZENDRC == 0) /* end date was valid*/
    FQ0CM001.FQ0CMEDT = FQZREC.FQZENDRG; /* End date*/

  else
    set FQ0CM001.FQ0CMEDT cursor, bold; /* Highlight*/
    FQWREC.XXXMODID = "550"; /* module identification*/

    if (FQZREC.FQZENDRC == 1) /* End date was invalid*/
      converseLib.validationFailed(34); /* Indicate invalid*/

    else
      converseLib.validationFailed(27); /* Indicate out of range*/

    end
  end

   /* ------------------------------------------------------------*/
   /* Check return codes for beginning date.*/
   /* ------------------------------------------------------------*/
  if (FQZREC.FQZBEGRC == 0) /* Beg date was valid*/
    FQ0CM001.FQ0CMBDT = FQZREC.FQZBEGRG; /* Begin date*/

  else
    FQWREC.XXXMODID = "550"; /* module identification*/
    set FQ0CM001.FQ0CMBDT cursor, bold; /* Highlight*/

    if (FQZREC.FQZBEGRC == 1) /* Beg date was invalid*/
      converseLib.validationFailed(34); /* Indicate invalid*/

    else
      converseLib.validationFailed(27); /* Indicate out of range*/

    end
  end


end // end FQ0C550

