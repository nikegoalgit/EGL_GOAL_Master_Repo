//*** PROGRAM=FQ0H ****
// This application allows a user to inquire and maintain rows
// in the parent defect codes DB2 table, DFQUAL.VPNTDF01.
// 
// There are two different screens that may be displayed.
// The first screen will display all the rows in the table.
// When a row is selected on this screen, the second screen is
// displayed, showing only information for that table row.
// The user may update the status for this row from active to
// inactive.  After changing the status, ENTER must be pressed
// update the data base.
// 
// After entering the first screen, the user may press PF18
// and the second screen will appear with no data. He/she may
// fill in the information for a row, press ENTER, and add the
// row to the data base.
// 
// When the user exits the second screen (via PF3 key) the
// the first screen is redisplayed, indicating with a "v"
// that the row was viewed.
// 
// The user may scroll forward and backward to view all the
// rows by pressing PF7 and PF8.  He may exit to any allowed
// application, via FAST PATH or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
//          09/14/92  MHUYNH  New application.
// y2kproj  12-08-97  oricha  y2k modifications:  regen to pick
//                            up xx0xs01 changes.
// *********************
Program FQ0H type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "FQ00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  FQ0HW00 FQ0HW00; // record
  FQ0HW01 FQ0HW01; // record
  FQ0HW02 FQ0HW02; // record
  FQWREC FQWREC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VPNTDF01 VPNTDF01; // record
  XX0XW01 XX0XW01; // record
  XX0XW02 XX0XW02; // record
  YDFREC YDFREC; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YDFTBL; // table
  use FQ0HM.FQ0HM001,  // forms
      FQ0HM.FQ0HM002, 
      FQ0HM.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPFQ001: XPFQ001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/

    set FQ0HM001 initial; /* M001 input/output*/
    set FQ0HM002 initial; /* M002 input/output*/
    set FQ0HW00 empty; /* Working storage*/
    set FQ0HW01 empty; /* M001 display storage*/
    set FQ0HW02 empty; /* M002 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set TA5REC empty; /* Temporary storage parameters*/
    set XX0XW01 empty; /* Date/time subroutine parameters*/

    FQ0HW00.FQ0HWMAP = "1"; /* Map to be displayed switch*/
    FQ0HW00.FQ0HWSWF = "Y"; /* Format the map switch*/
    FQ0HW00.FQ0HWSWR = "Y"; /* Request for data switch*/
    FQ0HW00.FQ0HWASW = "N"; /* Add function request switch (PF18)*/

     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FQ0H001; /* Converse the main process*/


    FQ0H001: FQ0H001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FQ0H001; /* Converse the main process*/


  end // end main
end // end FQ0H

// Main process to display a map
Function FQ0H001()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) If specified, display the first map.*/
   /* 2) Otherwise, if specified, display the second map.*/
   /* 3) Otherwise, indicate error and exit to Application Menu.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  if (FQ0HW00.FQ0HWMAP == "1") /* Map 1 was requested*/
    FQ0H100(); /* Display map 1*/
  else

    if (FQ0HW00.FQ0HWMAP == "2") /* Map 2 was requested*/
      FQ0H200(); /* Display map 2*/
    else

      /* ------------------------------------------------------------*/
       /* Write the error to the Error Diagnosis Reporting Facility.*/
      /* ------------------------------------------------------------*/
      FQWREC.XXXMODID = "001"; /* Module identification*/
      FQWREC.XXXMSGNO = 10; /* Unexpected error occurred*/

      TA1REC.TA1FUNC = "RETN"; /* Termination function*/
      TA1REC.TA1TYPE = "APPL"; /* Type of error*/
      TA1REC.TA1LOC = "INVALID MAP NUMBER WAS REQUESTED";
      TA1REC.TA1MAPNO = FQ0HW00.FQ0HWMAP; /* Map number*/
      TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
      TA1REC.TA1PGMNO = FQWREC.XXXMODID; /* Program number*/

      call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

      /* ------------------------------------------------------------*/
       /* Exit to display the application system menu.*/
      /* ------------------------------------------------------------*/

      COMMAREA.CATOAP = "FQ0M"; /* TO application*/
      XSFQS01(); /* Exit to another application*/
      /* ------------------------------------------------------------*/
       /* Abnormally terminate the transaction if the exit failed.*/
      /* ------------------------------------------------------------*/

      TA1REC.TA1FUNC = "ABND"; /* Termination function*/
      call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/
    end
  end


end // end FQ0H001

// List the rows in the table
Function FQ0H100()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  FQ0H110(); /* Obtain the data to display*/

  FQ0H120(); /* Format and display the map*/

  FQ0H130(); /* Check attention identifier*/

  FQ0H140(); /* Reset fields to normal intensity*/

  FQ0H150(); /* Verify the entered data*/

  FQ0H160(); /* Process valid input request*/


end // end FQ0H100

// Obtain the data to display
Function FQ0H110()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ0HW00.FQ0HWSWR != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* ------------------------------------------------------------*/
  FQ0HW00.XXXTSLEN = 320; /* Temporary storage page length*/

   /* ------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* ------------------------------------------------------------*/

  if (FQWREC.XXXMSGNO == 0) /* No message to display*/
    FQ0HW00.XXXTSPAG = 1; /* Page number to display*/
  end

   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/
  FQ0HS01(); /* Purge any temporary storage*/

   /* --------------------------------------------------------*/
   /* Open DEFEX.*/
   /* --------------------------------------------------------*/
  FQ0H111(); /* Open DEFEX*/

   /* ----------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ----------------------------------------------------------*/
  while (FQ0HW00.FQ0HWEOC == "N") /* Until end of cursor*/

    FQ0H112(); /* Fetch DEFEX*/

    if (FQ0HW00.FQ0HWEOC == "N") /* If not end-of-cursor*/

      /* ----------------------------------------------------------*/
       /* Save data for display.*/
      /* ----------------------------------------------------------*/
      FQ0H113(); /* Save data*/

      /* ---------------------------------------------------------*/
       /* If the page is full, add the page to the storage queue.*/
      /* ---------------------------------------------------------*/
      if (FQ0HW00.FQ0HWIXM > 16) /* Page is full*/
        FQ0H114(); /* Add page to temporary storage*/
      end

    end
  end

   /* ----------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ----------------------------------------------------*/
  if (FQ0HW00.FQ0HWIXM > 1) /* Page was formatted*/
    FQ0H114(); /* Add page to temporary storage*/
  end

   /* ------------------------------------------------------------*/
   /* Close DEFEX*/
   /* ------------------------------------------------------------*/
  FQ0H115(); /* Close DEFEX*/

   /* ------------------------------------------------------------*/
   /* If no pages formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/

  if (FQ0HW00.TA5TSQIX == 0) /* No pages formatted*/

    FQWREC.XXXMODID = "110"; /* Module identification*/
    converseLib.validationFailed(17); /* No data was found to display*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  FQ0HW00.FQ0HWSWR = "N"; /* Request for data switch*/


end // end FQ0H110

// Open DFQUAL.VPNTDF01
Function FQ0H111()
   /* ------------------------------------------------------------*/
   /* Clear copybook.  NOTE: there is no key for this call.*/
   /* ------------------------------------------------------------*/
  set VPNTDF01 empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
   /* Open DEFEX.*/
   /* ------------------------------------------------------------*/
  try
    call "IO6520" ("S1", SQLCA, VPNTDF01) {isNoRefresh = yes, isExternal = yes};
  end /* Select table set*/

   /* ------------------------------------------------------------*/
   /* If successful, init switches & return to previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    FQ0HWEOC = "N"; /* End of cursor switch*/
    FQ0HW00.FQ0HWIXM = 1; /* Display line index*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "111"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    set FQ0HM001 initial; /* Clear screen before display*/

  end


  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT SET OF TABLE ROWS";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL   "; /* Data base name*/
  TA1REC.TA1TBLVU = "VPNTDF01"; /* Table view name*/
  TA1REC.TA1TBLKE = VPNTDF01.VPNTDF01-KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0H111

// Fetch DFQUAL.VPNTDF01
Function FQ0H112()
   /* ------------------------------------------------------------*/
   /* Fetch PNTDF.*/
   /* ------------------------------------------------------------*/
  try
    call "IO6520" ("N1", SQLCA, VPNTDF01) {isNoRefresh = yes, isExternal = yes};
  end /* Fetch next row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == 100) /* Or row not found*/

      if (SQLCA.VAGen_SQLCODE == 100) /* If row not found*/
        FQ0HW00.FQ0HWEOC = "Y"; /* Indicate end of cursor*/
      end

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "112"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "FETCH NEXT TABLE ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL   "; /* ===> Data base name*/
  TA1REC.TA1TBLVU = "VPNTDF01"; /* ===> Table view name*/

  TA1REC.TA1TBLKE = VPNTDF01.VPNTDF01-KEY; /* ===>Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0H112

// Save data for display
Function FQ0H113()
   /* ------------------------------------------------------------*/
   /* Save the data for display.*/
   /* ------------------------------------------------------------*/

  FQ0HW01.YDFCD[FQ0HWIXM] = VPNTDF01.YDFCD; /* Defect*/
  FQ0HW01.FQ-SUM-DEF-IVC-CD[FQ0HWIXM] = VPNTDF01.FQ-SUM-DEF-IVC-CD;
                                                             /* Parent code*/
  FQ0HW01.XXXSTAT[FQ0HWIXM] = VPNTDF01.XXXSTAT; /* Status*/
  FQ0HW01.ZZZSDT[FQ0HWIXM] = VPNTDF01.ZZZSDT; /* Setup date*/
  FQ0HW01.ZZZCHGDT[FQ0HWIXM] = VPNTDF01.ZZZCHGDT; /* Change date*/
  FQ0HW01.ZZZCHGTM[FQ0HWIXM] = VPNTDF01.ZZZCHGTM; /* Change time*/

   /* ------------------------------------------------------------*/
   /* Add to line counter.*/
   /* ------------------------------------------------------------*/
  FQ0HW00.FQ0HWIXM = FQ0HW00.FQ0HWIXM + 1; /* Increment line index*/


end // end FQ0H113

// Add page to temporary storage
Function FQ0H114()
   /* ------------------------------------------------------------*/
   /* Add the next page to temporary storage.*/
   /* ------------------------------------------------------------*/

  FQ0HW01.XXXTSLEN = FQ0HW00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "A"; /* Function code*/

  try
    call "TA0050" (FQ0HW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, initialize the storage area and indicators*/
   /* to be used.  Return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    FQ0HW00.TA5TSQIX = TA5REC.TA5TSQIX; /* Highest page number*/
    TA5REC.TA5TSQIX = 0; /* Reset to 0*/
    FQ0HW00.FQ0HWIXM = 1; /* Reset to 1*/
    set FQ0HW01 empty; /* M001 display storage*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  FQWREC.XXXMODID = "114"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "ADD PAGE TO TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0H114

// Close DFQUAL.VPNTDF01
Function FQ0H115()
   /* ------------------------------------------------------------*/
   /* Close PNTDF.*/
   /* ------------------------------------------------------------*/
  try
    call "IO6520" ("C1", SQLCA, VPNTDF01) {isNoRefresh = yes, isExternal = yes};
  end /* Close table set*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "115"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "CLOSE SELECTED SET OF TABLE ROWS";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL   "; /* Data base name*/
  TA1REC.TA1TBLVU = "VPNTDF01"; /* Table view name*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0H115

// Format and converse the map
Function FQ0H120()
   /* ------------------------------------------------------------*/
   /* If error was found, save the error message number.*/
   /* ------------------------------------------------------------*/
  if (converseVar.validationMsgNum != 0) /* If error found*/

    FQWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/
  if (FQ0HW00.FQ0HWSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (FQ0HW00.FQ0HWSWF == "Y") /* Map must be formatted*/
      FQ0H121(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQ0HM001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FQ0HM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FQ0HM001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (FQWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (FQWREC.XXXMSGNO != 0) /* Message to display*/

      if (FQWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(FQWREC.XXXMSGNO);
      end /* Message number*/
    else
      FQ0HM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    FQ0HM001.XXXMODID = FQWREC.XXXMODID; /* Module identification*/

    set FQ0HM001 alarm; /* Indicate sound alarm*/

  end

  converse FQ0HM001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQWREC.XXXMODID = " "; /* Module identification*/
  FQWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/

  COMMAREA.CATOAP = FQ0HM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FQ0HM001.CAITEM; /* Application data*/


end // end FQ0H120

// Format the display data
Function FQ0H121()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set FQ0HM001 initial;

  FQ0HW00.FQ0HWSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (FQ0HW00.TA5TSQIX > 0) /* Pages were formatted*/

    if (FQ0HW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the current page has been changed, update the page in*/
       /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/

      if (FQ0HW00.FQ0HWSWU == "Y") /* Current page was changed*/

        FQ0HS02(); /* Update temporary storage page*/

        FQ0HW00.FQ0HWSWU = "N"; /* Storage update switch*/

      end

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/
      if (FQ0HW00.XXXTSPAG > FQ0HW00.TA5TSQIX) /* Desired greater highest*/
        FQ0HW00.XXXTSPAG = FQ0HW00.TA5TSQIX; /* Default to highest*/
      end

      FQ0HS03(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/
    FQ0HM001.XXXTSPAG = FQ0HW00.XXXTSPAG; /* Current page*/
    FQ0HM001.TA5TSQIX = FQ0HW00.TA5TSQIX; /* Highest page*/
    set FQ0HM001.XXXLITPG skip; /* "PAGE" literal*/
    set FQ0HM001.XXXLITOF skip; /* "OF" literal*/

  end
   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  if (FQ0HW01.XXXSTAT[1] > " ") /* If data exists*/
    set FQ0HM001.FQ0HMSEL[1] cursor, normal; /* Position cursor*/
  end

  FQ0HW00.FQ0HWIXM = 1; /* Initialize line index*/

  while (FQ0HW00.FQ0HWIXM <= 16 /* Until screen is full*/
   && FQ0HW01.XXXSTAT[FQ0HWIXM] > " ") /* and data exists*/

    FQ0HM001.FQ0HMSEL[FQ0HWIXM] = FQ0HW01.FQ0HWSEL[FQ0HWIXM];
    FQ0HM001.YDFCD[FQ0HWIXM] = FQ0HW01.YDFCD[FQ0HWIXM];
    FQ0HM001.FQ-SUM-DEF-IVC-CD[FQ0HWIXM] = FQ0HW01.FQ-SUM-DEF-IVC-CD[FQ0HWIXM];
    FQ0HM001.XXXSTAT[FQ0HWIXM] = FQ0HW01.XXXSTAT[FQ0HWIXM];

    /* ------------------------------------------------------------*/
     /* Make line visable.*/
    /* ------------------------------------------------------------*/
    set FQ0HM001.FQ0HMSEL[FQ0HWIXM] normal; /* Make visable*/

    /* ------------------------------------------------------------*/
     /* Add to counter.*/
    /* ------------------------------------------------------------*/
    FQ0HW00.FQ0HWIXM = FQ0HW00.FQ0HWIXM + 1; /* Increment line index*/

  end


end // end FQ0H121

// Check attention identifier
Function FQ0H130()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    if (FQ0HW00.FQ0HWSWR == "Y") /* Request for data switch*/
      set FQ0HW01 empty;
    end

    FQ0HW00.FQ0HWSWF = "Y"; /* Format the map switch*/
    FQ0HW00.FQ0HWSWR = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* Fast path to application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to redisplay map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (FQ0HM001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      FQ0HS01(); /* Purge any temporary storage*/

      XSFQS01(); /* Exit to another application*/

      set FQ0HM001.CATOAP cursor, bold; /* Highlight*/

    else
      FQWREC.XXXMSGNO = 18; /* Indicate PF key not valid*/
      FQWREC.XXXMODID = "130"; /* Process name*/

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1  /* PF1 was pressed*/
   || converseVar.eventKey is pf3  /* PF3 was pressed*/
   || converseVar.eventKey is pf4  /* PF4 was pressed*/
   || converseVar.eventKey is pf5  /* PF5 was pressed*/
   || converseVar.eventKey is pf13 /* PF13 was pressed*/
   || converseVar.eventKey is pf14 /* PF14 was pressed*/
   || converseVar.eventKey is pf15 /* PF15 was pressed*/
   || converseVar.eventKey is pf16 /* PF16 was pressed*/
   || converseVar.eventKey is pf17) /* PF17 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FQ0HM001"; /* Move in map name for help*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FQ0M"; /* Set to factory application*/
    end

    if (converseVar.eventKey is pf5) /* PF5 was pressed*/
      COMMAREA.CATOAP = "FQ0M"; /* Set to factory application*/
    end

    if (converseVar.eventKey is pf13) /* PF13 was pressed*/
      COMMAREA.CATOAP = "FQ0B"; /* Set to warehouse screen*/
    end

    if (converseVar.eventKey is pf14) /* PF14 was pressed*/
      COMMAREA.CATOAP = "FQ0C"; /* Set to factory screen*/
    end

    if (converseVar.eventKey is pf15) /* PF15 was pressed*/
      COMMAREA.CATOAP = "FQ0D"; /* Set to warehouse ratio scn*/
    end

    if (converseVar.eventKey is pf16) /* PF16 was pressed*/
      COMMAREA.CATOAP = "FQ0E"; /* Set to customer ratio screen*/
    end

    if (converseVar.eventKey is pf17) /* PF17 was pressed*/
      COMMAREA.CATOAP = "FQ0F"; /* Set to warehouse*/
    end

    FQ0HS01(); /* Purge any temporary storage*/

    XSFQS01(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf18) /* and PF18 was not pressed*/

    FQWREC.XXXMODID = "130"; /* Process name*/
    converseLib.validationFailed(4); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end FQ0H130

// Reset highlighted fields
Function FQ0H140()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set FQ0HM001.CATOAP initialAttributes; /* TO application*/
  set FQ0HM001.CAITEM initialAttributes; /* Application data*/

  FQ0HW00.FQ0HWIXM = 1; /* Initialize line index*/

  while (FQ0HW00.FQ0HWIXM <= 16 /* Until end of lines*/
   && FQ0HM001.XXXSTAT[FQ0HWIXM] > " ") /* and data exists*/

    set FQ0HM001.FQ0HMSEL[FQ0HWIXM] normal; /* Reset select flag*/

    FQ0HW00.FQ0HWIXM = FQ0HW00.FQ0HWIXM + 1; /* Increment line index*/

  end


end // end FQ0H140

// Verify the entered data
Function FQ0H150()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (FQ0HM001.CAITEM > " ") /* Application data was entered*/

    FQWREC.XXXMODID = "150"; /* Module identification*/
    set FQ0HM001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid request to scroll was entered.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    FQ0H151(); /* Verify scroll/add request*/

  else

    /* ------------------------------------------------------------*/
     /* Otherwise, verify data in the line.*/
    /* ------------------------------------------------------------*/

    FQ0HW00.FQ0HWIXM = 16; /* Initialize line index*/

    while (FQ0HW00.FQ0HWIXM > 0) /* Until end of lines*/

      FQ0H152(); /* Verify data in the line*/

      FQ0HW00.FQ0HWIXM = FQ0HW00.FQ0HWIXM - 1; /* Decrement line number*/

    end

  end

   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end FQ0H150

// Verify scroll request
Function FQ0H151()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && FQ0HW00.XXXTSPAG < 2)       /* and no previous pages*/

    FQWREC.XXXMODID = "FQ0H151"; /* Module identification*/
    converseLib.validationFailed(13); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && FQ0HW00.XXXTSPAG >= FQ0HW00.TA5TSQIX) /* and no more pages*/

    FQWREC.XXXMODID = "151"; /* Module identification*/
    converseLib.validationFailed(14); /* No more pages to display*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate an error for any data that was modified.*/
   /* ------------------------------------------------------------*/

  FQ0HW00.FQ0HWIXM = 16; /* Initialize line index*/

  while (FQ0HW00.FQ0HWIXM > 0) /* Until end of lines*/

    if (FQ0HM001.FQ0HMSEL[FQ0HWIXM] != " " /* If selection was made*/
     && FQ0HM001.FQ0HMSEL[FQ0HWIXM] != "V") /* and line not reviewed*/

      FQWREC.XXXMODID = "151"; /* Module identification*/
      set FQ0HM001.FQ0HMSEL[FQ0HWIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(9); /* Indicate data may not change*/

    end

    FQ0HW00.FQ0HWIXM = FQ0HW00.FQ0HWIXM - 1; /* Decrement line number*/

  end


end // end FQ0H151

// Verify selection
Function FQ0H152()
   /* ------------------------------------------------------------*/
   /* If selection was made, ensure an "S" & no prior selections.*/
   /* ------------------------------------------------------------*/

  if (FQ0HM001.FQ0HMSEL[FQ0HWIXM] > " ") /* Selection was entered*/

    if (FQ0HM001.FQ0HMSEL[FQ0HWIXM] != "S" /* Valid selection character*/
     && FQ0HM001.FQ0HMSEL[FQ0HWIXM] != "V") /* Valid reviewed character*/

      FQWREC.XXXMODID = "152"; /* Module identification*/
      set FQ0HM001.FQ0HMSEL[FQ0HWIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(12); /* Indicate only "S" may be used*/

    else

      if (FQ0HM001.FQ0HMSEL[FQ0HWIXM] != "V") /* If not reviewed*/
        /* ------------------------------------------------------------*/
         /* Verify that no other selection was made at the same time.*/
        /* ------------------------------------------------------------*/
        FQ0HW00.FQ0HWIX2 = FQ0HW00.FQ0HWIXM - 1; /* Set counter*/

        while (FQ0HW00.FQ0HWIX2 > 0) /* While counter greater than 0*/
          if (FQ0HM001.FQ0HMSEL[FQ0HWIX2] == "S") /* Another selection made*/

            FQWREC.XXXMODID = "152"; /* Module identification*/
            set FQ0HM001.FQ0HMSEL[FQ0HWIXM] cursor, bold; /* Highlight*/
            converseLib.validationFailed(8); /* More than one selection*/
          end

          FQ0HW00.FQ0HWIX2 = FQ0HW00.FQ0HWIX2 - 1; /* Decrement counter*/
        end

      end
    end
  end


end // end FQ0H152

// Process valid input request
Function FQ0H160()
   /* ------------------------------------------------------------*/
   /* Indicate that the map must be formatted before display.*/
   /* ------------------------------------------------------------*/
  FQ0HW00.FQ0HWSWF = "Y"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    set FQ0HM001 initial;
    FQ0HW00.XXXTSPAG = FQ0HW00.XXXTSPAG - 1; /* Decrement page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    set FQ0HM001 initial;
    FQ0HW00.XXXTSPAG = FQ0HW00.XXXTSPAG + 1; /* Increment page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If line selected, set switches for MAP2 and update temp*/
   /* storage.  EZETST contains pointer for selected line.*/
   /* ------------------------------------------------------------*/
  if ("S" in FQ0HM001.FQ0HMSEL) /* Line was selected*/

    FQ0HW00.FQ0HWMAP = "2"; /* Map to be displayed switch*/
    FQ0HW00.FQ0HWSWR = "Y"; /* Request for data switch*/
    FQ0HW00.FQ0HWASW = "N"; /* Add function request switch*/

    FQ0HW01.FQ0HWSEL[sysVar.arrayIndex] = "V"; /* Move 'v' for reviewed*/

  end

   /* ------------------------------------------------------------*/
   /* If PF18 was pressed, set add function request switch on.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf18) /* If PF18 was pressed*/

    FQ0HW00.FQ0HWMAP = "2"; /* Map to be displayed switch*/
    FQ0HW00.FQ0HWASW = "Y"; /* Add function request switch*/
    FQ0HW00.FQ0HWSWR = "N"; /* Request for data switch*/

  end


end // end FQ0H160

// Main process to display map2
Function FQ0H200()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  FQ0H210(); /* Obtain the data to display*/

  FQ0H220(); /* Format and display the map*/

  FQ0H230(); /* Check attention identifier*/

  FQ0H240(); /* Reset fields to normal intensity*/

  FQ0H250(); /* Verify the entered data*/

  FQ0H260(); /* Process valid input request*/


end // end FQ0H200

// Obtain the data to display
Function FQ0H210()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ0HW00.FQ0HWSWR != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Clear the map display storage.*/
   /* ------------------------------------------------------------*/
  set FQ0HW02 empty; /* M002 display storage*/

   /* ------------------------------------------------------------*/
   /* Save MAP1 display storage in MAP2 storage area.*/
   /* ------------------------------------------------------------*/

  if (FQ0HW00.FQ0HWASW == "N") /* Add function not requested*/
    FQ0HW02.YDFCD = FQ0HW01.YDFCD[sysVar.arrayIndex]; /* Defective code*/
    FQ0HW02.FQ-SUM-DEF-IVC-CD = FQ0HW01.FQ-SUM-DEF-IVC-CD[sysVar.arrayIndex];
                                                             /* Parent code*/
    FQ0HW02.XXXSTAT = FQ0HW01.XXXSTAT[sysVar.arrayIndex]; /* Status*/
    FQ0HW02.ZZZSDT = FQ0HW01.ZZZSDT[sysVar.arrayIndex]; /* Setup date*/
    FQ0HW02.ZZZCHGDT = FQ0HW01.ZZZCHGDT[sysVar.arrayIndex]; /* Change date*/
    FQ0HW02.ZZZCHGTM = FQ0HW01.ZZZCHGTM[sysVar.arrayIndex]; /* Change time*/

    FQ0HW00.FQ0HWPNT = sysVar.arrayIndex; /* Save pointer*/

    /* ------------------------------------------------------------*/
     /* Obtain descriptions.*/
    /* ------------------------------------------------------------*/
    FQ0H211(); /* Obtain descriptions*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  FQ0HW00.FQ0HWSWR = "N"; /* Request for data switch*/


end // end FQ0H210

// Obtain descriptions
Function FQ0H211()
   /* ------------------------------------------------------------*/
   /* Obtain defective name.*/
   /* ------------------------------------------------------------*/
  if (FQ0HW02.YDFCD != " ") /* If defect code entered*/
    YDFREC.XGPCD = FQWREC.XGPCD; /* Format key*/
    YDFREC.YDFCD = FQ0HW02.YDFCD; /* Format key*/

    if (YDFREC.YDFKEY in YDFTBL.YDFKEY)
      FQ0HW02.YDFDESC = YDFTBL.YDFDESC[sysVar.arrayIndex];
    end

  end


end // end FQ0H211

// Format and converse the map
Function FQ0H220()
   /* ----------------------------------------------------*/
   /* If an error was found, format data to be displayed.*/
   /* ----------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* If error found*/

    FQWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/

  if (FQ0HW00.FQ0HWSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (FQ0HW00.FQ0HWSWF == "Y") /* Map must be formatted*/
      FQ0H221(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQ0HM002.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FQ0HM002.CATOAP = COMMAREA.CATOAP; /* To application*/
  FQ0HM002.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (FQWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (FQWREC.XXXMSGNO != 0) /* Message to display*/

      if (FQWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(FQWREC.XXXMSGNO);
      end /* Message number*/
    else
      FQ0HM002.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    FQ0HM002.XXXMODID = FQWREC.XXXMODID; /* Module identification*/

    set FQ0HM002 alarm; /* Indicate sound alarm*/

  end


  converse FQ0HM002 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQWREC.XXXMODID = " "; /* Module identification*/
  FQWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/

  COMMAREA.CATOAP = FQ0HM002.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FQ0HM002.CAITEM; /* Application data*/


end // end FQ0H220

// Format the display data
Function FQ0H221()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set FQ0HM002 initial; /* m002 input/output area*/

  FQ0HW00.FQ0HWSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If updating, format the data from storage.*/
   /* ------------------------------------------------------------*/
  if (FQ0HW00.FQ0HWASW == "N") /* If not using add function*/

    FQ0HM002.YDFCD = FQ0HW02.YDFCD; /* Defective code*/
    FQ0HM002.YDFDESC = FQ0HW02.YDFDESC; /* Defect name*/
    FQ0HM002.FQ-SUM-DEF-IVC-CD = FQ0HW02.FQ-SUM-DEF-IVC-CD;
                                                             /* Parent code*/
    FQ0HM002.XXXSTAT = FQ0HW02.XXXSTAT; /* Defect code status*/
    FQ0HM002.ZZZSDT = FQ0HW02.ZZZSDT; /* Setup date*/
    FQ0HM002.ZZZCHGDT = FQ0HW02.ZZZCHGDT; /* Change date*/
    FQ0HM002.ZZZCHGTM = FQ0HW02.ZZZCHGTM; /* Change time*/

    if (FQ0HM002.XXXSTAT == "A") /* If status is active*/
      set FQ0HM002.XXXSTAT cursor, normal; /* Place cursor on status*/
    else
      set FQ0HM002.CATOAP cursor; /* Place cursor*/
    end

  end

   /* ------------------------------------------------------------*/
   /* If using add function, make fields enterable.*/
   /* ------------------------------------------------------------*/
  if (FQ0HW00.FQ0HWASW == "Y") /* If add function was requested*/

    set FQ0HM002.YDFCD normal; /* Defective code*/
    set FQ0HM002.FQ-SUM-DEF-IVC-CD normal; /* Parent code*/
    FQ0HM002.XXXSTAT = "A"; /* Default status to 'active'*/
    FQ0HW02.XXXSTAT = "A"; /* Default status to 'active'*/
    set FQ0HM002.FQ0HMADD skip; /* Add title*/

  end


end // end FQ0H221

// Check attention identifier
Function FQ0H230()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (FQ0HW00.FQ0HWSWR == "Y") /* Request for data switch*/
      set FQ0HW02 empty;
    end

    FQ0HW00.FQ0HWSWR = "N"; /* Data request switch*/
    FQ0HW00.FQ0HWSWF = "Y"; /* Map formatted switch*/
    FQWREC.FQWMODSW = "N"; /* Modified data exit switch*/

    COMMAREA.CATOAP = " "; /* Fast path to application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to redisplay map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (FQ0HM002.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      FQ0HS01(); /* Purge any temporary storage*/

      XSFQS01(); /* Exit to another application*/

      /* ------------------------------------------------------------*/
       /* If transfer failed, return to first map.*/
      /* ------------------------------------------------------------*/
      set FQ0HM002.CATOAP cursor, bold; /* Highlight*/

    else
      converseLib.validationFailed(18); /* Indicate PF key not valid*/
      FQWREC.XXXMODID = "230"; /* Process name*/
    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1  /* PF1 was pressed*/
   || converseVar.eventKey is pf3  /* PF3 was pressed*/
   || converseVar.eventKey is pf4  /* PF4 was pressed*/
   || converseVar.eventKey is pf5  /* PF5 was pressed*/
   || converseVar.eventKey is pf13 /* PF13 was pressed*/
   || converseVar.eventKey is pf14) /* PF14 was pressed*/
     /* OR EZEAID IS PF15             ; PF15 was pressed*/
     /* OR EZEAID IS PF16             ; PF16 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FQ0HM002"; /* Move in map name for help*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/

      if (FQ0HW00.FQ0HWASW == "Y") /* If add function performed*/
        FQ0HW00.FQ0HWSWR = "Y"; /* Data request switch*/
      end

      FQ0HW00.FQ0HWMAP = "1"; /* Indicate map 1*/
      FQ0HW00.FQ0HWSWF = "Y"; /* Map formatted switch*/

      exit stack; /* Exit to redisplay map*/

    end

    if (converseVar.eventKey is pf5) /* PF5 was pressed*/
      COMMAREA.CATOAP = "FQ0M"; /* Set to factory application*/
    end

    if (converseVar.eventKey is pf13) /* PF13 was pressed*/
      COMMAREA.CATOAP = "FQ0B"; /* Set to warehouse screen*/
    end

    if (converseVar.eventKey is pf14) /* PF14 was pressed*/
      COMMAREA.CATOAP = "FQ0C"; /* Set to factory screen*/
    end

     /* IF EZEAID IS PF15             ; PF15 was pressed*/
    /* MOVE 'FQ0D' TO COMMAREA.CATOAP     ; Set to whse ratio scn*/
     /* END ;*/

     /* IF EZEAID IS PF16             ; PF16 was pressed*/
    /* MOVE 'FQ0E' TO COMMAREA.CATOAP     ; Set to cust ratio scn*/
     /* END ;*/

    FQ0HS01(); /* Purge any temporary storage*/

    XSFQS01(); /* Exit to another application*/

    /* ------------------------------------------------------------*/
     /* If transfer failed, return to first map.*/
    /* ------------------------------------------------------------*/
    set FQ0HM001.CATOAP cursor, bold; /* Highlight*/
    FQ0HW00.FQ0HWMAP = "1"; /* Indicate map 1*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter) /* ENTER was not pressed*/

    FQWREC.XXXMODID = "230"; /* Process name*/
    converseLib.validationFailed(4); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end FQ0H230

// Reset highlighted fields
Function FQ0H240()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to defined intensity.*/
   /* ------------------------------------------------------------*/

  if (FQ0HW00.FQ0HWASW == "Y") /* If add function was requested*/

    set FQ0HM002.YDFCD normal; /* Defective code*/
    set FQ0HM002.FQ-SUM-DEF-IVC-CD normal; /* Parent code*/

  end

  set FQ0HM002.XXXSTAT initialAttributes; /* Defect code status*/

  set FQ0HM002.CATOAP initialAttributes; /* Commarea TO application*/
  set FQ0HM002.CAITEM initialAttributes; /* Commarea application data*/

   /* ------------------------------------------------------------*/
   /* If no lists were requested, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ0HM002.YDFCD == "?") /* defect list requested*/

    if (converseVar.eventKey not enter) /* ENTER was not pressed*/

      FQWREC.XXXMODID = "240"; /* Module identification*/
      converseLib.validationFailed(24); /* Indicate PF key not valid*/

      exit stack; /* Exit to display map*/
    end

  else
    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of defect codes.*/
   /* ------------------------------------------------------------*/
  if (FQ0HM002.YDFCD == "?") /* defect list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "DEFECT CODES"; /* Heading*/
    XX0XW02.XGPCD = FQWREC.XGPCD; /* GPC code*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPYDF01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FQ0HM002.YDFCD = FQWREC.YDFCD; /* Restore prior code*/
    else

      FQ0HM002.YDFCD = YDFTBL.YDFCD[XX0XWPIX]; /* Selected code*/
    end

    set FQ0HM002.YDFCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the requested lists have been displayed and*/
   /* that ENTER must be pressed to process any changes.*/
