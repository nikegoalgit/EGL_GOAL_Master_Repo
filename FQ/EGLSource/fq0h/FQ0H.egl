  VPNTDF01.ZZZSDT = XX0XW01.XX0XWDAT; /* Setup date*/
  VPNTDF01.ZZZCHGDT = XX0XW01.XX0XWDAT; /* Change date*/
  VPNTDF01.ZZZCHGTM = XX0XW01.XX0XWTIM; /* Change time*/

   /* ------------------------------------------------------------*/
   /* Insert DEFEX.*/
