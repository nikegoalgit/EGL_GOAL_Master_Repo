    end

  else
    set FQ03M001.SY1STNBR cursor, bold; /* Highlight*/
    set FQ03M001.SY2CLRID bold; /* Highlight*/
    set FQ03M001.XDMCD bold; /* Highlight*/
    converseLib.validationFailed(33); /* Indicate data missing or invalid*/
