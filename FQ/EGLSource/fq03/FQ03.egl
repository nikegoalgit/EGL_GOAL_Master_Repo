// Calculate percents
Function FQ03150()
   /* ------------------------------------------------------------*/
   /* If the total is greater than zero, calculate percents.*/
   /* ------------------------------------------------------------*/
  if (FQ03W01.FQ03WCAT > 0 /* If total greater than zero*/
   && FQ03W00.FQ03WDSW == "Y") /* and data was found*/

    /* ------------------------------------------------------------*/
     /* Calculate the percent each line is of the total defects.*/
    /* ------------------------------------------------------------*/
    FQ03W00.FQ03WIXM = 1; /* Initialize line index*/

    while (FQ03W00.FQ03WIXM <= 24) /* Until screen is full*/

      if (FQ03W01.FQ2DEFQT[FQ03WIXM] > 0) /* If amt GT 0*/
        /* ------------------------------------------------------------*/
         /* Divide line quantity by total defects for percent total.*/
        /* ------------------------------------------------------------*/
        FQ03W00.FQ03WFDA = mathLib.round(FQ03W01.FQ2DEFQT[FQ03WIXM] / FQ03W01.FQ03WCAT, -mathLib.decimals(FQ03W00.FQ03WFDA));

        /* ------------------------------------------------------------*/
         /* Multiply by 100 to get nnn.nn format.*/
        /* ------------------------------------------------------------*/
        FQ03W00.FQ03WFDA = FQ03W00.FQ03WFDA * 100; /* Times 100*/

        /* ------------------------------------------------------------*/
         /* Save percent in storage.*/
        /* ------------------------------------------------------------*/
        FQ03W01.FQ03WPCT[FQ03WIXM] = FQ03W00.FQ03WFDA; /* percent*/

      end
      /* ------------------------------------------------------------*/
       /* Add to counter.*/
      /* ------------------------------------------------------------*/
      FQ03W00.FQ03WIXM = FQ03W00.FQ03WIXM + 1; /* Add to counter*/

    end

    /* ------------------------------------------------------------*/
     /* Divide tot def qty by total cat defs for percent total.*/
    /* ------------------------------------------------------------*/
    FQ03W00.FQ03WFDA = mathLib.round(FQ03W01.FQ03WQTT / FQ03W01.FQ03WCAT, -mathLib.decimals(FQ03W00.FQ03WFDA));

    /* ------------------------------------------------------------*/
     /* Multiply by 100 to get nnn.nnn format.*/
    /* ------------------------------------------------------------*/
    FQ03W00.FQ03WFDA = FQ03W00.FQ03WFDA * 100; /* Times 100*/

    /* ------------------------------------------------------------*/
     /* Save total percent.*/
    /* ------------------------------------------------------------*/
    FQ03W01.FQ03WPTT = FQ03W00.FQ03WFDA; /* Total percent*/

  end


end // end FQ03150

// Format and converse the map
Function FQ03200()
   /* ------------------------------------------------------------*/
   /* If no error was found, save the error message number.*/
   /* ------------------------------------------------------------*/
  if (converseVar.validationMsgNum != 0) /* If error found*/

    FQWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/
  if (FQ03W00.FQ03WSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (FQ03W00.FQ03WSWF == "Y") /* Map must be formatted*/
      FQ03210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQ03M001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FQ03M001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FQ03M001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (FQWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (FQWREC.XXXMSGNO != 0) /* Message to display*/

      if (FQWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(FQWREC.XXXMSGNO);
      end /* Message number*/
    else
      FQ03M001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    FQ03M001.XXXMODID = FQWREC.XXXMODID; /* Module identification*/

    set FQ03M001 alarm; /* Indicate sound alarm*/

  end

  converse FQ03M001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQWREC.XXXMODID = " "; /* Module identification*/
  FQWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* message text*/

  COMMAREA.CATOAP = FQ03M001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FQ03M001.CAITEM; /* Application data*/


end // end FQ03200

// Load map fields
Function FQ03210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/
  set FQ03M001 initial;

  FQ03W00.FQ03WSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* Format key data in the map.*/
   /* ------------------------------------------------------------*/
  FQ03M001.FY1PFYCD = FQWREC.FY1PFYCD; /* origin factory*/
  FQ03M001.SY1STNBR = FQWREC.SY1STNBR; /* style nbr*/
  FQ03M001.SY2CLRID = FQWREC.SY2CLRID; /* color id*/
  FQ03M001.XDMCD = FQWREC.XDMCD; /* dimension*/
  FQ03M001.XDVCD = FQWREC.XDVCD; /* or division*/

  FQ03M001.FQ03MBDT = FQ03W00.FQ03MBYM; /* beg date*/
  FQ03M001.FQ03MEDT = FQ03W00.FQ03MEYM; /* end date*/

  FQ03M001.YDFCD = FQWREC.YDFCD; /* Defect code*/
  FQ03M001.YDFDESC = FQWREC.YDFDESC; /* Defect name*/

  FQ03M001.FQ03WCAT = FQ03W01.FQ03WCAT; /* Category total*/
  FQ03M001.FQ03WTOT = FQ03W01.FQ03WTOT; /* Grand total*/

   /* ------------------------------------------------------------*/
   /* If no data was found, return to previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ03W00.FQ03WDSW == "N") /* Data found switch*/
    return; /* Return to previous process*/
  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/
  FQ03W00.FQ03WIXM = 1; /* Initialize line index*/

  while (FQ03W00.FQ03WIXM <= FQ03W00.FQ03WNBR /* while count LE # mos*/
   && FQ03W00.FQ03WIXM <= 24) /* and LE max months*/

    FQ03M001.FQ03WHDR[FQ03WIXM] = FQ03W01.FQ03WHDR[FQ03WIXM]; /* MO*/
    FQ03M001.FQ2DEFQT[FQ03WIXM] = FQ03W01.FQ2DEFQT[FQ03WIXM]; /* QTY*/
    FQ03M001.FQ03WPCT[FQ03WIXM] = FQ03W01.FQ03WPCT[FQ03WIXM]; /* %*/

    /* ------------------------------------------------------------*/
     /* Add to counter.*/
    /* ------------------------------------------------------------*/
    FQ03W00.FQ03WIXM = FQ03W00.FQ03WIXM + 1; /* Increment line index*/

  end

   /* ------------------------------------------------------------*/
   /* Format the labels and totals.*/
   /* ------------------------------------------------------------*/
  FQ03211(); /* Format labels & totals*/


end // end FQ03210

// Format labels based on range
Function FQ03211()
   /* ------------------------------------------------------------*/
   /* Format the labels depending on range length.*/
   /* ------------------------------------------------------------*/
  if (FQ03W00.FQ03WNBR > 0) /* If months GT zero*/

    if (FQ03W00.FQ03WNBR <= 6) /* If 6 or less months*/
      set FQ03M001.FQ03WMTH[1] skip; /* make visable*/
      set FQ03M001.FQ03WQNM[1] skip; /* make visable*/
      set FQ03M001.FQ03WPNM[1] skip; /* make visable*/

    else
      if (FQ03W00.FQ03WNBR <= 12) /* If 12 or less months*/
        set FQ03M001.FQ03WMTH[1] skip; /* make visable*/
        set FQ03M001.FQ03WQNM[1] skip; /* make visable*/
        set FQ03M001.FQ03WPNM[1] skip; /* make visable*/
        set FQ03M001.FQ03WMTH[2] skip; /* make visable*/
        set FQ03M001.FQ03WQNM[2] skip; /* make visable*/
        set FQ03M001.FQ03WPNM[2] skip; /* make visable*/

      else
        if (FQ03W00.FQ03WNBR <= 18) /* If 18 or less months*/
          set FQ03M001.FQ03WMTH[1] skip; /* make visable*/
          set FQ03M001.FQ03WQNM[1] skip; /* make visable*/
          set FQ03M001.FQ03WPNM[1] skip; /* make visable*/
          set FQ03M001.FQ03WMTH[2] skip; /* make visable*/
          set FQ03M001.FQ03WQNM[2] skip; /* make visable*/
          set FQ03M001.FQ03WPNM[2] skip; /* make visable*/
          set FQ03M001.FQ03WMTH[3] skip; /* make visable*/
          set FQ03M001.FQ03WQNM[3] skip; /* make visable*/
          set FQ03M001.FQ03WPNM[3] skip; /* make visable*/

        else
          set FQ03M001.FQ03WMTH[1] skip; /* make visable*/
          set FQ03M001.FQ03WQNM[1] skip; /* make visable*/
          set FQ03M001.FQ03WPNM[1] skip; /* make visable*/
          set FQ03M001.FQ03WMTH[2] skip; /* make visable*/
          set FQ03M001.FQ03WQNM[2] skip; /* make visable*/
          set FQ03M001.FQ03WPNM[2] skip; /* make visable*/
          set FQ03M001.FQ03WMTH[3] skip; /* make visable*/
          set FQ03M001.FQ03WQNM[3] skip; /* make visable*/
          set FQ03M001.FQ03WPNM[3] skip; /* make visable*/
          set FQ03M001.FQ03WMTH[4] skip; /* make visable*/
          set FQ03M001.FQ03WQNM[4] skip; /* make visable*/
          set FQ03M001.FQ03WPNM[4] skip; /* make visable*/

        end
      end
    end

    /* ------------------------------------------------------------*/
     /* Format the totals depending on range length.*/
    /* ------------------------------------------------------------*/
    if (FQ03W00.FQ03WNBR == 6) /* If six months*/

      FQ03M001.FQ03WTNM[1] = "TOTAL"; /* Title name*/
      FQ03M001.FQ03WQTT[1] = FQ03W01.FQ03WQTT; /* QTY*/
      FQ03M001.FQ03WPTT[1] = FQ03W01.FQ03WPTT; /* Percent*/

    else
      if (FQ03W00.FQ03WNBR == 12) /* If 12 months*/

        FQ03M001.FQ03WTNM[2] = "TOTAL"; /* Title name*/
        FQ03M001.FQ03WQTT[2] = FQ03W01.FQ03WQTT; /* QTY*/
        FQ03M001.FQ03WPTT[2] = FQ03W01.FQ03WPTT; /* Percent*/

      else
        if (FQ03W00.FQ03WNBR == 18) /* If 18 months*/

          FQ03M001.FQ03WTNM[3] = "TOTAL"; /* Title name*/
          FQ03M001.FQ03WQTT[3] = FQ03W01.FQ03WQTT; /* QTY*/
          FQ03M001.FQ03WPTT[3] = FQ03W01.FQ03WPTT; /* Percent*/

        else
          if (FQ03W00.FQ03WNBR == 24) /* If 24 months*/

            FQ03M001.FQ03WTNM[4] = "TOTAL"; /* Title name*/
            FQ03M001.FQ03WQTT[4] = FQ03W01.FQ03WQTT; /* QTY*/
            FQ03M001.FQ03WPTT[4] = FQ03W01.FQ03WPTT; /* Percent*/

          else

            FQ03M001.FQ03WHDR[FQ03WIXM] = "TOTAL"; /* Titles*/
            FQ03M001.FQ2DEFQT[FQ03WIXM] = FQ03W01.FQ03WQTT; /* QTY*/
            FQ03M001.FQ03WPCT[FQ03WIXM] = FQ03W01.FQ03WPTT;
                                   /* Percent*/

          end
        end
      end
    end
  end


end // end FQ03211

// Check attention identifier
Function FQ03300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    if (FQ03W00.FQ03WSWR == "Y") /* Request for data switch*/
      set FQ03W01 empty;
    end

    FQ03W00.FQ03WSWF = "Y"; /* Format the map switch*/
    FQ03W00.FQ03WSWR = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* Fast path to application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to redisplay map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (FQ03M001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      XSFQS01(); /* Exit to another application*/

      set FQ03M001.CATOAP cursor, bold; /* Highlight*/

    else
      converseLib.validationFailed(18); /* Indicate PF key not valid*/
      FQWREC.XXXMODID = "300"; /* Process name*/
    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* PF3 was pressed*/
   || converseVar.eventKey is pf4 /* PF4 was pressed*/
   || converseVar.eventKey is pf5 /* PF5 was pressed*/
   || converseVar.eventKey is pf14) /* PF14 was pressed*/
    /* OR EZEAID IS PF15             ; PF15 was pressed*/
    /* OR EZEAID IS PF16             ; PF16 was pressed*/
    /* OR EZEAID IS PF17             ; PF17 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FQ03M001"; /* Move in map name for help*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FQ02"; /* Set to factory application*/
    end

    if (converseVar.eventKey is pf14) /* PF14 was pressed*/
      COMMAREA.CATOAP = "FQ0C"; /* Set to factory ratio*/
    end

    if (converseVar.eventKey is pf15) /* PF15 was pressed*/
      COMMAREA.CATOAP = "FQ0D"; /* Set to customer screen*/
    end

    if (converseVar.eventKey is pf16) /* PF16 was pressed*/
      COMMAREA.CATOAP = "FQ0E"; /* Set to customer ratio screen*/
    end

    if (converseVar.eventKey is pf17) /* PF17 was pressed*/
      COMMAREA.CATOAP = "FQ0F"; /* Set to warehouse screen*/
    end

    XSFQS01(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf9)  /* and PF9 was not pressed*/

    FQWREC.XXXMODID = "300"; /* Process name*/
    converseLib.validationFailed(4); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end FQ03300

// Reset highlighted fields
Function FQ03400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to defined intensity.*/
   /* ------------------------------------------------------------*/
  set FQ03M001.CATOAP initialAttributes; /* TO application*/
  set FQ03M001.CAITEM initialAttributes; /* Application data*/

  set FQ03M001.SY1STNBR initialAttributes; /* style nbr*/
  set FQ03M001.SY2CLRID initialAttributes; /* color id*/
  set FQ03M001.XDMCD initialAttributes; /* dimension*/
  set FQ03M001.XDVCD initialAttributes; /* division*/
  set FQ03M001.FY1PFYCD initialAttributes; /* origin factory*/

  set FQ03M001.FQ03MBDT initialAttributes; /* beg date*/
  set FQ03M001.FQ03MEDT initialAttributes; /* end date*/
  set FQ03M001.YDFCD initialAttributes; /* defect code*/

   /* ------------------------------------------------------------*/
   /* If no lists were requested, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ03M001.XDVCD == "?" /* division list requested*/
   || FQ03M001.YDFCD == "?") /* defect list requested*/

    if (converseVar.eventKey not enter) /* ENTER was not pressed*/

      FQWREC.XXXMODID = "400"; /* Module identification*/
      converseLib.validationFailed(24); /* Indicate PF key not valid*/

      exit stack; /* Exit to display map*/
    end

  else
    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of division codes.*/
   /* ------------------------------------------------------------*/
  if (FQ03M001.XDVCD == "?") /* division list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "DIVISION CODES"; /* Heading*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXDV01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FQ03M001.XDVCD = FQWREC.XDVCD; /* Restore prior code*/
    else

      FQ03M001.XDVCD = XDVTBL.XDVCD[XX0XWPIX]; /* Selected code*/
    end

    set FQ03M001.XDVCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of defect codes.*/
   /* ------------------------------------------------------------*/
  if (FQ03M001.YDFCD == "?") /* defect list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "DEFECT CODES"; /* Heading*/
    XX0XW02.XGPCD = FQWREC.XGPCD; /* GPC code*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPYDF01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FQ03M001.YDFCD = FQWREC.YDFCD; /* Restore prior code*/
    else

      FQ03M001.YDFCD = YDFTBL.YDFCD[XX0XWPIX]; /* Selected code*/
    end

    set FQ03M001.YDFCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the requested lists have been displayed and*/
   /* that ENTER must be pressed to process any changes.*/
   /* ------------------------------------------------------------*/

  converseLib.clearScreen();
   /* Clear screen before display*/

  FQWREC.XXXMODID = "400"; /* Module identification*/
  converseLib.validationFailed(25); /* Indicate press ENTER*/

  exit stack; /* Exit to display map*/


end // end FQ03400

// Verify the entered data
Function FQ03500()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (FQ03M001.CAITEM > " ") /* Application data was entered*/

    FQWREC.XXXMODID = "500"; /* Module identification*/
    set FQ03M001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* If the search key was changed, verify the key fields.*/
   /* ------------------------------------------------------------*/
  if (FQ03M001.SY1STNBR != FQWREC.SY1STNBR  /* Style*/
   || FQ03M001.SY2CLRID != FQWREC.SY2CLRID  /* Color*/
   || FQ03M001.XDMCD != FQWREC.XDMCD        /* Dimension*/
   || FQ03M001.XDVCD != FQWREC.XDVCD        /* Division*/
   || FQ03M001.FQ03MBDT != FQ03W00.FQ03MBYM /* Beg date*/
   || FQ03M001.FQ03MEDT != FQ03W00.FQ03MEYM /* End date*/
   || FQ03M001.YDFCD != FQWREC.YDFCD        /* Defect code*/
   || FQ03M001.FY1PFYCD != FQWREC.FY1PFYCD  /* Origin factory*/
                                            
   || FQ03M001.SY1STNBR == " "              /* Style*/
   || FQ03M001.SY2CLRID == " "              /* Color*/
   || FQ03M001.XDMCD == " "                 /* Dimension*/
   || FQ03M001.XDVCD == " "                 /* Division*/
   || FQ03M001.FQ03MBDT == 0                /* Beg date*/
   || FQ03M001.FQ03MEDT == 0                /* End date*/
   || FQ03M001.YDFCD == " "                 /* Defect*/
   || FQ03M001.FY1PFYCD == " ")             /* Origin factory*/

    FQ03510(); /* Verify style/color/dim*/
    FQ03520(); /* Verify division*/
    FQ03530(); /* Verify dates*/
    FQ03540(); /* Verify defect code*/
    FQ03550(); /* Verify factory*/

  end

   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end FQ03500

// Verify style/color/dim
Function FQ03510()
   /* ------------------------------------------------------------*/
    /* If missing, default dimension.*/
   /* ------------------------------------------------------------*/
  if (FQ03M001.XDMCD == " ") /* If dim missing*/
    FQ03M001.XDMCD = "00"; /* Default to '00'*/
  end

   /* ------------------------------------------------------------*/
    /* If missing, default color.*/
   /* ------------------------------------------------------------*/
  if (FQ03M001.SY2CLRID == " ") /* If color missing*/
    FQ03M001.SY2CLRID = "00"; /* Default to '00'*/
  end

   /* ------------------------------------------------------------*/
    /* Verify style/color/dimension.*/
   /* ------------------------------------------------------------*/
  if (FQ03M001.SY1STNBR != " ") /* Style*/

    /* ------------------------------------------------------------*/
     /* Select SYCLR.*/
    /* ------------------------------------------------------------*/
    FQ03511(); /* Select SYCLR*/

    /* ------------------------------------------------------------*/
     /* If row was not found, indicate error.*/
    /* ------------------------------------------------------------*/
    if (SQLCA.VAGen_SQLCODE != 0) /* If row was found*/

      set FQ03M001.SY1STNBR cursor, bold; /* Highlight*/
