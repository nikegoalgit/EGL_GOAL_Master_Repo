//*** PROGRAM=FQ0B ****
// This application allows users to view all defect categories,
// descriptions, quantities, and percent of total defects for
// a particular set of parameters.
// 
// The user must enter 'FACTORY', 'DIVISION', 'STYLE', and
// 'COLOR'.  Both 'PO MONTH RANGE' and 'DIMENSION' will default
// if left blank.  The program will obtain a list of all defect
// categories, the quantity of defects for the category, the
// description of the category, and will calculate the percent
// each category is of the total defects.
// 
// The user may select a particular category and be transfered
// to FQ02, NATURE DEFECT PERCENT SUMMARY.  There he will be
// able to see more details concerning a category.
// 
// The user may scroll forward and backward to view all the
// categories by pressing PF7 and PF8.  He may exit any to any
// allowed application, via FAST PATH or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      05/01/91  RHETRI  New application.
//          06/24/93  MHUYNH  Regen due to change to XSFQS05
// y2kproj  12-03-97  oricha  y2k modifications:  regen to
//                            pick up xx0xs01 changes & to
//                            pick up xsfqs05 century calc chgs
// *********************
Program FQ0B type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "FQ00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  FQ0BW00 FQ0BW00; // record
  FQ0BW01 FQ0BW01; // record
  FQ2REC FQ2REC; // record
  FQAREC FQAREC; // record
  FQWREC FQWREC; // record
  FQZREC FQZREC; // record
  FY1REC FY1REC; // record
  PG2REC PG2REC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TA6REC TA6REC; // record
  TAEREC TAEREC; // record
  XX0XW01 XX0XW01; // record
  XX0XW02 XX0XW02; // record
  YDCREC YDCREC; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XDMTBL; // table
  use XDVTBL; // table
  use YDCTBL; // table
  use FQ0BM.FQ0BM001,  // forms
      FQ0BM.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPFQ001: XPFQ001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/
    set FQ0BM001 initial; /* M001 input/output*/
    set FQ0BW00 empty; /* Working storage*/
    set FQ0BW01 empty; /* M001 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/

    FQ0BW00.FQ0BWSWF = "Y"; /* Format the map flag*/

     /* ------------------------------------------------------------*/
     /* If complete key is present, request data. (Defaults exist.)*/
     /* ------------------------------------------------------------*/
    if (FQWREC.FY1PFYCD > " " /* If origin factory exists*/
     && FQWREC.SY1STNBR > " " /* and style number exists*/
     && FQWREC.SY2CLRID > " " /* and color code exists*/
     && FQWREC.XDMCD > " ")   /* and dim exists*/

      FQ0BW00.FQ0BMBDT = FQWREC.FQ1IDBDT; /* Beg date*/
      FQ0BW00.FQ0BMEDT = FQWREC.FQ1IDXDT; /* End date*/

      FQ0BW00.FQ0BWSWR = "Y"; /* Request data*/
    else

      FQ0BW00.FQ0BWSWR = "N"; /* data not requested*/
    end

     /* ------------------------------------------------------------*/
     /* Clear out defective code range.*/
     /* ------------------------------------------------------------*/
    FQWREC.FQWBDFCT = " "; /* Begin defect code*/
    FQWREC.FQWEDFCT = " "; /* End defect code*/

     /* ------------------------------------------------------------*/
     /* Flow to check for default dates.*/
     /* ------------------------------------------------------------*/
    goto FQ0B000; /* Check for default dates*/


    FQ0B000: FQ0B000();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FQ0B001; /* Converse the main process*/


    FQ0B001: FQ0B001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FQ0B001; /* Converse the main process*/


  end // end main
end // end FQ0B

// Check for default dates
Function FQ0B000()
   /* ------------------------------------------------------------*/
   /* If date range is missing, call date routine for defaults.*/
   /* ------------------------------------------------------------*/
  if (FQWREC.FQ1IDBDT == 0 /* If begin date is missing*/
   || FQWREC.FQ1IDXDT == 0) /* Or end date is missing*/

    /* ------------------------------------------------------------*/
     /* Clear copybook.*/
    /* ------------------------------------------------------------*/
    set FQZREC empty; /* clear date edit record*/

    FQZREC.FQZBEGRG = FQWREC.FQ1IDBDT; /* Beg date*/
    FQZREC.FQZENDRG = FQWREC.FQ1IDXDT; /* end date*/

    /* ------------------------------------------------------------*/
     /* Edit range.  Send YYMMMM - receive back CCYYMMMM.*/
    /* ------------------------------------------------------------*/
    XSFQS05(); /* Common date edit routine*/

    /* ------------------------------------------------------------*/
     /* Move dates into FQWREC.*/
    /* ------------------------------------------------------------*/
    FQWREC.FQ1IDBDT = FQZREC.FQZBCYMM; /* Begin date*/
    FQWREC.FQ1IDXDT = FQZREC.FQZECYMM; /* End date*/

    FQ0BW00.FQ0BMBDT = FQZREC.FQZBCYMM; /* Begin date*/
    FQ0BW00.FQ0BMEDT = FQZREC.FQZECYMM; /* End date*/

  else

    FQ0BW00.FQ0BMBDT = FQWREC.FQ1IDBDT; /* Beg date*/
    FQ0BW00.FQ0BMEDT = FQWREC.FQ1IDXDT; /* End date*/

  end


end // end FQ0B000

// Main process to display a map
Function FQ0B001()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  FQ0B100(); /* Obtain the data to display*/

  FQ0B200(); /* Format and display the map*/

  FQ0B300(); /* Check attention identifier*/

  FQ0B400(); /* Reset fields to normal intensity*/

  FQ0B500(); /* Verify the entered data*/

  FQ0B600(); /* Process valid input request*/


end // end FQ0B001

// Obtain the data to display
Function FQ0B100()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ0BW00.FQ0BWSWR != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* ------------------------------------------------------------*/
  FQ0BW00.XXXTSLEN = 490; /* Temporary storage page length*/

   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/
  FQ0BS01(); /* Purge temporary storage*/

   /* --------------------------------------------------------*/
   /* Open DFDTL.*/
   /* --------------------------------------------------------*/
  FQ0B110(); /* Open DFDTL*/

   /* ----------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ----------------------------------------------------------*/
  while (FQ0BW00.FQ0BWEOC == "N") /* Until end of cursor*/

    FQ0B120(); /* Fetch DFDTL*/

    if (FQ0BW00.FQ0BWEOC == "N") /* If row found*/

      /* ----------------------------------------------------------*/
       /* Save data for display.*/
      /* ----------------------------------------------------------*/
      FQ0B130(); /* Save data*/

      /* ----------------------------------------------------------*/
       /* Add quantity to grand total.*/
      /* ----------------------------------------------------------*/
      FQ0BW00.FQ0BWTOT = FQ0BW00.FQ0BWTOT + FQ2REC.FQ2DEFQT;

      /* ---------------------------------------------------------*/
       /* If the page is full, add the page to the storage queue.*/
      /* ---------------------------------------------------------*/
      if (FQ0BW00.FQ0BWIXM > 14) /* Page is full*/
        FQ0B140(); /* Add page to temporary storage*/
      end

    end
  end

   /* ----------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ----------------------------------------------------*/
  if (FQ0BW00.FQ0BWIXM > 1) /* If line count greater than 1*/

    FQ0B140(); /* Add page to temp storage*/

  end

   /* ------------------------------------------------------------*/
   /* Close DFDTL*/
   /* ------------------------------------------------------------*/
  FQ0B150(); /* Close DFDTL*/

   /* ------------------------------------------------------------*/
   /* If no data to display, indicate no data to display.*/
   /* ------------------------------------------------------------*/

  if (FQ0BW00.TA5TSQIX == 0) /* No pages formatted*/

    FQWREC.XXXMODID = "100"; /* Module identification*/
    converseLib.validationFailed(17); /* No data was found to display*/

    return; /* Return to previous process*/

  end

   /* ---------------------------------------------------------*/
   /* If no message to display, reset the desired page number.*/
   /* ---------------------------------------------------------*/

  if (FQWREC.XXXMSGNO == 0) /* No message to display*/
    FQ0BW00.XXXTSPAG = 1; /* Desired page number*/
  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  FQ0BW00.FQ0BWSWR = "N"; /* Request for data switch*/


end // end FQ0B100

// Open DFDTL
Function FQ0B110()
   /* ------------------------------------------------------------*/
   /* Clear copybooks.*/
   /* ------------------------------------------------------------*/
  set FQAREC empty; /* Clear copybook*/
  set FQ2REC empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
   /* Format fqAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "S1"; /* Function code*/
  FQAREC.FQ1IDBDT = FQWREC.FQ1IDBDT; /* beg date*/
  FQAREC.FQ1IDXDT = FQWREC.FQ1IDXDT; /* ebd date*/

   /* ------------------------------------------------------------*/
   /* Format key.*/
   /* ------------------------------------------------------------*/
  FQ2REC.SY1STNBR = FQWREC.SY1STNBR; /* style*/
  FQ2REC.SY2CLRID = FQWREC.SY2CLRID; /* color*/
  FQ2REC.XDMCD = FQWREC.XDMCD; /* dim*/
  FQ2REC.FY1PFYCD = FQWREC.FY1PFYCD; /* fcty origin*/
  FQ2REC.XDVCD = FQWREC.XDVCD; /* division code*/

   /* ------------------------------------------------------------*/
   /* Open DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Open cursor*/

   /* ------------------------------------------------------------*/
   /* If successful, init switches & return to previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    FQ0BWEOC = "N"; /* End of cursor switch*/
    FQ0BW00.FQ0BWIXM = 1; /* Display line index*/
    FQ0BW00.FQ0BWTOT = 0; /* total defects*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "110"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    set FQ0BM001 initial; /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "OPEN CURSOR";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/
  TA1REC.TA1TBLKE = FQ2REC.FQ2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0B110

// Fetch DFDTL
Function FQ0B120()
   /* ------------------------------------------------------------*/
   /* Format FQAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "N1"; /* Function code*/

   /* ------------------------------------------------------------*/
   /* Fetch DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Fetch next row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == 100) /* Or row not found*/

      if (SQLCA.VAGen_SQLCODE == 100) /* If row not found*/
        FQ0BW00.FQ0BWEOC = "Y"; /* Indicate end of cursor*/
      end

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "120"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(2); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "FETCH NEXT TABLE ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/
  TA1REC.TA1TBLKE = FQ2REC.FQ2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0B120

// Save data
Function FQ0B130()
   /* ------------------------------------------------------------*/
   /* Obtain defect category description.*/
   /* ------------------------------------------------------------*/

  if (FQ2REC.YDCCD in YDCTBL.YDCCD)
    FQ0BW01.YDCDESC[FQ0BWIXM] = YDCTBL.YDCDESC[sysVar.arrayIndex];
  end

   /* ------------------------------------------------------------*/
   /* Save the data for display.*/
   /* ------------------------------------------------------------*/
  FQ0BW01.YDCCD[FQ0BWIXM] = FQ2REC.YDCCD; /* Defect category*/
  FQ0BW01.FQ2DEFQT[FQ0BWIXM] = FQ2REC.FQ2DEFQT; /* Defect quantity*/

   /* ------------------------------------------------------------*/
   /* Add to line counter.*/
   /* ------------------------------------------------------------*/
  FQ0BW00.FQ0BWIXM = FQ0BW00.FQ0BWIXM + 1; /* Increment line index*/


end // end FQ0B130

// Add page to temporary storage
Function FQ0B140()
   /* ------------------------------------------------------------*/
   /* Add the next page to temporary storage.*/
   /* ------------------------------------------------------------*/

  FQ0BW01.XXXTSLEN = FQ0BW00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "A"; /* Function code*/

  try
    call "TA0050" (FQ0BW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, initialize the storage area and indicators*/
   /* to be used.  Return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    FQ0BW00.TA5TSQIX = TA5REC.TA5TSQIX; /* Highest page number*/
    TA5REC.TA5TSQIX = 0; /* Reset to 0*/
    FQ0BW00.FQ0BWIXM = 1; /* Reset to 1*/
    set FQ0BW01 empty; /* M001 display storage*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  FQWREC.XXXMODID = "140"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "ADD PAGE TO TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0B140

// Close DFDTL
Function FQ0B150()
   /* ------------------------------------------------------------*/
   /* Format fqAREC.*/
   /* ------------------------------------------------------------*/
  FQAREC.FQAFUNCD = "C1"; /* Function code*/

   /* ------------------------------------------------------------*/
   /* Close DFDTL.*/
   /* ------------------------------------------------------------*/
  try
    call "IO5840" (FQAREC, SQLCA, FQ2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Close table set*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FQWREC.XXXMODID = "150"; /* Module identification*/
  converseLib.validationFailed(1); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "CLOSE CURSOR";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNM = FQWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFQUAL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VDFDTL01"; /* Table view name*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FQ0B150

// Format and converse the map
Function FQ0B200()
   /* ------------------------------------------------------------*/
   /* If no error was found, save the error message number.*/
   /* ------------------------------------------------------------*/
  if (converseVar.validationMsgNum != 0) /* If error found*/

    FQWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/
  if (FQ0BW00.FQ0BWSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (FQ0BW00.FQ0BWSWF == "Y") /* Map must be formatted*/
      FQ0B210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQ0BM001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FQ0BM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FQ0BM001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (FQWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (FQWREC.XXXMSGNO != 0) /* Message to display*/

      if (FQWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(FQWREC.XXXMSGNO);
      end /* Message number*/
    else
      FQ0BM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    FQ0BM001.XXXMODID = FQWREC.XXXMODID; /* Module identification*/

    set FQ0BM001 alarm; /* Indicate sound alarm*/

  end

  converse FQ0BM001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FQWREC.XXXMODID = " "; /* Module identification*/
  FQWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* message text*/

  COMMAREA.CATOAP = FQ0BM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FQ0BM001.CAITEM; /* Application data*/


end // end FQ0B200

// Load map fields
Function FQ0B210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set FQ0BM001 initial;

  FQ0BW00.FQ0BWSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (FQ0BW00.TA5TSQIX > 0) /* Pages were formatted*/

    if (FQ0BW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/
      if (FQ0BW00.XXXTSPAG > FQ0BW00.TA5TSQIX) /* Desired greater highest*/
        FQ0BW00.XXXTSPAG = FQ0BW00.TA5TSQIX; /* Default to highest*/
      end

      FQ0BS02(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/
    FQ0BM001.XXXTSPAG = FQ0BW00.XXXTSPAG; /* Current page*/
    FQ0BM001.TA5TSQIX = FQ0BW00.TA5TSQIX; /* Highest page*/

    set FQ0BM001.XXXLITPG skip; /* "PAGE" literal*/
    set FQ0BM001.XXXLITOF skip; /* "OF" literal*/
    set FQ0BM001.XXXTSPAG skip; /* current page*/
    set FQ0BM001.TA5TSQIX skip; /* max page*/

  end

   /* ------------------------------------------------------------*/
   /* Format key data in the map.*/
   /* ------------------------------------------------------------*/

  FQ0BM001.FY1PFYCD = FQWREC.FY1PFYCD; /* origin factory*/
  FQ0BM001.SY1STNBR = FQWREC.SY1STNBR; /* style nbr*/
  FQ0BM001.SY2CLRID = FQWREC.SY2CLRID; /* color id*/
  FQ0BM001.XDMCD = FQWREC.XDMCD; /* dimension*/
  FQ0BM001.XDVCD = FQWREC.XDVCD; /* or division*/

  FQ0BM001.FQ0BMBDT = FQ0BW00.FQ0BMBYM; /* beg date*/
  FQ0BM001.FQ0BMEDT = FQ0BW00.FQ0BMEYM; /* end date*/

   /* ------------------------------------------------------------*/
   /* If data exists, make fields visable, calc %s, & set cursor.*/
   /* ------------------------------------------------------------*/
  if (FQ0BW01.YDCCD[1] > " ") /* If data exists*/

    set FQ0BM001.FQ0BMHDR skip; /* make visable*/
    FQ0BM001.FQ0BWTOT = FQ0BW00.FQ0BWTOT; /* Total*/

    FQ0B211(); /* Calculate percents*/

    set FQ0BM001.FQ0BMSEL[1] cursor, normal; /* Position cursor*/
  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  FQ0BW00.FQ0BWIXM = 1; /* Initialize line index*/

  while (FQ0BW00.FQ0BWIXM <= 14 /* Until screen is full*/
   && FQ0BW01.YDCCD[FQ0BWIXM] > " ") /* and data exists*/

    FQ0BM001.FQ0BMSEL[FQ0BWIXM] = FQ0BW01.FQ0BMSEL[FQ0BWIXM];
    FQ0BM001.YDCCD[FQ0BWIXM] = FQ0BW01.YDCCD[FQ0BWIXM];
    FQ0BM001.YDCDESC[FQ0BWIXM] = FQ0BW01.YDCDESC[FQ0BWIXM];
    FQ0BM001.FQ2DEFQT[FQ0BWIXM] = FQ0BW01.FQ2DEFQT[FQ0BWIXM];
    FQ0BM001.FQ0BWPCT[FQ0BWIXM] = FQ0BW01.FQ0BWPCT[FQ0BWIXM];

    /* ------------------------------------------------------------*/
     /* Make line visable.*/
    /* ------------------------------------------------------------*/
    set FQ0BM001.FQ0BMSEL[FQ0BWIXM] normal; /* Make visable*/

    /* ------------------------------------------------------------*/
     /* Add to counter.*/
    /* ------------------------------------------------------------*/
    FQ0BW00.FQ0BWIXM = FQ0BW00.FQ0BWIXM + 1; /* Increment line index*/

  end


end // end FQ0B210

// Calculate percents
Function FQ0B211()
   /* ------------------------------------------------------------*/
   /* If the total is greater than zero, calculate percents.*/
   /* ------------------------------------------------------------*/
  if (FQ0BW00.FQ0BWTOT > 0) /* If total greater than zero*/

    /* ------------------------------------------------------------*/
     /* Calculate the percent each line is of the total defects.*/
    /* ------------------------------------------------------------*/
    FQ0BW00.FQ0BWIXM = 1; /* Initialize line index*/

    while (FQ0BW00.FQ0BWIXM <= 14 /* Until screen is full*/
     && FQ0BW01.YDCCD[FQ0BWIXM] > " ") /* and data exists*/

      /* ------------------------------------------------------------*/
       /* Divide line quantity by total defects for percent total.*/
      /* ------------------------------------------------------------*/
      FQ0BW00.FQ0BWFDA = mathLib.round(FQ0BW01.FQ2DEFQT[FQ0BWIXM] / FQ0BW00.FQ0BWTOT, -mathLib.decimals(FQ0BW00.FQ0BWFDA));

      /* ------------------------------------------------------------*/
       /* Multiply by 100 to get nnn.nn format.*/
      /* ------------------------------------------------------------*/
      FQ0BW00.FQ0BWFDA = FQ0BW00.FQ0BWFDA * 100; /* Times 100*/

      /* ------------------------------------------------------------*/
       /* Save percent in storage.*/
      /* ------------------------------------------------------------*/
      FQ0BW01.FQ0BWPCT[FQ0BWIXM] = FQ0BW00.FQ0BWFDA; /* percent*/

      /* ------------------------------------------------------------*/
       /* Add to counter.*/
      /* ------------------------------------------------------------*/
      FQ0BW00.FQ0BWIXM = FQ0BW00.FQ0BWIXM + 1; /* Add to counter*/

    end
  end


end // end FQ0B211

// Check attention identifier
Function FQ0B300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    if (FQ0BW00.FQ0BWSWR == "Y") /* Request for data switch*/
      set FQ0BW01 empty;
    end

    FQ0BW00.FQ0BWSWF = "Y"; /* Format the map switch*/
    FQ0BW00.FQ0BWSWR = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* Fast path to application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to redisplay map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (FQ0BM001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      FQ0BS01(); /* Purge any temporary storage*/

      XSFQS01(); /* Exit to another application*/

      set FQ0BM001.CATOAP cursor, bold; /* Highlight*/

    else
      converseLib.validationFailed(18); /* Indicate PF key not valid*/
      FQWREC.XXXMODID = "300"; /* Process name*/
    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* PF3 was pressed*/
   || converseVar.eventKey is pf4 /* PF4 was pressed*/
   || converseVar.eventKey is pf5 /* PF5 was pressed*/
   || converseVar.eventKey is pf14) /* PF14 was pressed*/
    /* OR EZEAID IS PF15             ; PF15 was pressed*/
    /* OR EZEAID IS PF16             ; PF16 was pressed*/
    /* OR EZEAID IS PF17             ; PF17 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FQ0BM001"; /* Move in map name for help*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FQ0M"; /* Set to factory application*/
    end

    if (converseVar.eventKey is pf14) /* PF14 was pressed*/
      COMMAREA.CATOAP = "FQ0C"; /* Set to factory ratio*/
    end

    if (converseVar.eventKey is pf15) /* PF15 was pressed*/
      COMMAREA.CATOAP = "FQ0D"; /* Set to customer screen*/
    end

    if (converseVar.eventKey is pf16) /* PF16 was pressed*/
      COMMAREA.CATOAP = "FQ0E"; /* Set to customer ratio screen*/
    end

    if (converseVar.eventKey is pf17) /* PF17 was pressed*/
      COMMAREA.CATOAP = "FQ0F"; /* Set to warehouse screen*/
    end

    FQ0BS01(); /* Purge any temporary storage*/

    XSFQS01(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf9)  /* and PF9 was not pressed*/

    FQWREC.XXXMODID = "300"; /* Process name*/
    converseLib.validationFailed(4); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end FQ0B300

// Reset highlighted fields
Function FQ0B400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to defined intensity.*/
   /* ------------------------------------------------------------*/
  set FQ0BM001.CATOAP initialAttributes; /* TO application*/
  set FQ0BM001.CAITEM initialAttributes; /* Application data*/

  set FQ0BM001.SY1STNBR initialAttributes; /* style nbr*/
  set FQ0BM001.SY2CLRID initialAttributes; /* color id*/
  set FQ0BM001.XDMCD initialAttributes; /* dimension*/
  set FQ0BM001.XDVCD initialAttributes; /* division*/
  set FQ0BM001.FY1PFYCD initialAttributes; /* origin factory*/

  set FQ0BM001.FQ0BMBDT initialAttributes; /* beg date*/
  set FQ0BM001.FQ0BMEDT initialAttributes; /* end date*/

   /* ------------------------------------------------------------*/
   /* Loop through lines, setting lines with data visable.*/
   /* ------------------------------------------------------------*/
  FQ0BW00.FQ0BWIXM = 1; /* Initialize line index*/

  while (FQ0BW00.FQ0BWIXM <= 14 /* Until end of lines*/
   && FQ0BW01.YDCCD[FQ0BWIXM] > " ") /* and data exists*/

    set FQ0BM001.FQ0BMSEL[FQ0BWIXM] normal; /* Reset select flag*/

    FQ0BW00.FQ0BWIXM = FQ0BW00.FQ0BWIXM + 1; /* Increment line index*/

  end

   /* ------------------------------------------------------------*/
   /* If no lists were requested, return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (FQ0BM001.XDVCD == "?") /* division list requested*/

    if (converseVar.eventKey not enter) /* ENTER was not pressed*/

      FQWREC.XXXMODID = "400"; /* Module identification*/
      converseLib.validationFailed(24); /* Indicate PF key not valid*/

      exit stack; /* Exit to display map*/
    end

  else
    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of division codes.*/
   /* ------------------------------------------------------------*/
  if (FQ0BM001.XDVCD == "?") /* division list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "DIVISION CODES"; /* Heading*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXDV01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FQ0BM001.XDVCD = FQWREC.XDVCD; /* Restore prior code*/
    else

      FQ0BM001.XDVCD = XDVTBL.XDVCD[XX0XWPIX]; /* Selected code*/
    end

    set FQ0BM001.XDVCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the requested lists have been displayed and*/
   /* that ENTER must be pressed to process any changes.*/
   /* ------------------------------------------------------------*/

  converseLib.clearScreen();
   /* Clear screen before display*/

  FQWREC.XXXMODID = "400"; /* Module identification*/
  converseLib.validationFailed(25); /* Indicate press ENTER*/

  exit stack; /* Exit to display map*/


end // end FQ0B400

// Verify the entered data
Function FQ0B500()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (FQ0BM001.CAITEM > " ") /* Application data was entered*/

    FQWREC.XXXMODID = "150"; /* Module identification*/
    set FQ0BM001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid request to scroll was entered.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    FQ0B510(); /* Verify scroll/add request*/

  else

    /* ------------------------------------------------------------*/
     /* Otherwise, verify data in the line.*/
    /* ------------------------------------------------------------*/

    FQ0BW00.FQ0BWIXM = 14; /* Initialize line index*/

    while (FQ0BW00.FQ0BWIXM > 0) /* Until end of lines*/

      FQ0B520(); /* Verify data in the line*/

      FQ0BW00.FQ0BWIXM = FQ0BW00.FQ0BWIXM - 1; /* Decrement line number*/

    end

    /* ------------------------------------------------------------*/
     /* If the search key was changed, verify the key fields.*/
    /* ------------------------------------------------------------*/
    if (FQ0BM001.SY1STNBR != FQWREC.SY1STNBR  /* Style*/
     || FQ0BM001.SY2CLRID != FQWREC.SY2CLRID  /* Color*/
     || FQ0BM001.XDMCD != FQWREC.XDMCD        /* Dimension*/
     || FQ0BM001.XDVCD != FQWREC.XDVCD        /* Division*/
     || FQ0BM001.FQ0BMBDT != FQ0BW00.FQ0BMBYM /* Beg date*/
     || FQ0BM001.FQ0BMEDT != FQ0BW00.FQ0BMEYM /* End date*/
     || FQ0BM001.FY1PFYCD != FQWREC.FY1PFYCD  /* Origin factory*/
                                              
     || FQ0BM001.SY1STNBR == " "              /* Style*/
     || FQ0BM001.SY2CLRID == " "              /* Color*/
     || FQ0BM001.XDMCD == " "                 /* Dimension*/
     || FQ0BM001.XDVCD == " "                 /* Division*/
     || FQ0BM001.FQ0BMBDT == 0                /* Beg date*/
     || FQ0BM001.FQ0BMEDT == 0                /* End date*/
     || FQ0BM001.FY1PFYCD == " ")             /* Origin factory*/

      FQ0B530(); /* Verify style/color/dim*/
      FQ0B540(); /* Verify division*/
      FQ0B550(); /* Verify dates*/
      FQ0B560(); /* Verify factory*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end FQ0B500

// Verify scroll
Function FQ0B510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && FQ0BW00.XXXTSPAG < 2)       /* and no previous pages*/

    FQWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(13); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && FQ0BW00.XXXTSPAG >= FQ0BW00.TA5TSQIX) /* and no more pages*/

    FQWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(14); /* No more pages to display*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate an error for any data that was modified.*/
   /* ------------------------------------------------------------*/

  FQ0BW00.FQ0BWIXM = 14; /* Initialize line index*/

  while (FQ0BW00.FQ0BWIXM > 0) /* Until end of lines*/

    if (FQ0BM001.FQ0BMSEL[FQ0BWIXM] != " " /* If selection was made*/
     && FQ0BM001.FQ0BMSEL[FQ0BWIXM] != "V") /* and line not reviewed*/

      FQWREC.XXXMODID = "510"; /* Module identification*/
      set FQ0BM001.FQ0BMSEL[FQ0BWIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(9); /* Indicate data may not change*/

    end

    FQ0BW00.FQ0BWIXM = FQ0BW00.FQ0BWIXM - 1; /* Decrement line number*/

  end


end // end FQ0B510

// Verify selection
Function FQ0B520()
   /* ------------------------------------------------------------*/
   /* If selection was made, ensure an "S" & no prior selections.*/
   /* ------------------------------------------------------------*/

  if (FQ0BM001.FQ0BMSEL[FQ0BWIXM] > " ") /* Selection was entered*/

    if (FQ0BM001.FQ0BMSEL[FQ0BWIXM] != "S") /* Valid selection character*/

      FQWREC.XXXMODID = "520"; /* Module identification*/
      set FQ0BM001.FQ0BMSEL[FQ0BWIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(12); /* Indicate only "S" may be used*/

    else
      /* ------------------------------------------------------------*/
       /* Verify that no other selection was made at the same time.*/
      /* ------------------------------------------------------------*/
      FQ0BW00.FQ0BWIX2 = FQ0BW00.FQ0BWIXM - 1; /* Set counter*/

      while (FQ0BW00.FQ0BWIX2 > 0) /* While counter greater than 0*/
        if (FQ0BM001.FQ0BMSEL[FQ0BWIX2] == "S") /* Another selection made*/

          FQWREC.XXXMODID = "520"; /* Module identification*/
          set FQ0BM001.FQ0BMSEL[FQ0BWIXM] cursor, bold; /* Highlight*/
          converseLib.validationFailed(8); /* More than one selection*/
        end

        FQ0BW00.FQ0BWIX2 = FQ0BW00.FQ0BWIX2 - 1; /* Decrement counter*/
      end

    end
  end


end // end FQ0B520

// Verify style/color/dim
Function FQ0B530()
   /* ------------------------------------------------------------*/
    /* If missing, default dimension.*/
