          set TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR] initialAttributes;
          set TI0BM001.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR] initialAttributes;
          set TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] initialAttributes;

          TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] = TI0BW01.XXX-LN-SEL-IND[XXX-INX-FLD-NBR];
          if (TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "*    ")
            TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] = "  *  ";
          end
          TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] = TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR];
          TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] = TI0BW01.FY-FCTY-CD[XXX-INX-FLD-NBR];
          TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] = TI0BW01.XCT-CTRY-CD[XXX-INX-FLD-NBR];
          TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] = TI0BW01.XOB-BASE-OT-CD[XXX-INX-FLD-NBR];
          TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] = TI0BW01.XGP-GPC-CD[XXX-INX-FLD-NBR];
          TI0BM001.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR] = TI0BW01.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR];
          TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR] = TI0BW01.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR];
          if (TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] > " ")
            TI0BM001.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR] = TI0BW01.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR];
          end
          TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] = TI0BW01.XXX-STAT[XXX-INX-FLD-NBR];


          TI0BW00.XXX-INX-FLD-NBR = TI0BW00.XXX-INX-FLD-NBR + 1;

