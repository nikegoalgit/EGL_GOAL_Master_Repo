//*** PROGRAM=TI0B ****
// This application allows a user to inquire on rows
// in a DB2 table.
// 
// The application will fill the screen initially with all rows
// from the table.  The user may then scroll within the
// selected
// set.
// 
// The user may exit to any allowed application, via FAST PATH
// or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE    USERID  DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      10/08/96  jbuch1  New application.
//          01/12/97  jbuch1  Added update functionality.
//          01/22/97  bchan1  fixed zz_sdt on insert row.
// *********************
Program TI0B type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "TI00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  TI0BW00 TI0BW00; // record
  TI0BW01 TI0BW01; // record
  TI0BW02 TI0BW02; // record
  TI0BWCA1 TI0BWCA1; // record
  TI0BWM01 TI0BWM01; // record
  TIWREC TIWREC; // record
  VFCTY002 VFCTY002; // record
  VNILOV01 VNILOV01; // record
  VTODFL01 VTODFL01; // record
  VTOPCD01 VTOPCD01; // record
  VTOXCD01 VTOXCD01; // record
  XX0XW01 XX0XW01; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XCTTBL {deleteAfterUse = yes}; // table
  use XOBTBL {deleteAfterUse = yes}; // table
  use XGPTBL; // table
  use TI0BM.TI0BM001 // form
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPTI-00000: XPTI-00000();
    if (COMMAREA.CAFRMSYS == "GD")

      TI0BWCA1.TI0BWCA1-REC = COMMAREA.CA-USER-WORKAREA-2K-B;

      TI0BW00.TI0BW00-REC = TI0BWCA1.TI0BWCA1-TI0BW00;

      TI0BW01.TI0BW01-REC = TI0BWCA1.TI0BWCA1-TI0BW01;

      TI0BW02.TI0BW02-REC = TI0BWCA1.TI0BWCA1-TI0BW02;

      TI0BWM01.TI0BWM01-REC = TI0BWCA1.TI0BWCA1-TI0BWM01;

      TA5REC.TA5DATA = TI0BWCA1.TI0BWCA1-TA5REC;

      COMMAREA.CAADD = TI0BWCA1.TI0BWCA1-CAADD;
      COMMAREA.CACHG = TI0BWCA1.TI0BWCA1-CACHG;
      COMMAREA.CADEL = TI0BWCA1.TI0BWCA1-CADEL;
      COMMAREA.CAINQ = TI0BWCA1.TI0BWCA1-CAINQ;

      TIWREC.CAUSERA = COMMAREA.CAUSERA;

      TIWREC.XXX-MSG-LOC-ID = TI0BWCA1.XXX-MSG-LOC-ID;

      TIWREC.XXX-MSG-NBR = TI0BWCA1.XXX-MSG-NBR;

      TIWREC.TI-CUTK-XMIT-CD = TI0BWCA1.TI-CUTK-XMIT-CD;

      TI0BM001.XXX-MSG-LOC-ID = TI0BWM01.XXX-MSG-LOC-ID;

      TI0BM001.XXX-CURR-DT = TI0BWM01.XXX-CURR-DT;

      TI0BM001.XXX-CURR-PAGE-DESC = TI0BWM01.XXX-CURR-PAGE-DESC;

      TI0BM001.XXX-CURR-PAGE-NBR = TI0BWM01.XXX-CURR-PAGE-NBR;

      TI0BM001.XXX-HI-PAGE-DESC = TI0BWM01.XXX-HI-PAGE-DESC;

      TI0BM001.XXX-HI-PAGE-NBR = TI0BWM01.XXX-HI-PAGE-NBR;

      TI0BW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

      TI0BW00.XXX-INX-LMT-NBR = 13;

      while (TI0BW00.XXX-INX-FLD-NBR <= TI0BW00.XXX-INX-LMT-NBR)

        TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] = TI0BWM01.XXX-LN-SEL-IND[XXX-INX-FLD-NBR];

        TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] = TI0BWM01.PC-PRDCU-ID[XXX-INX-FLD-NBR];

        TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] = TI0BWM01.FY-FCTY-CD[XXX-INX-FLD-NBR];

        TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] = TI0BWM01.XCT-CTRY-CD[XXX-INX-FLD-NBR];

        TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] = TI0BWM01.XOB-BASE-OT-CD[XXX-INX-FLD-NBR];

        TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] = TI0BWM01.XGP-GPC-CD[XXX-INX-FLD-NBR];

        TI0BM001.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR] = TI0BWM01.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR];

        TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR] = TI0BWM01.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR];

        TI0BM001.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR] = TI0BWM01.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR];

        TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] = TI0BWM01.XXX-STAT[XXX-INX-FLD-NBR];

        set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] initialAttributes;
        if (TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] > " ")
          set TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] protect, noHighLight, blue;
          set TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
          set TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
          set TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
          set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
        else
          set TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] initialAttributes;
          set TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] initialAttributes;
          set TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] initialAttributes;
          set TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] initialAttributes;
          set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] initialAttributes;
        end
        set TI0BM001.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR] initialAttributes;
        set TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR] initialAttributes;
        set TI0BM001.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR] initialAttributes;
        set TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] initialAttributes;

        TI0BW00.XXX-INX-FLD-NBR = TI0BW00.XXX-INX-FLD-NBR + 1;

      end

      TI0BW00.XXX-INX-LMT-NBR = 12;

      TI0BW00.XXX-RQT-DATA-FL = "N";

      TI0BW00.XXX-MAP-FMAT-FL = "N";

    else

      set TI0BM001 initial; /* M001 input/output*/
      set TI0BW00 empty; /* Working storage*/
      set TI0BW01 empty; /* M001 display storage*/
      set TI0BW02 empty; /* M002 display storage*/
      set TI0BWM01 empty; /* M001 display storage*/
      set SQLCA empty; /* SQL communication area*/
      set TAEREC empty; /* Error diagnosis information*/
      set TA1REC empty; /* Error diagnosis parameters*/
      set TA5REC empty; /* Temporary storage parameters*/
      set XX0XW01 empty; /* Date/time subroutine parameters*/

      TI0BW00.XXX-RQT-DATA-FL = "Y"; /* Request for data flag*/
      TI0BW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

      TI0BW00.XXX-MAP-DSP-CD = "1"; /* Map to be displayed*/

      TI0BW00.XXX-INX-LMT-NBR = 12; /* Number of lines on a page*/
    end

    TI0BW00.XXX-TEMP-STG-LNTH = 1024; /* Temporary storage length*/

    VGVar.handleHardIOErrors = 1; /* Return all file errors*/
    converseVar.commitOnConverse = 1; /* Commit on every converse*/

    goto TI0B-00000; /* Converse the main process*/
    TI0B-00000: TI0B-00000();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto TI0B-00000; /* Converse the main process*/


  end // end main
end // end TI0B

// Determine which map to display
Function TI0B-00000()
  /* -----------------------------------------------------*/
  /* drive the application*/
  /* -----------------------------------------------------*/

  TI0B-10000(); /* Display map 1*/

  exit stack; /* Exit to display map*/


end // end TI0B-00000

// Main process to display a map
Function TI0B-10000()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Verify the data that was entered.*/
   /* 5) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  TI0B-11000(); /* Obtain the data to display*/

  TI0B-12000(); /* Format and display the map*/

  TI0B-13000(); /* Check attention identifier*/

  TI0B-14000(); /* Verify the entered data*/

  TI0B-15000(); /* Process valid input request*/


end // end TI0B-10000

// Obtain the data to display
Function TI0B-11000()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (TI0BW00.XXX-RQT-DATA-FL != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* ------------------------------------------------------------*/

  if (TIWREC.XXX-MSG-NBR == 0) /* No message to display*/
    TI0BW00.XXX-CURR-PAGE-NBR = 1; /* Page number to display*/
  end

   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/

  TI0BS-TA0050-P(); /* Purge any temporary storage*/

  TI0BW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/


   /* ------------------------------------------------------------*/
   /* Select the information to display in the map heading.*/
   /* ------------------------------------------------------------*/


   /* ------------------------------------------------------------*/
   /* Select and open the cursor for the set to be formatted.*/
   /* ------------------------------------------------------------*/

  VTODFL01.PC-PRDCU-ID = "*";
  VTODFL01.FY-FCTY-CD = "*";
  VTODFL01.XCT-CTRY-CD = "*";
  VTODFL01.XOB-BASE-OT-CD = "*";
  VTODFL01.XGP-GPC-CD = "*";

  TI0BS-VTODFL01-SS(); /* Select set and open cursor*/

   /* ------------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ------------------------------------------------------------*/

  TI0BW00.XXX-INX-FLD-NBR = 1; /* Display line index*/

  TI0BW00.TI0BW-EOF-FL = "N"; /* End of cursor flag*/

  while (TI0BW00.TI0BW-EOF-FL == "N") /* Until end of cursor*/

    TI0BS-VTODFL01-SN(); /* Fetch next row in cursor*/

    /* ------------------------------------------------------------*/
    /* If not end of cursor, save the data in display storage.*/
    /* ------------------------------------------------------------*/

    if (TI0BW00.TI0BW-EOF-FL == "N") /* Not end of cursor*/

      TI0BW00.TI0BW-INIT-LKUP-IND = "N";
      /* ------------------------------------------------------------*/
      /* If first line on subsequent page, save heading information.*/
      /* ------------------------------------------------------------*/

      if (TI0BW00.XXX-HI-PAGE-NBR > 0 /* Not the first page*/
       && TI0BW00.XXX-INX-FLD-NBR == 1) /* and first line on page*/


      end

      /* ------------------------------------------------------------*/
      /* Save the data from the table row in display storage.*/
      /* ------------------------------------------------------------*/

      if (VTODFL01.PC-PRDCU-ID == "*    ")
        TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] = "  *  ";
      else
        TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] = VTODFL01.PC-PRDCU-ID;
      end
      TI0BW01.FY-FCTY-CD[XXX-INX-FLD-NBR] = VTODFL01.FY-FCTY-CD;
      TI0BW01.XCT-CTRY-CD[XXX-INX-FLD-NBR] = VTODFL01.XCT-CTRY-CD;
      TI0BW01.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] = VTODFL01.XOB-BASE-OT-CD;
      TI0BW01.XGP-GPC-CD[XXX-INX-FLD-NBR] = VTODFL01.XGP-GPC-CD;
      TI0BW01.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR] = VTODFL01.TI-CUTK-SPLR-CD;
      TI0BW01.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR] = VTODFL01.TI-CUTK-XMIT-CD;
      TI0BW01.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR] = VTODFL01.TI-CUTK-DAY-QTY;
      TI0BW01.XXX-STAT[XXX-INX-FLD-NBR] = VTODFL01.XXX-STAT;
      TI0BW01.ZZ-SDT[XXX-INX-FLD-NBR] = VTODFL01.ZZ-SDT;
      TI0BW01.ZZ-CHNG-DT[XXX-INX-FLD-NBR] = VTODFL01.ZZ-CHNG-DT;
      TI0BW01.ZZ-CHNG-HHMM[XXX-INX-FLD-NBR] = VTODFL01.ZZ-CHNG-HHMM;
      TI0BW01.ZZ-CHNG-CNT[XXX-INX-FLD-NBR] = VTODFL01.ZZ-CHNG-CNT;

      /* ------------------------------------------------------------*/
      /* Increment the line index to the next row in display storage.*/
      /* ------------------------------------------------------------*/

      TI0BW00.XXX-INX-FLD-NBR = TI0BW00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/

    end

    /* ------------------------------------------------------------*/
    /* If the page is full, add the page to the storage queue.*/
    /* ------------------------------------------------------------*/

    if (TI0BW00.XXX-INX-FLD-NBR > TI0BW00.XXX-INX-LMT-NBR) /* Page is full*/

      TI0BS-TA0050-A(); /* Add page to temporary storage*/

      TI0BW00.XXX-INX-FLD-NBR = 1; /* Display line index*/

    end

  end

   /* ------------------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ------------------------------------------------------------*/

  if (TI0BW00.XXX-INX-FLD-NBR > 1) /* Page was formatted*/
    TI0BS-TA0050-A(); /* Add page to temporary storage*/
  end

   /* ------------------------------------------------------------*/
   /* Close the cursor for the selected set.*/
   /* ------------------------------------------------------------*/

  TI0BS-VTODFL01-CS(); /* Close selected set cursor*/

   /* ------------------------------------------------------------*/
   /* If no pages were formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/

  if (TI0BW00.XXX-HI-PAGE-NBR == 0) /* No pages formatted*/

    TIWREC.XXX-MSG-LOC-ID = "11000"; /* Message location*/
    converseLib.validationFailed(18); /* No data was found to display*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  TI0BW00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/


end // end TI0B-11000

// Format and converse the map
Function TI0B-12000()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/
    TIWREC.XXX-MSG-NBR = converseVar.validationMsgNum; /* Message number*/
  end

   /* ------------------------------------------------------------*/
   /* If the map must be formatted, format the data in the map.*/
   /* ------------------------------------------------------------*/

  if (TI0BW00.XXX-MAP-FMAT-FL == "Y") /* Map must be formatted*/
    TI0B-12100(); /* Format the data in the map*/
  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  TI0BM001.XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/

  if (TI0BWM01.CATOAP > " ")
    TI0BM001.CATOAP = TI0BWM01.CATOAP; /* TO application*/
  else
    TI0BM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  end

  if (TI0BWM01.CAITEM > " ")
    TI0BM001.CAITEM = TI0BWM01.CAITEM; /* TO application*/
  else
    TI0BM001.CAITEM = COMMAREA.CAITEM; /* TO application*/
  end

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (TIWREC.XXX-MSG-NBR != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ")  /* or message text*/

    if (TIWREC.XXX-MSG-NBR != 0) /* Message to display*/

      if (TIWREC.XXX-MSG-NBR == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(TIWREC.XXX-MSG-NBR);
      end /* Message number*/
    else
      TI0BM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    TI0BM001.XXX-MSG-LOC-ID = TIWREC.XXX-MSG-LOC-ID;
                                   /* Message location*/

    set TI0BM001 alarm; /* Indicate sound alarm*/

  end

  converse TI0BM001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  TIWREC.XXX-MSG-LOC-ID = " "; /* Message location*/
  TIWREC.XXX-MSG-NBR = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/

  COMMAREA.CATOAP = TI0BM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = TI0BM001.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* Reset modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/


  TI0BW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (TI0BW00.XXX-INX-FLD-NBR <= TI0BW00.XXX-INX-LMT-NBR)
                                   /* Until end of screen*/
    set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] normal;
   /* SET TI0BM001.PC-PRDCU-ID(XXX-INX-FLD-NBR) NORMAL;*/
   /* SET TI0BM001.FY-FCTY-CD(XXX-INX-FLD-NBR) NORMAL;*/
   /* SET TI0BM001.XCT-CTRY-CD(XXX-INX-FLD-NBR) NORMAL;*/
   /* SET TI0BM001.XGP-GPC-CD(XXX-INX-FLD-NBR) NORMAL;*/
    set TI0BM001.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR] normal;
    set TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR] normal;
    set TI0BM001.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR] normal;
    set TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] normal;

    TI0BW00.XXX-INX-FLD-NBR = TI0BW00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/

  end

  set TI0BM001.CATOAP normal; /* TO application*/
  set TI0BM001.CAITEM normal; /* Application data*/


end // end TI0B-12000

// Format the data in the map
Function TI0B-12100()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set TI0BM001 initial; /* M001 input/output area*/

  TI0BW00.XXX-MAP-FMAT-FL = "N"; /* Format the map flag*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (TI0BW00.XXX-HI-PAGE-NBR > 0) /* Pages were formatted*/

    if (TI0BW00.XXX-CURR-PAGE-NBR != TA5REC.TA5TSQIX)
                                   /* Desired page not current*/

      /* ------------------------------------------------------------*/
      /* If the current page has been changed, update the page in*/
      /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/

      if (TI0BW00.XXX-PAGE-UPD-FL == "Y") /* Current page was changed*/

        TI0BS-TA0050-U(); /* Update temporary storage page*/

        TI0BW00.XXX-PAGE-UPD-FL = "N"; /* Page update flag*/

      end

      /* ------------------------------------------------------------*/
      /* If the desired page number is greater than the highest page*/
      /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (converseVar.eventKey is pf8 /* Scroll forward request*/
       && TI0BW00.XXX-CURR-PAGE-NBR >= TI0BW00.XXX-HI-PAGE-NBR)
                                  /* and no more pages*/
        if (TI0BW00.PAGE-FULL-SW == "Y")
          set TI0BW01 empty;
          TI0BW00.XXX-HI-PAGE-NBR = TI0BW00.XXX-HI-PAGE-NBR + 1;
          TI0BS-TA0050-A();

        end
      end

      if (TI0BW00.XXX-CURR-PAGE-NBR > TI0BW00.XXX-HI-PAGE-NBR)
                                   /* Desired greater highest*/
        TI0BW00.XXX-CURR-PAGE-NBR = TI0BW00.XXX-HI-PAGE-NBR;
                                   /* Default to highest*/
      end

      TI0BS-TA0050-R(); /* Read temporary storage page*/

    end

    /* ------------------------------------------------------------*/
    /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    TI0BM001.XXX-CURR-PAGE-DESC = "PAGE";

    set TI0BM001.XXX-CURR-PAGE-DESC skip;

    TI0BM001.XXX-CURR-PAGE-NBR = TI0BW00.XXX-CURR-PAGE-NBR;

    TI0BM001.XXX-HI-PAGE-DESC = "OF";

    set TI0BM001.XXX-HI-PAGE-DESC skip;

    TI0BM001.XXX-HI-PAGE-NBR = TI0BW00.XXX-HI-PAGE-NBR;

  end

   /* ------------------------------------------------------------*/
   /* If code is missing, position the cursor on that field.*/
   /* ------------------------------------------------------------*/

   /* END;*/

   /* ------------------------------------------------------------*/
   /* Format the map fields from the display storage area.*/
   /* ------------------------------------------------------------*/


  TI0BW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (TI0BW00.XXX-INX-FLD-NBR <= TI0BW00.XXX-INX-LMT-NBR)
                                   /* Until screen is full*/
                                  /* AND*/

    set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] initialAttributes;
    if (TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] > " ")
      set TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] protect, noHighLight, blue;
      set TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
      set TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
      set TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
      set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
    else
      set TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] initialAttributes;
      set TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] initialAttributes;
      set TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] initialAttributes;
      set TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] initialAttributes;
      set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] initialAttributes;
    end
    set TI0BM001.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR] initialAttributes;
    set TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR] initialAttributes;
    set TI0BM001.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR] initialAttributes;
