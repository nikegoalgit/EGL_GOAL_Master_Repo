   || TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "  *  "
   || TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "   * "
   || TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "    *")
   && TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] == "*"
   && TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] == "*")
  /* CONTINUE*/
  else
    if (TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] in XCTTBL.XCTABRV
     && XCTTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active status*/
  /* CONTINUE*/
    else
      TIWREC.XXX-MSG-LOC-ID = "14450"; /* Message location*/
      converseLib.validationFailed(81); /* INVALID COUNTRY ABRV*/
      set TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] cursor, bold;
      return;
    end
  end
  /* -------------------------------------------------------------*/
  /* Determine if order type is valid.*/
  /* -------------------------------------------------------------*/
  if ((TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "*    "
   || TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == " *   "
   || TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "  *  "
   || TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "   * "
   || TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "    *")
   && TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] == "*"
   && TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] == "*"
   && TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] == "*")
  /* CONTINUE*/
  else
    if (TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] in XOBTBL.XOBCD
     && XOBTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active status*/
  /* CONTINUE*/
    else
      TIWREC.XXX-MSG-LOC-ID = "14450"; /* Message location*/
      converseLib.validationFailed(82); /* INVALID ORDER TYPE*/
      set TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] cursor, bold;
      return;
    end
  end
  /* -------------------------------------------------------------*/
  /* Determine if GPC code is valid.*/
  /* -------------------------------------------------------------*/
  if (TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] in XGPTBL.XGPCD
   && XGPTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active status*/
  /* CONTINUE*/
  else
    TIWREC.XXX-MSG-LOC-ID = "14450"; /* Message location*/
    converseLib.validationFailed(28); /* INVALID GPC CODE*/
    set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] cursor, bold;
    return;
  end
  /* -------------------------------------------------------------*/
  /* Determine if supplier code is valid.*/
  /* -------------------------------------------------------------*/
  VTOPCD01.TI-CUTK-SPLR-CD = TI0BM001.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR];
  TI0BS-VTOPCD01-S();
  if (sysVar.errorCode == "00000000"
   && SQLCA.VAGen_SQLCODE == 0)
  /* CONTINUE*/
  else
    return;
  end
  /* -------------------------------------------------------------*/
  /* Determine if transmittal code is valid.*/
  /* -------------------------------------------------------------*/
  VTOXCD01.TI-CUTK-XMIT-CD = TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR];
  TI0BS-VTOXCD01-S();
  if (sysVar.errorCode == "00000000"
   && SQLCA.VAGen_SQLCODE == 0)
  /* CONTINUE*/
  else
    return;
  end
  /* -------------------------------------------------------------*/
  /* Determine if status code is valid.  It may be blank for*/
  /* Add because of below default. Otherwise must be 'A' or 'I'.*/
  /* -------------------------------------------------------------*/
  if (TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] != " "
   && TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] != "A"
   && TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] != "I")
    TIWREC.XXX-MSG-LOC-ID = "14450"; /* Message location*/
    set TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] cursor, bold;
    return;
  end
  /*  */
  /* -------------------------------------------------------------*/
  /* Determine if supplier code has changed. If so, validate and*/
  /* return supplier abbreviation.*/
  /* -------------------------------------------------------------*/
  /* IF TI0BM001.PC-PRDCU-ID(XXX-INX-FLD-NBR)*/
  /* NE TI0BW01.PC-PRDCU-ID(XXX-INX-FLD-NBR)*/
  /* OR TI0BM001.XXX-LN-SEL-IND(XXX-INX-FLD-NBR) = 'A';*/
  /* MOVE TI0BM001.PC-PRDCU-ID(XXX-INX-FLD-NBR) TO*/
  /* VTODFL01.PC-PRDCU-ID;*/
  /* TI0BS-VTODFL01-S;*/
  /* IF EZEMNO = 0;*/
  /* MOVE VTODFL01.FY-FCTY-CD TO*/
  /* TI0BM001.FY-FCTY-CD(XXX-INX-FLD-NBR);*/
  /* MOVE VTODFL01.XCT-CTRY-CD TO*/
  /* TI0BM001.XCT-CTRY-CD(XXX-INX-FLD-NBR);*/
  /* ELSE;*/
  /* EZERTN;*/
  /* END;*/
  /* END;*/
end // end TI0B-14450

