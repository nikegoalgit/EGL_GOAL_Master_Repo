      TI0BW00.XXX-INX-LMT-NBR = 13;

      while (TI0BW00.XXX-INX-FLD-NBR <= TI0BW00.XXX-INX-LMT-NBR)

        if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != " ")

          if (TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == "  *  ")
      /* MOVE '*    ' TO TI0BM001.PC-PRDCU-ID(XXX-INX-FLD-NBR);*/
