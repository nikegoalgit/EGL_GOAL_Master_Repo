// Verify scroll request
Function TI0B-14200()
   /* ------------------------------------------------------------*/
   /* Indicate key fields may not change when scroll is requested.*/
   /* ------------------------------------------------------------*/





   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && TI0BW00.XXX-CURR-PAGE-NBR < 2) /* and no previous pages*/

    TIWREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
    converseLib.validationFailed(2); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && TI0BW00.XXX-CURR-PAGE-NBR >= TI0BW00.XXX-HI-PAGE-NBR)
                                  /* and no more pages*/
    if (TI0BM001.PC-PRDCU-ID[XXX-INX-LMT-NBR] > " ")
      /* Something on last line of current pg*/
      TI0BW00.PAGE-FULL-SW = "Y";
    else
      TI0BW00.PAGE-FULL-SW = "N";
      TIWREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(3); /* No more pages to display*/

    end
  end


end // end TI0B-14200

// Verify data in the line
Function TI0B-14400()
  if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != " "
   && TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != "A"
   && TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != "C")
    TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
    converseLib.validationFailed(40); /* Only "A" or "C" may be entered*/
    set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
    return; /* Return to previous process*/
  end
  /*  */
  /* -------------------------------------------------------------*/
  /* Determine if row has changed. If action changed with no row*/
  /* changes, return error message.*/
  /* -------------------------------------------------------------*/
  TI0BW-ROW-CHANGED-FL = "N";
  if (TI0BM001.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR]
   != TI0BW01.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR]
   || TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR]
   != TI0BW01.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR]
   || TI0BM001.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR]
   != TI0BW01.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR]
   || TI0BM001.XXX-STAT[XXX-INX-FLD-NBR]
   != TI0BW01.XXX-STAT[XXX-INX-FLD-NBR])
    TI0BW-ROW-CHANGED-FL = "Y";
  else
    if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != " ")
      converseLib.validationFailed(46);
      set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] bold, cursor;
      return;
    end
  end

  /* -------------------------------------------------------------*/
  /* See if row changed with no action.*/
  /* -------------------------------------------------------------*/
  if (TI0BW-ROW-CHANGED-FL == "Y")
    if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == " "
    ) /* User changed line and didn't enter*/
                                   /* action*/
      TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
      converseLib.validationFailed(41); /* Action code missing/refresh*/
      set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
      return; /* Return to previous process*/
    end
  end

  /* -------------------------------------------------------------*/
  /* Check user's security clearance.*/
  /* -------------------------------------------------------------*/
  if (TI0BW-ROW-CHANGED-FL == "Y")
    if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "A"
    ) /* User attempting an ADD*/
      if (COMMAREA.CAADDFL != "Y")
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        converseLib.validationFailed(63); /* User not authorized for update*/
        TI0BS-RFR-LN(); /* Refresh the line*/
        set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
        return; /* Return to previous process*/
      else
        if (TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] == "03")
          if (COMMAREA.CAALVL[2] == "N") /* FOR GPC CODE 03*/
            set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] bold, cursor;
            converseLib.validationFailed(65);
            return; /* Return to previous process*/
          end
        else
          if (TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] == "02")
            if (COMMAREA.CAALVL[1] == "N") /* FOR GPC CODE 02*/
              set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] bold, cursor;
              converseLib.validationFailed(64);
              return; /* Return to previous process*/
            end
          end
        end
      end
    else
      if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "C"
      ) /* User attempting a CHANGE*/
        if (COMMAREA.CACHGFL != "Y")
          TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
          converseLib.validationFailed(63); /* User not authorized for update*/
          TI0BS-RFR-LN(); /* Refresh the line*/
          set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
          return; /* Return to previous process*/
        else
          if (TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] == "03")
            if (COMMAREA.CACLVL[2] == "N") /* FOR GPC CODE 03*/
              set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] bold, cursor;
              converseLib.validationFailed(65);
              return; /* Return to previous process*/
            end
          else
            if (TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] == "02")
              if (COMMAREA.CACLVL[1] == "N") /* FOR GPC CODE 02*/
                set TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] bold, cursor;
                converseLib.validationFailed(64);
                return; /* Return to previous process*/
              end
            end
          end
        end
      end
    end
  end

  /* -------------------------------------------------------------*/
  /* See if User is attempting to change key fields.*/
  /* -------------------------------------------------------------*/
  if (TI0BW-ROW-CHANGED-FL == "Y")
    if (TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR]
     != TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR])
      if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "C")
        if (TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] == " ")
          converseLib.validationFailed(75); /* Action code must be 'A' to add*/
          set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
        else
          converseLib.validationFailed(45); /* Cannot change key fields*/
          set TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] cursor, bold;
        end
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        return; /* Return to previous process*/
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* Determine if user is duplicating data on another row of the*/
   /* current screen.*/
   /* ------------------------------------------------------------*/
  if (TI0BW-ROW-CHANGED-FL == "Y")
    TI0BW-INX-FLD-NBR = 1;
    while (TI0BW-INX-FLD-NBR < XXX-INX-FLD-NBR)
      if (TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] == 
      TI0BM001.PC-PRDCU-ID[TI0BW-INX-FLD-NBR]
       && TI0BM001.FY-FCTY-CD[XXX-INX-FLD-NBR] == 
      TI0BM001.FY-FCTY-CD[TI0BW-INX-FLD-NBR]
       && TI0BM001.XCT-CTRY-CD[XXX-INX-FLD-NBR] == 
      TI0BM001.XCT-CTRY-CD[TI0BW-INX-FLD-NBR]
       && TI0BM001.XOB-BASE-OT-CD[XXX-INX-FLD-NBR] == 
      TI0BM001.XOB-BASE-OT-CD[TI0BW-INX-FLD-NBR]
       && TI0BM001.XGP-GPC-CD[XXX-INX-FLD-NBR] == 
      TI0BM001.XGP-GPC-CD[TI0BW-INX-FLD-NBR]
       && TI0BM001.TI-CUTK-SPLR-CD[XXX-INX-FLD-NBR] == 
      TI0BM001.TI-CUTK-SPLR-CD[TI0BW-INX-FLD-NBR]
       && TI0BM001.TI-CUTK-XMIT-CD[XXX-INX-FLD-NBR] == 
      TI0BM001.TI-CUTK-XMIT-CD[TI0BW-INX-FLD-NBR]
       && TI0BM001.TI-CUTK-DAY-QTY[XXX-INX-FLD-NBR] == 
      TI0BM001.TI-CUTK-DAY-QTY[TI0BW-INX-FLD-NBR]
       && TI0BM001.XXX-STAT[XXX-INX-FLD-NBR] == 
      TI0BM001.XXX-STAT[TI0BW-INX-FLD-NBR])
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        converseLib.validationFailed(44); /* Line is already on screen*/
        set TI0BM001.PC-PRDCU-ID[XXX-INX-FLD-NBR] bold, cursor;
        return; /* Return to previous process*/
      end
      TI0BW-INX-FLD-NBR = TI0BW-INX-FLD-NBR + 1;
    end
  end

   /* ------------------------------------------------------------*/
   /* If selection entered was "A", determine if user is attempting*/
   /* to add to existing data on the screen.*/
   /* ------------------------------------------------------------*/
  if (TI0BW-ROW-CHANGED-FL == "Y")
    if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "A")
      if (TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] != " "
       || TI0BW01.FY-FCTY-CD[XXX-INX-FLD-NBR] != " "
       || TI0BW01.XCT-CTRY-CD[XXX-INX-FLD-NBR] != " "
       || TI0BW01.XXX-STAT[XXX-INX-FLD-NBR] != " ")
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        converseLib.validationFailed(39); /* Only add to blank line at end*/
        set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
        return; /* Return to previous process*/
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* If selection entered was "C", determine if user is attempting*/
   /* change a blank line.*/
   /* ------------------------------------------------------------*/
  if (TI0BW-ROW-CHANGED-FL == "Y")
    if (TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "C")
      if (TI0BW01.PC-PRDCU-ID[XXX-INX-FLD-NBR] == " "
       && TI0BW01.FY-FCTY-CD[XXX-INX-FLD-NBR] == " "
       && TI0BW01.XCT-CTRY-CD[XXX-INX-FLD-NBR] == " "
       && TI0BW01.XXX-STAT[XXX-INX-FLD-NBR] == " ")
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        converseLib.validationFailed(42); /* Only Change existing line/refresh*/
        set TI0BM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
        return; /* Return to previous process*/
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* Validate entered detail.*/
   /* ------------------------------------------------------------*/
  if (TI0BW-ROW-CHANGED-FL == "Y")
    TI0B-14450();
    if (converseVar.validationMsgNum == 0)
      TI0BW00.XXX-PAGE-UPD-FL = "Y";
    end
  end

end // end TI0B-14400

