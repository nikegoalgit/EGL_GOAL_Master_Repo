//*** PROGRAM=TI0K ****
// This application allows a user to inquire on rows
// in a DB2 table.
// 
// The application will fill the screen initially with all rows
// from the table.  The user may then scroll within the
// selected
// set.
// 
// The user may exit to any allowed application, via FAST PATH
// or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE    USERID  DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      01/21/97  BCHAN1  New application.
// *********************
Program TI0K type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "TI00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  TI0KW00 TI0KW00; // record
  TI0KW01 TI0KW01; // record
  TI0KWCA1 TI0KWCA1; // record
  TI0KWM01 TI0KWM01; // record
  TIWREC TIWREC; // record
  VTODCD01 VTODCD01; // record
  XX0XW01 XX0XW01; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use TI0KM.TI0KM001 // form
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPTI-00000: XPTI-00000();
    if (COMMAREA.CAFRMSYS == "GD")

      TI0KWCA1.TI0KWCA1-REC = COMMAREA.CA-USER-WORKAREA-2K-B;

      TI0KW00.TI0KW00-REC = TI0KWCA1.TI0KWCA1-TI0KW00;

      TI0KW01.TI0KW01-REC = TI0KWCA1.TI0KWCA1-TI0KW01;

      TI0KWM01.TI0KWM01-REC = TI0KWCA1.TI0KWCA1-TI0KWM01;

      TIWREC.CAUSERA = COMMAREA.CAUSERA;

      TIWREC.XXX-MSG-LOC-ID = TI0KWCA1.XXX-MSG-LOC-ID;

      TIWREC.XXX-MSG-NBR = TI0KWCA1.XXX-MSG-NBR;

      TIWREC.TI-CUTK-XMIT-CD = TI0KWCA1.TI-CUTK-XMIT-CD;

      TI0KM001.XXX-MSG-LOC-ID = TI0KWM01.XXX-MSG-LOC-ID;

      TI0KM001.XXX-CURR-DT = TI0KWM01.XXX-CURR-DT;

      TI0KM001.XXX-CURR-PAGE-DESC = TI0KWM01.XXX-CURR-PAGE-DESC;

      TI0KM001.XXX-CURR-PAGE-NBR = TI0KWM01.XXX-CURR-PAGE-NBR;

      TI0KM001.XXX-HI-PAGE-DESC = TI0KWM01.XXX-HI-PAGE-DESC;

      TI0KM001.XXX-HI-PAGE-NBR = TI0KWM01.XXX-HI-PAGE-NBR;

      TI0KW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

      TI0KW00.XXX-INX-LMT-NBR = 13;

      while (TI0KW00.XXX-INX-FLD-NBR <= TI0KW00.XXX-INX-LMT-NBR)

        TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] = TI0KWM01.XXX-LN-SEL-IND[XXX-INX-FLD-NBR];

        TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] = TI0KWM01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR];

        if (TI0KWM01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] > " "
         || TI0KWM01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] > " "
         || TI0KWM01.XXX-STAT[XXX-INX-FLD-NBR] > " ")

          set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
        else

          set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] initialAttributes;

        end

        TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] = TI0KWM01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR];

        TI0KM001.XXX-STAT[XXX-INX-FLD-NBR] = TI0KWM01.XXX-STAT[XXX-INX-FLD-NBR];

        TI0KW00.XXX-INX-FLD-NBR = TI0KW00.XXX-INX-FLD-NBR + 1;
                                       /* Increment line index*/

      end

      TI0KW00.XXX-INX-LMT-NBR = 12;

      TI0KM001.CATOAP = TI0KWM01.CATOAP;

      TI0KM001.CAITEM = TI0KWM01.CAITEM;

      TI0KW00.XXX-RQT-DATA-FL = "N";

      TI0KW00.XXX-MAP-FMAT-FL = "N";

    else

      set TIWREC empty;
      set TI0KM001 initial;
      set TI0KW00 empty; /* Working storage*/
      set TI0KW01 empty; /* M001 display storage*/
      set SQLCA empty; /* SQL communication area*/
      set TAEREC empty; /* Error diagnosis information*/
      set TA1REC empty; /* Error diagnosis parameters*/
      set TA5REC empty; /* Temporary storage parameters*/
      set XX0XW01 empty; /* Date/time subroutine parameters*/

      TI0KW00.XXX-RQT-DATA-FL = "Y"; /* Request for data flag*/
      TI0KW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

      TI0KW00.XXX-MAP-DSP-CD = "1"; /* Map to be displayed*/

      TI0KW00.XXX-TEMP-STG-LNTH = 1024; /* Temporary storage length*/
      TI0KW00.XXX-INX-LMT-NBR = 12; /* Number of lines on a page*/

    end

    goto TI0K-00000; /* Converse the main process*/
    TI0K-00000: TI0K-00000();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto TI0K-00000; /* Converse the main process*/


  end // end main
end // end TI0K

// Determine which map to display
Function TI0K-00000()
  /* -----------------------------------------------------*/
  /* drive the application*/
  /* -----------------------------------------------------*/

  TI0K-10000(); /* Display map 1*/

  exit stack; /* Exit to display map*/


end // end TI0K-00000

// Main process to display a map
Function TI0K-10000()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Verify the data that was entered.*/
   /* 5) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  TI0K-11000(); /* Obtain the data to display*/

  TI0K-12000(); /* Format and display the map*/

  TI0K-13000(); /* Check attention identifier*/

  TI0K-14000(); /* Verify the entered data*/

  TI0K-15000(); /* Process valid input request*/


end // end TI0K-10000

// Obtain the data to display
Function TI0K-11000()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (TI0KW00.XXX-RQT-DATA-FL != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* ------------------------------------------------------------*/

  if (TIWREC.XXX-MSG-NBR == 0) /* No message to display*/
    TI0KW00.XXX-CURR-PAGE-NBR = 1; /* Page number to display*/
  end

   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/

  TI0KS-TA0050-P(); /* Purge any temporary storage*/

  TI0KW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/


   /* ------------------------------------------------------------*/
   /* Select the information to display in the map heading.*/
   /* ------------------------------------------------------------*/


   /* ------------------------------------------------------------*/
   /* Select and open the cursor for the set to be formatted.*/
   /* ------------------------------------------------------------*/

  TI0KS-VTODCD01-SS(); /* Select set and open cursor*/

   /* ------------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ------------------------------------------------------------*/

  TI0KW00.XXX-INX-FLD-NBR = 1; /* Display line index*/

  TI0KW00.TI0KW-EOF-FL = "N"; /* End of cursor flag*/

  while (TI0KW00.TI0KW-EOF-FL == "N") /* Until end of cursor*/

    TI0KS-VTODCD01-SN(); /* Fetch next row in cursor*/

    /* ------------------------------------------------------------*/
    /* If not end of cursor, save the data in display storage.*/
    /* ------------------------------------------------------------*/

    if (TI0KW00.TI0KW-EOF-FL == "N") /* Not end of cursor*/

      /* ------------------------------------------------------------*/
      /* If first line on subsequent page, save heading information.*/
      /* ------------------------------------------------------------*/

      if (TI0KW00.XXX-HI-PAGE-NBR > 0 /* Not the first page*/
       && TI0KW00.XXX-INX-FLD-NBR == 1) /* and first line on page*/


      end

      /* ------------------------------------------------------------*/
      /* Save the data from the table row in display storage.*/
      /* ------------------------------------------------------------*/

      TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] = VTODCD01.TI-CUTK-SEND-CD;
      TI0KW01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] = VTODCD01.TI-CUTK-SEND-DESC;
      TI0KW01.XXX-STAT[XXX-INX-FLD-NBR] = VTODCD01.XXX-STAT;
      TI0KW01.ZZ-SDT[XXX-INX-FLD-NBR] = VTODCD01.ZZ-SDT;
      TI0KW01.ZZ-CHNG-DT[XXX-INX-FLD-NBR] = VTODCD01.ZZ-CHNG-DT;
      TI0KW01.ZZ-CHNG-HHMM[XXX-INX-FLD-NBR] = VTODCD01.ZZ-CHNG-HHMM;
      TI0KW01.ZZ-CHNG-CNT[XXX-INX-FLD-NBR] = VTODCD01.ZZ-CHNG-CNT;

      /* ------------------------------------------------------------*/
      /* Increment the line index to the next row in display storage.*/
      /* ------------------------------------------------------------*/

      TI0KW00.XXX-INX-FLD-NBR = TI0KW00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/

    end

    /* ------------------------------------------------------------*/
    /* If the page is full, add the page to the storage queue.*/
    /* ------------------------------------------------------------*/

    if (TI0KW00.XXX-INX-FLD-NBR > TI0KW00.XXX-INX-LMT-NBR) /* Page is full*/

      TI0KS-TA0050-A(); /* Add page to temporary storage*/

      TI0KW00.XXX-INX-FLD-NBR = 1; /* Display line index*/

    end

  end

   /* ------------------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ------------------------------------------------------------*/

  if (TI0KW00.XXX-INX-FLD-NBR > 1) /* Page was formatted*/
    TI0KS-TA0050-A(); /* Add page to temporary storage*/
  end

   /* ------------------------------------------------------------*/
   /* Close the cursor for the selected set.*/
   /* ------------------------------------------------------------*/

  TI0KS-VTODCD01-CS(); /* Close selected set cursor*/

   /* ------------------------------------------------------------*/
   /* If no pages were formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/

  if (TI0KW00.XXX-HI-PAGE-NBR == 0) /* No pages formatted*/

    TIWREC.XXX-MSG-LOC-ID = "11000"; /* Message location*/
    converseLib.validationFailed(18); /* No data was found to display*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  TI0KW00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/


end // end TI0K-11000

// Format and converse the map
Function TI0K-12000()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/
    TIWREC.XXX-MSG-NBR = converseVar.validationMsgNum; /* Message number*/
  end

   /* ------------------------------------------------------------*/
   /* If the map must be formatted, format the data in the map.*/
   /* ------------------------------------------------------------*/

  if (TI0KW00.XXX-MAP-FMAT-FL == "Y") /* Map must be formatted*/
    TI0K-12100(); /* Format the data in the map*/
  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  TI0KM001.XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/

  if (TI0KWM01.CATOAP > " ")
    TI0KM001.CATOAP = TI0KWM01.CATOAP;
  else
    TI0KM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  end

  if (TI0KWM01.CAITEM > " ")
    TI0KM001.CAITEM = TI0KWM01.CAITEM;
  else
    TI0KM001.CAITEM = COMMAREA.CAITEM; /* TO application*/
  end

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (TIWREC.XXX-MSG-NBR != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ")  /* or message text*/

    if (TIWREC.XXX-MSG-NBR != 0) /* Message to display*/

      if (TIWREC.XXX-MSG-NBR == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(TIWREC.XXX-MSG-NBR);
      end /* Message number*/
    else
      TI0KM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    TI0KM001.XXX-MSG-LOC-ID = TIWREC.XXX-MSG-LOC-ID;
                                   /* Message location*/

    set TI0KM001 alarm; /* Indicate sound alarm*/

  end

  converse TI0KM001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  TIWREC.XXX-MSG-LOC-ID = " "; /* Message location*/
  TIWREC.XXX-MSG-NBR = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/

  COMMAREA.CATOAP = TI0KM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = TI0KM001.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* Reset modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/


  TI0KW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (TI0KW00.XXX-INX-FLD-NBR <= TI0KW00.XXX-INX-LMT-NBR)
                                   /* Until end of screen*/
    if (TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] > " "
     || TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] > " "
     || TI0KM001.XXX-STAT[XXX-INX-FLD-NBR] > " ")
      set TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] normal;
      set TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] normal;
      set TI0KM001.XXX-STAT[XXX-INX-FLD-NBR] normal;
                                   /* or end of formatted lines*/

      if (TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] > " "
       || TI0KW01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] > " "
       || TI0KW01.XXX-STAT[XXX-INX-FLD-NBR] > " ")
        set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
      else
        set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] initialAttributes;
      end
    else
      set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] initialAttributes;
    end

    TI0KW00.XXX-INX-FLD-NBR = TI0KW00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/

  end

  set TI0KM001.CATOAP normal; /* TO application*/
  set TI0KM001.CAITEM normal; /* Application data*/


end // end TI0K-12000

// Format the data in the map
Function TI0K-12100()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set TI0KM001 initial; /* M001 input/output area*/

  TI0KW00.XXX-MAP-FMAT-FL = "N"; /* Format the map flag*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (TI0KW00.XXX-HI-PAGE-NBR > 0) /* Pages were formatted*/

    if (TI0KW00.XXX-CURR-PAGE-NBR != TA5REC.TA5TSQIX)
                                   /* Desired page not current*/

      /* ------------------------------------------------------------*/
      /* If the current page has been changed, update the page in*/
      /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/

      if (TI0KW00.XXX-PAGE-UPD-FL == "Y") /* Current page was changed*/

        TI0KS-TA0050-U(); /* Update temporary storage page*/

        TI0KW00.XXX-PAGE-UPD-FL = "N"; /* Page update flag*/

      end

      /* ------------------------------------------------------------*/
      /* If the desired page number is greater than the highest page*/
      /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (TI0KW00.XXX-CURR-PAGE-NBR > TI0KW00.XXX-HI-PAGE-NBR)
                                   /* Desired greater highest*/
        TI0KW00.XXX-CURR-PAGE-NBR = TI0KW00.XXX-HI-PAGE-NBR;
                                   /* Default to highest*/
      end

      TI0KS-TA0050-R(); /* Read temporary storage page*/

    end

    /* ------------------------------------------------------------*/
    /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    TI0KM001.XXX-CURR-PAGE-DESC = "PAGE";
    set TI0KM001.XXX-CURR-PAGE-DESC skip; /* "PAGE" literal*/
    TI0KM001.XXX-CURR-PAGE-NBR = TI0KW00.XXX-CURR-PAGE-NBR;
                                   /* Current page*/
    TI0KM001.XXX-HI-PAGE-DESC = "OF";
    set TI0KM001.XXX-HI-PAGE-DESC skip; /* "PAGE" literal*/
    TI0KM001.XXX-HI-PAGE-NBR = TI0KW00.XXX-HI-PAGE-NBR;
                                   /* Highest page*/

  end

   /* ------------------------------------------------------------*/
   /* If code is missing, position the cursor on that field.*/
   /* ------------------------------------------------------------*/

   /* END;*/

   /* ------------------------------------------------------------*/
   /* Format the map fields from the display storage area.*/
   /* ------------------------------------------------------------*/


  TI0KW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (TI0KW00.XXX-INX-FLD-NBR <= TI0KW00.XXX-INX-LMT-NBR)
                                   /* Until screen is full*/
                                  /* AND*/

    TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] = TI0KW01.XXX-LN-SEL-IND[XXX-INX-FLD-NBR];
    TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] = TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR];

    if (TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] > " "
     || TI0KW01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] > " "
     || TI0KW01.XXX-STAT[XXX-INX-FLD-NBR] > " ")
      set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] protect, noHighLight, blue;
    else
      set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] initialAttributes;
    end

    TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] = TI0KW01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR];
    TI0KM001.XXX-STAT[XXX-INX-FLD-NBR] = TI0KW01.XXX-STAT[XXX-INX-FLD-NBR];

    TI0KW00.XXX-INX-FLD-NBR = TI0KW00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/

  end


end // end TI0K-12100

// Check attention identifier
Function TI0K-13000()
  if (converseVar.eventKey is pf3 /* END*/
   || converseVar.eventKey is pf4) /* MAIN*/

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "TI0M"; /* System Menu*/
    end

    if (converseVar.eventKey is pf4) /* PF3 was pressed*/
      COMMAREA.CATOAP = "MN0M"; /* System Menu*/
    end

    TI0KS-TA0050-P(); /* Purge any temporary storage*/

    XSTIS-EXIT(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (TI0KW00.XXX-RQT-DATA-FL == "Y") /* Request for data pending*/
      set TI0KW01 empty; /* M001 display storage*/
    end

    TI0KW00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/
    TI0KW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

  if (TI0KM001.CATOAP > " "        /* FAST PATH was requested*/
   && converseVar.eventKey not pf1 /* HELP*/
   && converseVar.eventKey not pf5 /* REFRESH SCREEN*/
   && converseVar.eventKey not pf6) /* REFRESH LINE*/

    if (converseVar.eventKey is enter) /* TRANSFER TO OTHER APPLICATION*/

      TI0KS-TA0050-P(); /* Purge any temporary storage*/

      XSTIS-EXIT(); /* Exit to another application*/

      set TI0KM001.CATOAP cursor, bold; /* Highlight*/

    else

      TIWREC.XXX-MSG-LOC-ID = "13000"; /* Message location*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/

    end

    exit stack; /* Exit to display map*/

  end

  if (converseVar.eventKey not enter /* PROCESS INPUT*/
   && converseVar.eventKey not pf1   /* HELP*/
   && converseVar.eventKey not pf7   /* SCROLL UP*/
   && converseVar.eventKey not pf8   /* SCROLL DOWN*/
   && converseVar.eventKey not pf5   /* REFRESH SCREEN*/
   && converseVar.eventKey not pf6)  /* REFRESH LINE*/

    TIWREC.XXX-MSG-LOC-ID = "13000"; /* Message location*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end
end // end TI0K-13000

// Verify data that was entered
Function TI0K-14000()
  if (converseVar.eventKey is pf1
   || converseVar.eventKey is pf5
   || converseVar.eventKey is pf6)

    return;

  end
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (TI0KM001.CAITEM > " ") /* Application data was entered*/

    TIWREC.XXX-MSG-LOC-ID = "14000"; /* Message location*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/
    set TI0KM001.CAITEM cursor, bold; /* Highlight*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll requested, determine if any other field changed.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    TI0K-14100(); /* Highlight other changed fields*/

    /* ------------------------------------------------------------*/
    /* Determine whether a valid request to scroll was entered.*/
    /* ------------------------------------------------------------*/
    if (converseVar.validationMsgNum == 0)
      TI0K-14200(); /* Verify scroll request*/
    end
  else

    /* ------------------------------------------------------------*/
    /* If key field changed, determine if any other field changed.*/
    /* ------------------------------------------------------------*/
                                   /* or style changed*/

      /* PERFORM TI0K-14100;          /* Highlight other changed fields*/

      /* ------------------------------------------------------------*/
      /* Determine if all the required key fields were entered.*/
      /* ------------------------------------------------------------*/

    TI0K-14300(); /* Check for required key fields*/


      /* ------------------------------------------------------------*/
      /* Otherwise, verify any modifiable data that was changed.*/
      /* ------------------------------------------------------------*/

    if (TI0KW00.XXX-HI-PAGE-NBR > 0) /* Page was formatted*/

      TI0KW00.XXX-INX-FLD-NBR = 13;

      while (TI0KW00.XXX-INX-FLD-NBR > 0) /* Until end of lines*/

        TI0K-14400(); /* Verify data in the line*/

        TI0KW00.XXX-INX-FLD-NBR = TI0KW00.XXX-INX-FLD-NBR - 1;
