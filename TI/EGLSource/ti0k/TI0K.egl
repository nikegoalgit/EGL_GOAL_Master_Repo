// Verify data in the line
Function TI0K-14400()
  /* -------------------------------------------------------------*/
  /* Check for refresh key.*/
  /* -------------------------------------------------------------*/
  if (converseVar.eventKey is pf5) /* Line refresh request*/
    move TI0KW01 to TI0KM001 withV60Compat; /* Move display fields to map*/
  end
  if (converseVar.eventKey is pf6) /* Line refresh request*/
    if (TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] is cursor)
      TI0KS-RFR-LN();
    end
    if (TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] is cursor)
      TI0KS-RFR-LN();
    end
    if (TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] is cursor)
      TI0KS-RFR-LN();
    end
    if (TI0KM001.XXX-STAT[XXX-INX-FLD-NBR] is cursor)
      TI0KS-RFR-LN();
    end
  end
  /* -------------------------------------------------------------*/
  /* Determine if valid action was entered.*/
  /* -------------------------------------------------------------*/
  if (TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != " "
   && TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != "A"
   && TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != "C")
    TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
    converseLib.validationFailed(40); /* Only "A" or "C" may be entered*/
    set TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
    return; /* Return to previous process*/
  end
  /*  */
  /* -------------------------------------------------------------*/
  /* Determine if row has changed. If action changed with no row*/
  /* changes, return error message.*/
  /* -------------------------------------------------------------*/
  TI0KW-ROW-CHANGED-FL = "N";
  if (TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR]
   != TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR]
   || TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR]
   != TI0KW01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR]
   || TI0KM001.XXX-STAT[XXX-INX-FLD-NBR]
   != TI0KW01.XXX-STAT[XXX-INX-FLD-NBR])
    TI0KW-ROW-CHANGED-FL = "Y";
  else
    if (TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != " ")
      converseLib.validationFailed(46);
      set TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] bold, cursor;
      return;
    end
  end

  /* -------------------------------------------------------------*/
  /* See if row changed with no action.*/
  /* -------------------------------------------------------------*/
  if (TI0KW-ROW-CHANGED-FL == "Y")
    if (TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == " "
    ) /* User changed line and didn't enter*/
                                   /* action*/
      TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
      converseLib.validationFailed(41); /* Action code missing/refresh*/
      set TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
      return; /* Return to previous process*/
    end
  end

  /* -------------------------------------------------------------*/
  /* See if User is attempting to change key fields.*/
  /* -------------------------------------------------------------*/
  if (TI0KW-ROW-CHANGED-FL == "Y")
    if (TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR]
     != TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR])
      if (TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "C")
        if (TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] == " ")
          converseLib.validationFailed(75); /* Action code must be 'A' to add*/
          set TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
        else
          converseLib.validationFailed(45); /* Cannot change key fields*/
          set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] cursor, bold;
          set TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] bold;
        end
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        return; /* Return to previous process*/
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* Determine if user is duplicating data on another row of the*/
   /* current screen.*/
   /* ------------------------------------------------------------*/
  if (TI0KW-ROW-CHANGED-FL == "Y")
    TI0KW-INX-FLD-NBR = 1;
    while (TI0KW-INX-FLD-NBR < XXX-INX-FLD-NBR)
      if (TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] == 
      TI0KM001.TI-CUTK-SEND-CD[TI0KW-INX-FLD-NBR]
       && TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] == 
      TI0KM001.TI-CUTK-SEND-DESC[TI0KW-INX-FLD-NBR])
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        converseLib.validationFailed(44); /* Line is already on screen*/
        set TI0KM001.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] bold, cursor;
        set TI0KM001.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] bold;
        return; /* Return to previous process*/
      end
      TI0KW-INX-FLD-NBR = TI0KW-INX-FLD-NBR + 1;
    end
  end

   /* ------------------------------------------------------------*/
   /* If selection entered was "A", determine if user is attempting*/
   /* to add to existing data on the screen.*/
   /* ------------------------------------------------------------*/
  if (TI0KW-ROW-CHANGED-FL == "Y")
    if (TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "A")
      if (TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] != " "
       || TI0KW01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] != " "
       || TI0KW01.XXX-STAT[XXX-INX-FLD-NBR] != " ")
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        converseLib.validationFailed(39); /* Only add to blank line at end*/
        set TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
        return; /* Return to previous process*/
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* If selection entered was "C", determine if user is attempting*/
   /* change a blank line.*/
   /* ------------------------------------------------------------*/
  if (TI0KW-ROW-CHANGED-FL == "Y")
    if (TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "C")
      if (TI0KW01.TI-CUTK-SEND-CD[XXX-INX-FLD-NBR] == " "
       && TI0KW01.TI-CUTK-SEND-DESC[XXX-INX-FLD-NBR] == " "
       && TI0KW01.XXX-STAT[XXX-INX-FLD-NBR] == " ")
        TIWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
        converseLib.validationFailed(42); /* Only Change existing line/refresh*/
        set TI0KM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
        return; /* Return to previous process*/
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* Validate entered detail.*/
   /* ------------------------------------------------------------*/
  if (TI0KW-ROW-CHANGED-FL == "Y")
    TI0K-14450();
    if (converseVar.validationMsgNum == 0)
      TI0KW00.XXX-PAGE-UPD-FL = "Y";
    end
  end

end // end TI0K-14400

