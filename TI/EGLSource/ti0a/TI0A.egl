      TI0AW-ROW-CHANGED-FL = "Y";
      set TI0AM001.TI-CUTK-SPLR-NM[XXX-INX-FLD-NBR] cursor, bold;
    end
    if (TI0AM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != 
