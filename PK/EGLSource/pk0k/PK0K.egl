Function PK0KSU4()
   /* -----------------------------------------------------------*/
   /* Select row from pool header*/
   /* -----------------------------------------------------------*/

  set PK4REC empty;
  PK4REC.XWHCD = PK0KM001.XWHCD;
  PK4REC.PK1CUOID = PK0KM001.PK1CUOID[PK0KWSB2];
