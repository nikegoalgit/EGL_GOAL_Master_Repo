// add order type select tbl
Function PK0MS81()


     /* **************  add  new record  *************/
  PK0MM030.XXXERRID = "PK0MS81";

   /* ******************************************************/
   /* *    first check to make sure the record has not    **/
   /* *    been added by someone else.                    **/
   /* ******************************************************/
  set SQLCA empty;
  set PKUREC empty;

  PKUREC.XWHCD = PK0MM030.XWHCD;
  PKUREC.PKURUNMD = " "; /* runmode*/
  PKUREC.PKUSQNBR = PK0MM030.PKUSQNBR[PK0MWNX2];
  PKUREC.PKUOTUNT = PK0MM030.PKUOTUNT[PK0MWNX2];
  PKUREC.XOTCD = PK0MM030.XOTCD[PK0MWNX2];

  call "IO2020" ("S ", SQLCA, PKUREC) {isNoRefresh = yes, isExternal = yes};

  if (SQLCA.VAGen_SQLCODE == 100) /* not found, thats good*/
    /* next sentece*/
  else
    if (SQLCA.VAGen_SQLCODE == +0)
      set PK0MM030.XOTCD[PK0MWNX2] cursor, modified, bold;
      PK0MW01.PK0MWUPD = "N"; /* d.b. updated flag*/
      converseLib.validationFailed(105);
      exit stack;
    else
      if (SQLCA.VAGen_SQLCODE == -911)
        TA1REC.TA1TYPE = "DB2 ";
        TA1REC.TA1FUNC = "POST";
        TA1REC.TA1PGMNM = "PK0M";
        TA1REC.TA1LOCAT = "SELECT";
        TA1REC.TA1MAP = "PK0MM030";
        TA1REC.TA1DBASE = "DPICK.";
        TA1REC.TA1TBLVU = "VSELOT01"; /* table view*/
        TA1REC.TA1TBLKE = PK0MM030.XWHCD; /* table key*/
        call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes};
        set PK0MM030.XOTCD[PK0MWNX2] cursor, modified, bold;
        converseLib.validationFailed(28);
        exit stack;
      else
        TA1REC.TA1TYPE = "DB2 ";
        TA1REC.TA1FUNC = "RETN";
        TA1REC.TA1PGMNM = "PK0M";
        TA1REC.TA1LOCAT = "SELECT";
        TA1REC.TA1MAP = "PK0MM030";
        TA1REC.TA1DBASE = "DPICK.";
        TA1REC.TA1TBLVU = "VSELOT01"; /* table view*/
        TA1REC.TA1TBLKE = PK0MM030.XWHCD; /* table key*/
        call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes};
        VAGen_EZEREPLY = VGVar.handleSysLibraryErrors;	  // save value
        VGVar.handleSysLibraryErrors = 0;		// VAGEN (REPLY option omitted
        sysLib.rollback();
        VGVar.handleSysLibraryErrors = VAGen_EZEREPLY;		//restore value
        
        converseLib.validationFailed(29);
        exit stack;
      end
    end
  end







   /* ******************************************************/
   /* *    now add the row.                               **/
   /* ******************************************************/

  set SQLCA empty;
  set PKUREC empty;
  PKUREC.XWHCD = PK0MM030.XWHCD;
  PKUREC.PKURUNMD = " "; /* runmode*/
  PKUREC.PKUSQNBR = PK0MM030.PKUSQNBR[PK0MWNX2];
  PKUREC.PKUOTUNT = PK0MM030.PKUOTUNT[PK0MWNX2];
