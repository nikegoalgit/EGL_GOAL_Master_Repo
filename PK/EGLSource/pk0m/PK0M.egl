      set PK0MM030.XOTCD[PK0MWNX2] initialAttributes;
    end

    if (PK0MM030.PKUSQNBR[PK0MWNX2] != PK0MW01.PKUSQNBR[PK0MWNDX])
      set PK0MM030.PKUSQNBR[PK0MWNX2] modified;
      PK0MW01.PK0MWED1 = "Y"; /* cust.modified flag*/
