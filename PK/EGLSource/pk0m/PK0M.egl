     /* **********************************************/
    if (PK0MM030.XOTCD[PK0MWNX2] != PK0MW01.XOTCD[PK0MWNDX])
      set PK0MM030.XOTCD[PK0MWNX2] modified;
      PK0MW01.PK0MWED1 = "Y"; /* cust.modified flag*/
