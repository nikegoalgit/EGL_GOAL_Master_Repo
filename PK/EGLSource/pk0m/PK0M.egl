// reset mdt's
Function PK0M051()

     /* --------------------------------------------------*/
     /* -   - reset mdt's to fields actually modified -  -*/
     /* --------------------------------------------------*/



  PK0MW01.PK0MWNX2 = 24;
  while (PK0MW01.PK0MWNX2 >= 1)
    PK0MW01.PK0MWNDX = PK0MW01.PK0MWTOP + PK0MW01.PK0MWNX2 - 1;


     /* **********************************************/
