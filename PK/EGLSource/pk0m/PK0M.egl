    if (PK0MW01.PK0MWNX2 < 4)
      if (PK0MM030.PKRMIUNT[PK0MWNX2] != PK0MW01.PKRMIUNT[PK0MWNX2]
       || PK0MM030.PKRMIUNT[PK0MWNX2] is modified)
