//*** PROGRAM=PK0M ****
// 
//  ************** pk0m: order type selection *****************
// 
// 
// purpose:  An order type entered here along with total units
//          for that order type will result in orders for that
//          order type being processed tonight.
//           The units selected for each order type and
//          sequence number will actually determine how many
//          orders for each order type are processed (that is
//          it may be that not all orders for a particular
//          order type are processed).
// 
// 
// 
// narrative:
//     Enter the order types that are to be selected along with
//    the number of units desired and the sequence number of
//    the order type.  The sequence number will determine the
//    order that the order types will be processed in.
//     There is a field at the bottom of the screen for
//    entering the units of stock transfers desired and the
//    limit of rps and ups units the user desires.
// 
//     All order types already selected will be displayed when
//    this screen is first entered (not including planning
//    order types).  They will be displayed in sequence number
//    order.
// 
//     In order to delete an order type the user will enter
//    0 in the units field.
// 
//     The user will add an order type by using a blank line
//    (if the screen is full the user may have to page forward
//    a blank line) or by overtyping an already existing ot.
// 
//   maintenance history:
// 
//    date      name   description
//  --------  -------  --------------------------------------
//  08-18-87  PRM      created
//  11-29-88  JWOOD    ADDED NONCSP OPTION ON CALLS
//  11-29-88  JWOOD    ADDED XSSEGTR LOGIC
//  07-21-89  jcampb   changed xfer from pk0i to pk09
//  03-19-91  BHARRI   Changed PK0M020 to exclude warehouse 51
//  08-11-95  CBRINE   CANADA Added XCTCD to PKWREC
// 
// ************************************************************
// 
//  02-04-98  NGANGO   Y2K MODIFICATIONS - Modified Stmt. Group
//                     Pk0M084 and Pk0M094. To follow the NIKE
//                     Standard of naming conventions the Stmt.
//                     Groups PK0M081,PK0M082,PK0M083,PK0M084,
//                     PK0M091,PK0M093 and PK0M094 are RENAMED
//                     as PK0MS81,PK0MS82,PK0MS83,PK0MS84,
//                     PK0MS91,PK0MS93 and PK0MS94 respectively
//                     Also the Process Pk0M080 and Pk0M090 are
//                     modified accordingly.
// ************************************************************
// 
// *********************
Program PK0M type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "PK00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  PK0MW01 PK0MW01; // record
  PK0MW02 PK0MW02; // record
  PKRREC PKRREC; // record
  PKUREC PKUREC; // record
  PKWREC PKWREC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XOTTBL {deleteAfterUse = yes}; // table
  use XWHTBL {deleteAfterUse = yes}; // table
  use PK0MM.PK0MM030 // form
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    PK0M005: PK0M005();

    goto PK0M010;



    PK0M010: PK0M010();

    goto PK0M010;


  end // end main
end // end PK0M

// main process
Function PK0M005()

   /* ----------------------------------------------------------*/
   /* initialization process....*/
   /* ----------------------------------------------------------*/






  set PKWREC empty; /* clear work records*/
  set PK0MW01 empty;
  set PK0MW02 empty;
  set SQLCA empty;


  XPPK001(); /* app initial.*/
  PK0MM030.XWHCD = PKWREC.XWHCD;
  if (converseVar.validationMsgNum == 2 /* invalid  warehouse*/
   || converseVar.validationMsgNum == 3 /* inactive warehouse*/
   || converseVar.validationMsgNum == 4) /* warehouse required*/
    set PK0MM030.XWHCD cursor, modified, bold; /* hilite warehouse*/
    PKWREC.XWHCD = " "; /* warehouse*/
  end
  if (converseVar.validationMsgNum == 0) /* valid warehouse*/
    PK0MM030.XWHCD = PKWREC.XWHCD;
    PK0M021(); /* Format map heading area*/
    PK0MW01.PK0MWTOP = 1; /* set top of page subscript*/
    PK0M022(); /* ss order type rows*/
    if (converseVar.validationMsgNum == 0) /* no database errors*/
      PK0M024(); /* ss misc. qtys rows*/
      if (converseVar.validationMsgNum == 0) /* no database errors*/
        PK0M023(); /* format screen*/
      end
    end
  end





  COMMAREA.CAFROMAP = COMMAREA.CACURRAP; /* save current application*/





  PKWREC.PKWINDEX = 0;
  PKWREC.PKWCNTRL = " ";




   /* -------------  end process pk0m005  --------------------*/
end // end PK0M005

// select by order type
Function PK0M010()


  PK0M020(); /* format output*/



  PK0M030(); /* converse map*/




   /* *********************************************/
   /* ** if table overflow has occured           **/
   /* **     go back to main selection menu      **/
   /* *********************************************/
  if (PK0MW01.PK0MWFL1 == "Y") /* table overflow flag*/
    converseLib.validationFailed(24);
    PK0M030(); /* converse map*/
    COMMAREA.CATOAP = "PK07"; /* selection Master Menu*/
    PKWREC.XWHCD = PK0MM030.XWHCD; /* Save in work area*/
    COMMAREA.CAUSERA = PKWREC.PKWGRPLV; /* COMMAREA user area*/
    XSEXIT(); /* Invoke selected application*/
    PK0MM030.VAGen_EZEMSG = COMMAREA.CAMSG; /* Returned error message*/
    exit stack; /* ; Exit to redisplay map*/
  end





  if (PKWREC.XWHCD == " " /* receiving warehouse from map*/
   && converseVar.eventKey not pf3)
    exit stack;
  end





  PK0MW01.PK0MWUPD = "N"; /* database update flag.*/

  PK0M040(); /* validate pfkeys keys*/




  if (converseVar.validationMsgNum == 0)
    PK0M050(); /* validate inputs*/
  end


  if (converseVar.validationMsgNum == 0) /* ; no errors*/
    if (PKWREC.XWHCD == PK0MM030.XWHCD)
      if (converseVar.eventKey is pf12)
            /* no more processing*/
      else
        if (PK0MW01.PK0MWED1 == "N" /* data modified*/
         && PK0MW01.PK0MWED2 == "N") /* data modified*/
          /* no more processing*/
        else
          if (PK0MW01.PK0MWED1 == "Y") /* edits-all-ok-flag*/
            PK0M080(); /* update databases*/
          end
          if (PK0MW01.PK0MWED2 == "Y") /* edits-all-ok-flag*/
            PK0M090(); /* update databases*/
          end
        end
      end
    end
  end



   /* ------------------- end pk0m010 -------------------------*/
end // end PK0M010

// Format the output display
Function PK0M020()


   /* ------------------------------------------------------------*/
   /* first time through or a change in warehouse, the map is*/
   /* reformatted with new data.*/
   /* warehouse is highlited if invalid.*/
   /* ------------------------------------------------------------*/





     /* ***********************************/
     /* *  if changed warehouse          **/
     /* *      get warehouse abbrev.     **/
     /* *      if lookup not successful  **/
     /* *         get out                **/
     /* *      else                      **/
     /* *         load new warehouse's   **/
     /* *         data into map and array**/
     /* ***********************************/

  if (PK0MM030.XWHCD == "51") /* if warehouse = '51'*/
    set PK0MM030.XWHCD cursor, modified, bold; /* highlight warehouse*/
    PKWREC.XWHCD = " "; /* init work rec warehouse*/
    converseLib.validationFailed(370); /* 'function unavailable'*/
    return; /* outa here*/
  end /* end if*/
  if (PKWREC.XWHCD != PK0MM030.XWHCD) /* change in warehouse*/
    set PK0MW01 empty;
    PKWREC.XWHCD = PK0MM030.XWHCD; /* warehouse*/
    XPPK002(); /* validate warehouse*/
    if (converseVar.validationMsgNum == 2 /* invalid  warehouse*/
     || converseVar.validationMsgNum == 3 /* inactive warehouse*/
     || converseVar.validationMsgNum == 4) /* warehouse required*/
      set PK0MM030.XWHCD cursor, modified, bold; /* hilite warehouse*/
      PKWREC.XWHCD = " "; /* warehouse*/
      return;
    end
    set PK0MM030.XWHCD normal;
    if (converseVar.validationMsgNum == 0) /* valid warehouse*/
      PK0M021(); /* Format map heading area*/
      PK0MW01.PK0MWTOP = 1; /* set top of page subscript*/
      PK0M022(); /* ss order type rows*/
      if (converseVar.validationMsgNum == 0) /* no database errors*/
        PK0M024(); /* ss misc. qtys rows*/
        if (converseVar.validationMsgNum == 0) /* no database errors*/
          PK0M023(); /* format screen*/
        end
      end
    end
  end

     /* ***********************************/
     /* *   * database was updated.      **/
     /* *   * refresh the array,         **/
     /* *   * the map, set ezemno        **/
     /* *   * to 'updated' and get out   **/
     /* ***********************************/
  if (PK0MW01.PK0MWUPD == "Y") /* database updated*/
    move PK0MW02 to PK0MW01 withV60Compat; /* initialize data base save area*/
    PK0M022(); /* ss customers*/
    PK0M024(); /* ss pack&hold row*/
    PK0M021(); /* format headings*/
    PK0M023(); /* format screen*/
    if (converseVar.eventKey not pa2)
      converseLib.validationFailed(32);
    end
    return;
  end




     /* ***********************************/
     /* *   * only paging is required *  **/
     /* *   *                            **/
     /* *   * if at the beginning or end **/
     /* *   * of the map, is's an error. **/
     /* *   * subtract 24 from variable  **/
     /* *   * holding top of map value   **/
     /* *   * when page backward is      **/
     /* *   * requested.                 **/
     /* *   * add 24 to variable         **/
     /* *   * holding top of map value   **/
     /* *   * when page forward is       **/
     /* *   * requested.                 **/
     /* ***********************************/

  if (converseVar.eventKey is pf7) /* page backward*/
    if (PK0MW01.PK0MWTOP == 1)
      converseLib.validationFailed(37);
    end
    if (PK0MW01.PK0MWTOP != 1)
      PK0MW01.PK0MWTOP = PK0MW01.PK0MWTOP - 24; /* set top of page*/
      PK0M021(); /* format map heading*/
      PK0M023(); /* format screen*/
    end
  end

  if (converseVar.eventKey is pf8) /* page forward*/
    PK0MW01.PK0MWTOP = PK0MW01.PK0MWTOP + 24; /* set top of page*/
    if (PK0MW01.PK0MWTOP <= PK0MW01.PK0MWMAX)
      PK0M021(); /* format map heading*/
      PK0M023(); /* format screen*/
    end
    if (PK0MW01.PK0MWTOP > PK0MW01.PK0MWMAX)
      PK0MW01.PK0MWTOP = PK0MW01.PK0MWTOP - 24; /* reset top of page*/
      converseLib.validationFailed(38); /* no more pages to display*/
    end
  end




  if (converseVar.validationMsgNum == 2 /* invalid  warehouse*/
   || converseVar.validationMsgNum == 3 /* inactive warehouse*/
   || converseVar.validationMsgNum == 4) /* warehouse required*/
    set PK0MM030.XWHCD cursor, modified, bold; /* hilite warehouse*/
  end





end // end PK0M020

// format map heading
Function PK0M021()
   /* ---------------------------------------------------*/
      /* format map headings...........*/
   /* ---------------------------------------------------*/


  set PK0MM030 initial; /* clear map area*/
  set PKUREC empty; /* empty select order type table*/
  set PKRREC empty; /* empty select pack & hold table*/

  PK0MM030.XXXERRID = "PK0M021"; /* format location*/
  PK0MM030.XXXCURDT = VGVar.currentShortGregorianDate; /* date to map*/
  PK0MM030.XWHCD = PKWREC.XWHCD; /* warehouse code*/
  PK0MM030.XWHABRV = PKWREC.XWHABRV; /* warehouse abbrev.*/
  PK0MM030.XXXRVNBR = "00"; /* warehouse abbrev.*/
  converseLib.validationFailed(0); /* reset any error*/



end // end PK0M021

// obtain set order type rows
Function PK0M022()
   /* ----------------------------------------------------*/
     /* load working storage with order type selection rows*/
             /* -  non planned only   -*/
   /* ----------------------------------------------------*/


  PKUREC.XWHCD = PKWREC.XWHCD;
  PKUREC.PKURUNMD = " "; /* ; non-planned mode*/

  call "IO2020" ("S1", SQLCA, PKUREC) {isNoRefresh = yes, isExternal = yes}; /* ; ss on customers*/

  if (SQLCA.VAGen_SQLCODE != 0) /* ; unexpected return*/
    if (SQLCA.VAGen_SQLCODE == -911)
      TA1REC.TA1FUNC = "POST"; /* ; term. function*/
      TA1REC.TA1LOC = "SELECT";
      TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
      TA1REC.TA1PGMNM = "PK0M022"; /* process name*/
      TA1REC.TA1MAP = "PK0MM030"; /* map name*/
      TA1REC.TA1DBASE = "DPICK   "; /* database name*/
      TA1REC.TA1TBLVU = "VSELOT01"; /* table view name*/
      TA1REC.TA1TBLKE = PKWREC.XWHCD; /* table key*/
      call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
      converseLib.validationFailed(28);
    else
      if (SQLCA.VAGen_SQLCODE != 100)
        converseLib.validationFailed(29); /* ; indicate system error*/
        TA1REC.TA1FUNC = "RETN"; /* ; term. function*/
        TA1REC.TA1LOC = "SELECT";
        TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
        TA1REC.TA1PGMNM = "PK0M022"; /* process name*/
        TA1REC.TA1MAP = "PK0MM030"; /* map name*/
        TA1REC.TA1DBASE = "DPICK   "; /* database name*/
        TA1REC.TA1TBLVU = "VSELOT01"; /* table view name*/
        TA1REC.TA1TBLKE = PKWREC.XWHCD; /* table key*/
        call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
        return;
      end
    end
  end



  if (SQLCA.VAGen_SQLCODE == 0) /* ; good return from s2 above*/
    PK0MW01.PK0MWMAX = 0; /* ; records read in counter*/
    while (SQLCA.VAGen_SQLCODE == 0 /* ; good return within loop*/
     && PK0MW01.PK0MWMAX <= 100) /* ; table maximum*/
      call "IO2020" ("N1", SQLCA, PKUREC) {isNoRefresh = yes, isExternal = yes}; /* fetch next record*/

      if (SQLCA.VAGen_SQLCODE == 0) /* ; good fetch*/
        PK0MW01.PK0MWMAX = PK0MW01.PK0MWMAX + 1; /* ;*/
        PK0MW01.XOTCD[PK0MWMAX] = PKUREC.XOTCD; /* ;*/
        PK0MW01.PK0MWCOT = PKUREC.XOTCD;
        PK0M026(); /* get order type abbreviation*/
        PK0MW01.XOTABRV[PK0MWMAX] = PK0MW01.PK0MWAOT;
        PK0MW01.PKUSQNBR[PK0MWMAX] = PKUREC.PKUSQNBR;
        PK0MW01.PKUOTUNT[PK0MWMAX] = PKUREC.PKUOTUNT;
        PK0MW01.PK0MWUCT[PK0MWMAX] = PKUREC.ZZZCHGCT;
      else
        if (SQLCA.VAGen_SQLCODE == -911)
          TA1REC.TA1FUNC = "POST"; /* ; term. function*/
          TA1REC.TA1LOC = "SELECT";
          TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
          TA1REC.TA1PGMNM = "PK0M022"; /* process name*/
          TA1REC.TA1MAP = "PK0MM030"; /* map name*/
          TA1REC.TA1DBASE = "DPICK   "; /* database name*/
          TA1REC.TA1TBLVU = "VSELOT01"; /* table view name*/
          TA1REC.TA1TBLKE = PKWREC.XWHCD; /* table key*/
          call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
          converseLib.validationFailed(28);
        else
          if (SQLCA.VAGen_SQLCODE != 100) /* ; not end of data*/
            converseLib.validationFailed(29); /* ; indicate system error*/
            TA1REC.TA1FUNC = "RETN"; /* ; term. function*/
            TA1REC.TA1LOC = "SELECT";
            TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
            TA1REC.TA1PGMNM = "PK0M022"; /* process name*/
            TA1REC.TA1MAP = "PK0MM030"; /* map name*/
            TA1REC.TA1DBASE = "DPICK   "; /* database name*/
            TA1REC.TA1TBLVU = "VSELOT01"; /* table view name*/
            TA1REC.TA1TBLKE = PKWREC.XWHCD; /* table key*/
            call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
            return;
          end
        end
      end
    end
  end



  if (SQLCA.VAGen_SQLCODE == 0 /* ; good return from s2 above*/
   && PK0MW01.PK0MWMAX > 100)  /* ; table maximum was reached*/
    converseLib.validationFailed(24); /* overflow*/
    PK0MW01.PK0MWFL1 = "Y"; /* table overflow flag*/
    return;
  end



end // end PK0M022

// load map page
Function PK0M023()
   /* ----------------------------------------------------*/
     /* format map.....*/
      /* pk0lwtop - location in ws. array of the top of screen*/
      /* pk0lwndx - this keeps track of location in working*/
                 /* storage array for moving to map*/
      /* pk0lwnx2 - locates position in map - pk0lm030*/
      /* pk0lwmax - end of valid data in working storage array*/
   /* ----------------------------------------------------*/




  PK0MW01.PK0MWNDX = PK0MW01.PK0MWTOP; /* hold current top of page*/

  PK0MW01.PK0MWNX2 = 1; /* map display subscript*/

  while (PK0MW01.PK0MWNX2 <= 24) /* only 24 displayed*/
    if (PK0MW01.PK0MWNDX <= PK0MW01.PK0MWMAX) /* place in ws < data end*/
      PK0MM030.PKUSQNBR[PK0MWNX2] = PK0MW01.PKUSQNBR[PK0MWNDX];
      PK0MM030.PKUOTUNT[PK0MWNX2] = PK0MW01.PKUOTUNT[PK0MWNDX];
      PK0MM030.XOTCD[PK0MWNX2] = PK0MW01.XOTCD[PK0MWNDX];
      PK0MM030.XOTABRV[PK0MWNX2] = PK0MW01.XOTABRV[PK0MWNDX];
    end
    if (PK0MW01.PK0MWNX2 <= 3)
      PK0MM030.PKRMIUNT[PK0MWNX2] = PK0MW01.PKRMIUNT[PK0MWNX2];
    end
    PK0MW01.PK0MWNX2 = PK0MW01.PK0MWNX2 + 1; /* set-up map subscript*/
    PK0MW01.PK0MWNDX = PK0MW01.PK0MWNDX + 1; /* set-up place in work.stor.*/
  end



end // end PK0M023

// obtain set misc. qtys rows
Function PK0M024()
   /* ----------------------------------------------------*/
     /* load map and working storage with ups/rps row*/
             /* -  non planned only   -*/
   /* ----------------------------------------------------*/


  set PKRREC empty;
  PKRREC.XWHCD = PKWREC.XWHCD;
  PKRREC.PKURUNMD = " "; /* non-planned mode*/

  call "IO2090" ("S1", SQLCA, PKRREC) {isNoRefresh = yes, isExternal = yes}; /* select unique ups/rps*/
  if (SQLCA.VAGen_SQLCODE == 100) /* no records found*/
    /* next sentence*/
  else
    if (SQLCA.VAGen_SQLCODE == -911)
      TA1REC.TA1FUNC = "POST"; /* ; term. function*/
      TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
      TA1REC.TA1PGMNM = "PK0M024"; /* process name*/
      TA1REC.TA1MAP = "PK0MM030"; /* map name*/
      TA1REC.TA1DBASE = "DPICK   "; /* database name*/
      TA1REC.TA1LOC = "SELECT SET                  ";
      TA1REC.TA1TBLVU = "VSELSR01"; /* table view name*/
      TA1REC.TA1TBLKE = PKWREC.XWHCD; /* table key*/
      call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
    else
      if (SQLCA.VAGen_SQLCODE != 0)
        TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
        TA1REC.TA1PGMNM = "PK0M024"; /* process name*/
        TA1REC.TA1MAP = "PK0MM030"; /* map name*/
        TA1REC.TA1DBASE = "DPICK   "; /* database name*/
        TA1REC.TA1LOC = "SELECT SET                  ";
        TA1REC.TA1TBLVU = "VSELSR01"; /* table view name*/
        TA1REC.TA1TBLKE = PKWREC.XWHCD; /* table key*/
        call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
        converseLib.validationFailed(29);
        return;
      end
    end
  end

  while (SQLCA.VAGen_SQLCODE == 0) /* ; good return within loop*/
    call "IO2090" ("N1", SQLCA, PKRREC) {isNoRefresh = yes, isExternal = yes}; /* fetch next record*/

    if (SQLCA.VAGen_SQLCODE == 0) /* ; good fetch*/
      if (PKRREC.PKRMCSCD == "S")
        PK0MW01.PKRMIUNT[1] = PKRREC.PKRMIUNT;
        PK0MW01.PKRMCSCD[1] = PKRREC.PKRMCSCD;
        PK0MW01.PK0MWRCT[1] = PKRREC.ZZZCHGCT;
      end
      if (PKRREC.PKRMCSCD == "U")
        PK0MW01.PKRMIUNT[2] = PKRREC.PKRMIUNT;
        PK0MW01.PKRMCSCD[2] = PKRREC.PKRMCSCD;
        PK0MW01.PK0MWRCT[2] = PKRREC.ZZZCHGCT;
      end
      if (PKRREC.PKRMCSCD == "R")
        PK0MW01.PKRMIUNT[3] = PKRREC.PKRMIUNT;
        PK0MW01.PKRMCSCD[3] = PKRREC.PKRMCSCD;
        PK0MW01.PK0MWRCT[3] = PKRREC.ZZZCHGCT;
      end
    else
      if (SQLCA.VAGen_SQLCODE == -911)
        TA1REC.TA1FUNC = "POST"; /* ; term. function*/
        TA1REC.TA1PGMNM = "PK0M024"; /* process name*/
        TA1REC.TA1MAP = "PK0MM030"; /* map name*/
        TA1REC.TA1LOC = "FETCH NEXT                  ";
        TA1REC.TA1DBASE = "DPICK   "; /* database name*/
        TA1REC.TA1TBLKE = PKWREC.XWHCD; /* table key*/
        TA1REC.TA1TBLVU = "VSELSR01"; /* table view name*/
        call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
      else
        if (SQLCA.VAGen_SQLCODE != 100)
          TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
          TA1REC.TA1PGMNM = "PK0M024"; /* process name*/
          TA1REC.TA1MAP = "PK0MM030"; /* map name*/
          TA1REC.TA1DBASE = "DPICK   "; /* database name*/
          TA1REC.TA1LOC = "FETCH NEXT                  ";
          TA1REC.TA1TBLVU = "VSELSR01"; /* table view name*/
          TA1REC.TA1TBLKE = PKWREC.XWHCD; /* table key*/
          call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
          converseLib.validationFailed(29);
        end
      end
    end
  end

end // end PK0M024

// get order type abbreviation
Function PK0M026()

   /* ---------------  pk0m026 get order type abbreviation -------*/




  if (PK0MW01.PK0MWCOT in XOTTBL.XOTCD)
    PK0MW01.PK0MWAOT = XOTTBL.XOTABRV[sysVar.arrayIndex];
  end

  if (sysVar.arrayIndex == 0) /* Table entry not found*/
    PK0MW01.PK0MWAOT = "INVALD";
  else

    if (XOTTBL.XXXSTAT[sysVar.arrayIndex] != "A") /* Not active status*/
      PK0MW01.PK0MWAOT = "INACTV";
    end
  end



   /* ----- if table entry found and active*/
   /* -----     if the order types general product*/
   /* -----     class code is not a valid gpc code*/
   /* -----     for the current warehouse*/
   /* -----         the order type is invalid.*/
  if (sysVar.arrayIndex != 0
   && XOTTBL.XXXSTAT[sysVar.arrayIndex] == "A")
    PK0MW01.XGPCD = XOTTBL.XGPCD[sysVar.arrayIndex];
    if (PKWREC.XWHCD in XWHTBL.XWHCD)
      PKWREC.XWHABRV = XWHTBL.XWHABRV[sysVar.arrayIndex];
    end
    if (sysVar.arrayIndex != 0 /* Table entry found*/
     && XWHTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* active warehouse status*/
