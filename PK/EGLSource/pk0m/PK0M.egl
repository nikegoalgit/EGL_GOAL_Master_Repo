// edit entered fields
Function PK0M053()

       /* --------------------------------------*/
       /* -  edit by units field               -*/
       /* --------------------------------------*/



   /* -----  if units was set to zero (delete requested)*/
   /* -----  and key fields have been modified*/
   /* -----      it is an error.*/
  if (PK0MM030.PKUOTUNT[PK0MWNX2] == 0)
    if (PK0MM030.XOTCD[PK0MWNX2] != PK0MW01.XOTCD[PK0MWNDX]
     || PK0MM030.PKUSQNBR[PK0MWNX2] != PK0MW01.PKUSQNBR[PK0MWNDX])
      set PK0MM030.PKUOTUNT[PK0MWNX2] cursor, modified, bold;
      converseLib.validationFailed(128); /* cannot delete and change keys*/
    end
  end



   /* -----  if sequence was set to zero*/
   /* -----  and key fields have been modified*/
   /* -----      it is an error (invalid sequence number)*/
  if (PK0MM030.PKUSQNBR[PK0MWNX2] == 0)
    if (PK0MM030.XOTCD[PK0MWNX2] != PK0MW01.XOTCD[PK0MWNDX]
     || PK0MM030.PKUSQNBR[PK0MWNX2] != PK0MW01.PKUSQNBR[PK0MWNDX])
      set PK0MM030.PKUSQNBR[PK0MWNX2] cursor, modified, bold;
      converseLib.validationFailed(141); /* invalid or missing sequence number*/
    end
  end




   /* -----  if adding a new order type row*/
   /* -----      if sequence number was exists already*/
   /* -----          it is an error*/
   /* -----      if order type was exists already*/
   /* -----          it is an error.*/
  if (PK0MM030.XOTCD[PK0MWNX2] != PK0MW01.XOTCD[PK0MWNDX])
    PK0MW01.PK0MWNX3 = 1;
    while (PK0MW01.PK0MWNX3 <= PK0MW01.PK0MWMAX)
      if (PK0MM030.PKUSQNBR[PK0MWNX2] == PK0MW01.PKUSQNBR[PK0MWNX3])
        set PK0MM030.PKUSQNBR[PK0MWNX2] cursor, modified, bold;
        converseLib.validationFailed(130);
      end
      if (PK0MM030.XOTCD[PK0MWNX2] == PK0MW01.XOTCD[PK0MWNX3])
        set PK0MM030.XOTCD[PK0MWNX2] cursor, modified, bold;
        converseLib.validationFailed(44);
      end
      PK0MW01.PK0MWNX3 = PK0MW01.PK0MWNX3 + 1;
    end
  end



   /* -----  if adding a new order type row*/
   /* -----      if sequence number was entered already*/
   /* -----          it is an error*/
   /* -----      if order type was entered already*/
   /* -----          it is an error.*/
  if (PK0MM030.XOTCD[PK0MWNX2] != PK0MW01.XOTCD[PK0MWNDX])
    PK0MW01.PK0MWNX3 = 1;
    while (PK0MW01.PK0MWNX3 <= 24)
      if (PK0MM030.PKUSQNBR[PK0MWNX2] == PK0MM030.PKUSQNBR[PK0MWNX3]
       && PK0MWNX2 != PK0MWNX3
       && PK0MM030.PKUSQNBR[PK0MWNX2] != 0)
        set PK0MM030.PKUSQNBR[PK0MWNX2] cursor, modified, bold;
        converseLib.validationFailed(130);
      end
      if (PK0MM030.XOTCD[PK0MWNX2] == PK0MM030.XOTCD[PK0MWNX3]
       && PK0MWNX2 != PK0MWNX3)
        set PK0MM030.XOTCD[PK0MWNX2] cursor, modified, bold;
        converseLib.validationFailed(44);
      end
      PK0MW01.PK0MWNX3 = PK0MW01.PK0MWNX3 + 1;
    end
  end






   /* -----  if changeing sequence*/
   /* -----      if sequence number was entered already*/
   /* -----          it is an error.*/
  if (PK0MM030.PKUSQNBR[PK0MWNX2] != PK0MW01.PKUSQNBR[PK0MWNDX])
    PK0MW01.PK0MWNX3 = 1;
    while (PK0MW01.PK0MWNX3 <= 24)
      if (PK0MM030.PKUSQNBR[PK0MWNX2] == PK0MM030.PKUSQNBR[PK0MWNX3]
       && PK0MWNX2 != PK0MWNX3
       && PK0MM030.PKUSQNBR[PK0MWNX2] != 0)
        set PK0MM030.PKUSQNBR[PK0MWNX2] cursor, modified, bold;
        converseLib.validationFailed(130);
      end
      PK0MW01.PK0MWNX3 = PK0MW01.PK0MWNX3 + 1;
    end
  end






   /* -----  if changeing sequence*/
   /* -----      if sequence number exists already*/
   /* -----          it is an error.*/
  if (PK0MM030.PKUSQNBR[PK0MWNX2] != PK0MW01.PKUSQNBR[PK0MWNDX])
    PK0MW01.PK0MWNX3 = 1;
    while (PK0MW01.PK0MWNX3 <= PK0MW01.PK0MWMAX)
      if (PK0MM030.PKUSQNBR[PK0MWNX2] == PK0MW01.PKUSQNBR[PK0MWNX3]
       && PK0MM030.PKUSQNBR[PK0MWNX2] != 0)
        PK0M054(); /* check if it's a resequence*/
      end
      PK0MW01.PK0MWNX3 = PK0MW01.PK0MWNX3 + 1;
    end
  end




   /* -----  if changeing sequence*/
   /* -----  and sequence number is zero*/
   /* -----      it is an error.*/
  if (PK0MM030.PKUSQNBR[PK0MWNX2] != PK0MW01.PKUSQNBR[PK0MWNDX]
   && PK0MM030.PKUSQNBR[PK0MWNX2] is modified
   && PK0MM030.PKUSQNBR[PK0MWNX2] == 0)
