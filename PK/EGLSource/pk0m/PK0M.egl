  if (PK0MM030.XOTCD[PK0MWNX2] == "  ")
    if (PK0MM030.PKUSQNBR[PK0MWNX2] != PK0MW01.PKUSQNBR[PK0MWNDX]
     || PK0MM030.PKUOTUNT[PK0MWNX2] != PK0MW01.PKUOTUNT[PK0MWNDX])
      set PK0MM030.XOTCD[PK0MWNX2] cursor, modified, bold;
      converseLib.validationFailed(129); /* invalid order type*/
    end
  end




