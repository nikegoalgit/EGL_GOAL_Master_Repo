  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL01M001.SL01M-XGP-GPC-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(157); /* invalid gpc*/

  end

  if (SL01M001.SL01M-XDM-DIM-CD in XDMTBL.XDMCD)

    /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL01M001.SL01M-XDM-DIM-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(367); /* invalid dimension code*/

  end

  if (SL01M001.SL01M-XQA-QUAL-CD in XQATBL.XQACD)

    /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL01M001.SL01M-XQA-QUAL-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(366); /* invalid quality code*/

  end
   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end SL01-500

// Verify scroll request
Function SL01-510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && SL01W00.XXXTSPAG < 2)       /* and no previous pages*/

    SLWREC.XXXLOCID = "510"; /* Module identification*/
    converseLib.validationFailed(7); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && SL01W00.XXXTSPAG >= SL01W00.TA5TSQIX) /* and no more pages*/

    SLWREC.XXXLOCID = "510"; /* Module identification*/
    converseLib.validationFailed(8); /* No more pages to display*/

  end

end // end SL01-510

// Verify data in the line
Function SL01-520()
   /* ------------------------------------------------------------*/
   /* selection may be made only with a single 's'.  Multiple*/
   /* 's' and characters other than 's' and ' ' are not allowed.*/
   /* ------------------------------------------------------------*/

  if (SL01M001.SL01M-SELECT[SL01W-MAP-FIELD-INX] != " ")
    if (SL01M001.SL01M-SELECT[SL01W-MAP-FIELD-INX] == "S")
      if (SL01W00.SL01W-ROW-SELECTED == "Y")

        SLWREC.XXXLOCID = "520"; /* Module identification*/
        set SL01M001.SL01M-SELECT[SL01W-MAP-FIELD-INX] cursor, bold;
                                  /* highlight*/
        converseLib.validationFailed(51); /* Only one row may be selected*/

      else

        /* move selected line to slwrec to be passed*/

        SLWREC.PC2INSEG = SL01W01.SL01W-PC-CU-IN-SEG-CD[SL01W-MAP-FIELD-INX]; /* inventory segment code*/

        SLWREC.XEG-ENGR-DIFF-CD = SL01W01.SL01W-XEG-ENGR-DIFF-CD[SL01W-MAP-FIELD-INX]; /* engineering difference code*/

        SLWREC.XSLCD = SL01W01.SL01W-XSL-CLASS-CD[SL01W-MAP-FIELD-INX]; /* class code*/

        SLWREC.XUMCD = SL01W01.SL01W-XUM-UOM-CD[SL01W-MAP-FIELD-INX]; /* unit of measure code*/

        SLWREC.XOWCD = SL01W01.SL01W-XOW-OWN-GRP-CD[SL01W-MAP-FIELD-INX]; /* own group code*/

        SL01W00.SL01W-ROW-SELECTED = "Y";

      end

    else

      SLWREC.XXXLOCID = "520"; /* Module identification*/
      set SL01M001.SL01M-SELECT[SL01W-MAP-FIELD-INX] cursor, bold;
                                  /* highlight*/
      converseLib.validationFailed(228); /* Only "S" may be entered*/

    end
  end

end // end SL01-520

// Process valid input request
Function SL01-600()
   /* ------------------------------------------------------------*/
   /* move map working storage to slwrec*/
   /* ------------------------------------------------------------*/

  SL01-610(); /* load slwrec*/

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    SL01W00.XXXTSPAG = SL01W00.XXXTSPAG - 1; /* Decrement page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    SL01W00.XXXTSPAG = SL01W00.XXXTSPAG + 1; /* Increment page number*/

    return; /* Return to previous process*/

  end

  if (converseVar.eventKey is pf13) /* PF13 was pressed*/
    COMMAREA.CATOAP = "SL02"; /* TO application*/
  end

  if (converseVar.eventKey is pf14) /* PF14 was pressed*/
    COMMAREA.CATOAP = "SL03"; /* TO application*/
  end

  if (converseVar.eventKey is pf15) /* PF15 was pressed*/
    COMMAREA.CATOAP = "SL04"; /* TO application*/
  end

  if (converseVar.eventKey is pf16) /* PF16 was pressed*/
    COMMAREA.CATOAP = "SL05"; /* TO application*/
  end

  if (converseVar.eventKey is pf17) /* PF17 was pressed*/
    COMMAREA.CATOAP = "SL0Q"; /* TO application*/
    SLWREC.SLWCASNO = "7"; /* option number*/
  end

  if (converseVar.eventKey is pf18) /* PF18 was pressed*/
    COMMAREA.CATOAP = "SL0Q"; /* TO application*/
    SLWREC.SLWCASNO = "8"; /* option number*/
  end


  if (COMMAREA.CATOAP > " ") /* if transfer requested*/

    SL01-110(); /* Purge temporary storage*/

    XSSL001(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end
  /* --------------------------------------------------------------*/
     /* indicate the map needs to be formatted*/
  /* --------------------------------------------------------------*/

  SL01W00.SL01W-FORMAT-MAP-SW = "Y"; /* format map*/

  /* --------------------------------------------------------------*/
     /* verify header information has not changed*/
  /* --------------------------------------------------------------*/

  if (SL01W01.SL01W-XWH-WHSE-CD != SL01M001.SL01M-XWH-WHSE-CD
   || SL01W01.SL01W-XGP-GPC-CD != SL01M001.SL01M-XGP-GPC-CD
   || SL01W01.SL01W-SY-STY-NBR != SL01M001.SL01M-SY-STY-NBR
   || SL01W01.SL01W-SY-COLR-CD-ID != SL01M001.SL01M-SY-COLR-CD-ID
   || SL01W01.SL01W-XDM-DIM-CD != SL01M001.SL01M-XDM-DIM-CD
   || SL01W01.SL01W-XQA-QUAL-CD != SL01M001.SL01M-XQA-QUAL-CD)

    SL01W00.SL01W-RQT-DATA-SW = "Y"; /* Request for data switch*/

  end

  if (converseVar.eventKey is enter) /* enter was pressed*/

    if (SL01W00.SL01W-RQT-DATA-SW == "Y")

      exit stack;

    else

      COMMAREA.CATOAP = "SL02"; /* TO application*/
      SL01W00.SL01W-FORMAT-MAP-SW = "N";
      SL01-110(); /* Purge temporary storage*/
      XSSL001(); /* Exit to another application*/
      exit stack; /* Exit to display map*/

    end
  end


end // end SL01-600

