// Format and converse the map
Function SL01-200()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/
    SLWREC.SLWMSGNO = converseVar.validationMsgNum; /* Message number*/
  end

   /* ------------------------------------------------------------*/
   /* If obtain data failed or no error, format data to display.*/
   /* ------------------------------------------------------------*/

  if (SL01W00.SL01W-RQT-DATA-SW == "Y" /* Obtain data failed*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (SL01W00.SL01W-FORMAT-MAP-SW == "Y") /* Map must be formatted*/
      SL01-210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  SL01M001.SL01M-XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/

  SL01M001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  SL01M001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (SLWREC.SLWMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (SLWREC.SLWMSGNO != 0) /* Message to display*/

      if (SLWREC.SLWMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(SLWREC.SLWMSGNO);
      end /* Message number*/
    else
      SL01M001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    SL01M001.SL01M-XXX-MODID = SLWREC.XXXLOCID; /* Module ID*/

    set SL01M001 alarm; /* Indicate sound alarm*/

  end

  converse SL01M001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  SLWREC.XXXLOCID = " "; /* Module identification*/
  SLWREC.SLWMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/

  COMMAREA.CATOAP = SL01M001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = SL01M001.CAITEM; /* Application data*/


end // end SL01-200

// Format the display data
Function SL01-210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set SL01M001 initial; /* map input/output area*/

  SL01W00.SL01W-FORMAT-MAP-SW = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (SL01W00.TA5TSQIX > 0) /* Pages were formatted*/

    if (SL01W00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the current page has been changed, update the page in*/
       /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/

      if (SL01W00.SL01W-UPD-PAGE-SW == "Y") /* Current page was changed*/

        SL01S01(); /* Update temporary storage page*/

        SL01W00.SL01W-UPD-PAGE-SW = "N"; /* Storage update switch*/

      end

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (SL01W00.XXXTSPAG > SL01W00.TA5TSQIX) /* Desired greater highest*/
        SL01W00.XXXTSPAG = SL01W00.TA5TSQIX; /* Default to highest*/
      end

      SL01S02(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set SL01M001.XXXLITPG skip; /* "PAGE" literal*/
    SL01M001.XXXTSPAG = SL01W00.XXXTSPAG; /* Current page*/
    set SL01M001.XXXLITOF skip; /* "OF" literal*/
    SL01M001.TA5TSQIX = SL01W00.TA5TSQIX; /* Highest page*/

  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  SL01M001.SL01M-XWH-WHSE-CD = SL01W01.SL01W-XWH-WHSE-CD; /* warehouse code*/

  SL01M001.SL01M-XWH-WHSE-NM = SL01W01.SL01W-XWH-WHSE-NM; /* warehouse code*/

  SL01M001.SL01M-XGP-GPC-CD = SL01W01.SL01W-XGP-GPC-CD; /* GPC code*/

  SL01M001.SL01M-SY-STY-NBR = SL01W01.SL01W-SY-STY-NBR; /* Style number*/

  SL01M001.SL01M-SY-COLR-CD-ID = SL01W01.SL01W-SY-COLR-CD-ID; /* color code*/

  SL01M001.SL01M-XDM-DIM-CD = SL01W01.SL01W-XDM-DIM-CD; /* dimension code*/

  SL01M001.SL01M-XQA-QUAL-CD = SL01W01.SL01W-XQA-QUAL-CD; /* quality code*/

  SL01M001.SL01M-SY-STY-NM = SL01W01.SL01W-SY-STY-NM; /* Style name*/

  SL01M001.SL01M-STY-COLR-NM = SL01W01.SL01W-STY-COLR-NM; /* color name*/

  if (SL01W01.SL01W-SY-STY-NBR == " ") /* set cursor if style nbr blank*/
    set SL01M001.SL01M-SY-STY-NBR cursor;
  end


  SL01W00.SL01W-MAP-FIELD-INX = 1; /* Initialize line index*/

  while (SL01W00.SL01W-MAP-FIELD-INX <= 14 /* Until screen is full*/
   && SL01W01.SL01W-PC-CU-IN-SEG-CD[SL01W-MAP-FIELD-INX] > " ")

    if (SL01W00.SL01W-MAP-FIELD-INX == 1) /* First select field*/

      set SL01M001.SL01M-SELECT[SL01W-MAP-FIELD-INX] cursor, normal;
    else
      set SL01M001.SL01M-SELECT[SL01W-MAP-FIELD-INX] normal; /* Unprotect*/

    end

   /* --------------------------------------------------------------------*/
   /* move display fields from working storage*/
   /* --------------------------------------------------------------------*/

    /* selection column*/
    SL01M001.SL01M-SELECT[SL01W-MAP-FIELD-INX] = SL01W01.SL01W-SELECT[SL01W-MAP-FIELD-INX];

   /* inventory segment code column*/
    SL01M001.SL01M-PC-CU-IN-SEG-CD[SL01W-MAP-FIELD-INX] = SL01W01.SL01W-PC-CU-IN-SEG-CD[SL01W-MAP-FIELD-INX];

    /* engineering difference code column*/
    SL01M001.SL01M-XEG-ENGR-DIFF-CD[SL01W-MAP-FIELD-INX] = SL01W01.SL01W-XEG-ENGR-DIFF-CD[SL01W-MAP-FIELD-INX];

    /* class code column*/
    SL01M001.SL01M-XSL-CLASS-CD[SL01W-MAP-FIELD-INX] = SL01W01.SL01W-XSL-CLASS-CD[SL01W-MAP-FIELD-INX];

    /* unit of measure column*/
    SL01M001.SL01M-XUM-UOM-CD[SL01W-MAP-FIELD-INX] = SL01W01.SL01W-XUM-UOM-CD[SL01W-MAP-FIELD-INX];

    /* own group code column*/
    SL01M001.SL01M-XOW-OWN-GRP-CD[SL01W-MAP-FIELD-INX] = SL01W01.SL01W-XOW-OWN-GRP-CD[SL01W-MAP-FIELD-INX];

    SL01W00.SL01W-MAP-FIELD-INX = SL01W00.SL01W-MAP-FIELD-INX + 1;
                                  /* increment line index*/

  end


end // end SL01-210

// Check attention identifier
Function SL01-300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (SL01W00.SL01W-RQT-DATA-SW == "Y") /* Request for data*/
      set SL01W01 empty; /* M001 display storage*/
    end

    SL01W00.SL01W-FORMAT-MAP-SW = "Y"; /* Format the map switch*/
    SL01W00.SL01W-RQT-DATA-SW = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (SL01M001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      SL01-110(); /* Purge temporary storage*/

      XSSL001(); /* Exit to another application*/

      set SL01M001.CATOAP cursor, bold; /* Highlight*/

    else

      SLWREC.XXXLOCID = "300"; /* Module identification*/
      converseLib.validationFailed(258); /* PA/PF key may not be used*/

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4 /* or PF4 was pressed*/
   || converseVar.eventKey is pf5) /* or PF5 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "SL01M001"; /* Application find code*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "SL0A"; /* TO application*/
    end

    if (converseVar.eventKey is pf4) /* PF4 was pressed*/
      COMMAREA.CATOAP = "MN0M"; /* TO application*/
    end

    if (converseVar.eventKey is pf5) /* PF5 was pressed*/
      COMMAREA.CATOAP = "SL0M"; /* TO application*/
    end

    SL01-110(); /* Purge temporary storage*/

    XSSL001(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf13  /* and PF8 was not pressed*/
   && converseVar.eventKey not pf14  /* and PF8 was not pressed*/
   && converseVar.eventKey not pf15  /* and PF8 was not pressed*/
   && converseVar.eventKey not pf16  /* and PF8 was not pressed*/
   && converseVar.eventKey not pf17  /* and PF8 was not pressed*/
   && converseVar.eventKey not pf18) /* and PF8 was not pressed*/

    SLWREC.XXXLOCID = "300"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end SL01-300

// Reset highlighted fields
Function SL01-400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set SL01M001.SL01M-XWH-WHSE-CD initialAttributes; /* warehouse code*/
  set SL01M001.SL01M-XGP-GPC-CD initialAttributes; /* GPC code*/
  set SL01M001.SL01M-SY-STY-NBR initialAttributes; /* Style code*/
  set SL01M001.SL01M-SY-COLR-CD-ID initialAttributes; /* color code*/
  set SL01M001.SL01M-XDM-DIM-CD initialAttributes; /* dimension code*/
  set SL01M001.SL01M-XQA-QUAL-CD initialAttributes; /* quality code*/

  SL01W00.SL01W-MAP-FIELD-INX = 1; /* Initialize line index*/

  while (SL01W00.SL01W-MAP-FIELD-INX <= 14 /* Until end of lines*/
   && SL01M001.SL01M-PC-CU-IN-SEG-CD[SL01W-MAP-FIELD-INX] > " ")
                                  /* or end of display*/

    set SL01M001.SL01M-SELECT[SL01W-MAP-FIELD-INX] normal;
                                  /* reset select flag*/

    SL01W00.SL01W-MAP-FIELD-INX = SL01W00.SL01W-MAP-FIELD-INX + 1;
                                  /* Increment line index*/

  end

  set SL01M001.CATOAP initialAttributes; /* TO application*/
  set SL01M001.CAITEM initialAttributes; /* Application data*/

   /* ----------------------------------------------------------------------*/
    /* pop-up window option -- if not selected, return to previous process*/
   /* ----------------------------------------------------------------------*/

  if (SL01M001.SL01M-XWH-WHSE-CD == "?"
   || SL01M001.SL01M-XGP-GPC-CD == "?"
   || SL01M001.SL01M-XDM-DIM-CD == "?"
   || SL01M001.SL01M-XQA-QUAL-CD == "?")

    if (converseVar.eventKey not enter) /* enter was not pressed*/

      SLWREC.XXXLOCID = "400"; /* module identification*/
      converseLib.validationFailed(4); /* pkkey invalid*/

      exit stack; /* exit to display map*/

    end

  else

    return; /* return to previous processing*/

  end

  SL01-410();

   /* ----------------------------------------------------------------------*/
   /* clear screen for pop-ups, then display map*/
   /* ----------------------------------------------------------------------*/

  converseLib.clearScreen();
   /* clear screen before display*/

  exit stack; /* exit to display map*/
end // end SL01-400

