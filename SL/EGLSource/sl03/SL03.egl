Function SL03-590()
  /* -----------------------------------------------------------------------*/
   /* loop through array returned from sy1190 to determine if size*/
   /* entered in header is among valid sizes*/
  /* -----------------------------------------------------------------------*/

  if (SYPREC.SY4IDDSC[SL03W-SIZE-INX] == SL03W00.SL03W-HEADER-SIZE)

    SL03W00.SL03W-SIZE-FOUND-SW = "Y";

  else

    SL03W00.SL03W-SIZE-INX = SL03W00.SL03W-SIZE-INX + 1;

  end

end // end SL03-590

// Process valid input request
Function SL03-600()
   /* ------------------------------------------------------------*/
   /* load map working storage into slwrec*/
   /* ------------------------------------------------------------*/

  SL03-610(); /* move working storage to slwrec*/

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    SL03W00.XXXTSPAG = SL03W00.XXXTSPAG - 1; /* Decrement page number*/
    SL03W00.SL03W-FORMAT-MAP-SW = "Y"; /* format map*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    SL03W00.XXXTSPAG = SL03W00.XXXTSPAG + 1; /* Increment page number*/
    SL03W00.SL03W-FORMAT-MAP-SW = "Y"; /* format map*/

    return; /* Return to previous process*/

  end

  if (converseVar.eventKey is pf12) /* PF12 was pressed*/
    COMMAREA.CATOAP = "SL01"; /* TO application*/
  end

  if (converseVar.eventKey is pf13) /* PF14 was pressed*/
    COMMAREA.CATOAP = "SL02"; /* TO application*/
  end

  if (converseVar.eventKey is pf15) /* PF15 was pressed*/
    COMMAREA.CATOAP = "SL04"; /* TO application*/
  end

  if (converseVar.eventKey is pf16) /* PF16 was pressed*/
    COMMAREA.CATOAP = "SL05"; /* TO application*/
  end

  if (converseVar.eventKey is pf17) /* PF17 was pressed*/
    COMMAREA.CATOAP = "SL0Q"; /* TO application*/
    SLWREC.SLWCASNO = "7"; /* option number*/
  end

  if (converseVar.eventKey is pf18) /* PF18 was pressed*/
    COMMAREA.CATOAP = "SL0Q"; /* TO application*/
    SLWREC.SLWCASNO = "8"; /* option number*/
  end

  if (COMMAREA.CATOAP > " ") /* if transfer requested*/

    SL03-110(); /* Purge temporary storage*/

    XSSL001(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end
  /* --------------------------------------------------------------*/
     /* indicate the map needs to be formatted*/
  /* --------------------------------------------------------------*/

  SL03W00.SL03W-FORMAT-MAP-SW = "Y"; /* format map*/

  /* --------------------------------------------------------------*/
     /* verify header information has not changed*/
  /* --------------------------------------------------------------*/

  if (SL03W01.SL03W-XWH-WHSE-CD != SL03M001.SL03M-XWH-WHSE-CD
   || SL03W01.SL03W-XGP-GPC-CD != SL03M001.SL03M-XGP-GPC-CD
   || SL03W01.SL03W-SY-STY-NBR != SL03M001.SL03M-SY-STY-NBR
   || SL03W01.SL03W-SY-COLR-CD-ID != SL03M001.SL03M-SY-COLR-CD-ID
   || SL03W01.SL03W-XDM-DIM-CD != SL03M001.SL03M-XDM-DIM-CD
   || SL03W01.SL03W-XQA-QUAL-CD != SL03M001.SL03M-XQA-QUAL-CD
   || SL03W01.SL03W-LIFE-CYCLE != SL03M001.SL03M-LIFE-CYCLE
   || SL03W01.SL03W-PC-PO-NBR != SL03M001.SL03M-PC-PO-NBR
   || SL03W01.SL03W-PC-SAP-PO-NBR != SL03M001.SL03M-PC-SAP-PO-NBR
   || SL03W01.SL03W-PO-ID != SL03M001.SL03M-PO-ID
   || SL03W01.SL03W-PO-TYPE != SL03M001.SL03M-PO-TYPE
   || SL03W01.SL03W-SIZE != SL03M001.SL03M-SIZE)

    SL03W00.SL03W-RQT-DATA-SW = "Y"; /* Request for data switch*/

  end

  if (converseVar.eventKey is enter) /* enter was pressed*/

    if (SL03W00.SL03W-RQT-DATA-SW == "Y"
    )
      exit stack;

    else

      COMMAREA.CATOAP = "SL04"; /* TO application*/
      SL03-110(); /* Purge temporary storage*/
      XSSL001(); /* Exit to another application*/
      SL03W00.SL03W-FORMAT-MAP-SW = "N"; /* keep 's' on screen*/
      exit stack;

    end
  end

end // end SL03-600

