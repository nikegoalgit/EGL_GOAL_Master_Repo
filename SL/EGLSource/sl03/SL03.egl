        SLWREC.SY-UNITS-PER-CASE = SL03W01.SL03W-UNPCS[SL03W-MAP-FIELD-INX];

        SL03W00.SL03W-ROW-SELECTED = "Y";

      end

    else
      SLWREC.XXXLOCID = "520"; /* Module identification*/
      set SL03M001.SL03M-SELECT[SL03W-MAP-FIELD-INX] cursor, bold;
