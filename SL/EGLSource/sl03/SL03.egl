  TA1REC.TA1MAP = "SL03M001";

  call "SY1190" (SQLCA, SYPREC) {isNoRefresh = yes, isExternal = yes}; /* convert size format*/

  if (SYPREC.SYPRC != 0)
    if (SYPREC.SYPRC == 9)

      SLWREC.XXXLOCID = "500"; /* Module identification*/
      set SL03M001.SL03M-SIZE cursor, bold; /* Highlight*/
      converseLib.validationFailed(238); /* invalid key*/
