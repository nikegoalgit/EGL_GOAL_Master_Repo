  set SL03M001.SL03M-XDM-DIM-CD initialAttributes; /* dimension code*/
  set SL03M001.SL03M-XQA-QUAL-CD initialAttributes; /* quality code*/
  set SL03M001.SL03M-LIFE-CYCLE initialAttributes; /* life cycle*/
  set SL03M001.SL03M-XOW-OWN-GRP-CD initialAttributes; /* own group*/
  set SL03M001.SL03M-PC-CU-IN-SEG-CD initialAttributes; /* inventory segment*/
  set SL03M001.SL03M-XEG-ENGR-DIFF-CD initialAttributes; /* engineering diff*/
  set SL03M001.SL03M-XUM-UOM-CD initialAttributes; /* unit of measure*/
  set SL03M001.SL03M-XSL-CLASS-CD initialAttributes; /* class code*/
  set SL03M001.SL03M-PO-ID initialAttributes; /* po id*/
  set SL03M001.SL03M-PO-TYPE initialAttributes; /* po type*/
  set SL03M001.SL03M-PC-PO-NBR initialAttributes; /* po nbr*/
  set SL03M001.SL03M-PC-SAP-PO-NBR initialAttributes; /* po nbr*/
  set SL03M001.SL03M-SIZE initialAttributes; /* size*/

  SL03W00.SL03W-MAP-FIELD-INX = 1; /* Initialize line index*/

  while (SL03W00.SL03W-MAP-FIELD-INX <= 13 /* Until end of lines*/
   && SL03M001.SL03M-NET-QTY[SL03W-MAP-FIELD-INX] > 0)
                                  /* or end of display*/

    set SL03M001.SL03M-SELECT[SL03W-MAP-FIELD-INX] normal;
                                  /* reset select flag*/

    SL03W00.SL03W-MAP-FIELD-INX = SL03W00.SL03W-MAP-FIELD-INX + 1;
                                  /* Increment line index*/

  end

  set SL03M001.CATOAP initialAttributes; /* TO application*/
  set SL03M001.CAITEM initialAttributes; /* Application data*/

   /* ----------------------------------------------------------------------*/
    /* pop-up window option -- if not selected, return to previous process*/
   /* ----------------------------------------------------------------------*/

  if (SL03M001.SL03M-XWH-WHSE-CD == "?"
   || SL03M001.SL03M-XGP-GPC-CD == "?"
   || SL03M001.SL03M-XDM-DIM-CD == "?"
   || SL03M001.SL03M-XQA-QUAL-CD == "?"
   || SL03M001.SL03M-XEG-ENGR-DIFF-CD == "?"
   || SL03M001.SL03M-XUM-UOM-CD == "?"
   || SL03M001.SL03M-XOW-OWN-GRP-CD == "?"
   || SL03M001.SL03M-XSL-CLASS-CD == "?")

    if (converseVar.eventKey not enter) /* enter was not pressed*/

      SLWREC.XXXLOCID = "400"; /* module identification*/
      converseLib.validationFailed(4); /* pkkey invalid*/

      exit stack; /* exit to display map*/

    end

  else

    return; /* return to previous processing*/

  end

  SL03-410();

   /* ----------------------------------------------------------------------*/
   /* clear screen for pop-ups, then display map*/
   /* ----------------------------------------------------------------------*/

  converseLib.clearScreen();
   /* clear screen before display*/

  exit stack; /* exit to display map*/
end // end SL03-400

