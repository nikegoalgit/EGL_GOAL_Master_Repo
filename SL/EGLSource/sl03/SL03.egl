   /* ------------------------------------------------------------*/
  if (SL03M001.SL03M-XGP-GPC-CD in XWHTBL.XGPCD1
   || SL03M001.SL03M-XGP-GPC-CD in XWHTBL.XGPCD2
   || SL03M001.SL03M-XGP-GPC-CD in XWHTBL.XGPCD3
   || SL03M001.SL03M-XGP-GPC-CD in XWHTBL.XGPCD4
   || SL03M001.SL03M-XGP-GPC-CD in XWHTBL.XGPCD5
   || SL03M001.SL03M-XGP-GPC-CD in XWHTBL.XGPCD6
   || SL03M001.SL03M-XGP-GPC-CD in XWHTBL.XGPCD7
   || SL03M001.SL03M-XGP-GPC-CD in XWHTBL.XGPCD8)
    if (XWHTBL.XXXSTAT[sysVar.arrayIndex] == "A")

    /* next sentence*/

    else

      SLWREC.XXXLOCID = "500"; /* Module identification*/
      set SL03M001.SL03M-XGP-GPC-CD cursor, bold; /* Highlight*/
      converseLib.validationFailed(157); /* invalid gpc*/

    end
  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL03M001.SL03M-XGP-GPC-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(157); /* invalid gpc*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- warehouse code*/
   /* -------------------------------------------------------------*/
  if (SL03M001.SL03M-XWH-WHSE-CD in XWHTBL.XWHCD
   && XWHTBL.XXXSTAT[sysVar.arrayIndex] == "A")

    /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL03M001.SL03M-XWH-WHSE-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(373); /* invalid warehouse*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- po nbr or (po id/po type)--not both*/
   /* move map fields to storage so don't have to re-calc*/
   /* -------------------------------------------------------------*/

  SL03W00.SL03W-MAP-PO-NBR = SL03M001.SL03M-PC-PO-NBR;
  SL03W00.SL03W-MAP-PC-SAP-PO-NBR = SL03M001.SL03M-PC-SAP-PO-NBR;
  SL03W00.SL03W-MAP-PO-ID = SL03M001.SL03M-PO-ID;
  SL03W00.SL03W-MAP-PO-TYPE = SL03M001.SL03M-PO-TYPE;

   /* IF SL03M001.SL03M-PC-PO-NBR EQ -1;*/

   /* IF SL03M001.SL03M-PO-TYPE EQ '*';*/
     /* AND SL03M001.SL03M-PO-ID EQ '*';*/

     /* MOVE -1 TO SL03W00.SL03W-PC-PO-ID-MO-RNG;*/
     /* mOVE '*' TO SL03W00.SL03W-PC-PO-ID-SESN-CD;*/
     /* MOVE '*' TO SL03W00.SL03W-FY-FCTY-CD;*/
     /* MOVE '*' TO SL03W00.SL03W-PC-PO-OT-CD;*/

     /* MOVE SL03M001.SL03M-PC-PO-NBR TO SL03W00.SL03W-PO-NUM;*/

   /* END;*/

   /* ELSE;*/

  if (SL03M001.SL03M-PO-TYPE == "*"
   && SL03M001.SL03M-PO-ID == "*")

    SL03W00.SL03W-PC-PO-ID-MO-RNG = -1;
    SL03W00.SL03W-PC-PO-ID-SESN-CD = "*";
    SL03W00.SL03W-FY-FCTY-CD = "*";
    SL03W00.SL03W-PC-PO-OT-CD = "*";

    SL03W00.SL03W-PO-NUM = SL03M001.SL03M-PC-PO-NBR;

    /* ELSE;*/

     /* MOVE '500' TO SLWREC.XXXLOCID;/* Module identification*/
     /* SET SL03M001.SL03M-PO-TYPE CURSOR,BRIGHT;/* Highlight*/
     /* SET SL03M001.SL03M-PO-ID CURSOR,BRIGHT;/* Highlight*/
     /* SET SL03M001.SL03M-PC-PO-NBR CURSOR,BRIGHT;/* Highlight*/
     /* MOVE 192 TO EZEMNO;          /* only po nbr 'or' po type/po id*/

    /* END;*/
  end

   /* ------------------------------------------------------------*/
   /* validate size entered*/
   /* ------------------------------------------------------------*/

  if (SL03M001.SL03M-SIZE == " ")

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL03M001.SL03M-SIZE cursor, bold; /* Highlight*/
    converseLib.validationFailed(238); /* invalid key*/
    exit stack;

  else
    SL03W00.SL03W-HEADER-SIZE = SL03M001.SL03M-SIZE;

    set SYPREC empty;

    SYPREC.SYPFC = "PC"; /* get all valid sizes*/
    SYPREC.SYPCMPSW = "C"; /* indicate compressed format*/

    SYPREC.XGPCD = SL03M001.SL03M-XGP-GPC-CD;
    SYPREC.SY1STNBR = SL03M001.SL03M-SY-STY-NBR;
    SYPREC.SY2CLRID = SL03M001.SL03M-SY-COLR-CD-ID;
    SYPREC.XDMCD = SL03M001.SL03M-XDM-DIM-CD;
    SYPREC.XQACD = SL03M001.SL03M-XQA-QUAL-CD;

    TA1REC.TA1LOCAT[2] = "SL03500 ";
    TA1REC.TA1MAP = "SL03M001";

    call "SY1190" (SQLCA, SYPREC) {isNoRefresh = yes, isExternal = yes};

    if (SYPREC.SYPRC != 0)
      if (SYPREC.SYPRC == 9
       || SYPREC.SYPRC == 10
       || SYPREC.SYPRC == 11)

        SLWREC.XXXLOCID = "500"; /* Module identification*/
        set SL03M001.SL03M-XWH-WHSE-CD cursor, bold; /* Highlight*/
        converseLib.validationFailed(238); /* invalid key*/
        exit stack;

      else

        TA1REC.TA1TYPE = "DB2 ";
        TA1REC.TA1FUNC = "RETN";
        TA1REC.TA1PGMNM = "XSSYPI0 ";
        TA1REC.TA1LOCAT[1] = "SY1190  ";
        TA1REC.TA1LOCAT[3] = SYPREC.SYPRCX;
        TA1REC.TA1LOCAT[4] = "SZ XLATE";
        TA1REC.TA1DBASE = "MULTIPLE";
        TA1REC.TA1TBLVU = "DB VIEWS";
        TA1REC.TA1TBLKE = SYPREC.SYPKEY;
        move SQLCA to ERRSQLCA withV60Compat;
        VGVar.handleSysLibraryErrors = 0;
        sysLib.rollback();
        call "TA0020" (TA1REC, ERRSQLCA) {isExternal = yes};

      end
    end

    SL03W00.SL03W-SIZE-INX = 1;
    SL03W00.SL03W-SIZE-FOUND-SW = "N";

    while (SL03W00.SL03W-SIZE-FOUND-SW == "N"
     && SL03W00.SL03W-SIZE-INX <= 40)

      SL03-590();

    end

    if (SL03W00.SL03W-SIZE-FOUND-SW == "N")

      SLWREC.XXXLOCID = "500"; /* Module identification*/
      set SL03M001.SL03M-SIZE cursor, bold; /* Highlight*/
      converseLib.validationFailed(238); /* invalid key*/
      exit stack;

    end
  end

   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/


   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end SL03-500

// Verify scroll request
Function SL03-510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && SL03W00.XXXTSPAG < 2)       /* and no previous pages*/

    SLWREC.XXXLOCID = "510"; /* Module identification*/
    converseLib.validationFailed(7); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && SL03W00.XXXTSPAG >= SL03W00.TA5TSQIX) /* and no more pages*/

    SLWREC.XXXLOCID = "510"; /* Module identification*/
    converseLib.validationFailed(8); /* No more pages to display*/

  end

end // end SL03-510

