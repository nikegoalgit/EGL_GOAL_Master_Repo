// Format the display data
Function SL03-210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set SL03M001 initial; /* map input/output area*/

  SL03W00.SL03W-FORMAT-MAP-SW = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (SL03W00.TA5TSQIX > 0) /* Pages were formatted*/

    if (SL03W00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the current page has been changed, update the page in*/
       /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/

      if (SL03W00.SL03W-UPD-PAGE-SW == "Y") /* Current page was changed*/

        SL03S01(); /* Update temporary storage page*/

        SL03W00.SL03W-UPD-PAGE-SW = "N"; /* Storage update switch*/

      end

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (SL03W00.XXXTSPAG > SL03W00.TA5TSQIX) /* Desired greater highest*/
        SL03W00.XXXTSPAG = SL03W00.TA5TSQIX; /* Default to highest*/
      end

      SL03S02(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set SL03M001.XXXLITPG skip; /* "PAGE" literal*/
    SL03M001.XXXTSPAG = SL03W00.XXXTSPAG; /* Current page*/
    set SL03M001.XXXLITOF skip; /* "OF" literal*/
    SL03M001.TA5TSQIX = SL03W00.TA5TSQIX; /* Highest page*/

  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  SL03M001.SL03M-XWH-WHSE-CD = SL03W01.SL03W-XWH-WHSE-CD; /* warehouse code*/

  SL03M001.SL03M-XWH-WHSE-NM = SL03W01.SL03W-XWH-WHSE-NM; /* warehouse code*/

  SL03M001.SL03M-XGP-GPC-CD = SL03W01.SL03W-XGP-GPC-CD; /* GPC code*/

  SL03M001.SL03M-SY-STY-NBR = SL03W01.SL03W-SY-STY-NBR; /* Style number*/

  SL03M001.SL03M-SY-COLR-CD-ID = SL03W01.SL03W-SY-COLR-CD-ID; /* color code*/

  SL03M001.SL03M-XDM-DIM-CD = SL03W01.SL03W-XDM-DIM-CD; /* dimension code*/

  SL03M001.SL03M-XQA-QUAL-CD = SL03W01.SL03W-XQA-QUAL-CD; /* quality code*/

  SL03M001.SL03M-SY-STY-NM = SL03W01.SL03W-SY-STY-NM; /* Style name*/

  SL03M001.SL03M-STY-COLR-NM = SL03W01.SL03W-STY-COLR-NM; /* color name*/

  SL03M001.SL03M-PC-CU-IN-SEG-CD = SL03W01.SL03W-PC-CU-IN-SEG-CD;

  SL03M001.SL03M-XEG-ENGR-DIFF-CD = SL03W01.SL03W-XEG-ENGR-DIFF-CD;

  SL03M001.SL03M-XSL-CLASS-CD = SL03W01.SL03W-XSL-CLASS-CD;

  SL03M001.SL03M-XUM-UOM-CD = SL03W01.SL03W-XUM-UOM-CD;

  SL03M001.SL03M-XOW-OWN-GRP-CD = SL03W01.SL03W-XOW-OWN-GRP-CD;

  SL03M001.SL03M-LIFE-CYCLE = SL03W01.SL03W-LIFE-CYCLE;

  SL03M001.SL03M-PO-ID = SL03W01.SL03W-PO-ID;

  SL03M001.SL03M-PC-PO-NBR = SL03W01.SL03W-PC-PO-NBR;

  SL03M001.SL03M-PC-SAP-PO-NBR = SL03W01.SL03W-PC-SAP-PO-NBR;

  SL03M001.SL03M-PO-TYPE = SL03W01.SL03W-PO-TYPE;

  SL03M001.SL03M-SIZE = SL03W01.SL03W-SIZE;

   /* set cursor to blank fields*/

  if (SL03M001.SL03M-SY-STY-NBR == " ")
    set SL03M001.SL03M-SY-STY-NBR cursor;
  else
    if (SL03M001.SL03M-SIZE == " ")
      set SL03M001.SL03M-SIZE cursor;
    end
  end


  SL03W00.SL03W-MAP-FIELD-INX = 1; /* Initialize line index*/

  while (SL03W00.SL03W-MAP-FIELD-INX <= 13 /* Until screen is full*/
   && SL03W01.SL03W-SIZES[SL03W-MAP-FIELD-INX] > " ")

    if (SL03W00.SL03W-MAP-FIELD-INX == 1) /* First select field*/

      set SL03M001.SL03M-SELECT[SL03W-MAP-FIELD-INX] cursor, normal;
    else
      set SL03M001.SL03M-SELECT[SL03W-MAP-FIELD-INX] normal; /* Unprotect*/

    end

   /* --------------------------------------------------------------------*/
   /* move display fields from working storage*/
   /* --------------------------------------------------------------------*/

    /* selection column*/
    SL03M001.SL03M-SELECT[SL03W-MAP-FIELD-INX] = SL03W01.SL03W-SELECT[SL03W-MAP-FIELD-INX];

    SL03M001.SL03M-SIZES[SL03W-MAP-FIELD-INX] = SL03W01.SL03W-SIZES[SL03W-MAP-FIELD-INX];

    SL03M001.SL03M-NET-QTY[SL03W-MAP-FIELD-INX] = SL03W01.SL03W-NET-QTY[SL03W-MAP-FIELD-INX];

    SL03M001.SL03M-SL-SY-LOC-OH-QTY[SL03W-MAP-FIELD-INX] = SL03W01.SL03W-SL-SY-LOC-OH-QTY[SL03W-MAP-FIELD-INX];

