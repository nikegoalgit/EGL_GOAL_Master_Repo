    SLWREC.XXXLOCID = "500";
    SLWREC.SLWMSGNO = 365; /* <--- invalid value msg*/
    set SL03M001.SL03M-PC-CU-IN-SEG-CD cursor, bold;
  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- quality code*/
   /* ------------------------------------------------------------*/
  if (SL03M001.SL03M-XQA-QUAL-CD in XQATBL.XQACD)

    /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL03M001.SL03M-XQA-QUAL-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(366); /* invalid quality code*/

