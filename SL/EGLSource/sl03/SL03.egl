//*** PROGRAM=SL03 ****
// This application allows a user to inquire and maintain rows
// in a DB2 table.
// 
// There are two different screens that may be displayed.
// 
// The first screen displayed to the user allows specification
// of the search criteria to be used to list the rows in the
// table.  The list is displayed and the user may page backward
// and forward to view the information.
// 
// When a line is selected on the screen, the second screen is
// displayed, showing all information for that table row.
// 
// If the user has sufficient security to change the data, the
// modifiable fields remain unprotected.  Otherwise, they are
// protected from modification by the user.
// 
// If any data is modified, it is verified and, if complete and
// correct, the table row is updated with the changed data.
// The screen is redisplayed to the user, indicating that the
// update is completed.
// 
// If more that one line was selected on the first screen, the
// next selection is displayed when the user presses the ENTER
// key without making any change to the displayed data.
// 
// When the user exits the second screen (either via PF3 key or
// after all selections have been viewed) the first screen is
// redisplayed, indicating the selections that were viewed.
// 
// The user may exit to any allowed application, via FAST PATH
// or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      07/15/93  hchapm  New application.
// CANADA   08/55/95  CBRINE  Added call to SY2000. Added XCTCD
//                            to SLWREC.
// US NSC   04/18/01  KRAMAL  Included Sap po number to SLWREC
// 
// *********************
Program SL03 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "SL00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  ERRSQLCA ERRSQLCA; // record
  PC2REC PC2REC; // record
  PKNREC PKNREC; // record
  SL03W00 SL03W00; // record
  SL03W01 SL03W01; // record
  SLWREC SLWREC; // record
  SQLCA SQLCA; // record
  SWVREC SWVREC; // record
  SYCREC SYCREC; // record
  SYPREC SYPREC; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VSYLOC02 VSYLOC02; // record
  WSY2000 WSY2000; // record
  XSSLW01 XSSLW01; // record
  XX0XW02 XX0XW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XWHTBL; // table
  use XDMTBL; // table
  use XQATBL; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use XOWTBL; // table
  use XEGTBL; // table
  use XUMTBL; // table
  use XSLTBL; // table
  use SL03M.SL03M001,  // forms
      SL03M.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPSL001: XPSL001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/

    set SL03M001 initial; /* M001 input/output*/
    set SL03W00 empty; /* Working storage*/
    set SL03W01 empty; /* M001 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set TA5REC empty; /* Temporary storage parameters*/

    SL03W00.SL03W-FORMAT-MAP-SW = "Y"; /* Format the map switch*/
    SL03W00.SL03W-RQT-DATA-SW = "N"; /* Request for data switch*/

     /* ------------------------------------------------------------*/
     /* If all key info is passed in SLWREC, use as initial key*/
     /* ------------------------------------------------------------*/

    if (SLWREC.SY1STNBR > " " /* style nbr is passed*/
     && SLWREC.SY-SZ-ID-DESC > " ") /* size is passed*/

      SL03W00.SL03W-RQT-DATA-SW = "Y"; /* request for data switch*/

    end

    if (SLWREC.SY-SZ-ID-DESC > " ")
      SL03M001.SL03M-SIZE = SLWREC.SY-SZ-ID-DESC;
    end

    if (SLWREC.SY1STNBR > " ") /* if style nbr is passed in slwrec*/
      SL03M001.SL03M-SY-STY-NBR = SLWREC.SY1STNBR; /* move to work area*/
    end


    if (SLWREC.XWHCD > " ") /* if whse code is passed in slwrec*/
      SL03M001.SL03M-XWH-WHSE-CD = SLWREC.XWHCD; /* move it to work area*/
    else
      if (COMMAREA.XWHCD > " ") /* if whse code is passed in commarea*/
        SL03M001.SL03M-XWH-WHSE-CD = COMMAREA.XWHCD; /* move towrk area*/
      end
    end


    if (SLWREC.XGPCD > " ") /* if gpc code is passed in slwrec*/
      SL03M001.SL03M-XGP-GPC-CD = SLWREC.XGPCD; /* move to work area*/
    else
      if (COMMAREA.XGPCD > " ") /* if gpc code is passed in commarea*/
        SL03M001.SL03M-XGP-GPC-CD = COMMAREA.XGPCD; /* move to work area*/
      else
        if (SL03M001.SL03M-XWH-WHSE-CD == "51") /* if warehouse 51*/
          SL03M001.SL03M-XGP-GPC-CD = "03"; /* move apparel code to gpc*/
        else
          SL03M001.SL03M-XGP-GPC-CD = "02"; /* move footwr code to gpc*/
        end
      end
    end


    if (SLWREC.SY2CLRID > " ") /* if color code is passed*/
      SL03M001.SL03M-SY-COLR-CD-ID = SLWREC.SY2CLRID; /* move to ws*/
    else
      SL03M001.SL03M-SY-COLR-CD-ID = "00 "; /* move to ws*/
    end

    if (SLWREC.XDMCD > " ") /* if dimension code is passed*/
      SL03M001.SL03M-XDM-DIM-CD = SLWREC.XDMCD; /* move to work area*/
    else
      SL03M001.SL03M-XDM-DIM-CD = "00"; /* default to dimension code 00*/
    end


    if (SLWREC.XQACD > " ") /* if quality code is passed*/
      SL03M001.SL03M-XQA-QUAL-CD = SLWREC.XQACD; /* move to work area*/
    else
      SL03M001.SL03M-XQA-QUAL-CD = "01"; /* default to quality code 01*/
    end


    if (SLWREC.XOWCD > " ") /* if own group is passed*/
      SL03M001.SL03M-XOW-OWN-GRP-CD = SLWREC.XOWCD; /* move to ws*/
    else
      SL03M001.SL03M-XOW-OWN-GRP-CD = "00"; /* move to ws*/
    end


    if (SLWREC.PC2INSEG > " ") /* inventory segment is passed*/
      SL03M001.SL03M-PC-CU-IN-SEG-CD = SLWREC.PC2INSEG; /* move to ws*/
    else
      SL03M001.SL03M-PC-CU-IN-SEG-CD = "000"; /* move to ws*/
    end


    if (SLWREC.XEG-ENGR-DIFF-CD > " ") /* engr diff cd passed*/
      SL03M001.SL03M-XEG-ENGR-DIFF-CD = SLWREC.XEG-ENGR-DIFF-CD;
    else
      SL03M001.SL03M-XEG-ENGR-DIFF-CD = "000";
    end


    if (SLWREC.XUMCD > " ") /* unit of measure passed*/
      SL03M001.SL03M-XUM-UOM-CD = SLWREC.XUMCD; /* move to ws*/
    else
      SL03M001.SL03M-XUM-UOM-CD = "01"; /* move to ws*/
    end


    if (SLWREC.XSLCD > " ") /* class code is passed*/
      SL03M001.SL03M-XSL-CLASS-CD = SLWREC.XSLCD; /* move to ws*/
    else
      SL03M001.SL03M-XSL-CLASS-CD = "000"; /* move to ws*/
    end

    if (SLWREC.PC-SAP-PO-NBR > " ") /* Sap Po nbr is passed*/
      SL03M001.SL03M-PC-SAP-PO-NBR = SLWREC.PC-SAP-PO-NBR; /* move to ws*/
    else
      SL03M001.SL03M-PC-SAP-PO-NBR = "*"; /* move to ws*/
    end


    if (SLWREC.PC3POIDT > 0   /* po month range*/
     && SLWREC.PC4ORTYP > " " /* po type passed*/
     && SLWREC.PCFSECD > " "  /* season codeis passed*/
     && SLWREC.FY1FCTYC > " ") /* factory code is passed*/

      SL03W00.SL03W-PC-PO-ID-MO-RNG = SLWREC.PC3POIDT; /* date range*/
      SL03W00.SL03W-PC-PO-OT-CD = SLWREC.PC4ORTYP; /* move type to ws*/
      SL03W00.SL03W-PC-PO-ID-SESN-CD = SLWREC.PCFSECD; /* season cd*/
      SL03W00.SL03W-FY-FCTY-CD = SLWREC.FY1FCTYC; /* factory cd*/
      SL03M001.SL03M-PC-PO-NBR = -1;

      SL03W00.SL03W-FORMAT-DISPLAY-PO-SW = "Y";
    /* flag to format po info into po id and po type for map display*/

    else

      if (SLWREC.PC3PONBR > 0)
        SL03M001.SL03M-PC-PO-NBR = SLWREC.PC3PONBR;
        SL03M001.SL03M-PO-TYPE = "*"; /* wildcard for map display*/
        SL03M001.SL03M-PO-ID = "*";
        SL03W00.SL03W-PC-PO-ID-MO-RNG = -1; /* date range*/
        SL03W00.SL03W-PC-PO-ID-SESN-CD = "*"; /* season cd*/
        SL03W00.SL03W-FY-FCTY-CD = "*"; /* factory cd*/
      else
        SL03M001.SL03M-PO-TYPE = "*"; /* wildcard for map display*/
        SL03M001.SL03M-PO-ID = "*";
        SL03M001.SL03M-PC-PO-NBR = -1;
        SL03W00.SL03W-PC-PO-ID-MO-RNG = -1; /* date range*/
        SL03W00.SL03W-PC-PO-ID-SESN-CD = "*"; /* season cd*/
        SL03W00.SL03W-FY-FCTY-CD = "*"; /* factory cd*/

      end
    end

     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto SL03001; /* Converse the main process*/


    SL03001: SL03001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto SL03001; /* Converse the main process*/


  end // end main
end // end SL03

// Obtain the data to display
Function SL03-100()
   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/

  if (SL03W00.SL03W-RQT-DATA-SW == "Y") /* Data was not requested*/

    SL03-120(); /* Purge any temporary storage*/

  end


  SL03W01.SL03W-XWH-WHSE-CD = SL03M001.SL03M-XWH-WHSE-CD; /* whse cd*/
  SL03W01.SL03W-XGP-GPC-CD = SL03M001.SL03M-XGP-GPC-CD; /* GPC code*/
  SL03W01.SL03W-SY-STY-NBR = SL03M001.SL03M-SY-STY-NBR; /* Style nbr*/
  SL03W01.SL03W-XQA-QUAL-CD = SL03M001.SL03M-XQA-QUAL-CD; /* qual cd*/
  SL03W01.SL03W-SY-COLR-CD-ID = SL03M001.SL03M-SY-COLR-CD-ID;
  SL03W01.SL03W-XDM-DIM-CD = SL03M001.SL03M-XDM-DIM-CD;
  SL03W01.SL03W-XOW-OWN-GRP-CD = SL03M001.SL03M-XOW-OWN-GRP-CD;
  SL03W01.SL03W-PC-CU-IN-SEG-CD = SL03M001.SL03M-PC-CU-IN-SEG-CD;
  SL03W01.SL03W-XEG-ENGR-DIFF-CD = SL03M001.SL03M-XEG-ENGR-DIFF-CD;
  SL03W01.SL03W-XUM-UOM-CD = SL03M001.SL03M-XUM-UOM-CD;
  SL03W01.SL03W-XSL-CLASS-CD = SL03M001.SL03M-XSL-CLASS-CD;
  SL03W01.SL03W-PO-ID = SL03M001.SL03M-PO-ID;
  SL03W01.SL03W-PO-TYPE = SL03M001.SL03M-PO-TYPE;
  SL03W01.SL03W-PC-PO-NBR = SL03M001.SL03M-PC-PO-NBR;
  SL03W01.SL03W-PC-SAP-PO-NBR = SL03M001.SL03M-PC-SAP-PO-NBR;
  SL03W01.SL03W-SIZE = SL03M001.SL03M-SIZE;
  SL03W00.SL03W-HEADER-SIZE = SL03M001.SL03M-SIZE;

  SLWREC.SY-UNITS-PER-CASE = 9999; /* initialize to nines so sl04*/
   /* can differetiate between a 'passed' zero and an empty numeric field*/

   /* ------------------------------------------------------------*/
   /* get warehouse name*/
   /* ------------------------------------------------------------*/

  if (SL03W01.SL03W-XWH-WHSE-CD in XWHTBL.XWHCD)
    SL03W01.SL03W-XWH-WHSE-NM = XWHTBL.XWHABRV[sysVar.arrayIndex];
    SLWREC.XCTCD = XWHTBL.XCTCD[sysVar.arrayIndex]; /* CANA save country code*/
  end

   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (SL03W00.SL03W-RQT-DATA-SW != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* ------------------------------------------------------------*/

  SL03W00.XXXTSLEN = 935; /* Temporary storage page length*/

   /* ------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* ------------------------------------------------------------*/

  if (SLWREC.SLWMSGNO == 0) /* No message to display*/
    SL03W00.XXXTSPAG = 1; /* Page number to display*/
  end

   /* ------------------------------------------------------------*/
   /* get current date to use in determining life cycle code.*/
   /* ------------------------------------------------------------*/

  SL03-110(); /* get current date*/

   /* ------------------------------------------------------------*/
   /* if po info passed in slwrec, format for map display.*/
   /* else - format from user input to po nbr components.*/
   /* ------------------------------------------------------------*/

  if (SL03W00.SL03W-FORMAT-DISPLAY-PO-SW == "Y") /* if po info is*/
                                  /* passed and needs to be formatted*/
    set SWVREC empty; /* initialize swvrec*/

    TA1REC.TA1PGMNM = "SL03-100"; /* calling process for diagnosis*/

    SWVREC.SWVWFNC = "T2"; /* indicate function to perform*/

    SWVREC.XGPCD = SL03W01.SL03W-XGP-GPC-CD; /* gpc cd*/
    SWVREC.PC3POIDT = SL03W00.SL03W-PC-PO-ID-MO-RNG; /* date range*/
    SWVREC.PCFSECD = SL03W00.SL03W-PC-PO-ID-SESN-CD; /* /season code*/
    SWVREC.FY1FCTYC = SL03W00.SL03W-FY-FCTY-CD; /* factory code*/


    XSSLS10(); /* subroutine to format po info*/

    SL03W01.SL03W-PO-ID = SWVREC.SWVWPOID; /* formatted po id*/
    SL03W01.SL03W-PO-TYPE = SWVREC.PC4ORTYP; /* formatted po type*/

    SL03W00.SL03W-FORMAT-DISPLAY-PO-SW = "N";

  else

    if (SL03W01.SL03W-PO-ID != "*"
     && SL03W01.SL03W-PO-TYPE != "*") /* if map contains wildcards in*/

      set SWVREC empty; /* initialize swvrec*/

      TA1REC.TA1PGMNM = "SL03-100"; /* calling process for diagnosis*/

      SWVREC.SWVWFNC = "T1"; /* indicate function to perform*/
      SWVREC.XGPCD = SL03W01.SL03W-XGP-GPC-CD; /* gpc cd*/
      SWVREC.SWVWPOID = SL03W01.SL03W-PO-ID; /* po id*/

      XSSLS10(); /* subroutine to format po info*/

      SL03W00.SL03W-PC-PO-ID-MO-RNG = SWVREC.PC3POIDT; /* date*/
      /* MOVE SWVREC.PCFSECD TO sl03W00.sl03W-PC-PO-ID-SESN-CD;/* season*/
      SL03W00.SL03W-FY-FCTY-CD = SWVREC.FY1FCTYC; /* factory*/
      /* MOVE SWVREC.PC4ORTYP TO sl03W00.sl03W-PC-PO-OT-CD;/*ordertype*/

    end
  end

   /* ------------------------------------------------------------*/
   /* Select the information to display in the map heading.*/
   /* ------------------------------------------------------------*/

  SL03-130(); /* Select the heading information*/

  SL03W01.SL03W-SY-STY-NM = PKNREC.SY1SSTNM; /* Style name*/
  SL03W01.SL03W-STY-COLR-NM = PKNREC.SY2CLRAB; /* color name*/

   /* ------------------------------------------------------------*/
   /* Determine life cycle code from current and effective dates*/
   /* ------------------------------------------------------------*/

  if (PKNREC.SY2CEDT1 <= SL03W00.SL03W-CURRENT-DATE)
    SL03W01.SL03W-LIFE-CYCLE = PKNREC.XLCCD1;
    SL03W00.SL03W-MAP-LIFE-CYCLE = PKNREC.XLCCD1;
  else

    if (PKNREC.SY2CEDT2 <= SL03W00.SL03W-CURRENT-DATE)
      SL03W01.SL03W-LIFE-CYCLE = PKNREC.XLCCD2;
      SL03W00.SL03W-MAP-LIFE-CYCLE = PKNREC.XLCCD2;
    else

      if (PKNREC.SY2CEDT3 <= SL03W00.SL03W-CURRENT-DATE)
        SL03W01.SL03W-LIFE-CYCLE = PKNREC.XLCCD3;
        SL03W00.SL03W-MAP-LIFE-CYCLE = PKNREC.XLCCD3;
      else

        if (PKNREC.SY2CEDT4 <= SL03W00.SL03W-CURRENT-DATE)
          SL03W01.SL03W-LIFE-CYCLE = PKNREC.XLCCD4;
          SL03W00.SL03W-MAP-LIFE-CYCLE = PKNREC.XLCCD4;
        end

      end
    end
  end

   /* ------------------------------------------------------------*/
   /* Select and open the cursor for the set to be formatted.*/
   /* ------------------------------------------------------------*/

  SL03-140(); /* Select set and open cursor*/

  SL03W00.SL03W-RSRV-CALC = 0;
  SL03W00.SL03W-OH-CALC = 0;
  SL03W00.SL03W-SAS-CALC = 0;
  SL03W00.SL03W-FIRST-FETCH-SW = "Y";

   /* ------------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ------------------------------------------------------------*/

  SL03W00.SL03W-END-CURSOR-SW = "N"; /* End of cursor switch*/
  SL03W00.SL03W-MAP-FIELD-INX = 1; /* Display line index*/
  while (SL03W00.SL03W-END-CURSOR-SW == "N") /* Until end of cursor*/

    SL03-150(); /* Fetch next row in the cursor*/

    if (SL03W00.SL03W-FIRST-FETCH-SW == "Y")
      SL03W00.SL03W-UNPCS-NUM = VSYLOC02.SY1UNPCS; /* init hold field*/
      SL03W00.SL03W-FIRST-FETCH-SW = "N";
    end

    /* ------------------------------------------------------------*/
     /* If not end of cursor, save the data in display storage.*/
    /* ------------------------------------------------------------*/

    if (SL03W00.SL03W-END-CURSOR-SW == "N") /* Not end of cursor*/
      SL03-160(); /* Save data in display storage*/
    end

    /* ------------------------------------------------------------*/
     /* If the page is full, add the page to the storage queue.*/
    /* ------------------------------------------------------------*/

    if (SL03W00.SL03W-MAP-FIELD-INX > 13) /* Page is full*/

      SL03-170(); /* Add page to temporary storage*/

      SL03W00.SL03W-MAP-FIELD-INX = 1; /* Display line index*/

    end

  end

   /* ------------------------------------------------------------*/
   /* Move last accumulated line*/
   /* ------------------------------------------------------------*/

  SL03W01.SL03W-SIZES[SL03W-MAP-FIELD-INX] = SL03W00.SL03W-HEADER-SIZE;

  SL03W00.SL03W-NET-CALC = SL03W00.SL03W-OH-CALC -   SL03W00.SL03W-RSRV-CALC   - SL03W00.SL03W-SAS-CALC;

  SL03W01.SL03W-NET-QTY[SL03W-MAP-FIELD-INX] = SL03W00.SL03W-NET-CALC;
  SL03W01.SL03W-SL-SY-LOC-RSRV-QTY[SL03W-MAP-FIELD-INX] = SL03W00.SL03W-RSRV-CALC;
  SL03W01.SL03W-SL-SY-LOC-OH-QTY[SL03W-MAP-FIELD-INX] = SL03W00.SL03W-OH-CALC;
  SL03W01.SL03W-SL-SY-LOC-SAS-QTY[SL03W-MAP-FIELD-INX] = SL03W00.SL03W-SAS-CALC;

  SL03W01.SL03W-UNPCS[SL03W-MAP-FIELD-INX] = SL03W00.SL03W-UNPCS-NUM;
  SL03W00.SL03W-UNPCS-NUM = VSYLOC02.SY1UNPCS;

  SL03W00.SL03W-MAP-FIELD-INX = SL03W00.SL03W-MAP-FIELD-INX + 1;

   /* ------------------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ------------------------------------------------------------*/

  if (SL03W00.SL03W-MAP-FIELD-INX > 1) /* Page was formatted*/
    SL03-170(); /* Add page to temporary storage*/
  end

   /* ------------------------------------------------------------*/
   /* Close the cursor for the selected set.*/
   /* ------------------------------------------------------------*/

  SL03-180(); /* Close the selected set cursor*/

   /* ------------------------------------------------------------*/
   /* If no pages were formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/

  if (SL03W00.TA5TSQIX == 0) /* No pages formatted*/

    SLWREC.XXXLOCID = "100"; /* Module identification*/
    SLWREC.SLWMSGNO = 44; /* No data was found to display*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  SL03W00.SL03W-RQT-DATA-SW = "N"; /* Request for data switch*/


end // end SL03-100

Function SL03-110()
  /* -----------------------------------------------------------------------*/
   /* obtain current date (ccyymmdd) using TA0040*/
  /* -----------------------------------------------------------------------*/

  set TA2REC empty; /* clear TA2REC*/

  TA2REC.TA2DTOPT = "GETDATE "; /* use getdate option in date routine*/
  TA2REC.TA2GRTYP = "G";

  call "TA0040" (TA2REC) {isNoRefresh = yes, isExternal = yes}; /* call date routine*/

  if (TA5REC.TA5RTNCD == 0) /* and normal return code*/

    SL03W00.SL03W-CURRENT-DATE = TA2REC.TA2GRGGN; /* current date*/
    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  SLWREC.XXXLOCID = "110"; /* Module identification*/
  converseLib.validationFailed(01); /* Unexpected error occurred*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "GET CURRENT DATE           ";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = SLWREC.XXXLOCID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end SL03-110

// Purge any temporary storage
Function SL03-120()
   /* ------------------------------------------------------------*/
   /* Initialize the storage areas and indicators to be used.*/
   /* ------------------------------------------------------------*/

  set SL03W01 empty; /* M001 display storage*/

  SL03W00.SL03W-UPD-PAGE-SW = "N"; /* Storage update switch*/

  TA5REC.TA5TSQIX = 0; /* Current page number*/
  SL03W00.TA5TSQIX = 0; /* Highest page number*/

   /* ------------------------------------------------------------*/
   /* Purge any temporary storage for this application.*/
   /* ------------------------------------------------------------*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "P"; /* Function code*/

  try
    call "TA0050" (SL03W01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

