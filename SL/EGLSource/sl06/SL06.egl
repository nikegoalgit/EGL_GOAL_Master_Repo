// Format and converse the map
Function SL06-200()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/
    SLWREC.SLWMSGNO = converseVar.validationMsgNum; /* Message number*/
  end

   /* ------------------------------------------------------------*/
   /* If obtain data failed or no error, format data to display.*/
   /* ------------------------------------------------------------*/

  if (SL06W00.SL06W-RQT-DATA-SW == "Y" /* Obtain data failed*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (SL06W00.SL06W-FORMAT-MAP-SW == "Y") /* Map must be formatted*/
      SL06-210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  SL06M001.SL06M-XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/

  SL06M001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  SL06M001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (SLWREC.SLWMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (SLWREC.SLWMSGNO != 0) /* Message to display*/

      if (SLWREC.SLWMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(SLWREC.SLWMSGNO);
      end /* Message number*/
    else
      SL06M001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    SL06M001.SL06M-XXX-MODID = SLWREC.XXXLOCID; /* Module ID*/

    set SL06M001 alarm; /* Indicate sound alarm*/

  end

  converse SL06M001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  SLWREC.XXXLOCID = " "; /* Module identification*/
  SLWREC.SLWMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/

  COMMAREA.CATOAP = SL06M001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = SL06M001.CAITEM; /* Application data*/


end // end SL06-200

// Format the display data
Function SL06-210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set SL06M001 initial; /* map input/output area*/

  SL06W00.SL06W-FORMAT-MAP-SW = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (SL06W00.TA5TSQIX > 0) /* Pages were formatted*/

    if (SL06W00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the current page has been changed, update the page in*/
       /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/

      if (SL06W00.SL06W-UPD-PAGE-SW == "Y") /* Current page was changed*/

        SL06S01(); /* Update temporary storage page*/

        SL06W00.SL06W-UPD-PAGE-SW = "N"; /* Storage update switch*/

      end

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (SL06W00.XXXTSPAG > SL06W00.TA5TSQIX) /* Desired greater highest*/
        SL06W00.XXXTSPAG = SL06W00.TA5TSQIX; /* Default to highest*/
      end

      SL06S02(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set SL06M001.XXXLITPG skip; /* "PAGE" literal*/
    SL06M001.XXXTSPAG = SL06W00.XXXTSPAG; /* Current page*/
    set SL06M001.XXXLITOF skip; /* "OF" literal*/
    SL06M001.TA5TSQIX = SL06W00.TA5TSQIX; /* Highest page*/

  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

