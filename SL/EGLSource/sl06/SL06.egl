//*** PROGRAM=SL06 ****
// This application allows a user to inquire and maintain rows
// in a DB2 table.
// 
// There are two different screens that may be displayed.
// 
// The first screen displayed to the user allows specification
// of the search criteria to be used to list the rows in the
// table.  The list is displayed and the user may page backward
// and forward to view the information.
// 
// When a line is selected on the screen, the second screen is
// displayed, showing all information for that table row.
// 
// If the user has sufficient security to change the data, the
// modifiable fields remain unprotected.  Otherwise, they are
// protected from modification by the user.
// 
// If any data is modified, it is verified and, if complete and
// correct, the table row is updated with the changed data.
// The screen is redisplayed to the user, indicating that the
// update is completed.
// 
// If more that one line was selected on the first screen, the
// next selection is displayed when the user presses the ENTER
// key without making any change to the displayed data.
// 
// When the user exits the second screen (either via PF3 key or
// after all selections have been viewed) the first screen is
// redisplayed, indicating the selections that were viewed.
// 
// The user may exit to any allowed application, via FAST PATH
// or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      07/15/93  hchapm  New application.
// CANADA   08/25/95  CBRINE  Added call to SY2000. Added XCTCD
//                            to SLWREC.
// dlvrusa  04/20/01  ssethu  change for US go live. SAP PO
//                            will be displayed for US whse
//                            if PO is not available
// *********************
Program SL06 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "SL00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  ERRSQLCA ERRSQLCA; // record
  PC2REC PC2REC; // record
  PKNREC PKNREC; // record
  SL06W00 SL06W00; // record
  SL06W01 SL06W01; // record
  SL4REC SL4REC; // record
  SL5REC SL5REC; // record
  SLWREC SLWREC; // record
  SQLCA SQLCA; // record
  SWVREC SWVREC; // record
  SYCREC SYCREC; // record
  SYPREC SYPREC; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VSYLOC02 VSYLOC02; // record
  WSY2000 WSY2000; // record
  XSSLW01 XSSLW01; // record
  XX0XW02 XX0XW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XWHTBL; // table
  use XDMTBL; // table
  use XQATBL; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use XOWTBL; // table
  use XEGTBL; // table
  use XUMTBL; // table
  use XARTBL; // table
  use XSLTBL; // table
  use YLSTBL; // table
  use SL06M.SL06M001,  // forms
      SL06M.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPSL001: XPSL001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/

    set SL06M001 initial; /* M001 input/output*/
    set SL06W00 empty; /* Working storage*/
    set SL06W01 empty; /* M001 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set TA5REC empty; /* Temporary storage parameters*/

    SL06W00.SL06W-FORMAT-MAP-SW = "Y"; /* Format the map switch*/
    SL06W00.SL06W-RQT-DATA-SW = "N"; /* Request for data switch*/

     /* ------------------------------------------------------------*/
     /* If all key info is passed in SLWREC, use as initial key*/
     /* else, set cursor at missing field*/
     /* ------------------------------------------------------------*/

    if (SLWREC.SY1STNBR > " "      /* style nbr is passed*/
     && SLWREC.SY-SZ-ID-DESC > " " /* size is passed*/
     && SLWREC.XAR-AREA-CD > " "   /* area is passed*/
     && SLWREC.SL-ZONE-CD > " "    /* zone is passed*/
     && SLWREC.SL-LOC-CD > " ")    /* location is passed*/

      SL06W00.SL06W-RQT-DATA-SW = "Y"; /* request for data switch*/

    end

    if (SLWREC.SY1STNBR > " ") /* if style nbr is passed in slwrec*/
      SL06M001.SL06M-SY-STY-NBR = SLWREC.SY1STNBR; /* move to work area*/
    end


    if (SLWREC.SY-SZ-ID-DESC > " ")
      SL06M001.SL06M-SIZE = SLWREC.SY-SZ-ID-DESC;
    end


    if (SLWREC.XAR-AREA-CD > " ")
      SL06M001.SL06M-XAR-AREA-CD = SLWREC.XAR-AREA-CD;
    end


    if (SLWREC.SL-ZONE-CD > " ")
      SL06M001.SL06M-SL-ZONE-CD = SLWREC.SL-ZONE-CD;
    end

    if (SLWREC.SL-LOC-CD > " ")
      SL06M001.SL06M-SL-LOC-CD = SLWREC.SL-LOC-CD;

    else

      set SL06M001.SL06M-SL-LOC-CD cursor; /* set cursor at location*/

    end


    if (SLWREC.XWHCD > " ") /* if whse code is passed in slwrec*/
      SL06M001.SL06M-XWH-WHSE-CD = SLWREC.XWHCD; /* move it to work area*/
    else
      if (COMMAREA.XWHCD > " ") /* if whse code is passed in commarea*/
        SL06M001.SL06M-XWH-WHSE-CD = COMMAREA.XWHCD; /* move towrk area*/
      end
    end


    if (SLWREC.XGPCD > " ") /* if gpc code is passed in slwrec*/
      SL06M001.SL06M-XGP-GPC-CD = SLWREC.XGPCD; /* move to work area*/
    else
      if (COMMAREA.XGPCD > " ") /* if gpc code is passed in commarea*/
        SL06M001.SL06M-XGP-GPC-CD = COMMAREA.XGPCD; /* move to work area*/
      else
        if (SL06M001.SL06M-XWH-WHSE-CD == "51") /* if warehouse 51*/
          SL06M001.SL06M-XGP-GPC-CD = "03"; /* move apparel code to gpc*/
        else
          SL06M001.SL06M-XGP-GPC-CD = "02"; /* move footwr code to gpc*/
        end
      end
    end


    if (SLWREC.SY2CLRID > " ") /* if color code is passed*/
      SL06M001.SL06M-SY-COLR-CD-ID = SLWREC.SY2CLRID; /* move to ws*/
    else
      SL06M001.SL06M-SY-COLR-CD-ID = "00 "; /* move to ws*/
    end


    if (SLWREC.XDMCD > " ") /* if dimension code is passed*/
      SL06M001.SL06M-XDM-DIM-CD = SLWREC.XDMCD; /* move to work area*/
    else
      SL06M001.SL06M-XDM-DIM-CD = "00"; /* default to dimension code 00*/
    end
    if (SLWREC.XQACD > " ") /* if quality code is passed*/
      SL06M001.SL06M-XQA-QUAL-CD = SLWREC.XQACD; /* move to work area*/
    else
      SL06M001.SL06M-XQA-QUAL-CD = "01"; /* default to quality code 01*/
    end


    if (SLWREC.XOWCD > " ") /* if own group is passed*/
      SL06M001.SL06M-XOW-OWN-GRP-CD = SLWREC.XOWCD; /* move to ws*/
    else
      SL06M001.SL06M-XOW-OWN-GRP-CD = "00"; /* move to ws*/
    end


    if (SLWREC.PC2INSEG > " ") /* inventory segment is passed*/
      SL06M001.SL06M-PC-CU-IN-SEG-CD = SLWREC.PC2INSEG; /* move to ws*/
    else
      SL06M001.SL06M-PC-CU-IN-SEG-CD = "000"; /* move to ws*/
    end


    if (SLWREC.XEG-ENGR-DIFF-CD > " ") /* engr diff code is passed*/
      SL06M001.SL06M-XEG-ENGR-DIFF-CD = SLWREC.XEG-ENGR-DIFF-CD;
    else
      SL06M001.SL06M-XEG-ENGR-DIFF-CD = "000";
    end


    if (SLWREC.XUMCD > " ") /* unit of measure passed*/
      SL06M001.SL06M-XUM-UOM-CD = SLWREC.XUMCD; /* move to ws*/
    else
      SL06M001.SL06M-XUM-UOM-CD = "01"; /* move to ws*/
    end


    if (SLWREC.XSLCD > " ") /* class code is passed*/
      SL06M001.SL06M-XSL-CLASS-CD = SLWREC.XSLCD; /* move to ws*/
    else
      SL06M001.SL06M-XSL-CLASS-CD = "000"; /* move to ws*/
    end



     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto SL06001; /* Converse the main process*/


    SL06001: SL06001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto SL06001; /* Converse the main process*/


  end // end main
end // end SL06

// Obtain the data to display
Function SL06-100()
   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/

  if (SL06W00.SL06W-RQT-DATA-SW == "Y") /* Data was not requested*/

    SL06-120(); /* Purge any temporary storage*/

  end

  SL06W01.SL06W-XWH-WHSE-CD = SL06M001.SL06M-XWH-WHSE-CD; /* whse cd*/
  SL06W01.SL06W-XGP-GPC-CD = SL06M001.SL06M-XGP-GPC-CD; /* GPC code*/
  SL06W01.SL06W-SY-STY-NBR = SL06M001.SL06M-SY-STY-NBR; /* Style nbr*/
  SL06W01.SL06W-XQA-QUAL-CD = SL06M001.SL06M-XQA-QUAL-CD; /* qual cd*/
  SL06W01.SL06W-SY-COLR-CD-ID = SL06M001.SL06M-SY-COLR-CD-ID;
  SL06W01.SL06W-XDM-DIM-CD = SL06M001.SL06M-XDM-DIM-CD; /* dimens. cd*/
  SL06W01.SL06W-XOW-OWN-GRP-CD = SL06M001.SL06M-XOW-OWN-GRP-CD;
  SL06W01.SL06W-PC-CU-IN-SEG-CD = SL06M001.SL06M-PC-CU-IN-SEG-CD;
  SL06W01.SL06W-XEG-ENGR-DIFF-CD = SL06M001.SL06M-XEG-ENGR-DIFF-CD;
  SL06W01.SL06W-XUM-UOM-CD = SL06M001.SL06M-XUM-UOM-CD;
  SL06W01.SL06W-XSL-CLASS-CD = SL06M001.SL06M-XSL-CLASS-CD;
  SL06W01.SL06W-SIZE = SL06M001.SL06M-SIZE;
  SL06W00.SL06W-HEADER-SIZE = SL06M001.SL06M-SIZE;
  SL06W01.SL06W-XAR-AREA-CD = SL06M001.SL06M-XAR-AREA-CD;
  SL06W01.SL06W-SL-ZONE-CD = SL06M001.SL06M-SL-ZONE-CD;
  SL06W01.SL06W-SL-LOC-CD = SL06M001.SL06M-SL-LOC-CD;

   /* ------------------------------------------------------------*/
   /* get warehouse*/
   /* ------------------------------------------------------------*/
  if (SL06W01.SL06W-XWH-WHSE-CD in XWHTBL.XWHCD)
    SL06W01.SL06W-XWH-WHSE-NM = XWHTBL.XWHABRV[sysVar.arrayIndex];
    SLWREC.XCTCD = XWHTBL.XCTCD[sysVar.arrayIndex]; /* CANA save country code*/
  end

   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (SL06W00.SL06W-RQT-DATA-SW != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* ------------------------------------------------------------*/

  SL06W00.XXXTSLEN = 1010; /* Temporary storage page length*/

   /* ------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* ------------------------------------------------------------*/

  if (SLWREC.SLWMSGNO == 0) /* No message to display*/
    SL06W00.XXXTSPAG = 1; /* Page number to display*/
  end

   /* ------------------------------------------------------------*/
   /* get current date to use in determining life cycle code.*/
   /* ------------------------------------------------------------*/

  SL06-110(); /* get current date*/

   /* ------------------------------------------------------------*/
   /* Select the information to display in the map heading.*/
   /* ------------------------------------------------------------*/

  SL06-130(); /* Select the heading information*/

  SL06W01.SL06W-SY-STY-NM = PKNREC.SY1SSTNM; /* Style name*/
  SL06W01.SL06W-STY-COLR-NM = PKNREC.SY2CLRAB; /* color name*/

   /* ------------------------------------------------------------*/
   /* Determine life cycle code from current and effective dates*/
   /* ------------------------------------------------------------*/

  if (PKNREC.SY2CEDT1 <= SL06W00.SL06W-CURRENT-DATE)
    SL06W01.SL06W-LIFE-CYCLE = PKNREC.XLCCD1;
    SL06W00.SL06W-MAP-LIFE-CYCLE = PKNREC.XLCCD1;
  else

    if (PKNREC.SY2CEDT2 <= SL06W00.SL06W-CURRENT-DATE)
      SL06W01.SL06W-LIFE-CYCLE = PKNREC.XLCCD2;
      SL06W00.SL06W-MAP-LIFE-CYCLE = PKNREC.XLCCD2;
    else

      if (PKNREC.SY2CEDT3 <= SL06W00.SL06W-CURRENT-DATE)
        SL06W01.SL06W-LIFE-CYCLE = PKNREC.XLCCD3;
        SL06W00.SL06W-MAP-LIFE-CYCLE = PKNREC.XLCCD3;
      else

        if (PKNREC.SY2CEDT4 <= SL06W00.SL06W-CURRENT-DATE)
          SL06W01.SL06W-LIFE-CYCLE = PKNREC.XLCCD4;
          SL06W00.SL06W-MAP-LIFE-CYCLE = PKNREC.XLCCD4;
        end

      end
    end
  end

   /* ------------------------------------------------------------*/
   /* find location status code and description*/
   /* ------------------------------------------------------------*/

  SL5REC.XARCD = SL06M001.SL06M-XAR-AREA-CD; /* area code*/
  SL5REC.SL5LOCTN = SL06M001.SL06M-SL-LOC-CD; /* location*/
  SL5REC.SL4ZONE = SL06M001.SL06M-SL-ZONE-CD; /* zone code*/

  SL06-140(); /* find status from header information*/

  SL06W01.SL06W-YLS-LOC-STAT-CD = SL5REC.YLSCD;

  if (SL5REC.YLSCD in YLSTBL.YLSCD)

    SL06W01.SL06W-YLS-LOC-STAT-DESC = YLSTBL.YLSABRV[sysVar.arrayIndex];

  else

    SL06W01.SL06W-YLS-LOC-STAT-DESC = " ";

  end

   /* ------------------------------------------------------------*/
   /* Select and open the cursor for the set to be formatted.*/
   /* ------------------------------------------------------------*/

  SL06-150(); /* Select set and open cursor*/

   /* ------------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ------------------------------------------------------------*/

  SL06W00.SL06W-END-CURSOR-SW = "N"; /* End of cursor switch*/
  SL06W00.SL06W-MAP-FIELD-INX = 1; /* Display line index*/
  while (SL06W00.SL06W-END-CURSOR-SW == "N") /* Until end of cursor*/

    SL06-160(); /* Fetch next row in the cursor*/

    /* ------------------------------------------------------------*/
     /* If not end of cursor, save the data in display storage.*/
    /* ------------------------------------------------------------*/

    if (SL06W00.SL06W-END-CURSOR-SW == "N") /* Not end of cursor*/
      SL06-170(); /* Save data in display storage*/
    end

    /* ------------------------------------------------------------*/
     /* If the page is full, add the page to the storage queue.*/
    /* ------------------------------------------------------------*/

    if (SL06W00.SL06W-MAP-FIELD-INX > 13) /* Page is full*/

      SL06-180(); /* Add page to temporary storage*/

      SL06W00.SL06W-MAP-FIELD-INX = 1; /* Display line index*/

    end

  end

   /* ------------------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ------------------------------------------------------------*/

  if (SL06W00.SL06W-MAP-FIELD-INX > 1) /* Page was formatted*/
    SL06-180(); /* Add page to temporary storage*/
  end

   /* ------------------------------------------------------------*/
   /* Close the cursor for the selected set.*/
   /* ------------------------------------------------------------*/

  SL06-190(); /* Close the selected set cursor*/

   /* ------------------------------------------------------------*/
   /* If no pages were formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/

  if (SL06W00.TA5TSQIX == 0) /* No pages formatted*/

    SLWREC.XXXLOCID = "100"; /* Module identification*/
    SLWREC.SLWMSGNO = 44; /* No data was found to display*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  SL06W00.SL06W-RQT-DATA-SW = "N"; /* Request for data switch*/


end // end SL06-100

Function SL06-110()
  /* -----------------------------------------------------------------------*/
   /* obtain current date (ccyymmdd) using TA0040*/
  /* -----------------------------------------------------------------------*/

  set TA2REC empty; /* clear TA2REC*/

  TA2REC.TA2DTOPT = "GETDATE "; /* use getdate option in date routine*/
  TA2REC.TA2GRTYP = "G";

  call "TA0040" (TA2REC) {isNoRefresh = yes, isExternal = yes}; /* call date routine*/

  if (TA5REC.TA5RTNCD == 0) /* and normal return code*/

    SL06W00.SL06W-CURRENT-DATE = TA2REC.TA2GRGGN; /* current date*/
    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  SLWREC.XXXLOCID = "110"; /* Module identification*/
  converseLib.validationFailed(01); /* Unexpected error occurred*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "GET CURRENT DATE           ";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = SLWREC.XXXLOCID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end SL06-110

// Purge any temporary storage
Function SL06-120()
   /* ------------------------------------------------------------*/
   /* Initialize the storage areas and indicators to be used.*/
   /* ------------------------------------------------------------*/

  set SL06W01 empty; /* M001 display storage*/

  SL06W00.SL06W-UPD-PAGE-SW = "N"; /* Storage update switch*/

  TA5REC.TA5TSQIX = 0; /* Current page number*/
  SL06W00.TA5TSQIX = 0; /* Highest page number*/

   /* ------------------------------------------------------------*/
   /* Purge any temporary storage for this application.*/
   /* ------------------------------------------------------------*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "P"; /* Function code*/

  try
    call "TA0050" (SL06W01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, return to previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  SLWREC.XXXLOCID = "110"; /* Module identification*/
  converseLib.validationFailed(01); /* Unexpected error occurred*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "PURGE ANY TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = SLWREC.XXXLOCID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end SL06-120

// Select the heading information
Function SL06-130()
   /* ------------------------------------------------------------*/
   /* Select the requested row from the table.*/
   /* ------------------------------------------------------------*/

  set PKNREC empty; /* Clear row storage area*/

  PKNREC.XGPCD = SL06W01.SL06W-XGP-GPC-CD; /* GPC code*/
  PKNREC.SY1STNBR = SL06W01.SL06W-SY-STY-NBR; /* Style number*/
  PKNREC.SY2CLRID = SL06W01.SL06W-SY-COLR-CD-ID; /* color id*/
  PKNREC.XDMCD = SL06W01.SL06W-XDM-DIM-CD; /* dimension code*/

  try
    call "IO1810" ("S ", SQLCA, PKNREC) {isNoRefresh = yes, isExternal = yes};
  end /* Select table row*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/

      /* CANADA 08/25/95*/

      if (SLWREC.XCTCD == "CANA")

        /* format ta1rec in case of bad return code from call to SY2000*/

        TA1REC.TA1PGMNM = "SL06"; /* CANA program name*/
        TA1REC.TA1LOC = "SL06130"; /* CANA sgrp name*/

        XSSL-CALL-SY2000(); /* CANA call SY2000 to get*/
                              /* product region record*/
        if (WSY2000.WSY2000-RETURN-CODE == 0) /* CANA row found*/
          return;
        else
          if (WSY2000.WSY2000-RETURN-CODE == 02) /* CANA row not found*/

            converseLib.validationFailed(456); /* invalid style/color*/

            SLWREC.XXXLOCID = "130"; /* module identification*/

            exit stack; /* Return to flow*/

          end
          if (WSY2000.WSY2000-RETURN-CODE == 98) /* CANA database busy*/

            converseLib.validationFailed(31); /* Data in use by another*/
            TA1REC.TA1FUNC = "POST"; /* Termination function*/

          else

            converseLib.validationFailed(01); /* Unexpected error occurred*/
            TA1REC.TA1FUNC = "RETN"; /* Termination function*/

            converseLib.clearScreen();
             /* Clear screen before display*/

          end
          converseLib.validationFailed(0001); /* Indicate system error*/
          TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
          TA1REC.TA1LOC = "CALL SY2000";
          TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program appl*/
          TA1REC.TA1PGMNM = "SL06130"; /* Program number*/
          TA1REC.TA1MAP = SLWREC.XXXMAPNM; /* Map name*/
          TA1REC.TA1DBASE = "DPROD  "; /* Data base name*/
          TA1REC.TA1TBLVU = "VSYREG02"; /* Table view name*/
          TA1REC.TA1TBLKE = PKNREC.PKNKEY; /* Table key*/

                              /* Error diagnosis routine*/
          call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes};

          exit stack; /* Exit to display map*/

        end
      end
      return; /* Return to previous process*/
    end

    if (SQLCA.VAGen_SQLCODE == 100) /* Row not found*/

      converseLib.validationFailed(56); /* invalid style/color*/

      SLWREC.XXXLOCID = "130"; /* module identification*/

      exit stack; /* Return to flow*/

    end

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  SLWREC.XXXLOCID = "130"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
