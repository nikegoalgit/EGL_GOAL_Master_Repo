// Check attention identifier
Function SL06-300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (SL06W00.SL06W-RQT-DATA-SW == "Y") /* Request for data*/
      set SL06W01 empty; /* M001 display storage*/
    end

    SL06W00.SL06W-FORMAT-MAP-SW = "Y"; /* Format the map switch*/
    SL06W00.SL06W-RQT-DATA-SW = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (SL06M001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      SL06-110(); /* Purge temporary storage*/

      XSSL001(); /* Exit to another application*/

      set SL06M001.CATOAP cursor, bold; /* Highlight*/

    else

      SLWREC.XXXLOCID = "300"; /* Module identification*/
      converseLib.validationFailed(258); /* PA/PF key may not be used*/

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4 /* or PF4 was pressed*/
   || converseVar.eventKey is pf5) /* or PF5 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "SL06M001"; /* Application find code*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "SL0A"; /* TO application*/
    end

    if (converseVar.eventKey is pf4) /* PF4 was pressed*/
      COMMAREA.CATOAP = "MN0M"; /* TO application*/
    end

    if (converseVar.eventKey is pf5) /* PF5 was pressed*/
      COMMAREA.CATOAP = "SL0M"; /* TO application*/
    end

    SL06-110(); /* Purge temporary storage*/

    XSSL001(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/


  if (converseVar.eventKey is pf6   /* and PF6 was  pressed*/
   || converseVar.eventKey is pf12  /* and PF12 was  pressed*/
   || converseVar.eventKey is pf13  /* and PF13 was  pressed*/
   || converseVar.eventKey is pf14  /* and PF14 was  pressed*/
   || converseVar.eventKey is pf15  /* and PF15 was  pressed*/
   || converseVar.eventKey is pf16  /* and PF16 was  pressed*/
   || converseVar.eventKey is pf17  /* and PF17 was  pressed*/
   || converseVar.eventKey is pf18  /* and PF18 was  pressed*/
   || converseVar.eventKey is enter /* ENTER was not pressed*/
   || converseVar.eventKey is pf7   /* and PF7 was  pressed*/
   || converseVar.eventKey is pf8)  /* and PF8 was  pressed*/

    /* next sentence*/

  else

    SLWREC.XXXLOCID = "300"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end SL06-300

// Reset highlighted fields
Function SL06-400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set SL06M001.SL06M-XWH-WHSE-CD initialAttributes; /* warehouse code*/
  set SL06M001.SL06M-XGP-GPC-CD initialAttributes; /* GPC code*/
  set SL06M001.SL06M-SY-STY-NBR initialAttributes; /* Style code*/
  set SL06M001.SL06M-SY-COLR-CD-ID initialAttributes; /* color code*/
  set SL06M001.SL06M-XDM-DIM-CD initialAttributes; /* dimension code*/
  set SL06M001.SL06M-XQA-QUAL-CD initialAttributes; /* quality code*/
  set SL06M001.SL06M-LIFE-CYCLE initialAttributes; /* life cycle*/
  set SL06M001.SL06M-XOW-OWN-GRP-CD initialAttributes; /* own group*/
  set SL06M001.SL06M-PC-CU-IN-SEG-CD initialAttributes; /* inventory segment*/
  set SL06M001.SL06M-XEG-ENGR-DIFF-CD initialAttributes; /* engineering diff*/
  set SL06M001.SL06M-XUM-UOM-CD initialAttributes; /* unit of measure*/
  set SL06M001.SL06M-XSL-CLASS-CD initialAttributes; /* class code*/
  set SL06M001.SL06M-SIZE initialAttributes; /* size*/
  set SL06M001.SL06M-XAR-AREA-CD initialAttributes; /* area*/
  set SL06M001.SL06M-SL-ZONE-CD initialAttributes; /* zone*/
  set SL06M001.SL06M-SL-LOC-CD initialAttributes; /* location*/

  SL06W00.SL06W-MAP-FIELD-INX = 1; /* Initialize line index*/

  while (SL06W00.SL06W-MAP-FIELD-INX <= 13 /* Until end of lines*/
   && SL06W01.SL06W-ROW-USED-SW[SL06W-MAP-FIELD-INX] == "Y")
                                  /* or end of display*/

    set SL06M001.SL06M-SELECT[SL06W-MAP-FIELD-INX] normal;
                                  /* reset select flag*/

    SL06W00.SL06W-MAP-FIELD-INX = SL06W00.SL06W-MAP-FIELD-INX + 1;
                                  /* Increment line index*/

  end

  set SL06M001.CATOAP initialAttributes; /* TO application*/
  set SL06M001.CAITEM initialAttributes; /* Application data*/

   /* ----------------------------------------------------------------------*/
    /* pop-up window option -- if not selected, return to previous process*/
   /* ----------------------------------------------------------------------*/

  if (SL06M001.SL06M-XWH-WHSE-CD == "?"
   || SL06M001.SL06M-XGP-GPC-CD == "?"
   || SL06M001.SL06M-XDM-DIM-CD == "?"
   || SL06M001.SL06M-XQA-QUAL-CD == "?"
   || SL06M001.SL06M-XEG-ENGR-DIFF-CD == "?"
   || SL06M001.SL06M-XUM-UOM-CD == "?"
   || SL06M001.SL06M-XOW-OWN-GRP-CD == "?"
   || SL06M001.SL06M-XSL-CLASS-CD == "?")

    if (converseVar.eventKey not enter) /* enter was not pressed*/

      SLWREC.XXXLOCID = "400"; /* module identification*/
      converseLib.validationFailed(4); /* pkkey invalid*/

      exit stack; /* exit to display map*/

    end

  else

    return; /* return to previous processing*/

  end

  SL06-410();

   /* ----------------------------------------------------------------------*/
   /* clear screen for pop-ups, then display map*/
   /* ----------------------------------------------------------------------*/

  converseLib.clearScreen();
   /* clear screen before display*/

  exit stack; /* exit to display map*/
end // end SL06-400

