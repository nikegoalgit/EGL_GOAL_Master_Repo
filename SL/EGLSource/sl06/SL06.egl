// Verify data that was entered
Function SL06-500()
   /* -----------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (SL06M001.CAITEM > " ") /* Application data was entered*/

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(259); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid request to scroll was entered.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    SL06-510(); /* Verify scroll request*/

  else

    if (SL06W00.TA5TSQIX > 0) /* page was formatted*/

      SL06W00.SL06W-MAP-FIELD-INX = 13; /* initialize map index*/
      SL06W00.SL06W-ROW-SELECTED = "N";

      while (SL06W00.SL06W-MAP-FIELD-INX > 0) /* until end of lines*/

        SL06-520(); /* verify data in line*/

        SL06W00.SL06W-MAP-FIELD-INX = SL06W00.SL06W-MAP-FIELD-INX - 1;
              /* decrement index*/
      end
    end
  end
   /* ------------------------------------------------------------*/
   /* bypass edits if want to transfer without selecting line*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf12 /* transfer pf keys are pressed*/
   || converseVar.eventKey is pf13 
   || converseVar.eventKey is pf14 
   || converseVar.eventKey is pf15 
   || converseVar.eventKey is pf16 
   || converseVar.eventKey is pf17 
   || converseVar.eventKey is pf18)

    return; /* return to logic flow to skip edits*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- own group code*/
   /* -------------------------------------------------------------*/

  if (SL06M001.SL06M-XOW-OWN-GRP-CD in XOWTBL.XOWCD
   && XOWTBL.XXXSTAT[sysVar.arrayIndex] == "A")

    /* next sentence*/

  else
    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XOW-OWN-GRP-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(361); /* invalid code*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- unit of measure*/
   /* ------------------------------------------------------------*/

  if (SL06M001.SL06M-XUM-UOM-CD in XUMTBL.XUMCD
   && XUMTBL.XXXSTAT[sysVar.arrayIndex] == "A")

    /* next sentence*/

  else
    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XUM-UOM-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(362); /* invalid quality code*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- class code*/
   /* ------------------------------------------------------------*/

  if (SL06M001.SL06M-XSL-CLASS-CD in XSLTBL.XSLCD
   && XSLTBL.XXXSTAT[sysVar.arrayIndex] == "A")

   /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XSL-CLASS-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(360); /* invalid code*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- engineering difference code*/
   /* ------------------------------------------------------------*/

  if (SL06M001.SL06M-XEG-ENGR-DIFF-CD in XEGTBL.XEGCD)

   /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XEG-ENGR-DIFF-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(364); /* invalid code*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- inventory segment*/
   /* ------------------------------------------------------------*/

  set PC2REC empty; /* clear row storage area*/

  PC2REC.XGPCD = SL06M001.SL06M-XGP-GPC-CD;
  PC2REC.PC2INSEG = SL06M001.SL06M-PC-CU-IN-SEG-CD;
  call "IO3340" ("SG", SQLCA, PC2REC) {isNoRefresh = yes, isExternal = yes};
  if (SQLCA.VAGen_SQLCODE == +0)
     /* next sentence*/
  else

    SLWREC.XXXLOCID = "500";
    SLWREC.SLWMSGNO = 365; /* <--- invalid value msg*/
    set SL06M001.SL06M-PC-CU-IN-SEG-CD cursor, bold;

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- quality code*/
   /* ------------------------------------------------------------*/
  if (SL06M001.SL06M-XQA-QUAL-CD in XQATBL.XQACD)

    /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XQA-QUAL-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(366); /* invalid quality code*/

  end
   /* ------------------------------------------------------------*/
   /* Verify header fields--- dimension code*/
   /* ------------------------------------------------------------*/
  if (SL06M001.SL06M-XDM-DIM-CD in XDMTBL.XDMCD)

    /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XDM-DIM-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(367); /* invalid dimension code*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- gpc code*/
   /* ------------------------------------------------------------*/
  if (SL06M001.SL06M-XGP-GPC-CD in XWHTBL.XGPCD1
   || SL06M001.SL06M-XGP-GPC-CD in XWHTBL.XGPCD2
   || SL06M001.SL06M-XGP-GPC-CD in XWHTBL.XGPCD3
   || SL06M001.SL06M-XGP-GPC-CD in XWHTBL.XGPCD4
   || SL06M001.SL06M-XGP-GPC-CD in XWHTBL.XGPCD5
   || SL06M001.SL06M-XGP-GPC-CD in XWHTBL.XGPCD6
   || SL06M001.SL06M-XGP-GPC-CD in XWHTBL.XGPCD7
   || SL06M001.SL06M-XGP-GPC-CD in XWHTBL.XGPCD8)
    if (XWHTBL.XXXSTAT[sysVar.arrayIndex] == "A")

    /* next sentence*/

    else

      SLWREC.XXXLOCID = "500"; /* Module identification*/
      set SL06M001.SL06M-XGP-GPC-CD cursor, bold; /* Highlight*/
      converseLib.validationFailed(157); /* invalid gpc*/

    end
  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XGP-GPC-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(157); /* invalid gpc*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- warehouse code*/
   /* -------------------------------------------------------------*/
  if (SL06M001.SL06M-XWH-WHSE-CD in XWHTBL.XWHCD
   && XWHTBL.XXXSTAT[sysVar.arrayIndex] == "A")

  /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XWH-WHSE-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(373); /* invalid warehouse*/

  end

   /* ------------------------------------------------------------*/
   /* Verify header fields--- area code*/
   /* -------------------------------------------------------------*/

  if (SL06M001.SL06M-XAR-AREA-CD in XARTBL.XARCD
   && XARTBL.XXXSTAT[sysVar.arrayIndex] == "A")

   /* next sentence*/

  else

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-XAR-AREA-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(36); /* invalid area code*/

  end

   /* ------------------------------------------------------------*/
   /* Validate area/zone*/
   /* ------------------------------------------------------------*/

  set SL4REC empty; /* Clear row storage area*/

  SL4REC.XWHCD = SL06M001.SL06M-XWH-WHSE-CD;
  SL4REC.XARCD = SL06M001.SL06M-XAR-AREA-CD;
  SL4REC.SL4ZONE = SL06M001.SL06M-SL-ZONE-CD;

  try
    call "IO1480" ("S ", SQLCA, SL4REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select table row*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE != 0) /* Normal return code*/
      if (SQLCA.VAGen_SQLCODE == 100) /* Row not found*/

        converseLib.validationFailed(35); /* invalid area/zone*/
        set SL06M001.SL06M-XAR-AREA-CD cursor, bold; /* hilite*/
        set SL06M001.SL06M-SL-ZONE-CD bold; /* hilite*/

        SLWREC.XXXLOCID = "500"; /* module identification*/

        exit stack; /* Return to flow*/

      else

        if (SQLCA.VAGen_SQLCODE == -911) /* and resource not available*/

          converseLib.validationFailed(31); /* Data in use by another*/
          TA1REC.TA1FUNC = "POST"; /* Termination function*/
          SLWREC.XXXLOCID = "500"; /* Module identification*/

        else

          converseLib.validationFailed(01); /* Unexpected error occurred*/
          TA1REC.TA1FUNC = "RETN"; /* Termination function*/

          converseLib.clearScreen();
           /* Clear screen before display*/

          TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
          TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
          TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
          TA1REC.TA1LOC = "OBTAIN STYLE NAME FOR HEADING";
          TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
          TA1REC.TA1PGMNO = SLWREC.XXXLOCID; /* Program number*/
          TA1REC.TA1DBASE = "DSTLOC  "; /* Data base name*/
          TA1REC.TA1TBLVU = "VAZONE01"; /* Table view name*/
          TA1REC.TA1TBLKE = SL4REC.SL4KEY; /* Table key*/
          TA1REC.TA1MAPNO = "M001"; /* map number*/

          call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes};
                                   /* Error diagnosis routine*/

          exit stack; /* Exit to display map*/

        end
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* Validate area/zone/location*/
   /* ------------------------------------------------------------*/

  set SL5REC empty; /* Clear row storage area*/

  SL5REC.XWHCD = SL06M001.SL06M-XWH-WHSE-CD;
  SL5REC.XARCD = SL06M001.SL06M-XAR-AREA-CD;
  SL5REC.SL4ZONE = SL06M001.SL06M-SL-ZONE-CD;
  SL5REC.SL5LOCTN = SL06M001.SL06M-SL-LOC-CD;

  try
    call "IO1490" ("S ", SQLCA, SL5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select table row*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE != 0) /* Normal return code*/
      if (SQLCA.VAGen_SQLCODE == 100) /* Row not found*/

        converseLib.validationFailed(38); /* invalid area/zone/location*/

        set SL06M001.SL06M-XAR-AREA-CD cursor, bold; /* hilite*/
        set SL06M001.SL06M-SL-ZONE-CD bold; /* hilite*/
        set SL06M001.SL06M-SL-LOC-CD bold; /* hilite*/
        SLWREC.XXXLOCID = "500"; /* module identification*/

        exit stack; /* Return to flow*/

      else

        if (SQLCA.VAGen_SQLCODE == -911) /* and resource not available*/

          converseLib.validationFailed(31); /* Data in use by another*/
          TA1REC.TA1FUNC = "POST"; /* Termination function*/
          SLWREC.XXXLOCID = "500"; /* Module identification*/

        else

          converseLib.validationFailed(01); /* Unexpected error occurred*/
          TA1REC.TA1FUNC = "RETN"; /* Termination function*/

          converseLib.clearScreen();
           /* Clear screen before display*/

          TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
          TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
          TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
          TA1REC.TA1LOC = "OBTAIN STYLE NAME FOR HEADING";
          TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
          TA1REC.TA1PGMNO = SLWREC.XXXLOCID; /* Program number*/
          TA1REC.TA1DBASE = "DSTLOC  "; /* Data base name*/
          TA1REC.TA1TBLVU = "VLOCAT01"; /* Table view name*/
          TA1REC.TA1TBLKE = SL4REC.SL4KEY; /* Table key*/
          TA1REC.TA1MAPNO = "M001"; /* map number*/

          call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes};
                                   /* Error diagnosis routine*/

          exit stack; /* Exit to display map*/

        end
      end
    end
  end
   /* ------------------------------------------------------------*/
   /* validate style and color*/
   /* ------------------------------------------------------------*/

  set PKNREC empty; /* Clear row storage area*/

  PKNREC.XGPCD = SL06M001.SL06M-XGP-GPC-CD; /* GPC code*/
  PKNREC.SY1STNBR = SL06M001.SL06M-SY-STY-NBR; /* Style number*/
  PKNREC.SY2CLRID = SL06M001.SL06M-SY-COLR-CD-ID; /* color id*/
  PKNREC.XDMCD = SL06M001.SL06M-XDM-DIM-CD; /* dimension code*/

  try
    call "IO1810" ("S ", SQLCA, PKNREC) {isNoRefresh = yes, isExternal = yes};
  end /* Select table row*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/

      /* CANADA 08/25/95*/

      if (SLWREC.XCTCD == "CANA")

        /* format ta1rec in case of bad return code from call to SY2000*/

        TA1REC.TA1PGMNM = "SL06"; /* CANA program name*/
        TA1REC.TA1LOC = "SL06500"; /* CANA sgrp name*/

        XSSL-CALL-SY2000(); /* CANA call SY2000 to get*/
                              /* product region record*/
        if (WSY2000.WSY2000-RETURN-CODE == 0) /* CANA row found*/
          return; /* Continue*/
        else
          if (WSY2000.WSY2000-RETURN-CODE == 02) /* CANA row not found*/

            converseLib.validationFailed(456); /* invalid style/color*/
            set SL06M001.SL06M-SY-STY-NBR cursor, bold; /* hilite*/
            set SL06M001.SL06M-SY-COLR-CD-ID bold;

            SLWREC.XXXLOCID = "130"; /* module identification*/

            exit stack; /* Return to flow*/

          end
          if (WSY2000.WSY2000-RETURN-CODE == 98) /* CANA database busy*/

            converseLib.validationFailed(31); /* Data in use by another*/
            TA1REC.TA1FUNC = "POST"; /* Termination function*/
            SLWREC.XXXLOCID = "130"; /* Module identification*/

          else

            converseLib.validationFailed(0001); /* Indicate system error*/
            TA1REC.TA1FUNC = "RETN"; /* Termination function*/
          end
          TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
          TA1REC.TA1LOC = "CALL SY2000";
          TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program appl*/
          TA1REC.TA1PGMNM = "SL0ESCI"; /* Program number*/
          TA1REC.TA1MAP = SLWREC.XXXMAPNM; /* Map name*/
          TA1REC.TA1DBASE = "DPROD  "; /* Data base name*/
          TA1REC.TA1TBLVU = "VSYREG02"; /* Table view name*/
          TA1REC.TA1TBLKE = PKNREC.PKNKEY; /* Table key*/

                              /* Error diagnosis routine*/
          call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes};

          exit stack; /* Return to flow*/

        end
      end
      return; /* Continue*/
    else
      if (SQLCA.VAGen_SQLCODE == 100) /* Row not found*/

        converseLib.validationFailed(56); /* invalid style/color*/
        set SL06M001.SL06M-SY-STY-NBR cursor, bold; /* hilite*/
        set SL06M001.SL06M-SY-COLR-CD-ID bold;

        SLWREC.XXXLOCID = "130"; /* module identification*/

        exit stack; /* Return to flow*/

      else

        if (SQLCA.VAGen_SQLCODE == -911) /* and resource not available*/

          converseLib.validationFailed(31); /* Data in use by another*/
          TA1REC.TA1FUNC = "POST"; /* Termination function*/
          SLWREC.XXXLOCID = "130"; /* Module identification*/

        else

          converseLib.validationFailed(01); /* Unexpected error occurred*/
          TA1REC.TA1FUNC = "RETN"; /* Termination function*/

          converseLib.clearScreen();
           /* Clear screen before display*/

          TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
          TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
          TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
          TA1REC.TA1LOC = "OBTAIN STYLE NAME FOR HEADING";
          TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
          TA1REC.TA1PGMNO = SLWREC.XXXLOCID; /* Program number*/
          TA1REC.TA1DBASE = "DPROD   "; /* Data base name*/
          TA1REC.TA1TBLVU = "VSTYLE01"; /* Table view name*/
          TA1REC.TA1TBLKE = PKNREC.PKNKEY; /* Table key*/
          TA1REC.TA1MAPNO = "M001"; /* map number*/

          call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes};
                                   /* Error diagnosis routine*/

          exit stack; /* Exit to display map*/

        end
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* validate size entered*/
   /* ------------------------------------------------------------*/

  if (SL06M001.SL06M-SIZE == " "
  )
    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-SIZE cursor, bold; /* Highlight*/
    converseLib.validationFailed(238); /* invalid key*/
    exit stack;

  end

  SL06W00.SL06W-HEADER-SIZE = SL06M001.SL06M-SIZE;

  set SYPREC empty;

  SYPREC.SYPFC = "PC"; /* get all valid sizes*/
  SYPREC.SYPCMPSW = "C"; /* indicate compressed format*/

  SYPREC.XGPCD = SL06M001.SL06M-XGP-GPC-CD;
  SYPREC.SY1STNBR = SL06M001.SL06M-SY-STY-NBR;
  SYPREC.SY2CLRID = SL06M001.SL06M-SY-COLR-CD-ID;
  SYPREC.XDMCD = SL06M001.SL06M-XDM-DIM-CD;
  SYPREC.XQACD = SL06M001.SL06M-XQA-QUAL-CD;

  TA1REC.TA1LOCAT[2] = "SL06500 ";
  TA1REC.TA1MAP = "SL06M001";

  call "SY1190" (SQLCA, SYPREC) {isNoRefresh = yes, isExternal = yes};

  if (SYPREC.SYPRC != 0)
    if (SYPREC.SYPRC == 9
     || SYPREC.SYPRC == 10)

      SLWREC.XXXLOCID = "500"; /* Module identification*/
      set SL06M001.SL06M-SIZE cursor, bold; /* Highlight*/
      converseLib.validationFailed(238); /* invalid key*/

    else

      TA1REC.TA1TYPE = "DB2 ";
      TA1REC.TA1FUNC = "RETN";
      TA1REC.TA1PGMNM = "SL06500 ";
      TA1REC.TA1LOCAT[1] = "SY1190  ";
      TA1REC.TA1LOCAT[3] = SYPREC.SYPRCX;
      TA1REC.TA1LOCAT[4] = "SZ XLATE";
      TA1REC.TA1DBASE = "MULTIPLE";
      TA1REC.TA1TBLVU = "DB VIEWS";
      TA1REC.TA1TBLKE = SYPREC.SYPKEY;
      move SQLCA to ERRSQLCA withV60Compat;
      VGVar.handleSysLibraryErrors = 0;
      sysLib.rollback();
      call "TA0020" (TA1REC, ERRSQLCA) {isExternal = yes};

    end
  end

  SL06W00.SL06W-SIZE-INX = 1;
  SL06W00.SL06W-SIZE-FOUND-SW = "N";

  while (SL06W00.SL06W-SIZE-FOUND-SW == "N"
   && SL06W00.SL06W-SIZE-INX <= 40)

    SL06-590();

  end

  if (SL06W00.SL06W-SIZE-FOUND-SW == "N")

    SLWREC.XXXLOCID = "500"; /* Module identification*/
    set SL06M001.SL06M-SIZE cursor, bold; /* Highlight*/
    converseLib.validationFailed(238); /* invalid key*/

  end

   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/


   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end SL06-500

// Verify scroll request
Function SL06-510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && SL06W00.XXXTSPAG < 2)       /* and no previous pages*/

    SLWREC.XXXLOCID = "510"; /* Module identification*/
    converseLib.validationFailed(7); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && SL06W00.XXXTSPAG >= SL06W00.TA5TSQIX) /* and no more pages*/

    SLWREC.XXXLOCID = "510"; /* Module identification*/
    converseLib.validationFailed(8); /* No more pages to display*/

  end

end // end SL06-510

// Verify data in the line
Function SL06-520()
   /* ------------------------------------------------------------*/
   /* selection may be made only with a single 's'.  Multiple*/
   /* 's' and characters other than 's' and ' ' are not allowed.*/
   /* ------------------------------------------------------------*/

  if (SL06M001.SL06M-SELECT[SL06W-MAP-FIELD-INX] != " ")
    if (SL06M001.SL06M-SELECT[SL06W-MAP-FIELD-INX] == "S")
      if (SL06W00.SL06W-ROW-SELECTED == "Y")

        SLWREC.XXXLOCID = "520"; /* Module identification*/
        set SL06M001.SL06M-SELECT[SL06W-MAP-FIELD-INX] cursor, bold;
                                  /* highlight*/
        converseLib.validationFailed(51); /* Only one row may be selected*/

      else

        SLWREC.SY-UNITS-PER-CASE = SL06M001.SL06M-SY-UNITS-PER-CASE[SL06W-MAP-FIELD-INX];

        SLWREC.PC3PONBR = SL06M001.SL06M-PC-PO-NBR[SL06W-MAP-FIELD-INX];

        SLWREC.PC4ORTYP = SL06M001.SL06M-PO-TYPE[SL06W-MAP-FIELD-INX];

        SLWREC.PC3POIDT = SL06W00.SL06W-PC-PO-ID-MO-RNG[SL06W-MAP-FIELD-INX];

        SLWREC.PCFSECD = SL06W00.SL06W-PC-PO-ID-SESN-CD[SL06W-MAP-FIELD-INX];

        SLWREC.FY1FCTYC = SL06W00.SL06W-FY-FCTY-CD[SL06W-MAP-FIELD-INX];

        SL06W00.SL06W-ROW-SELECTED = "Y";

      end

    else

      SLWREC.XXXLOCID = "520"; /* Module identification*/
      set SL06M001.SL06M-SELECT[SL06W-MAP-FIELD-INX] cursor, bold;
                                  /* highlight*/
      converseLib.validationFailed(228); /* Only "S" may be entered*/

    end
  end

end // end SL06-520

Function SL06-590()
  /* -----------------------------------------------------------------------*/
   /* loop through array returned from sy1190 to determine if size*/
   /* entered in header is among valid sizes*/
  /* -----------------------------------------------------------------------*/

  if (SYPREC.SY4IDDSC[SL06W-SIZE-INX] == SL06W00.SL06W-HEADER-SIZE)

    SL06W00.SL06W-SIZE-FOUND-SW = "Y";

  else

    SL06W00.SL06W-SIZE-INX = SL06W00.SL06W-SIZE-INX + 1;

  end

end // end SL06-590

// Process valid input request
Function SL06-600()
   /* ------------------------------------------------------------*/
   /* load slwrec with info to be passed*/
   /* ------------------------------------------------------------*/

  SL06-610(); /* move map working storage to slwrec*/

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    SL06W00.XXXTSPAG = SL06W00.XXXTSPAG - 1; /* Decrement page number*/
    SL06W00.SL06W-FORMAT-MAP-SW = "Y"; /* format map*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    SL06W00.XXXTSPAG = SL06W00.XXXTSPAG + 1; /* Increment page number*/
    SL06W00.SL06W-FORMAT-MAP-SW = "Y"; /* format map*/

    return; /* Return to previous process*/

  end

  if (converseVar.eventKey is pf6) /* PF6 was pressed*/
    COMMAREA.CATOAP = "SL05"; /* TO application*/
  end

  if (converseVar.eventKey is pf12) /* PF12 was pressed*/
    COMMAREA.CATOAP = "SL01"; /* TO application*/
  end

  if (converseVar.eventKey is pf13) /* PF13 was pressed*/
    COMMAREA.CATOAP = "SL02"; /* TO application*/
  end

  if (converseVar.eventKey is pf14) /* PF14 was pressed*/
    COMMAREA.CATOAP = "SL03"; /* TO application*/
  end

  if (converseVar.eventKey is pf15) /* PF15 was pressed*/
    COMMAREA.CATOAP = "SL04"; /* TO application*/
  end

  if (converseVar.eventKey is pf16) /* PF16 was pressed*/
    COMMAREA.CATOAP = "SL05"; /* TO application*/
  end

  if (converseVar.eventKey is pf17) /* PF17 was pressed*/
    COMMAREA.CATOAP = "SL0Q"; /* TO application*/
    SLWREC.SLWCASNO = "7"; /* option number*/
  end

  if (converseVar.eventKey is pf18) /* PF18 was pressed*/
    COMMAREA.CATOAP = "SL0Q"; /* TO application*/
    SLWREC.SLWCASNO = "8"; /* option number*/
  end

  if (COMMAREA.CATOAP > " ")

    SL06-110(); /* Purge temporary storage*/

    XSSL001(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

  /* --------------------------------------------------------------*/
     /* indicate the map needs to be formatted*/
  /* --------------------------------------------------------------*/

  SL06W00.SL06W-FORMAT-MAP-SW = "Y"; /* format map*/

  /* --------------------------------------------------------------*/
     /* verify header information has not changed*/
  /* --------------------------------------------------------------*/

  if (SL06W01.SL06W-XWH-WHSE-CD != SL06M001.SL06M-XWH-WHSE-CD
   || SL06W01.SL06W-XGP-GPC-CD != SL06M001.SL06M-XGP-GPC-CD
   || SL06W01.SL06W-SY-STY-NBR != SL06M001.SL06M-SY-STY-NBR
   || SL06W01.SL06W-SY-COLR-CD-ID != SL06M001.SL06M-SY-COLR-CD-ID
   || SL06W01.SL06W-XDM-DIM-CD != SL06M001.SL06M-XDM-DIM-CD
   || SL06W01.SL06W-XQA-QUAL-CD != SL06M001.SL06M-XQA-QUAL-CD
   || SL06W01.SL06W-LIFE-CYCLE != SL06M001.SL06M-LIFE-CYCLE
   || SL06W01.SL06W-XAR-AREA-CD != SL06M001.SL06M-XAR-AREA-CD
   || SL06W01.SL06W-SIZE != SL06M001.SL06M-SIZE
   || SL06W01.SL06W-SL-LOC-CD != SL06M001.SL06M-SL-LOC-CD
   || SL06W01.SL06W-SL-ZONE-CD != SL06M001.SL06M-SL-ZONE-CD)

    SL06W00.SL06W-RQT-DATA-SW = "Y"; /* Request for data switch*/

  end

  if (converseVar.eventKey is enter) /* enter was pressed*/
    if (SL06W00.SL06W-RQT-DATA-SW == "Y")
      exit stack;
    else
      SL06W00.SL06W-FORMAT-MAP-SW = "N"; /* keep 's' on screen*/
      COMMAREA.CATOAP = "SL01"; /* TO application*/
      SL06-110(); /* Purge temporary storage*/
      XSSL001(); /* Exit to another application*/
      exit stack;
    end
  end

end // end SL06-600

// Verify data in the line
Function SL06-610()
   /* ------------------------------------------------------------*/
   /* load map header working storage into slwrec to be passed*/
   /* ------------------------------------------------------------*/

  SLWREC.XWHCD = SL06W01.SL06W-XWH-WHSE-CD; /* whse code*/

  SLWREC.XGPCD = SL06W01.SL06W-XGP-GPC-CD; /* gpc code*/

  SLWREC.SY1STNBR = SL06W01.SL06W-SY-STY-NBR; /* style nbr*/

  SLWREC.SY2CLRID = SL06W01.SL06W-SY-COLR-CD-ID; /* color cd*/

  SLWREC.XDMCD = SL06W01.SL06W-XDM-DIM-CD; /* dimension code*/

  SLWREC.XQACD = SL06W01.SL06W-XQA-QUAL-CD; /* quality code*/

