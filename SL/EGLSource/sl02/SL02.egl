// Format and converse the map
Function SL02-200()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/
    SLWREC.SLWMSGNO = converseVar.validationMsgNum; /* Message number*/
  end

   /* ------------------------------------------------------------*/
   /* If obtain data failed or no error, format data to display.*/
   /* ------------------------------------------------------------*/

  if (SL02W00.SL02W-RQT-DATA-SW == "Y" /* Obtain data failed*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (SL02W00.SL02W-FORMAT-MAP-SW == "Y") /* Map must be formatted*/
      SL02-210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  SL02M001.SL02M-XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/

  SL02M001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  SL02M001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (SLWREC.SLWMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (SLWREC.SLWMSGNO != 0) /* Message to display*/

      if (SLWREC.SLWMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(SLWREC.SLWMSGNO);
      end /* Message number*/
    else
      SL02M001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    SL02M001.SL02M-XXX-MODID = SLWREC.XXXLOCID; /* Module ID*/

    set SL02M001 alarm; /* Indicate sound alarm*/

  end

  converse SL02M001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  SLWREC.XXXLOCID = " "; /* Module identification*/
  SLWREC.SLWMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/

  COMMAREA.CATOAP = SL02M001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = SL02M001.CAITEM; /* Application data*/


end // end SL02-200

// Format the display data
Function SL02-210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set SL02M001 initial; /* map input/output area*/

  SL02W00.SL02W-FORMAT-MAP-SW = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (SL02W00.TA5TSQIX > 0) /* Pages were formatted*/

    if (SL02W00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the current page has been changed, update the page in*/
       /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/

      if (SL02W00.SL02W-UPD-PAGE-SW == "Y") /* Current page was changed*/

        SL02S01(); /* Update temporary storage page*/

        SL02W00.SL02W-UPD-PAGE-SW = "N"; /* Storage update switch*/

      end

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (SL02W00.XXXTSPAG > SL02W00.TA5TSQIX) /* Desired greater highest*/
        SL02W00.XXXTSPAG = SL02W00.TA5TSQIX; /* Default to highest*/
      end

      SL02S02(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set SL02M001.XXXLITPG skip; /* "PAGE" literal*/
    SL02M001.XXXTSPAG = SL02W00.XXXTSPAG; /* Current page*/
    set SL02M001.XXXLITOF skip; /* "OF" literal*/
    SL02M001.TA5TSQIX = SL02W00.TA5TSQIX; /* Highest page*/

  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  SL02M001.SL02M-XWH-WHSE-CD = SL02W01.SL02W-XWH-WHSE-CD; /* warehouse code*/

  SL02M001.SL02M-XWH-WHSE-NM = SL02W01.SL02W-XWH-WHSE-NM; /* warehouse code*/

  SL02M001.SL02M-XGP-GPC-CD = SL02W01.SL02W-XGP-GPC-CD; /* GPC code*/

  SL02M001.SL02M-SY-STY-NBR = SL02W01.SL02W-SY-STY-NBR; /* Style number*/

  SL02M001.SL02M-SY-COLR-CD-ID = SL02W01.SL02W-SY-COLR-CD-ID; /* color code*/

  SL02M001.SL02M-XDM-DIM-CD = SL02W01.SL02W-XDM-DIM-CD; /* dimension code*/

  SL02M001.SL02M-XQA-QUAL-CD = SL02W01.SL02W-XQA-QUAL-CD; /* quality code*/

  SL02M001.SL02M-SY-STY-NM = SL02W01.SL02W-SY-STY-NM; /* Style name*/

  SL02M001.SL02M-STY-COLR-NM = SL02W01.SL02W-STY-COLR-NM; /* color name*/

  SL02M001.SL02M-PC-CU-IN-SEG-CD = SL02W01.SL02W-PC-CU-IN-SEG-CD;

  SL02M001.SL02M-XEG-ENGR-DIFF-CD = SL02W01.SL02W-XEG-ENGR-DIFF-CD;

  SL02M001.SL02M-XSL-CLASS-CD = SL02W01.SL02W-XSL-CLASS-CD;

  SL02M001.SL02M-XUM-UOM-CD = SL02W01.SL02W-XUM-UOM-CD;

  SL02M001.SL02M-XOW-OWN-GRP-CD = SL02W01.SL02W-XOW-OWN-GRP-CD;

  SL02M001.SL02M-LIFE-CYCLE = SL02W01.SL02W-LIFE-CYCLE;

  SL02M001.SL02M-PO-ID = SL02W01.SL02W-PO-ID;

  SL02M001.SL02M-PC-PO-NBR = SL02W01.SL02W-PC-PO-NBR;

  SL02M001.SL02M-PO-TYPE = SL02W01.SL02W-PO-TYPE;

  SL02M001.SL02M-PC-SAP-PO-NBR = SL02W01.SL02W-PC-SAP-PO-NBR;

   /* set blank fields to cursor*/

  if (SL02M001.SL02M-SY-STY-NBR == " ")
    set SL02M001.SL02M-SY-STY-NBR cursor;
  end

  SL02W00.SL02W-MAP-FIELD-INX = 1; /* Initialize line index*/

  while (SL02W00.SL02W-MAP-FIELD-INX <= 13 /* Until screen is full*/
   && SL02W01.SL02W-SIZES[SL02W-MAP-FIELD-INX] > " ")
    /* AND SL02W01.SL02W-NET-QTY(SL02W-MAP-FIELD-INX) GT 0*/
    if (SL02W00.SL02W-MAP-FIELD-INX == 1) /* First select field*/

      set SL02M001.SL02M-SELECT[SL02W-MAP-FIELD-INX] cursor, normal;
    else
      set SL02M001.SL02M-SELECT[SL02W-MAP-FIELD-INX] normal; /* Unprotect*/

    end

   /* --------------------------------------------------------------------*/
   /* move display fields from working storage*/
   /* --------------------------------------------------------------------*/

    /* selection column*/
    SL02M001.SL02M-SELECT[SL02W-MAP-FIELD-INX] = SL02W01.SL02W-SELECT[SL02W-MAP-FIELD-INX];

    SL02M001.SL02M-SIZES[SL02W-MAP-FIELD-INX] = SL02W01.SL02W-SIZES[SL02W-MAP-FIELD-INX];

    SL02M001.SL02M-NET-QTY[SL02W-MAP-FIELD-INX] = SL02W01.SL02W-NET-QTY[SL02W-MAP-FIELD-INX];

    SL02M001.SL02M-SL-SY-LOC-OH-QTY[SL02W-MAP-FIELD-INX] = SL02W01.SL02W-SL-SY-LOC-OH-QTY[SL02W-MAP-FIELD-INX];

    SL02M001.SL02M-SL-SY-LOC-RSRV-QTY[SL02W-MAP-FIELD-INX] = SL02W01.SL02W-SL-SY-LOC-RSRV-QTY[SL02W-MAP-FIELD-INX];

    SL02M001.SL02M-SL-SY-LOC-SAS-QTY[SL02W-MAP-FIELD-INX] = SL02W01.SL02W-SL-SY-LOC-SAS-QTY[SL02W-MAP-FIELD-INX];

    if (SL02W01.SL02W-MORE-UNPCS-FLAG[SL02W-MAP-FIELD-INX] == "Y")

      SLWREC.XXXLOCID = "210"; /* module identification*/
      set SL02M001.SL02M-SY-UNITS-PER-CASE[SL02W-MAP-FIELD-INX] bold;
      converseLib.validationFailed(453); /* all units per case cannot be*/
                                   /* displayed*/

    end

    SL02M001.SL02M-SY-UNITS-PER-CASE[SL02W-MAP-FIELD-INX] = SL02W01.SL02W-UNPCS[SL02W-MAP-FIELD-INX];

    SL02W00.SL02W-MAP-FIELD-INX = SL02W00.SL02W-MAP-FIELD-INX + 1;
                                  /* increment line index*/

  end


end // end SL02-210

// Check attention identifier
Function SL02-300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (SL02W00.SL02W-RQT-DATA-SW == "Y") /* Request for data*/
      set SL02W01 empty; /* M001 display storage*/
    end

    SL02W00.SL02W-FORMAT-MAP-SW = "Y"; /* Format the map switch*/
    SL02W00.SL02W-RQT-DATA-SW = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (SL02M001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      SL02-110(); /* Purge temporary storage*/

      XSSL001(); /* Exit to another application*/

      set SL02M001.CATOAP cursor, bold; /* Highlight*/

    else

      SLWREC.XXXLOCID = "300"; /* Module identification*/
      converseLib.validationFailed(258); /* PA/PF key may not be used*/

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4 /* or PF4 was pressed*/
   || converseVar.eventKey is pf5) /* or PF5 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "SL02M001"; /* Application find code*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "SL0A"; /* TO application*/
    end

    if (converseVar.eventKey is pf4) /* PF4 was pressed*/
      COMMAREA.CATOAP = "MN0M"; /* TO application*/
    end

    if (converseVar.eventKey is pf5) /* PF5 was pressed*/
      COMMAREA.CATOAP = "SL0M"; /* TO application*/
    end

    SL02-110(); /* Purge temporary storage*/

    XSSL001(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf12  /* and PF12 was not pressed*/
   && converseVar.eventKey not pf14  /* and PF14 was not pressed*/
   && converseVar.eventKey not pf15  /* and PF15 was not pressed*/
   && converseVar.eventKey not pf16  /* and PF16 was not pressed*/
   && converseVar.eventKey not pf17  /* and PF17 was not pressed*/
   && converseVar.eventKey not pf18) /* and PF18 was not pressed*/

    SLWREC.XXXLOCID = "300"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end SL02-300

// Reset highlighted fields
Function SL02-400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set SL02M001.SL02M-XWH-WHSE-CD initialAttributes; /* warehouse code*/
  set SL02M001.SL02M-XGP-GPC-CD initialAttributes; /* GPC code*/
  set SL02M001.SL02M-SY-STY-NBR initialAttributes; /* Style code*/
  set SL02M001.SL02M-SY-COLR-CD-ID initialAttributes; /* color code*/
  set SL02M001.SL02M-XDM-DIM-CD initialAttributes; /* dimension code*/
  set SL02M001.SL02M-XQA-QUAL-CD initialAttributes; /* quality code*/
  set SL02M001.SL02M-LIFE-CYCLE initialAttributes; /* life cycle*/
  set SL02M001.SL02M-XOW-OWN-GRP-CD initialAttributes; /* own group*/
  set SL02M001.SL02M-PC-CU-IN-SEG-CD initialAttributes; /* inventory segment*/
  set SL02M001.SL02M-XEG-ENGR-DIFF-CD initialAttributes; /* engineering diff*/
  set SL02M001.SL02M-XUM-UOM-CD initialAttributes; /* unit of measure*/
  set SL02M001.SL02M-XSL-CLASS-CD initialAttributes; /* class code*/
  set SL02M001.SL02M-PO-ID initialAttributes; /* po id*/
  set SL02M001.SL02M-PO-TYPE initialAttributes; /* po type*/
  set SL02M001.SL02M-PC-PO-NBR initialAttributes; /* po nbr*/
  set SL02M001.SL02M-PC-SAP-PO-NBR initialAttributes; /* sap po nbr*/

  SL02W00.SL02W-MAP-FIELD-INX = 1; /* Initialize line index*/

  while (SL02W00.SL02W-MAP-FIELD-INX <= 13 /* Until end of lines*/
   && SL02M001.SL02M-NET-QTY[SL02W-MAP-FIELD-INX] > 0)
                                  /* or end of display*/

    set SL02M001.SL02M-SELECT[SL02W-MAP-FIELD-INX] normal;
                                  /* reset select flag*/

    SL02W00.SL02W-MAP-FIELD-INX = SL02W00.SL02W-MAP-FIELD-INX + 1;
                                  /* Increment line index*/

  end

  set SL02M001.CATOAP initialAttributes; /* TO application*/
  set SL02M001.CAITEM initialAttributes; /* Application data*/

   /* ----------------------------------------------------------------------*/
    /* pop-up window option -- if not selected, return to previous process*/
   /* ----------------------------------------------------------------------*/

  if (SL02M001.SL02M-XWH-WHSE-CD == "?"
   || SL02M001.SL02M-XGP-GPC-CD == "?"
   || SL02M001.SL02M-XDM-DIM-CD == "?"
   || SL02M001.SL02M-XQA-QUAL-CD == "?"
   || SL02M001.SL02M-XEG-ENGR-DIFF-CD == "?"
   || SL02M001.SL02M-XUM-UOM-CD == "?"
   || SL02M001.SL02M-XOW-OWN-GRP-CD == "?"
   || SL02M001.SL02M-XSL-CLASS-CD == "?")

    if (converseVar.eventKey not enter) /* enter was not pressed*/

      SLWREC.XXXLOCID = "400"; /* module identification*/
      converseLib.validationFailed(4); /* pkkey invalid*/

      exit stack; /* exit to display map*/

    end

  else

    return; /* return to previous processing*/

  end

  SL02-410();

   /* ----------------------------------------------------------------------*/
   /* clear screen for pop-ups, then display map*/
   /* ----------------------------------------------------------------------*/

  converseLib.clearScreen();
   /* clear screen before display*/

  exit stack; /* exit to display map*/
end // end SL02-400

// Pop-up window lists
Function SL02-410()
   /* ------------------------------------------------------------*/
   /* warehouse pop-up window*/
   /* ------------------------------------------------------------*/

  if (SL02M001.SL02M-XWH-WHSE-CD == "?") /* Warehouse list requested*/

    XPXX001(); /* Window initialization*/
    XX0XW02.XX0XWHDR = "  WAREHOUSE"; /* Heading*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXWH01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      SL02M001.SL02M-XWH-WHSE-CD = SL02W01.SL02W-XWH-WHSE-CD;
    else
      SL02M001.SL02M-XWH-WHSE-CD = XWHTBL.XWHCD[XX0XWPIX];
      SL02W01.SL02W-XWH-WHSE-CD = XWHTBL.XWHCD[XX0XWPIX];

    end

    set SL02M001.SL02M-XWH-WHSE-CD cursor;
    SLWREC.XXXLOCID = "410"; /* module indentification*/
    return;

  end

   /* ------------------------------------------------------------*/
   /* gpc pop-up window*/
   /* ------------------------------------------------------------*/

  if (SL02M001.SL02M-XGP-GPC-CD == "?") /* Warehouse list requested*/

    XPXX001(); /* Window initialization*/
    XX0XW02.XX0XWHDR = "GPC CODE   "; /* Heading*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXGP01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      SL02M001.SL02M-XGP-GPC-CD = SL02W01.SL02W-XGP-GPC-CD;
    else
      SL02M001.SL02M-XGP-GPC-CD = XGPTBL.XGPCD[XX0XWPIX];
      SL02W01.SL02W-XGP-GPC-CD = XGPTBL.XGPCD[XX0XWPIX];

    end

    set SL02M001.SL02M-XGP-GPC-CD cursor; /* Establish cursor position*/
    SLWREC.XXXLOCID = "410"; /* module indentification*/
    return;

  end

   /* ------------------------------------------------------------*/
