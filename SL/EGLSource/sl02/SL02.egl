// Verify scroll request
Function SL02-510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && SL02W00.XXXTSPAG < 2)       /* and no previous pages*/

    SLWREC.XXXLOCID = "510"; /* Module identification*/
    converseLib.validationFailed(7); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && SL02W00.XXXTSPAG >= SL02W00.TA5TSQIX) /* and no more pages*/

    SLWREC.XXXLOCID = "510"; /* Module identification*/
    converseLib.validationFailed(8); /* No more pages to display*/

  end

end // end SL02-510

// Verify data in the line
Function SL02-520()
   /* ------------------------------------------------------------*/
   /* selection may be made only with a single 's'.  Multiple*/
   /* 's' and characters other than 's' and ' ' are not allowed.*/
   /* ------------------------------------------------------------*/

  if (SL02M001.SL02M-SELECT[SL02W-MAP-FIELD-INX] != " ")
    if (SL02M001.SL02M-SELECT[SL02W-MAP-FIELD-INX] == "S")
      if (SL02W00.SL02W-ROW-SELECTED == "Y")

        SLWREC.XXXLOCID = "520"; /* Module identification*/
        set SL02M001.SL02M-SELECT[SL02W-MAP-FIELD-INX] cursor, bold;
                                  /* highlight*/
        converseLib.validationFailed(51); /* Only one row may be selected*/

      else

        SLWREC.SY-SZ-ID-DESC = SL02W01.SL02W-SIZES[SL02W-MAP-FIELD-INX];

        SL02W00.SL02W-ROW-SELECTED = "Y";

      end

    else

      SLWREC.XXXLOCID = "520"; /* Module identification*/
      set SL02M001.SL02M-SELECT[SL02W-MAP-FIELD-INX] cursor, bold;
                                  /* highlight*/
      converseLib.validationFailed(228); /* Only "S" may be entered*/

    end
  end

end // end SL02-520

// Process valid input request
Function SL02-600()
   /* ------------------------------------------------------------*/
   /* move map working storage to slwrec*/
   /* ------------------------------------------------------------*/

  SL02-610(); /* move passed info to slwrec*/

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    SL02W00.XXXTSPAG = SL02W00.XXXTSPAG - 1; /* Decrement page number*/
    SL02W00.SL02W-FORMAT-MAP-SW = "Y"; /* format map*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    SL02W00.XXXTSPAG = SL02W00.XXXTSPAG + 1; /* Increment page number*/
    SL02W00.SL02W-FORMAT-MAP-SW = "Y"; /* format map*/

    return; /* Return to previous process*/

  end

  if (converseVar.eventKey is pf12) /* PF12 was pressed*/
    COMMAREA.CATOAP = "SL01"; /* TO application*/
  end

  if (converseVar.eventKey is pf14) /* PF14 was pressed*/
    COMMAREA.CATOAP = "SL03"; /* TO application*/
  end

  if (converseVar.eventKey is pf15) /* PF15 was pressed*/
    COMMAREA.CATOAP = "SL04"; /* TO application*/
  end

  if (converseVar.eventKey is pf16) /* PF16 was pressed*/
    COMMAREA.CATOAP = "SL05"; /* TO application*/
  end

  if (converseVar.eventKey is pf17) /* PF17 was pressed*/
    COMMAREA.CATOAP = "SL0Q"; /* TO application*/
    SLWREC.SLWCASNO = "7"; /* option number*/
  end

  if (converseVar.eventKey is pf18) /* PF18 was pressed*/
    COMMAREA.CATOAP = "SL0Q"; /* TO application*/
    SLWREC.SLWCASNO = "8"; /* option number*/
  end

  if (COMMAREA.CATOAP > " ") /* transfer requested*/

    SL02-110(); /* Purge temporary storage*/

    XSSL001(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end
  /* --------------------------------------------------------------*/
     /* indicate the map needs to be formatted*/
  /* --------------------------------------------------------------*/

  SL02W00.SL02W-FORMAT-MAP-SW = "Y"; /* format map*/

  /* --------------------------------------------------------------*/
     /* verify header information has not changed*/
  /* --------------------------------------------------------------*/

  if (SL02W01.SL02W-XWH-WHSE-CD != SL02M001.SL02M-XWH-WHSE-CD
   || SL02W01.SL02W-XGP-GPC-CD != SL02M001.SL02M-XGP-GPC-CD
   || SL02W01.SL02W-SY-STY-NBR != SL02M001.SL02M-SY-STY-NBR
   || SL02W01.SL02W-SY-COLR-CD-ID != SL02M001.SL02M-SY-COLR-CD-ID
   || SL02W01.SL02W-XDM-DIM-CD != SL02M001.SL02M-XDM-DIM-CD
   || SL02W01.SL02W-XQA-QUAL-CD != SL02M001.SL02M-XQA-QUAL-CD
   || SL02W01.SL02W-PC-CU-IN-SEG-CD != SL02M001.SL02M-PC-CU-IN-SEG-CD
   || SL02W01.SL02W-XEG-ENGR-DIFF-CD != SL02M001.SL02M-XEG-ENGR-DIFF-CD
   || SL02W01.SL02W-XSL-CLASS-CD != SL02M001.SL02M-XSL-CLASS-CD
   || SL02W01.SL02W-XUM-UOM-CD != SL02M001.SL02M-XUM-UOM-CD
   || SL02W01.SL02W-XOW-OWN-GRP-CD != SL02M001.SL02M-XOW-OWN-GRP-CD
   || SL02W01.SL02W-LIFE-CYCLE != SL02M001.SL02M-LIFE-CYCLE
   || SL02W01.SL02W-PC-PO-NBR != SL02M001.SL02M-PC-PO-NBR
   || SL02W01.SL02W-PC-SAP-PO-NBR != SL02M001.SL02M-PC-SAP-PO-NBR
   || SL02W01.SL02W-PO-ID != SL02M001.SL02M-PO-ID
   || SL02W01.SL02W-PO-TYPE != SL02M001.SL02M-PO-TYPE)

    SL02W00.SL02W-RQT-DATA-SW = "Y"; /* Request for data switch*/

  end

  if (converseVar.eventKey is enter) /* enter was pressed*/

    if (SL02W00.SL02W-RQT-DATA-SW == "Y"
    )
      exit stack;

    else

      COMMAREA.CATOAP = "SL03"; /* TO application*/
      SL02W00.SL02W-FORMAT-MAP-SW = "N"; /* keep 's' on screen*/
      SL02-110(); /* Purge temporary storage*/
      XSSL001(); /* Exit to another application*/
      exit stack;

    end
  end
end // end SL02-600

