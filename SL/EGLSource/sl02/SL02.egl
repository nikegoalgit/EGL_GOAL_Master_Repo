// Save data in display storage
Function SL02-160()
   /* ------------------------------------------------------------*/
   /* If first line on subsequent page, save heading information.*/
   /* ------------------------------------------------------------*/

  if (SL02W00.TA5TSQIX > 0 /* Not the first page*/
   && SL02W00.SL02W-MAP-FIELD-INX == 1) /* and first line on page*/

    SL02W01.SL02W-XGP-GPC-CD = PKNREC.XGPCD; /* GPC code*/
    SL02W01.SL02W-SY-STY-NBR = PKNREC.SY1STNBR; /* Style number*/
    SL02W01.SL02W-SY-COLR-CD-ID = PKNREC.SY2CLRID; /* color id*/
    SL02W01.SL02W-XDM-DIM-CD = PKNREC.XDMCD; /* dimension code*/
    SL02W01.SL02W-SY-STY-NM = PKNREC.SY1SSTNM; /* style name*/
    SL02W01.SL02W-STY-COLR-NM = PKNREC.SY2CLRAB; /* color name*/
    SL02W01.SL02W-XWH-WHSE-CD = VSYLOC02.XWHCD; /* warehouse code*/
    SL02W01.SL02W-XWH-WHSE-NM = SL02M001.SL02M-XWH-WHSE-NM;
    SL02W01.SL02W-XQA-QUAL-CD = VSYLOC02.XQACD; /* quality code*/
    SL02W01.SL02W-XOW-OWN-GRP-CD = VSYLOC02.XOWCD; /* own group*/
    SL02W01.SL02W-PC-CU-IN-SEG-CD = VSYLOC02.PC2INSEG; /* invent seg*/
    SL02W01.SL02W-XEG-ENGR-DIFF-CD = VSYLOC02.XEGCD; /* engineer diff*/
    SL02W01.SL02W-XUM-UOM-CD = VSYLOC02.XUMCD; /* unit of measure*/
    SL02W01.SL02W-XSL-CLASS-CD = VSYLOC02.XSLCD; /* class code*/
    SL02W01.SL02W-PC-PO-NBR = SL02W00.SL02W-MAP-PO-NBR;
    SL02W01.SL02W-PC-SAP-PO-NBR = SL02W00.SL02W-MAP-PC-SAP-PO-NBR;
    SL02W01.SL02W-PO-ID = SL02W00.SL02W-MAP-PO-ID;
    SL02W01.SL02W-PO-TYPE = SL02W00.SL02W-MAP-PO-TYPE;
    SL02W01.SL02W-LIFE-CYCLE = SL02W00.SL02W-MAP-LIFE-CYCLE;

  end

  if (SL02W00.SL02W-FIRST-FETCH == "Y")

    SL02W00.SL02W-UNPCS-LINE = " ";
    SL02W00.SL02W-UNPCS-CHAR-USED = 0;
    SL02W00.SL02W-UNPCS-SIGNIF-DIG = 0;
    SL02W00.SL02W-UNPCS-LINE-INX = 1;
    SL02W00.SL02W-UNPCS-NBR-INX = 1;
    SL02W00.SL02W-FIRST-FETCH = "N";
    SL02W00.SL02W-NEW-SIZE-SW = "Y";
    SL02W00.SL02W-CAN-DISPLAY-ALL = "Y";
    SL02W00.SL02W-HOLD-SIZE = VSYLOC02.SY5SZID;

  end

   /* ------------------------------------------------------------*/
   /* Save the data from the table row in display storage.*/
   /* ------------------------------------------------------------*/

  if (VSYLOC02.SY5SZID != SL02W00.SL02W-HOLD-SIZE)

    SL02-161(); /* convert size code to size*/

    if (SYPREC.SYPRC == 0) /* if no error*/
      SL02W01.SL02W-SIZES[SL02W-MAP-FIELD-INX] = SYPREC.SY4IDDSC;
    else
      if (SYPREC.SYPRC == 9)
        converseLib.validationFailed(011);
        TA1REC.TA1FUNC = "POST";
        exit stack;
      else
        if (SYPREC.SYPRC == 10)
          SL02W01.SL02W-SIZES[SL02W-MAP-FIELD-INX] = "*NF*";
                                   /* not found*/
        else
   /* ------------------------------------------------------------*/
    /* indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/

          TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
          TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

          SLWREC.XXXLOCID = "160"; /* Module identification*/

          TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
          TA1REC.TA1LOC = "SELECT SET OF TABLE ROWS";
          TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
          TA1REC.TA1PGMNO = SLWREC.XXXLOCID; /* Program number*/
          TA1REC.TA1DBASE = "DSTLOC  "; /* Data base name*/
          TA1REC.TA1TBLVU = "VSYLOC02"; /* Table view name*/
          TA1REC.TA1TBLKE = VSYLOC02.VSYLOC02-KEY; /* Table key*/
          TA1REC.TA1MAPNO = "M001"; /* map number*/

          call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes};
                                   /* Error diagnosis routine*/

          exit stack; /* Exit to display map*/

        end
      end
    end

    SL02W00.SL02W-CALC-NET-QTY = SL02W00.SL02W-ACCUM-OH-QTY -     SL02W00.SL02W-ACCUM-RSRV-QTY - SL02W00.SL02W-ACCUM-SAS-QTY;

    SL02W01.SL02W-NET-QTY[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-CALC-NET-QTY;
    SL02W01.SL02W-SL-SY-LOC-OH-QTY[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-ACCUM-OH-QTY;
    SL02W01.SL02W-SL-SY-LOC-RSRV-QTY[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-ACCUM-RSRV-QTY;
    SL02W01.SL02W-SL-SY-LOC-SAS-QTY[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-ACCUM-SAS-QTY;

    SL02W01.SL02W-UNPCS[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-UNPCS-LINE;

    if (SL02W00.SL02W-CAN-DISPLAY-ALL == "N")
      SL02W01.SL02W-MORE-UNPCS-FLAG[SL02W-MAP-FIELD-INX] = "Y";
    end

    SL02W00.SL02W-UNPCS-CHAR-USED = 0;
    SL02W00.SL02W-UNPCS-SIGNIF-DIG = 0;
    SL02W00.SL02W-UNPCS-LINE = " ";
    SL02W00.SL02W-UNPCS-NBR = 0;
    SL02W00.SL02W-NEW-SIZE-SW = "Y";
    SL02W00.SL02W-CAN-DISPLAY-ALL = "Y";
    SL02W00.SL02W-UNPCS-LINE-INX = 1;

    SL02W00.SL02W-HOLD-SIZE = VSYLOC02.SY5SZID;

    SL02W00.SL02W-CALC-NET-QTY = 0;
    SL02W00.SL02W-ACCUM-RSRV-QTY = 0;
    SL02W00.SL02W-ACCUM-OH-QTY = 0;
    SL02W00.SL02W-ACCUM-SAS-QTY = 0;
    SL02W00.SL02W-MAP-FIELD-INX = SL02W00.SL02W-MAP-FIELD-INX + 1;
                                  /* increment line index*/

  end

  SL02W00.SL02W-ACCUM-RSRV-QTY = SL02W00.SL02W-ACCUM-RSRV-QTY +   VSYLOC02.SL6PKRSV;
  SL02W00.SL02W-ACCUM-OH-QTY = SL02W00.SL02W-ACCUM-OH-QTY +   VSYLOC02.SL6ONHND;
  SL02W00.SL02W-ACCUM-SAS-QTY = SL02W00.SL02W-ACCUM-SAS-QTY +   VSYLOC02.SL6STASD;

  SL02W00.SL02W-UNPCS-NBR-INX = 1;
  SL02W00.SL02W-UNPCS-NBR = 0;
  SL02W00.SL02W-UNPCS-SIGNIF-DIG = 0;
  SL02W00.SL02W-UNPCS-NBR = VSYLOC02.SY1UNPCS;

  if (SL02W00.SL02W-NEW-SIZE-SW == "Y"
   || SL02W00.SL02W-HOLD-UNIT != SL02W00.SL02W-UNPCS-NBR)

    SL02W00.SL02W-HOLD-UNIT = SL02W00.SL02W-UNPCS-NBR;

    if (SL02W00.SL02W-CAN-DISPLAY-ALL == "Y")
      while (SL02W00.SL02W-UNPCS-NBR-INX < 5
      )
        SL02-162();

      end

      SL02W00.SL02W-UNPCS-CHAR-USED = SL02W00.SL02W-UNPCS-CHAR-USED + SL02W00.SL02W-UNPCS-SIGNIF-DIG       + 1; /* will comma + another nbr fit?*/

      if (SL02W00.SL02W-UNPCS-CHAR-USED > 24) /* if not room on line*/

        SL02W00.SL02W-CAN-DISPLAY-ALL = "N";

      else

        if (SL02W00.SL02W-NEW-SIZE-SW == "Y")

          SL02W00.SL02W-NEW-SIZE-SW = "N";

        else

          SL02W00.SL02W-UNPCS-LINE-CHAR[SL02W-UNPCS-LINE-INX] = ",";
          SL02W00.SL02W-UNPCS-LINE-INX = SL02W00.SL02W-UNPCS-LINE-INX + 1;
        end
      end

      if (SL02W00.SL02W-CAN-DISPLAY-ALL == "Y"
      )
        SL02W00.SL02W-UNPCS-SIGNIF-DIG = 0; /* initialize*/
        SL02W00.SL02W-UNPCS-NBR-INX = 1; /* initialize*/

        while (SL02W00.SL02W-UNPCS-NBR-INX < 5)
          SL02-163(); /* move new unpcs nbr to unpcs line*/

        end

        if (SL02W00.SL02W-UNPCS-SIGNIF-DIG == 0)

          SL02W00.SL02W-UNPCS-LINE-CHAR[SL02W-UNPCS-LINE-INX] = "0";
          SL02W00.SL02W-UNPCS-CHAR-USED = SL02W00.SL02W-UNPCS-CHAR-USED + 1; /* only counting comma*/
          SL02W00.SL02W-UNPCS-LINE-INX = SL02W00.SL02W-UNPCS-LINE-INX + 1;

        end
      end
    end
  end
end // end SL02-160

// obtain external size
Function SL02-161()
