//*** PROGRAM=SL02 ****
// This application allows a user to inquire and maintain rows
// in a DB2 table.
// 
// There are two different screens that may be displayed.
// 
// The first screen displayed to the user allows specification
// of the search criteria to be used to list the rows in the
// table.  The list is displayed and the user may page backward
// and forward to view the information.
// 
// When a line is selected on the screen, the second screen is
// displayed, showing all information for that table row.
// 
// If the user has sufficient security to change the data, the
// modifiable fields remain unprotected.  Otherwise, they are
// protected from modification by the user.
// 
// If any data is modified, it is verified and, if complete and
// correct, the table row is updated with the changed data.
// The screen is redisplayed to the user, indicating that the
// update is completed.
// 
// If more that one line was selected on the first screen, the
// next selection is displayed when the user presses the ENTER
// key without making any change to the displayed data.
// 
// When the user exits the second screen (either via PF3 key or
// after all selections have been viewed) the first screen is
// redisplayed, indicating the selections that were viewed.
// 
// The user may exit to any allowed application, via FAST PATH
// or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      07/15/93  hchapm  New application.
// CANADA   08/25/95  CBRINE  Added call to SY2000. Added XCTCD
//                            to SLWREC.
// NSC      04/16/01  KRAMAL  Use new rec VSYLOC02 instead of
//                            SL6REC.
//                            The new record structure has the
//                            SAP PO Number
// *********************
Program SL02 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "SL00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  ERRSQLCA ERRSQLCA; // record
  PC2REC PC2REC; // record
  PKNREC PKNREC; // record
  SL02W00 SL02W00; // record
  SL02W01 SL02W01; // record
  SLWREC SLWREC; // record
  SQLCA SQLCA; // record
  SWVREC SWVREC; // record
  SYCREC SYCREC; // record
  SYPREC SYPREC; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VSYLOC02 VSYLOC02; // record
  WSY2000 WSY2000; // record
  XSSLW01 XSSLW01; // record
  XX0XW02 XX0XW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XWHTBL; // table
  use XDMTBL; // table
  use XQATBL; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use XOWTBL; // table
  use XEGTBL; // table
  use XUMTBL; // table
  use XSLTBL; // table
  use SL02M.SL02M001,  // forms
      SL02M.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPSL001: XPSL001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/

    set SL02M001 initial; /* M001 input/output*/
    set SL02W00 empty; /* Working storage*/
    set SL02W01 empty; /* M001 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set TA5REC empty; /* Temporary storage parameters*/

    SL02W00.SL02W-FORMAT-MAP-SW = "Y"; /* Format the map switch*/
    SL02W00.SL02W-RQT-DATA-SW = "N"; /* Request for data switch*/

     /* ------------------------------------------------------------*/
     /* If all key info is passed in SLWREC, use as initial key*/
     /* else, set cursor to needed field*/
     /* ------------------------------------------------------------*/

    if (SLWREC.SY1STNBR > " ") /* style nbr is passed*/
      SL02W00.SL02W-RQT-DATA-SW = "Y"; /* request for data switch*/
    end


    if (SLWREC.XWHCD > " ") /* if whse code is passed in slwrec*/
      SL02M001.SL02M-XWH-WHSE-CD = SLWREC.XWHCD; /* move it to work area*/
    else
      if (COMMAREA.XWHCD > " ") /* if whse code is passed in commarea*/
        SL02M001.SL02M-XWH-WHSE-CD = COMMAREA.XWHCD; /* move towrk area*/
      end
    end


    if (SLWREC.XGPCD > " ") /* if gpc code is passed in slwrec*/
      SL02M001.SL02M-XGP-GPC-CD = SLWREC.XGPCD; /* move to work area*/
    else
      if (COMMAREA.XGPCD > " ") /* if gpc code is passed in commarea*/
        SL02M001.SL02M-XGP-GPC-CD = COMMAREA.XGPCD; /* move to work area*/
      else
        if (SL02M001.SL02M-XWH-WHSE-CD == "51") /* if warehouse 51*/
          SL02M001.SL02M-XGP-GPC-CD = "03"; /* move apparel code to gpc*/
        else
          SL02M001.SL02M-XGP-GPC-CD = "02"; /* move footwr code to gpc*/
        end
      end
    end


    if (SLWREC.SY1STNBR > " ") /* if style nbr is passed in slwrec*/
      SL02M001.SL02M-SY-STY-NBR = SLWREC.SY1STNBR; /* move to work area*/
    end


    if (SLWREC.SY2CLRID > " ") /* if color code is passed*/
      SL02M001.SL02M-SY-COLR-CD-ID = SLWREC.SY2CLRID; /* move to ws*/
    else
      SL02M001.SL02M-SY-COLR-CD-ID = "00 "; /* move to ws*/
    end


    if (SLWREC.XDMCD > " ") /* if dimension code is passed*/
      SL02M001.SL02M-XDM-DIM-CD = SLWREC.XDMCD; /* move to work area*/
    else
      SL02M001.SL02M-XDM-DIM-CD = "00"; /* default to dimension code 00*/
    end


    if (SLWREC.XQACD > " ") /* if quality code is passed*/
      SL02M001.SL02M-XQA-QUAL-CD = SLWREC.XQACD; /* move to work area*/
    else
      SL02M001.SL02M-XQA-QUAL-CD = "01"; /* default to quality code 01*/
    end


    if (SLWREC.XOWCD > " ") /* if own group is passed*/
      SL02M001.SL02M-XOW-OWN-GRP-CD = SLWREC.XOWCD; /* move to ws*/
    else
      SL02M001.SL02M-XOW-OWN-GRP-CD = "00";
    end


    if (SLWREC.PC2INSEG > " ") /* inventory segment is passed*/
      SL02M001.SL02M-PC-CU-IN-SEG-CD = SLWREC.PC2INSEG; /* move to ws*/
    else
      SL02M001.SL02M-PC-CU-IN-SEG-CD = "000"; /* move to ws*/
    end


    if (SLWREC.XEG-ENGR-DIFF-CD > " ") /* engr diff cd passed*/
      SL02M001.SL02M-XEG-ENGR-DIFF-CD = SLWREC.XEG-ENGR-DIFF-CD;
    else
      SL02M001.SL02M-XEG-ENGR-DIFF-CD = "000";
    end


    if (SLWREC.XUMCD > " ") /* unit of measure passed*/
      SL02M001.SL02M-XUM-UOM-CD = SLWREC.XUMCD; /* move to ws*/
    else
      SL02M001.SL02M-XUM-UOM-CD = "01"; /* move to ws*/
    end


    if (SLWREC.XSLCD > " ") /* class code is passed*/
      SL02M001.SL02M-XSL-CLASS-CD = SLWREC.XSLCD; /* move to ws*/
    else
      SL02M001.SL02M-XSL-CLASS-CD = "000"; /* move to ws*/
    end

    if (SLWREC.PC-SAP-PO-NBR > " ") /* Sap PO Number is passed*/
      SL02M001.SL02M-PC-SAP-PO-NBR = SLWREC.PC-SAP-PO-NBR; /* move to ws*/
    else
      SL02M001.SL02M-PC-SAP-PO-NBR = "*"; /* move to ws*/
    end


    if (SLWREC.PC3POIDT > 0   /* po month range*/
     && SLWREC.PC4ORTYP > " " /* po type passed*/
     && SLWREC.PCFSECD > " "  /* season codeis passed*/
     && SLWREC.FY1FCTYC > " ") /* factory code is passed*/

      SL02W00.SL02W-PC-PO-ID-MO-RNG = SLWREC.PC3POIDT; /* date range*/
      SL02W00.SL02W-PC-PO-OT-CD = SLWREC.PC4ORTYP; /* move type to ws*/
      SL02W00.SL02W-PC-PO-ID-SESN-CD = SLWREC.PCFSECD; /* season cd*/
      SL02W00.SL02W-FY-FCTY-CD = SLWREC.FY1FCTYC; /* factory cd*/
      SL02M001.SL02M-PC-PO-NBR = -1;

      SL02W00.SL02W-FORMAT-DISPLAY-PO-SW = "Y";
    /* flag to format po info into po id and po type for map display*/

    else
      if (SLWREC.PC3PONBR > 0)
        SL02M001.SL02M-PC-PO-NBR = SLWREC.PC3PONBR;
        SL02M001.SL02M-PO-TYPE = "*"; /* wildcard for map display*/
        SL02M001.SL02M-PO-ID = "*";
        SL02W00.SL02W-PC-PO-ID-MO-RNG = -1;
        SL02W00.SL02W-PC-PO-ID-SESN-CD = "*"; /* season cd*/
        SL02W00.SL02W-FY-FCTY-CD = "*"; /* factory cd*/
      else

        SL02M001.SL02M-PO-TYPE = "*"; /* wildcard for map display*/
        SL02M001.SL02M-PO-ID = "*";
        SL02M001.SL02M-PC-PO-NBR = -1;
        SL02W00.SL02W-PC-PO-ID-MO-RNG = -1;
        SL02W00.SL02W-PC-PO-ID-SESN-CD = "*"; /* season cd*/
        SL02W00.SL02W-FY-FCTY-CD = "*"; /* factory cd*/

      end
    end

     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto SL02001; /* Converse the main process*/


    SL02001: SL02001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto SL02001; /* Converse the main process*/


  end // end main
end // end SL02

// Obtain the data to display
Function SL02-100()
   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/

  if (SL02W00.SL02W-RQT-DATA-SW == "Y") /* Data was requested*/

    SL02-120(); /* Purge any temporary storage*/

  end

  SL02W01.SL02W-XWH-WHSE-CD = SL02M001.SL02M-XWH-WHSE-CD; /* whse cd*/
  SL02W01.SL02W-XGP-GPC-CD = SL02M001.SL02M-XGP-GPC-CD; /* GPC code*/
  SL02W01.SL02W-SY-STY-NBR = SL02M001.SL02M-SY-STY-NBR; /* Style nbr*/
  SL02W01.SL02W-XQA-QUAL-CD = SL02M001.SL02M-XQA-QUAL-CD; /* qual cd*/
  SL02W01.SL02W-SY-COLR-CD-ID = SL02M001.SL02M-SY-COLR-CD-ID;
  SL02W01.SL02W-XDM-DIM-CD = SL02M001.SL02M-XDM-DIM-CD; /* dimens. cd*/
  SL02W01.SL02W-XOW-OWN-GRP-CD = SL02M001.SL02M-XOW-OWN-GRP-CD;
  SL02W01.SL02W-PC-CU-IN-SEG-CD = SL02M001.SL02M-PC-CU-IN-SEG-CD;
  SL02W01.SL02W-XEG-ENGR-DIFF-CD = SL02M001.SL02M-XEG-ENGR-DIFF-CD;
  SL02W01.SL02W-XUM-UOM-CD = SL02M001.SL02M-XUM-UOM-CD;
  SL02W01.SL02W-XSL-CLASS-CD = SL02M001.SL02M-XSL-CLASS-CD;
  SL02W01.SL02W-PO-ID = SL02M001.SL02M-PO-ID;
  SL02W01.SL02W-PO-TYPE = SL02M001.SL02M-PO-TYPE;
  SL02W01.SL02W-PC-PO-NBR = SL02M001.SL02M-PC-PO-NBR;
  SL02W01.SL02W-PC-SAP-PO-NBR = SL02M001.SL02M-PC-SAP-PO-NBR;

   /* ------------------------------------------------------------*/
   /* get warehouse code*/
   /* ------------------------------------------------------------*/

  if (SL02W01.SL02W-XWH-WHSE-CD in XWHTBL.XWHCD)
    SL02W01.SL02W-XWH-WHSE-NM = XWHTBL.XWHABRV[sysVar.arrayIndex];
    SLWREC.XCTCD = XWHTBL.XCTCD[sysVar.arrayIndex]; /* CANA save country code*/
  end

   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (SL02W00.SL02W-RQT-DATA-SW != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* ------------------------------------------------------------*/

  SL02W00.XXXTSLEN = 948; /* Temporary storage page length*/

   /* ------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* ------------------------------------------------------------*/

  if (SLWREC.SLWMSGNO == 0) /* No message to display*/
    SL02W00.XXXTSPAG = 1; /* Page number to display*/
  end

   /* ------------------------------------------------------------*/
   /* get current date to use in determining life cycle code.*/
   /* ------------------------------------------------------------*/

  SL02-110(); /* get current date*/

   /* ------------------------------------------------------------*/
   /* if po info passed in slwrec, format for map display.*/
   /* else - format from user input to po nbr components.*/
   /* ------------------------------------------------------------*/

  if (SL02W00.SL02W-FORMAT-DISPLAY-PO-SW == "Y") /* if po info is*/
                                  /* passedand needs to be formatted*/
    set SWVREC empty; /* initialize swvrec*/

    TA1REC.TA1PGMNM = "SL02-100"; /* calling process for diagnosis*/

    SWVREC.SWVWFNC = "T2"; /* indicate function to perform*/

    SWVREC.XGPCD = SL02W01.SL02W-XGP-GPC-CD; /* gpc cd*/
    SWVREC.PC3POIDT = SL02W00.SL02W-PC-PO-ID-MO-RNG; /* date range*/
    SWVREC.PCFSECD = SL02W00.SL02W-PC-PO-ID-SESN-CD; /* /season code*/
    SWVREC.FY1FCTYC = SL02W00.SL02W-FY-FCTY-CD; /* factory code*/


    XSSLS10(); /* subroutine to format po info*/

    SL02W01.SL02W-PO-ID = SWVREC.SWVWPOID; /* formatted po id*/
    SL02W01.SL02W-PO-TYPE = SWVREC.PC4ORTYP; /* formatted po type*/

    SL02W00.SL02W-FORMAT-DISPLAY-PO-SW = "N";

  else

    if (SL02W01.SL02W-PO-ID != "*"
     && SL02W01.SL02W-PO-TYPE != "*") /* if map contains wildcards in*/

      set SWVREC empty; /* initialize swvrec*/

      TA1REC.TA1PGMNM = "SL02-100"; /* calling process for diagnosis*/

      SWVREC.SWVWFNC = "T1"; /* indicate function to perform*/
      SWVREC.XGPCD = SL02W01.SL02W-XGP-GPC-CD; /* gpc cd*/
      SWVREC.SWVWPOID = SL02W01.SL02W-PO-ID; /* po id*/

      XSSLS10(); /* subroutine to format po info*/

      SL02W00.SL02W-PC-PO-ID-MO-RNG = SWVREC.PC3POIDT; /* date*/
      /* MOVE SWVREC.PCFSECD TO SL02W00.SL02W-PC-PO-ID-SESN-CD;/* season*/
      SL02W00.SL02W-FY-FCTY-CD = SWVREC.FY1FCTYC; /* factory*/
      /* MOVE SWVREC.PC4ORTYP TO SL02W00.SL02W-PC-PO-OT-CD;/*ordertype*/

    end
  end

   /* ------------------------------------------------------------*/
   /* Select the information to display in the map heading.*/
   /* ------------------------------------------------------------*/

  SL02-130(); /* Select the heading information*/

  SL02W01.SL02W-SY-STY-NM = PKNREC.SY1SSTNM; /* Style name*/
  SL02W01.SL02W-STY-COLR-NM = PKNREC.SY2CLRAB; /* color name*/

   /* ------------------------------------------------------------*/
   /* Determine life cycle code from current and effective dates*/
   /* ------------------------------------------------------------*/

  if (PKNREC.SY2CEDT1 <= SL02W00.SL02W-CURRENT-DATE)
    SL02W01.SL02W-LIFE-CYCLE = PKNREC.XLCCD1;
    SL02W00.SL02W-MAP-LIFE-CYCLE = PKNREC.XLCCD1;
  else

    if (PKNREC.SY2CEDT2 <= SL02W00.SL02W-CURRENT-DATE)
      SL02W01.SL02W-LIFE-CYCLE = PKNREC.XLCCD2;
      SL02W00.SL02W-MAP-LIFE-CYCLE = PKNREC.XLCCD2;
    else

      if (PKNREC.SY2CEDT3 <= SL02W00.SL02W-CURRENT-DATE)
        SL02W01.SL02W-LIFE-CYCLE = PKNREC.XLCCD3;
        SL02W00.SL02W-MAP-LIFE-CYCLE = PKNREC.XLCCD3;
      else

        if (PKNREC.SY2CEDT4 <= SL02W00.SL02W-CURRENT-DATE)
          SL02W01.SL02W-LIFE-CYCLE = PKNREC.XLCCD4;
          SL02W00.SL02W-MAP-LIFE-CYCLE = PKNREC.XLCCD4;
        end

      end
    end
  end

   /* ------------------------------------------------------------*/
   /* Select and open the cursor for the set to be formatted.*/
   /* ------------------------------------------------------------*/

  SL02-140(); /* Select set and open cursor*/

   /* ------------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ------------------------------------------------------------*/

  SL02W00.SL02W-END-CURSOR-SW = "N"; /* End of cursor switch*/
  SL02W00.SL02W-FIRST-FETCH = "Y"; /* first fetch indicator*/
  SL02W00.SL02W-MAP-FIELD-INX = 1; /* Display line index*/
  SL02W00.SL02W-HOLD-SIZE = 0; /* initialize hold size*/
  while (SL02W00.SL02W-END-CURSOR-SW == "N") /* Until end of cursor*/

    SL02-150(); /* Fetch next row in the cursor*/

    /* ------------------------------------------------------------*/
     /* If not end of cursor, save the data in display storage.*/
    /* ------------------------------------------------------------*/

    if (SL02W00.SL02W-END-CURSOR-SW == "N") /* Not end of cursor*/
      SL02-160(); /* Save data in display storage*/
    end

    /* ------------------------------------------------------------*/
     /* If the page is full, add the page to the storage queue.*/
    /* ------------------------------------------------------------*/

    if (SL02W00.SL02W-MAP-FIELD-INX > 13) /* Page is full*/

      SL02-170(); /* Add page to temporary storage*/

      SL02W00.SL02W-MAP-FIELD-INX = 1; /* Display line index*/

    end

  end

   /* ------------------------------------------------------------*/
   /* save header information if only one line on last page*/
   /* ------------------------------------------------------------*/

  if (SL02W00.TA5TSQIX > 0 /* Not the first page*/
   && SL02W00.SL02W-MAP-FIELD-INX == 1) /* and first line on page*/

    SL02W01.SL02W-XWH-WHSE-CD = SL02M001.SL02M-XWH-WHSE-CD; /* whse cd*/
    SL02W01.SL02W-XGP-GPC-CD = SL02M001.SL02M-XGP-GPC-CD; /* GPC code*/
    SL02W01.SL02W-SY-STY-NBR = SL02M001.SL02M-SY-STY-NBR; /* Style nbr*/
    SL02W01.SL02W-XQA-QUAL-CD = SL02M001.SL02M-XQA-QUAL-CD; /* qual cd*/
    SL02W01.SL02W-SY-COLR-CD-ID = SL02M001.SL02M-SY-COLR-CD-ID;
    SL02W01.SL02W-XDM-DIM-CD = SL02M001.SL02M-XDM-DIM-CD;
                                   /* dimens. cd*/
    SL02W01.SL02W-XOW-OWN-GRP-CD = SL02M001.SL02M-XOW-OWN-GRP-CD;
    SL02W01.SL02W-PC-CU-IN-SEG-CD = SL02M001.SL02M-PC-CU-IN-SEG-CD;
    SL02W01.SL02W-XEG-ENGR-DIFF-CD = SL02M001.SL02M-XEG-ENGR-DIFF-CD;
    SL02W01.SL02W-XUM-UOM-CD = SL02M001.SL02M-XUM-UOM-CD;
    SL02W01.SL02W-XSL-CLASS-CD = SL02M001.SL02M-XSL-CLASS-CD;
    SL02W01.SL02W-PO-ID = SL02M001.SL02M-PO-ID;
    SL02W01.SL02W-PO-TYPE = SL02M001.SL02M-PO-TYPE;
    SL02W01.SL02W-PC-PO-NBR = SL02M001.SL02M-PC-PO-NBR;
    SL02W01.SL02W-PC-SAP-PO-NBR = SL02M001.SL02M-PC-SAP-PO-NBR;
    SL02W01.SL02W-LIFE-CYCLE = SL02W00.SL02W-MAP-LIFE-CYCLE;
    if (SL02W01.SL02W-XWH-WHSE-CD in XWHTBL.XWHCD)
      SL02W01.SL02W-XWH-WHSE-NM = XWHTBL.XWHABRV[sysVar.arrayIndex];
      SLWREC.XCTCD = XWHTBL.XCTCD[sysVar.arrayIndex]; /* CANA save country code*/
    end
    SL02W01.SL02W-SY-STY-NM = PKNREC.SY1SSTNM; /* Style name*/
    SL02W01.SL02W-STY-COLR-NM = PKNREC.SY2CLRAB; /* color name*/

  end

   /* ------------------------------------------------------------*/
   /* display last line accumulated*/
   /* ------------------------------------------------------------*/

  SL02-161(); /* convert size code to size*/

  if (SYPREC.SYPRC == 0) /* if no error*/
    SL02W01.SL02W-SIZES[SL02W-MAP-FIELD-INX] = SYPREC.SY4IDDSC;
  else
    SL02W01.SL02W-SIZES[SL02W-MAP-FIELD-INX] = "*NF*"; /* not found*/
  end

  SL02W00.SL02W-CALC-NET-QTY = SL02W00.SL02W-ACCUM-OH-QTY -   SL02W00.SL02W-ACCUM-RSRV-QTY - SL02W00.SL02W-ACCUM-SAS-QTY;

  SL02W01.SL02W-NET-QTY[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-CALC-NET-QTY;
  SL02W01.SL02W-SL-SY-LOC-OH-QTY[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-ACCUM-OH-QTY;
  SL02W01.SL02W-SL-SY-LOC-RSRV-QTY[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-ACCUM-RSRV-QTY;
  SL02W01.SL02W-SL-SY-LOC-SAS-QTY[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-ACCUM-SAS-QTY;

  SL02W01.SL02W-UNPCS[SL02W-MAP-FIELD-INX] = SL02W00.SL02W-UNPCS-LINE;

  SL02W00.SL02W-MAP-FIELD-INX = SL02W00.SL02W-MAP-FIELD-INX + 1;

   /* ------------------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ------------------------------------------------------------*/

  if (SL02W00.SL02W-MAP-FIELD-INX > 1) /* Page was formatted*/
    SL02-170(); /* Add page to temporary storage*/
  end

   /* ------------------------------------------------------------*/
   /* Close the cursor for the selected set.*/
   /* ------------------------------------------------------------*/

  SL02-180(); /* Close the selected set cursor*/

   /* ------------------------------------------------------------*/
   /* If no pages were formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/

  if (SL02W00.TA5TSQIX == 0) /* No pages formatted*/

    SLWREC.XXXLOCID = "100"; /* Module identification*/
    SLWREC.SLWMSGNO = 44; /* No data was found to display*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  SL02W00.SL02W-RQT-DATA-SW = "N"; /* Request for data switch*/


end // end SL02-100

// get current date
Function SL02-110()
  /* -----------------------------------------------------------------------*/
   /* obtain current date (ccyymmdd) using TA0040*/
  /* -----------------------------------------------------------------------*/

  set TA2REC empty; /* clear TA2REC*/

