// DRIVER FOR APPLICATION
Function DJ02P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DJ02P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DJ02P20-BUILD-MAP2(); /* Build the second map*/
  else
    DJ02P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DJ02P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DJ02P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DJ02P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DJ02P00 *******************************/
  /* **************/
end // end DJ02P00-DRIVER

// INITIALIZE THE APPLICATION
Function DJ02P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DJ02W01 empty; /* Working storage record*/
  set DJ02W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DJ02M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ02M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ02M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DJ02M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ02M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ02M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DJ02M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DJ02P10 ******************************/
  /* **************/
end // end DJ02P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DJ02P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DJ02P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DJ02M001.DAW-CURR-PAGE)
    DJ02S-TS-READ-PAGE(); /* read temp storage page*/
    DJ02P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DJ02P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ02P20 **********************************/
  /* **************/
end // end DJ02P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DJ02P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DJ02M002.DA-CHNG-USER-ID = sysVar.userID;
  DJ02M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DJ02W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ02M002.DA-PLOG-TXT1 = " ";
    DJ02M002.DA-PLOG-TXT2 = " ";
    DJ02M002.YDUCD = " ";
    DJ02M002.YDUABRV = " ";
    DJ02M002.YDUSABRV = " ";
    DJ02M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DJ02W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DJ02W01.YDUGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DJ02W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DJ02W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DJ02M002.YDUCD = DJ02W01.YDUCD; /* move to the map*/
      DJ02M002.YDUABRV = DJ02W01.YDUABRV;
      DJ02M002.YDUSABRV = DJ02W01.YDUSABRV;
      DJ02M002.XXXSTAT = DJ02W01.XXXSTAT;
      DJ02M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DJ02M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DJ02M002.YDUCD = DJ02W02.YDUCD[DAW-ROW-NBR];
                                         /* get the tbl row*/
        if (DJ02M002.YDUCD in YDUTBL.YDUCD)
          DJ02W01.YDUCD = YDUTBL.YDUCD[sysVar.arrayIndex]; /* save for a*/
          DJ02W01.YDUABRV = YDUTBL.YDUABRV[sysVar.arrayIndex]; /* later compare*/
          DJ02W01.YDUSABRV = YDUTBL.YDUSABRV[sysVar.arrayIndex];
          DJ02W01.XXXSTAT = YDUTBL.XXXSTAT[sysVar.arrayIndex];
          DJ02W01.ZZZSDT = YDUTBL.ZZZSDT[sysVar.arrayIndex];
          DJ02W01.ZZZCHGDT = YDUTBL.ZZZCHGDT[sysVar.arrayIndex];
          DJ02W01.ZZZCHGTM = YDUTBL.ZZZCHGTM[sysVar.arrayIndex];
          DJ02W01.ZZZCHGCT = YDUTBL.ZZZCHGCT[sysVar.arrayIndex];

          DJ02M002.YDUABRV = YDUTBL.YDUABRV[sysVar.arrayIndex]; /* move to map*/
          DJ02M002.YDUSABRV = YDUTBL.YDUSABRV[sysVar.arrayIndex];
          DJ02M002.XXXSTAT = YDUTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DJ02W01.YDUGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DJ02W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DJ02W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DJ02M002.YDUCD = DJ02W01.YDUCD; /* move to the map*/
    DJ02M002.YDUABRV = DJ02W01.YDUABRV;
    DJ02M002.YDUSABRV = DJ02W01.YDUSABRV;
    DJ02M002.XXXSTAT = DJ02W01.XXXSTAT;
    DJ02M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DJ02M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DJ02M002.XXXSTAT == "A")
    DJ02M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DJ02M002.XXXSTAT == "I")
    DJ02M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DJ02M002.XXXSTAT == " ")
    DJ02M002.DAW-TBL-STATUS-DESC = " ";
  end

  DJ02P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ02P20 **********************************/
  /* **************/
end // end DJ02P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DJ02P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DJ02W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ02P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ02S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YDUTBL.YDUCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ02P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ02S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows eYDUt*/
      DJ02S-TS-ADD-PAGE();
    end

    DJ02M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DJ02M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DJ02P22 **********************************/
  /* **************/
end // end DJ02P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DJ02P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DJ02W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DJ02W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DJ02W01.YDUGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DJ02W02.YDUCD[DAW-ROW-NBR] = DJ02W01.YDUCD;
  DJ02W02.YDUABRV[DAW-ROW-NBR] = DJ02W01.YDUABRV;
  DJ02W02.YDUSABRV[DAW-ROW-NBR] = DJ02W01.YDUSABRV;
  DJ02W02.XXXSTAT[DAW-ROW-NBR] = DJ02W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DJ02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DJ02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DJ02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DJ02W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DJ02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DJ02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DJ02W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DJ02W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DJ02W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DJ02W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DJ02P221 ******************************/
  /* ***************/
end // end DJ02P23-DB2-TO-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DJ02P24-CSP-TO-TSQ()
  /* **************************************************************/
  /* Move table row to temp storage*/
  /* **************************************************************/

  DJ02W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TBL ";
  DJ02W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";

