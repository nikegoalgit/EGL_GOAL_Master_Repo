  end

  /* **************/
  /* ***************** END DJ02P71 **********************************/
  /* **************/
end // end DJ02P71-COMPARE

// MOVE TEMP STORAGE PAGE TO MAP
Function DJ02P71-TSQ-TO-MAP()
  /* ************************************************************/
  /* Move temp storage page to map*/
  /* ************************************************************/

  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DJ02M001.DAW-SELECT-CD[sysVar.arrayIndex] = DJ02W02.DAW-SELECT-CD[sysVar.arrayIndex];
    DJ02M001.YDUCD[sysVar.arrayIndex] = DJ02W02.YDUCD[sysVar.arrayIndex];
    DJ02M001.YDUABRV[sysVar.arrayIndex] = DJ02W02.YDUABRV[sysVar.arrayIndex];
    DJ02M001.YDUSABRV[sysVar.arrayIndex] = DJ02W02.YDUSABRV[sysVar.arrayIndex];
    DJ02M001.XXXSTAT[sysVar.arrayIndex] = DJ02W02.XXXSTAT[sysVar.arrayIndex];
    DJ02M001.DAW-STATUS-DESC[sysVar.arrayIndex] = DJ02W02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DJ02M001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DJ02W02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DJ02M001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DJ02W02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  /* **************/
  /* ****************** END DJ02P71 *****************************/
  /* **************/
end // end DJ02P71-TSQ-TO-MAP

// BUILD NEW VDATRN01 RECORD
Function DJ02P72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DJ02M002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DJ02M002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DJ02W01.DA-CCAT-KEY-TXT = " ";
  DJ02W01.DJ02W-KEY-YDUCD = DJ02M002.YDUCD;
  VDATRN01.DA-CCAT-KEY-TXT = DJ02W01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DJ02W01.YDUCD = DJ02M002.YDUCD;
  DJ02W01.YDUABRV = DJ02M002.YDUABRV;
  DJ02W01.YDUSABRV = DJ02M002.YDUSABRV;
  DJ02W01.XXXSTAT = DJ02M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 33;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DJ02W01.YDUGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DJ02P72 **********************************/
  /* **************/
end // end DJ02P72-NEW-TRAN

