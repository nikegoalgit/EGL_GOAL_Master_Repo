Program DJ0G type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DJ0GW01 DJ0GW01; // record
  DJ0GW02 DJ0GW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XDSTBL {deleteAfterUse = yes}; // table
  use DJ0GM.DJ0GM001,  // forms
      DJ0GM.DJ0GM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DJ0GP00-DRIVER: DJ0GP00-DRIVER();
    exit program;
  end // end main
end // end DJ0G

// DRIVER FOR APPLICATION
Function DJ0GP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DJ0GP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DJ0GP20-BUILD-MAP2(); /* Build the second map*/
  else
    DJ0GP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DJ0GP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DJ0GP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DJ0GP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DJ0GP00 *******************************/
  /* **************/
end // end DJ0GP00-DRIVER

// INITIALIZE THE APPLICATION
Function DJ0GP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DJ0GW01 empty; /* Working storage record*/
  set DJ0GW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DJ0GM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ0GM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ0GM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DJ0GM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ0GM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ0GM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DJ0GM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DJ0GP10 ******************************/
  /* **************/
end // end DJ0GP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DJ0GP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DJ0GP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DJ0GM001.DAW-CURR-PAGE)
    DJ0GS-TS-READ-PAGE(); /* read temp storage page*/
    DJ0GP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DJ0GP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ0GP20 **********************************/
  /* **************/
end // end DJ0GP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DJ0GP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DJ0GM002.DA-CHNG-USER-ID = sysVar.userID;
  DJ0GM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DJ0GW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ0GM002.DA-PLOG-TXT1 = " ";
    DJ0GM002.DA-PLOG-TXT2 = " ";
    DJ0GM002.XDSCD = " ";
    DJ0GM002.XDSDESC = " ";
    DJ0GM002.XDSABRV = " ";
    DJ0GM002.XDSWHSEF = " ";
    DJ0GM002.XDSPCT = 0;
    DJ0GM002.XDS-MAX-DIRSHP-PCT = 0;
    DJ0GM002.GLFCD = " ";
    DJ0GM002.XDSAVDFL = " ";
    DJ0GM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DJ0GW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DJ0GW01.XDSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DJ0GW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DJ0GW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DJ0GM002.XDSCD = DJ0GW01.XDSCD; /* move to the map*/
      DJ0GM002.XDSDESC = DJ0GW01.XDSDESC;
      DJ0GM002.XDSABRV = DJ0GW01.XDSABRV;
      DJ0GM002.XDSWHSEF = DJ0GW01.XDSWHSEF;
      DJ0GM002.XDSPCT = DJ0GW01.XDSPCT;
      DJ0GM002.XDS-MAX-DIRSHP-PCT = DJ0GW01.XDS-MAX-DIRSHP-PCT;
      DJ0GM002.GLFCD = DJ0GW01.GLFCD;
      DJ0GM002.XDSAVDFL = DJ0GW01.XDSAVDFL;
      DJ0GM002.XXXSTAT = DJ0GW01.XXXSTAT;
      DJ0GM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DJ0GM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DJ0GM002.XDSCD = DJ0GW02.XDSCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DJ0GM002.XDSCD in XDSTBL.XDSCD)
          DJ0GW01.XDSCD = XDSTBL.XDSCD[sysVar.arrayIndex]; /* save for a*/
          DJ0GW01.XDSDESC = XDSTBL.XDSDESC[sysVar.arrayIndex]; /* later compare*/
          DJ0GW01.XDSABRV = XDSTBL.XDSABRV[sysVar.arrayIndex];
          DJ0GW01.XDSWHSEF = XDSTBL.XDSWHSEF[sysVar.arrayIndex];
          DJ0GW01.XDSPCT = XDSTBL.XDSPCT[sysVar.arrayIndex];
          DJ0GW01.XDS-MAX-DIRSHP-PCT = XDSTBL.XDS-MAX-DIRSHP-PCT[sysVar.arrayIndex];
          DJ0GW01.GLFCD = XDSTBL.GLFCD[sysVar.arrayIndex];
          DJ0GW01.XDSAVDFL = XDSTBL.XDSAVDFL[sysVar.arrayIndex];
          DJ0GW01.XXXSTAT = XDSTBL.XXXSTAT[sysVar.arrayIndex];
          DJ0GW01.ZZZSDT = XDSTBL.ZZZSDT[sysVar.arrayIndex];
          DJ0GW01.ZZZCHGDT = XDSTBL.ZZZCHGDT[sysVar.arrayIndex];
          DJ0GW01.ZZZCHGTM = XDSTBL.ZZZCHGTM[sysVar.arrayIndex];
          DJ0GW01.ZZZCHGCT = XDSTBL.ZZZCHGCT[sysVar.arrayIndex];

          DJ0GM002.XDSDESC = XDSTBL.XDSDESC[sysVar.arrayIndex]; /* move to map*/
          DJ0GM002.XDSABRV = XDSTBL.XDSABRV[sysVar.arrayIndex];
          DJ0GM002.XDSWHSEF = XDSTBL.XDSWHSEF[sysVar.arrayIndex];
          DJ0GM002.XDSPCT = XDSTBL.XDSPCT[sysVar.arrayIndex];
          DJ0GM002.XDS-MAX-DIRSHP-PCT = XDSTBL.XDS-MAX-DIRSHP-PCT[sysVar.arrayIndex];
          DJ0GM002.GLFCD = XDSTBL.GLFCD[sysVar.arrayIndex];
          DJ0GM002.XDSAVDFL = XDSTBL.XDSAVDFL[sysVar.arrayIndex];
          DJ0GM002.XXXSTAT = XDSTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DJ0GW01.XDSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DJ0GW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DJ0GW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DJ0GM002.XDSCD = DJ0GW01.XDSCD; /* move to the map*/
    DJ0GM002.XDSDESC = DJ0GW01.XDSDESC;
    DJ0GM002.XDSABRV = DJ0GW01.XDSABRV;
    DJ0GM002.XDSWHSEF = DJ0GW01.XDSWHSEF;
    DJ0GM002.XDSPCT = DJ0GW01.XDSPCT;
    DJ0GM002.XDS-MAX-DIRSHP-PCT = DJ0GW01.XDS-MAX-DIRSHP-PCT;
    DJ0GM002.GLFCD = DJ0GW01.GLFCD;
    DJ0GM002.XDSAVDFL = DJ0GW01.XDSAVDFL;
    DJ0GM002.XXXSTAT = DJ0GW01.XXXSTAT;
    DJ0GM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DJ0GM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DJ0GM002.XXXSTAT == "A")
    DJ0GM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DJ0GM002.XXXSTAT == "I")
    DJ0GM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DJ0GM002.XXXSTAT == " ")
    DJ0GM002.DAW-TBL-STATUS-DESC = " ";
  end

  DJ0GP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ0GP20 **********************************/
  /* **************/
end // end DJ0GP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DJ0GP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DJ0GW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ0GP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ0GS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XDSTBL.XDSCD[sysVar.arrayIndex] != "9") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ0GP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ0GS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DJ0GS-TS-ADD-PAGE();
    end

    DJ0GM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DJ0GM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DJ0GP22 **********************************/
  /* **************/
end // end DJ0GP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DJ0GP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DJ0GW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DJ0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DJ0GW01.XDSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DJ0GW02.XDSCD[DAW-ROW-NBR] = DJ0GW01.XDSCD;
  DJ0GW02.XDSDESC[DAW-ROW-NBR] = DJ0GW01.XDSDESC;
  DJ0GW02.XDSABRV[DAW-ROW-NBR] = DJ0GW01.XDSABRV;
  DJ0GW02.XDSWHSEF[DAW-ROW-NBR] = DJ0GW01.XDSWHSEF;
  DJ0GW02.XDSAVDFL[DAW-ROW-NBR] = DJ0GW01.XDSAVDFL;
  DJ0GW02.XXXSTAT[DAW-ROW-NBR] = DJ0GW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DJ0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DJ0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DJ0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DJ0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DJ0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DJ0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DJ0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DJ0GW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DJ0GW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DJ0GW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DJ0GP221 ******************************/
  /* ***************/
end // end DJ0GP23-DB2-TO-TSQ

