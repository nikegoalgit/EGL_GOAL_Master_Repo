// SET MAP 1 FIELD ATTRIBUTES
Function DJ0GP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DJ0GM001.DAW-PAGE-LIT = "PAGE";
    set DJ0GM001.DAW-CURR-PAGE initialAttributes;
    DJ0GM001.DAW-OF-LIT = "OF";
    set DJ0GM001.DAW-LAST-PAGE initialAttributes;
    DJ0GM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DJ0GM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DJ0GM001.DAW-PAGE-LIT = " ";
    set DJ0GM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DJ0GM001.DAW-OF-LIT = " ";
    set DJ0GM001.DAW-LAST-PAGE skip, invisible;
    DJ0GM001.DAW-PF7-LIT = " ";
    DJ0GM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DJ0GM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DJ0GM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DJ0GM001.XDSCD[sysVar.arrayIndex] == " "
       && DJ0GM001.XDSDESC[sysVar.arrayIndex] == " ")
        set DJ0GM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
      else
        set DJ0GM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DJ0GP25 **************************/
  /* **************/
end // end DJ0GP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DJ0GP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DJ0GM002.XDSCD initialAttributes;
    set DJ0GM002.XDSDESC initialAttributes;
    set DJ0GM002.XDSABRV initialAttributes;
    set DJ0GM002.XDSWHSEF initialAttributes;
    set DJ0GM002.XDSPCT initialAttributes;
    set DJ0GM002.XDS-MAX-DIRSHP-PCT initialAttributes;
    set DJ0GM002.GLFCD initialAttributes;
    set DJ0GM002.XDSAVDFL initialAttributes;
    set DJ0GM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DJ0GM002.XDSCD skip;
    set DJ0GM002.XDSDESC skip;
    set DJ0GM002.XDSABRV skip;
    set DJ0GM002.XDSWHSEF skip;
    set DJ0GM002.XDSPCT skip;
    set DJ0GM002.XDS-MAX-DIRSHP-PCT skip;
    set DJ0GM002.GLFCD skip;
    set DJ0GM002.XDSAVDFL skip;
    set DJ0GM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DJ0GM002.XDSCD skip;
    set DJ0GM002.XDSDESC initialAttributes;
    set DJ0GM002.XDSABRV initialAttributes;
    set DJ0GM002.XDSWHSEF initialAttributes;
    set DJ0GM002.XDSPCT initialAttributes;
    set DJ0GM002.XDS-MAX-DIRSHP-PCT initialAttributes;
    set DJ0GM002.GLFCD initialAttributes;
    set DJ0GM002.XDSAVDFL initialAttributes;
    set DJ0GM002.XXXSTAT initialAttributes;
  end

  set DJ0GM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DJ0GM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ0GM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DJ0GM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DJ0GM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DJ0GM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
