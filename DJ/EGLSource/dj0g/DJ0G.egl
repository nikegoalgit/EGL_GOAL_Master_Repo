  /* ************************************************************/

  TA5REC.TA5APPID = "DJ0G"; /* appl identification*/
  TA5REC.TA5FNCCD = "A"; /* function code*/
  DJ0GW02.DAW-TEMP-STORAGE-LENGTH = 1236;

  call "TA0050" (DJ0GW02, TA5REC) {isNoRefresh = yes, isExternal = yes}; /* temp storage subroutine*/

  if (TA5REC.TA5RTNCD == 0)
    DAWREC.DAW-MAX-PAGE = TA5REC.TA5TSQIX;
