// SET MAP 1 FIELD ATTRIBUTES
Function DJ0IP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DJ0IM001.DAW-PAGE-LIT = "PAGE";
    set DJ0IM001.DAW-CURR-PAGE initialAttributes;
    DJ0IM001.DAW-OF-LIT = "OF";
    set DJ0IM001.DAW-LAST-PAGE initialAttributes;
    DJ0IM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DJ0IM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DJ0IM001.DAW-PAGE-LIT = " ";
    set DJ0IM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DJ0IM001.DAW-OF-LIT = " ";
    set DJ0IM001.DAW-LAST-PAGE skip, invisible;
    DJ0IM001.DAW-PF7-LIT = " ";
    DJ0IM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DJ0IM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DJ0IM001.XSHCD[sysVar.arrayIndex] == " ")
      set DJ0IM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DJ0IM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DJ0IP25 **************************/
  /* **************/
end // end DJ0IP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DJ0IP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DJ0IM002.XSHCD initialAttributes;
    set DJ0IM002.XSHDESC initialAttributes;
    set DJ0IM002.XSHABRV initialAttributes;
    set DJ0IM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DJ0IM002.XSHCD skip;
    set DJ0IM002.XSHDESC skip;
    set DJ0IM002.XSHABRV skip;
    set DJ0IM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DJ0IM002.XSHCD skip;
    set DJ0IM002.XSHDESC initialAttributes;
    set DJ0IM002.XSHABRV initialAttributes;
    set DJ0IM002.XXXSTAT initialAttributes;
  end

  set DJ0IM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DJ0IM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ0IM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DJ0IM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DJ0IM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DJ0IM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DJ0IM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DJ0IM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DJ0IP25 **************************/
  /* **************/
end // end DJ0IP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DJ0IP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DJ0IP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DJ0IM001.VAGen_EZEMSG = " ";
  DJ0IM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DJ0IP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DJ0IP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DJ0IP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DJ0IP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DJ0IP30 *****************************/
  /* **************/
end // end DJ0IP30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DJ0IP30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DJ0IP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DJ0IM002.VAGen_EZEMSG = " ";
  DJ0IM002.DAW-INFO-MESSAGE = " ";
