//                         DJ0IP70-PROC-MAP-2
// *********************
Program DJ0I type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DJ0IW01 DJ0IW01; // record
  DJ0IW02 DJ0IW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XSHTBL {deleteAfterUse = yes}; // table
  use DJ0IM.DJ0IM001,  // forms
      DJ0IM.DJ0IM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DJ0IP00-DRIVER: DJ0IP00-DRIVER();
    exit program;
  end // end main
end // end DJ0I

// DRIVER FOR APPLICATION
Function DJ0IP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DJ0IP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DJ0IP20-BUILD-MAP2(); /* Build the second map*/
  else
    DJ0IP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DJ0IP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DJ0IP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DJ0IP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DJ0IP00 *******************************/
  /* **************/
end // end DJ0IP00-DRIVER

// INITIALIZE THE APPLICATION
Function DJ0IP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DJ0IW01 empty; /* Working storage record*/
  set DJ0IW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DJ0IM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ0IM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ0IM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DJ0IM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ0IM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ0IM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DJ0IM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DJ0IP10 ******************************/
  /* **************/
end // end DJ0IP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DJ0IP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DJ0IP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DJ0IM001.DAW-CURR-PAGE)
    DJ0IS-TS-READ-PAGE(); /* read temp storage page*/
    DJ0IP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DJ0IP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ0IP20 **********************************/
  /* **************/
end // end DJ0IP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DJ0IP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DJ0IM002.DA-CHNG-USER-ID = sysVar.userID;
  DJ0IM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DJ0IW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ0IM002.DA-PLOG-TXT1 = " ";
    DJ0IM002.DA-PLOG-TXT2 = " ";
    DJ0IM002.XSHCD = " ";
    DJ0IM002.XSHDESC = " ";
    DJ0IM002.XSHABRV = " ";
    DJ0IM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DJ0IW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DJ0IW01.XSHGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DJ0IW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DJ0IW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DJ0IM002.XSHCD = DJ0IW01.XSHCD; /* move to map*/
      DJ0IM002.XSHDESC = DJ0IW01.XSHDESC;
      DJ0IM002.XSHABRV = DJ0IW01.XSHABRV;
      DJ0IM002.XXXSTAT = DJ0IW01.XXXSTAT;
      DJ0IM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DJ0IM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DJ0IM002.XSHCD = DJ0IW02.XSHCD[DAW-ROW-NBR];
                                         /* get the table row*/
        if (DJ0IM002.XSHCD in XSHTBL.XSHCD)
          DJ0IW01.XSHCD = XSHTBL.XSHCD[sysVar.arrayIndex]; /* save for*/
          DJ0IW01.XSHDESC = XSHTBL.XSHDESC[sysVar.arrayIndex]; /* later compare*/
          DJ0IW01.XSHABRV = XSHTBL.XSHABRV[sysVar.arrayIndex];
          DJ0IW01.XXXSTAT = XSHTBL.XXXSTAT[sysVar.arrayIndex];
          DJ0IW01.ZZZSDT = XSHTBL.ZZZSDT[sysVar.arrayIndex];
          DJ0IW01.ZZZCHGDT = XSHTBL.ZZZCHGDT[sysVar.arrayIndex];
          DJ0IW01.ZZZCHGTM = XSHTBL.ZZZCHGTM[sysVar.arrayIndex];
          DJ0IW01.ZZZCHGCT = XSHTBL.ZZZCHGCT[sysVar.arrayIndex];

          DJ0IM002.XSHDESC = XSHTBL.XSHDESC[sysVar.arrayIndex]; /* movetomap*/
          DJ0IM002.XSHABRV = XSHTBL.XSHABRV[sysVar.arrayIndex];
          DJ0IM002.XXXSTAT = XSHTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DJ0IW01.XSHGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DJ0IW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DJ0IW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DJ0IM002.XSHCD = DJ0IW01.XSHCD; /* move to the map*/
    DJ0IM002.XSHDESC = DJ0IW01.XSHDESC;
    DJ0IM002.XSHABRV = DJ0IW01.XSHABRV;
    DJ0IM002.XXXSTAT = DJ0IW01.XXXSTAT;
    DJ0IM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DJ0IM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DJ0IM002.XXXSTAT == "A")
    DJ0IM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DJ0IM002.XXXSTAT == "I")
    DJ0IM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DJ0IM002.XXXSTAT == " ")
    DJ0IM002.DAW-TBL-STATUS-DESC = " ";
  end

  DJ0IP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ0IP20 **********************************/
  /* **************/
end // end DJ0IP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DJ0IP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DJ0IW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ0IP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ0IS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XSHTBL.XSHCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ0IP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ0IS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DJ0IS-TS-ADD-PAGE();
    end

    DJ0IM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DJ0IM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DJ0IP22 **********************************/
  /* **************/
end // end DJ0IP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DJ0IP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DJ0IW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DJ0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DJ0IW01.XSHGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DJ0IW02.XSHCD[DAW-ROW-NBR] = DJ0IW01.XSHCD;
  DJ0IW02.XSHDESC[DAW-ROW-NBR] = DJ0IW01.XSHDESC;
  DJ0IW02.XSHABRV[DAW-ROW-NBR] = DJ0IW01.XSHABRV;
  DJ0IW02.XXXSTAT[DAW-ROW-NBR] = DJ0IW01.XXXSTAT;
  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DJ0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DJ0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DJ0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DJ0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DJ0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DJ0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DJ0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DJ0IW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DJ0IW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DJ0IW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DJ0IP221 ******************************/
  /* ***************/
end // end DJ0IP23-DB2-TO-TSQ

