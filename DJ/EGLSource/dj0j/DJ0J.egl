// SET MAP 1 FIELD ATTRIBUTES
Function DJ0JP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DJ0JM001.DAW-PAGE-LIT = "PAGE";
    set DJ0JM001.DAW-CURR-PAGE initialAttributes;
    DJ0JM001.DAW-OF-LIT = "OF";
    set DJ0JM001.DAW-LAST-PAGE initialAttributes;
    DJ0JM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DJ0JM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DJ0JM001.DAW-PAGE-LIT = " ";
    set DJ0JM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DJ0JM001.DAW-OF-LIT = " ";
    set DJ0JM001.DAW-LAST-PAGE skip, invisible;
    DJ0JM001.DAW-PF7-LIT = " ";
    DJ0JM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DJ0JM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DJ0JM001.XMDCD[sysVar.arrayIndex] == " ")
      set DJ0JM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DJ0JM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DJ0JP25 **************************/
  /* **************/
end // end DJ0JP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DJ0JP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DJ0JM002.XMDCD initialAttributes;
    set DJ0JM002.XMDDESC initialAttributes;
    set DJ0JM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DJ0JM002.XMDCD skip;
    set DJ0JM002.XMDDESC skip;
    set DJ0JM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DJ0JM002.XMDCD skip;
    set DJ0JM002.XMDDESC initialAttributes;
    set DJ0JM002.XXXSTAT initialAttributes;
  end

  set DJ0JM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DJ0JM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ0JM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DJ0JM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DJ0JM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DJ0JM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DJ0JM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DJ0JM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DJ0JP25 **************************/
  /* **************/
end // end DJ0JP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DJ0JP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DJ0JP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DJ0JM001.VAGen_EZEMSG = " ";
  DJ0JM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DJ0JP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DJ0JP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DJ0JP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DJ0JP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DJ0JP30 *****************************/
  /* **************/
end // end DJ0JP30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DJ0JP30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DJ0JP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DJ0JM002.VAGen_EZEMSG = " ";
  DJ0JM002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DJ0JP25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DJ0JP50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DJ0JP60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DJ0JP70-PROC-MAP-2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DJ0JP30 *****************************/
  /* **************/
end // end DJ0JP30-MAIN-MAP-2

