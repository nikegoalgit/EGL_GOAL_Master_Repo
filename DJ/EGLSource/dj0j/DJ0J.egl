  DJ0JW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = " ";
  DJ0JW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = " ";

  /* ***************/
  /* ****************** END DJ0JP221 ******************************/
  /* ***************/
