// DRIVER FOR APPLICATION
Function DJ0JP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DJ0JP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DJ0JP20-BUILD-MAP2(); /* Build the second map*/
  else
    DJ0JP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DJ0JP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DJ0JP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DJ0JP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DJ0JP00 *******************************/
  /* **************/
end // end DJ0JP00-DRIVER

// INITIALIZE THE APPLICATION
Function DJ0JP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DJ0JW01 empty; /* Working storage record*/
  set DJ0JW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DJ0JM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ0JM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ0JM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DJ0JM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ0JM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ0JM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DJ0JM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DJ0JP10 ******************************/
  /* **************/
end // end DJ0JP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DJ0JP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DJ0JP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DJ0JM001.DAW-CURR-PAGE)
    DJ0JS-TS-READ-PAGE(); /* read temp storage page*/
    DJ0JP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DJ0JP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ0JP20 **********************************/
  /* **************/
end // end DJ0JP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DJ0JP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DJ0JM002.DA-CHNG-USER-ID = sysVar.userID;
  DJ0JM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DJ0JW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ0JM002.DA-PLOG-TXT1 = " ";
    DJ0JM002.DA-PLOG-TXT2 = " ";
    DJ0JM002.XMDCD = " ";
    DJ0JM002.XMDDESC = " ";
    DJ0JM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DJ0JW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DJ0JW01.XMDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DJ0JW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DJ0JW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DJ0JM002.XMDCD = DJ0JW01.XMDCD; /* move to map*/
      DJ0JM002.XMDDESC = DJ0JW01.XMDDESC;
      DJ0JM002.XXXSTAT = DJ0JW01.XXXSTAT;
      DJ0JM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DJ0JM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DJ0JM002.XMDCD = DJ0JW02.XMDCD[DAW-ROW-NBR];
                                         /* get the table row*/
        if (DJ0JM002.XMDCD in XMDTBL.XMDCD)
          DJ0JW01.XMDCD = XMDTBL.XMDCD[sysVar.arrayIndex]; /* save for*/
          DJ0JW01.XMDDESC = XMDTBL.XMDDESC[sysVar.arrayIndex]; /* later compare*/
          DJ0JW01.XXXSTAT = XMDTBL.XXXSTAT[sysVar.arrayIndex];
          DJ0JW01.ZZZSDT = XMDTBL.ZZZSDT[sysVar.arrayIndex];
          DJ0JW01.ZZZCHGDT = XMDTBL.ZZZCHGDT[sysVar.arrayIndex];
          DJ0JW01.ZZZCHGTM = XMDTBL.ZZZCHGTM[sysVar.arrayIndex];
          DJ0JW01.ZZZCHGCT = XMDTBL.ZZZCHGCT[sysVar.arrayIndex];

          DJ0JM002.XMDDESC = XMDTBL.XMDDESC[sysVar.arrayIndex]; /* move to map*/
          DJ0JM002.XXXSTAT = XMDTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DJ0JW01.XMDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DJ0JW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DJ0JW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DJ0JM002.XMDCD = DJ0JW01.XMDCD; /* move to the map*/
    DJ0JM002.XMDDESC = DJ0JW01.XMDDESC;
    DJ0JM002.XXXSTAT = DJ0JW01.XXXSTAT;
    DJ0JM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DJ0JM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DJ0JM002.XXXSTAT == "A")
    DJ0JM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DJ0JM002.XXXSTAT == "I")
    DJ0JM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DJ0JM002.XXXSTAT == " ")
    DJ0JM002.DAW-TBL-STATUS-DESC = " ";
  end

  DJ0JP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ0JP20 **********************************/
  /* **************/
end // end DJ0JP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DJ0JP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DJ0JW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ0JP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ0JS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XMDTBL.XMDCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ0JP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ0JS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DJ0JS-TS-ADD-PAGE();
    end

    DJ0JM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DJ0JM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DJ0JP22 **********************************/
  /* **************/
end // end DJ0JP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DJ0JP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DJ0JW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DJ0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DJ0JW01.XMDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DJ0JW02.XMDCD[DAW-ROW-NBR] = DJ0JW01.XMDCD;
  DJ0JW02.XMDDESC[DAW-ROW-NBR] = DJ0JW01.XMDDESC;
  DJ0JW02.XXXSTAT[DAW-ROW-NBR] = DJ0JW01.XXXSTAT;
  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DJ0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DJ0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DJ0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DJ0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DJ0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DJ0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DJ0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DJ0JW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DJ0JW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DJ0JW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DJ0JP221 ******************************/
  /* ***************/
end // end DJ0JP23-DB2-TO-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DJ0JP24-CSP-TO-TSQ()
  /* **************************************************************/
  /* Move table row to temp storage*/
