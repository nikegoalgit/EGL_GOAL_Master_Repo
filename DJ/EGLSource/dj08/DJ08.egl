  end

  /* **************/
  /* ***************** END DJ08P71 **********************************/
  /* **************/
end // end DJ08P71-COMPARE

// MOVE TEMP STORAGE PAGE TO MAP
Function DJ08P71-TSQ-TO-MAP()
  /* ************************************************************/
  /* Move temp storage page to map*/
  /* ************************************************************/

  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DJ08M001.DAW-SELECT-CD[sysVar.arrayIndex] = DJ08W02.DAW-SELECT-CD[sysVar.arrayIndex];
    DJ08M001.XLCCD[sysVar.arrayIndex] = DJ08W02.XLCCD[sysVar.arrayIndex];
    DJ08M001.XLCDESC[sysVar.arrayIndex] = DJ08W02.XLCDESC[sysVar.arrayIndex];
    DJ08M001.XLCABRV[sysVar.arrayIndex] = DJ08W02.XLCABRV[sysVar.arrayIndex];
    DJ08M001.COCMFL[sysVar.arrayIndex] = DJ08W02.COCMFL[sysVar.arrayIndex];
    DJ08M001.XXXSTAT[sysVar.arrayIndex] = DJ08W02.XXXSTAT[sysVar.arrayIndex];
    DJ08M001.DAW-STATUS-DESC[sysVar.arrayIndex] = DJ08W02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DJ08M001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DJ08W02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DJ08M001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DJ08W02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  /* **************/
  /* ****************** END DJ08P71 *****************************/
  /* **************/
end // end DJ08P71-TSQ-TO-MAP

// BUILD NEW VDATRN01 RECORD
Function DJ08P72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DJ08M002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DJ08M002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DJ08W01.DA-CCAT-KEY-TXT = " ";
  DJ08W01.DJ08W-KEY-XLCCD = DJ08M002.XLCCD;
  VDATRN01.DA-CCAT-KEY-TXT = DJ08W01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DJ08W01.XLCCD = DJ08M002.XLCCD;
  DJ08W01.XLCDESC = DJ08M002.XLCDESC;
  DJ08W01.XLCABRV = DJ08M002.XLCABRV;
  DJ08W01.COCMFL = DJ08M002.COCMFL;
  DJ08W01.XXXSTAT = DJ08M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 37;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DJ08W01.XLCGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DJ08P72 **********************************/
  /* **************/
end // end DJ08P72-NEW-TRAN

