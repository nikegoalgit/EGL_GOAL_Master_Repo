//                         DJ08P70-PROC-MAP-2
// *********************
Program DJ08 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DJ08W01 DJ08W01; // record
  DJ08W02 DJ08W02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XLCTBL {deleteAfterUse = yes}; // table
  use DJ08M.DJ08M001,  // forms
      DJ08M.DJ08M002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DJ08P00-DRIVER: DJ08P00-DRIVER();
    exit program;
  end // end main
end // end DJ08

// DRIVER FOR APPLICATION
Function DJ08P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DJ08P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DJ08P20-BUILD-MAP2(); /* Build the second map*/
  else
    DJ08P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DJ08P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DJ08P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DJ08P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DJ08P00 *******************************/
  /* **************/
end // end DJ08P00-DRIVER

// INITIALIZE THE APPLICATION
Function DJ08P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DJ08W01 empty; /* Working storage record*/
  set DJ08W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DJ08M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ08M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ08M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DJ08M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ08M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ08M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DJ08M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DJ08P10 ******************************/
  /* **************/
end // end DJ08P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DJ08P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DJ08P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DJ08M001.DAW-CURR-PAGE)
    DJ08S-TS-READ-PAGE(); /* read temp storage page*/
    DJ08P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DJ08P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ08P20 **********************************/
  /* **************/
end // end DJ08P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DJ08P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DJ08M002.DA-CHNG-USER-ID = sysVar.userID;
  DJ08M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DJ08W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ08M002.DA-PLOG-TXT1 = " ";
    DJ08M002.DA-PLOG-TXT2 = " ";
    DJ08M002.XLCCD = " ";
    DJ08M002.XLCDESC = " ";
    DJ08M002.XLCABRV = " ";
    DJ08M002.COCMFL = " ";
    DJ08M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DJ08W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DJ08W01.XLCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DJ08W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DJ08W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DJ08M002.XLCCD = DJ08W01.XLCCD; /* move to map*/
      DJ08M002.XLCDESC = DJ08W01.XLCDESC;
      DJ08M002.XLCABRV = DJ08W01.XLCABRV;
      DJ08M002.COCMFL = DJ08W01.COCMFL;
      DJ08M002.XXXSTAT = DJ08W01.XXXSTAT;
      DJ08M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DJ08M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DJ08M002.XLCCD = DJ08W02.XLCCD[DAW-ROW-NBR];
                                         /* get the table row*/
        if (DJ08M002.XLCCD in XLCTBL.XLCCD)
          DJ08W01.XLCCD = XLCTBL.XLCCD[sysVar.arrayIndex]; /* save for*/
          DJ08W01.XLCDESC = XLCTBL.XLCDESC[sysVar.arrayIndex]; /* later compare*/
          DJ08W01.XLCABRV = XLCTBL.XLCABRV[sysVar.arrayIndex];
          DJ08W01.COCMFL = XLCTBL.COCMFL[sysVar.arrayIndex];
          DJ08W01.XXXSTAT = XLCTBL.XXXSTAT[sysVar.arrayIndex];
          DJ08W01.ZZZSDT = XLCTBL.ZZZSDT[sysVar.arrayIndex];
          DJ08W01.ZZZCHGDT = XLCTBL.ZZZCHGDT[sysVar.arrayIndex];
          DJ08W01.ZZZCHGTM = XLCTBL.ZZZCHGTM[sysVar.arrayIndex];
          DJ08W01.ZZZCHGCT = XLCTBL.ZZZCHGCT[sysVar.arrayIndex];

          DJ08M002.XLCDESC = XLCTBL.XLCDESC[sysVar.arrayIndex]; /* move*/
          DJ08M002.XLCABRV = XLCTBL.XLCABRV[sysVar.arrayIndex]; /* to map*/
          DJ08M002.COCMFL = XLCTBL.COCMFL[sysVar.arrayIndex];
          DJ08M002.XXXSTAT = XLCTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DJ08W01.XLCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DJ08W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DJ08W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DJ08M002.XLCCD = DJ08W01.XLCCD; /* move to the map*/
    DJ08M002.XLCDESC = DJ08W01.XLCDESC;
    DJ08M002.XLCABRV = DJ08W01.XLCABRV;
    DJ08M002.COCMFL = DJ08W01.COCMFL;
    DJ08M002.XXXSTAT = DJ08W01.XXXSTAT;
    DJ08M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DJ08M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DJ08M002.XXXSTAT == "A")
    DJ08M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DJ08M002.XXXSTAT == "I")
    DJ08M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DJ08M002.XXXSTAT == " ")
    DJ08M002.DAW-TBL-STATUS-DESC = " ";
  end

  DJ08P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ08P20 **********************************/
  /* **************/
end // end DJ08P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DJ08P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DJ08W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ08P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ08S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XLCTBL.XLCCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ08P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ08S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DJ08S-TS-ADD-PAGE();
    end

    DJ08M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DJ08M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DJ08P22 **********************************/
  /* **************/
end // end DJ08P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DJ08P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DJ08W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DJ08W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DJ08W01.XLCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DJ08W02.XLCCD[DAW-ROW-NBR] = DJ08W01.XLCCD;
  DJ08W02.XLCDESC[DAW-ROW-NBR] = DJ08W01.XLCDESC;
  DJ08W02.XLCABRV[DAW-ROW-NBR] = DJ08W01.XLCABRV;
  DJ08W02.COCMFL[DAW-ROW-NBR] = DJ08W01.COCMFL;
  DJ08W02.XXXSTAT[DAW-ROW-NBR] = DJ08W01.XXXSTAT;
  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DJ08W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DJ08W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DJ08W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DJ08W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DJ08W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DJ08W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DJ08W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DJ08W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DJ08W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DJ08W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DJ08P221 ******************************/
  /* ***************/
end // end DJ08P23-DB2-TO-TSQ

