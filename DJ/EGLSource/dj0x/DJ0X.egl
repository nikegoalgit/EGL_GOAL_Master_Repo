// MOVE TEMP STORAGE PAGE TO MAP
Function DJ0XP71-TSQ-TO-MAP()
  /* ************************************************************/
  /* Move temp storage page to map*/
  /* ************************************************************/

  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DJ0XM001.DAW-SELECT-CD[sysVar.arrayIndex] = DJ0XW02.DAW-SELECT-CD[sysVar.arrayIndex];
    DJ0XM001.XTCCD[sysVar.arrayIndex] = DJ0XW02.XTCCD[sysVar.arrayIndex];
    DJ0XM001.XTCDESC[sysVar.arrayIndex] = DJ0XW02.XTCDESC[sysVar.arrayIndex];
    DJ0XM001.XTCSDESC[sysVar.arrayIndex] = DJ0XW02.XTCSDESC[sysVar.arrayIndex];
    DJ0XM001.XXXSTAT[sysVar.arrayIndex] = DJ0XW02.XXXSTAT[sysVar.arrayIndex];
    DJ0XM001.DAW-STATUS-DESC[sysVar.arrayIndex] = DJ0XW02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DJ0XM001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DJ0XW02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DJ0XM001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DJ0XW02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  /* **************/
  /* ****************** END DJ0XP71 *****************************/
  /* **************/
end // end DJ0XP71-TSQ-TO-MAP

// BUILD NEW VDATRN01 RECORD
Function DJ0XP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DJ0XM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DJ0XM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DJ0XW01.DA-CCAT-KEY-TXT = " ";
  DJ0XW01.DJ0XW-KEY-XTCCD = DJ0XM002.XTCCD;
  VDATRN01.DA-CCAT-KEY-TXT = DJ0XW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DJ0XW01.XTCCD = DJ0XM002.XTCCD;
  DJ0XW01.XTCDESC = DJ0XM002.XTCDESC;
  DJ0XW01.XTCSDESC = DJ0XM002.XTCSDESC;
  DJ0XW01.XXXSTAT = DJ0XM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 34;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DJ0XW01.XTCGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DJ0XP72 **********************************/
  /* **************/
end // end DJ0XP72-NEW-TRAN

