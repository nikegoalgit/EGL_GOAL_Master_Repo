//  DATE CREATED: 04/24/98
//        AUTHOR: Eric Chan
// 
//  MAINTENANCE HISTORY:
// 
//      date     by whom     overview of changes made
//    --------  ----------  -----------------------------------
//    03-02-99  ECHAN       Add NOHILITE in DJ0XP25-SET-ATTR-1
//                          Chg TRAMSACTION to TRANSACTION in
//                          DJ0XP70-PROC-MAP-1
//                          Check duplicate and deadlock in
//                          DJ0XP70-PROC-MAP-2
// *********************
Program DJ0X type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DJ0XW01 DJ0XW01; // record
  DJ0XW02 DJ0XW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XTCTBL {deleteAfterUse = yes}; // table
  use DJ0XM.DJ0XM001,  // forms
      DJ0XM.DJ0XM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DJ0XP00-DRIVER: DJ0XP00-DRIVER();
    exit program;
  end // end main
end // end DJ0X

// DRIVER FOR APPLICATION
Function DJ0XP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DJ0XP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DJ0XP20-BUILD-MAP2(); /* Build the second map*/
  else
    DJ0XP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DJ0XP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DJ0XP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DJ0XP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DJ0XP00 *******************************/
  /* **************/
end // end DJ0XP00-DRIVER

// INITIALIZE THE APPLICATION
Function DJ0XP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DJ0XW01 empty; /* Working storage record*/
  set DJ0XW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DJ0XM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ0XM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ0XM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DJ0XM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DJ0XM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DJ0XM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DJ0XM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DJ0XP10 ******************************/
  /* **************/
end // end DJ0XP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DJ0XP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DJ0XP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DJ0XM001.DAW-CURR-PAGE)
    DJ0XS-TS-READ-PAGE(); /* read temp storage page*/
    DJ0XP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DJ0XP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ0XP20 **********************************/
  /* **************/
end // end DJ0XP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DJ0XP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DJ0XM002.DA-CHNG-USER-ID = sysVar.userID;
  DJ0XM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DJ0XW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ0XM002.DA-PLOG-TXT1 = " ";
    DJ0XM002.DA-PLOG-TXT2 = " ";
    DJ0XM002.XTCCD = " ";
    DJ0XM002.XTCDESC = " ";
    DJ0XM002.XTCSDESC = " ";
    DJ0XM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DJ0XW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DJ0XW01.XTCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DJ0XW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DJ0XW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DJ0XM002.XTCCD = DJ0XW01.XTCCD; /* move to the map*/
      DJ0XM002.XTCDESC = DJ0XW01.XTCDESC;
      DJ0XM002.XTCSDESC = DJ0XW01.XTCSDESC;
      DJ0XM002.XXXSTAT = DJ0XW01.XXXSTAT;
      DJ0XM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DJ0XM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DJ0XM002.XTCCD = DJ0XW02.XTCCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DJ0XM002.XTCCD in XTCTBL.XTCCD)
          DJ0XW01.XTCCD = XTCTBL.XTCCD[sysVar.arrayIndex]; /* save for later*/
          DJ0XW01.XTCDESC = XTCTBL.XTCDESC[sysVar.arrayIndex]; /* compare*/
          DJ0XW01.XTCSDESC = XTCTBL.XTCSDESC[sysVar.arrayIndex];
          DJ0XW01.XXXSTAT = XTCTBL.XXXSTAT[sysVar.arrayIndex];
          DJ0XW01.ZZZSDT = XTCTBL.ZZZSDT[sysVar.arrayIndex];
          DJ0XW01.ZZZCHGDT = XTCTBL.ZZZCHGDT[sysVar.arrayIndex];
          DJ0XW01.ZZZCHGTM = XTCTBL.ZZZCHGTM[sysVar.arrayIndex];
          DJ0XW01.ZZZCHGCT = XTCTBL.ZZZCHGCT[sysVar.arrayIndex];

          DJ0XM002.XTCDESC = XTCTBL.XTCDESC[sysVar.arrayIndex]; /* move to map*/
          DJ0XM002.XTCSDESC = XTCTBL.XTCSDESC[sysVar.arrayIndex];
          DJ0XM002.XXXSTAT = XTCTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DJ0XW01.XTCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DJ0XW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DJ0XW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DJ0XM002.XTCCD = DJ0XW01.XTCCD; /* move to the map*/
    DJ0XM002.XTCDESC = DJ0XW01.XTCDESC;
    DJ0XM002.XTCSDESC = DJ0XW01.XTCSDESC;
    DJ0XM002.XXXSTAT = DJ0XW01.XXXSTAT;
    DJ0XM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DJ0XM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DJ0XM002.XXXSTAT == "A")
    DJ0XM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DJ0XM002.XXXSTAT == "I")
    DJ0XM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DJ0XM002.XXXSTAT == " ")
    DJ0XM002.DAW-TBL-STATUS-DESC = " ";
  end

  DJ0XP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DJ0XP20 **********************************/
  /* **************/
end // end DJ0XP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DJ0XP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DJ0XW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ0XP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ0XS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XTCTBL.XTCCD[sysVar.arrayIndex] != "999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DJ0XP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DJ0XS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DJ0XS-TS-ADD-PAGE();
    end

    DJ0XM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DJ0XM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DJ0XP22 **********************************/
  /* **************/
end // end DJ0XP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DJ0XP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DJ0XW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DJ0XW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DJ0XW01.XTCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DJ0XW02.XTCCD[DAW-ROW-NBR] = DJ0XW01.XTCCD;
  DJ0XW02.XTCDESC[DAW-ROW-NBR] = DJ0XW01.XTCDESC;
  DJ0XW02.XTCSDESC[DAW-ROW-NBR] = DJ0XW01.XTCSDESC;
  DJ0XW02.XXXSTAT[DAW-ROW-NBR] = DJ0XW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DJ0XW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DJ0XW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DJ0XW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DJ0XW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DJ0XW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DJ0XW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DJ0XW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DJ0XW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DJ0XW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DJ0XW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DJ0XP221 ******************************/
  /* ***************/
end // end DJ0XP23-DB2-TO-TSQ

