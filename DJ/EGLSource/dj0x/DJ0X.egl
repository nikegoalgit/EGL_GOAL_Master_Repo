// SET MAP 1 FIELD ATTRIBUTES
Function DJ0XP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DJ0XM001.DAW-PAGE-LIT = "PAGE";
    set DJ0XM001.DAW-CURR-PAGE initialAttributes;
    DJ0XM001.DAW-OF-LIT = "OF";
    set DJ0XM001.DAW-LAST-PAGE initialAttributes;
    DJ0XM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DJ0XM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DJ0XM001.DAW-PAGE-LIT = " ";
    set DJ0XM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DJ0XM001.DAW-OF-LIT = " ";
    set DJ0XM001.DAW-LAST-PAGE skip, invisible;
    DJ0XM001.DAW-PF7-LIT = " ";
    DJ0XM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DJ0XM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DJ0XM001.XTCCD[sysVar.arrayIndex] == " ")
      set DJ0XM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DJ0XM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DJ0XP25 **************************/
  /* **************/
end // end DJ0XP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DJ0XP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DJ0XM002.XTCCD initialAttributes;
    set DJ0XM002.XTCDESC initialAttributes;
    set DJ0XM002.XTCSDESC initialAttributes;
    set DJ0XM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DJ0XM002.XTCCD skip;
    set DJ0XM002.XTCDESC skip;
    set DJ0XM002.XTCSDESC skip;
    set DJ0XM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DJ0XM002.XTCCD skip;
    set DJ0XM002.XTCDESC initialAttributes;
    set DJ0XM002.XTCSDESC initialAttributes;
    set DJ0XM002.XXXSTAT initialAttributes;
  end

  set DJ0XM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DJ0XM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DJ0XM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DJ0XM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DJ0XM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DJ0XM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DJ0XM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DJ0XM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DJ0XP25 **************************/
  /* **************/
end // end DJ0XP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DJ0XP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DJ0XP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DJ0XM001.VAGen_EZEMSG = " ";
  DJ0XM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DJ0XP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DJ0XP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DJ0XP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DJ0XP70-PROC-MAP1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DJ0XP30 *****************************/
  /* **************/
end // end DJ0XP30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DJ0XP30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DJ0XP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DJ0XM002.VAGen_EZEMSG = " ";
  DJ0XM002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DJ0XP25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DJ0XP50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DJ0XP60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DJ0XP70-PROC-MAP2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DJ0XP30 *****************************/
  /* **************/
end // end DJ0XP30-MAIN-MAP-2

// CONVERSE MAP 001
Function DJ0XP40-CONVERSE-1()
  /* **********************************************************/
  /* CONVERSE MAP 001 - LIST TABLE*/
  /* **********************************************************/
  converse DJ0XM001 ;

  /* **************/
  /* ****************** END DJ0XP40 *****************************/
