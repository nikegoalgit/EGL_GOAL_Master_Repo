// DRIVER FOR APPLICATION
Function DQ03P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DQ03P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DQ03P20-BUILD-MAP2(); /* Build the second map*/
  else
    DQ03P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DQ03P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DQ03P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DQ03P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DQ03P00 *******************************/
  /* **************/
end // end DQ03P00-DRIVER

// INITIALIZE THE APPLICATION
Function DQ03P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DQ03W01 empty; /* Working storage record*/
  set DQ03W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DQ03M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ03M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ03M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DQ03M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ03M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ03M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DQ03M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DQ03P10 ******************************/
  /* **************/
end // end DQ03P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DQ03P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DQ03P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DQ03M001.DAW-CURR-PAGE)
    DQ03S-TS-READ-PAGE(); /* read temp storage page*/
    DQ03P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DQ03P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ03P20 **********************************/
  /* **************/
end // end DQ03P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DQ03P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DQ03M002.DA-CHNG-USER-ID = sysVar.userID;
  DQ03M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DQ03W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DQ03M002.XGPABRV = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ03M002.DA-PLOG-TXT1 = " ";
    DQ03M002.DA-PLOG-TXT2 = " ";
    DQ03M002.XRYCD = " ";
    DQ03M002.XGPCD = " ";
    DQ03M002.XTVCD = " ";
    DQ03M002.XRYDESC = " ";
    DQ03M002.XRYABRV = " ";
    DQ03M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DQ03W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DQ03W01.XRYGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DQ03W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DQ03W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DQ03M002.XRYCD = DQ03W01.XRYCD; /* move to the map*/
      DQ03M002.XGPCD = DQ03W01.XGPCD;
      DQ03M002.XTVCD = DQ03W01.XTVCD;
      DQ03M002.XRYDESC = DQ03W01.XRYDESC;
      DQ03M002.XRYABRV = DQ03W01.XRYABRV;
      DQ03M002.XXXSTAT = DQ03W01.XXXSTAT;
      DQ03M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DQ03M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DQ03M002.XRYCD = DQ03W02.XRYCD[DAW-ROW-NBR];
        DQ03M002.XGPCD = DQ03W02.XGPCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        DQ03W01.DQ03W-FND-FL = "N";
        sysVar.arrayIndex = 1;
        while (DQ03W01.DQ03W-FND-FL != "Y"
         && XRYTBL.XRYCD[sysVar.arrayIndex] != "999")
          if (XRYTBL.XRYCD[sysVar.arrayIndex] == DQ03M002.XRYCD
           && XRYTBL.XGPCD[sysVar.arrayIndex] == DQ03M002.XGPCD)
            DQ03W01.DQ03W-FND-FL = "Y";
          else
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
          end
        end
        if (DQ03W01.DQ03W-FND-FL == "Y")
          DQ03W01.XRYCD = XRYTBL.XRYCD[sysVar.arrayIndex]; /* save for*/
          DQ03W01.XTVCD = XRYTBL.XTVCD[sysVar.arrayIndex];
          DQ03W01.XRYDESC = XRYTBL.XRYDESC[sysVar.arrayIndex];
          DQ03W01.XRYABRV = XRYTBL.XRYABRV[sysVar.arrayIndex];
          DQ03W01.XXXSTAT = XRYTBL.XXXSTAT[sysVar.arrayIndex];
          DQ03W01.ZZZSDT = XRYTBL.ZZZSDT[sysVar.arrayIndex];
          DQ03W01.ZZZCHGDT = XRYTBL.ZZZCHGDT[sysVar.arrayIndex];
          DQ03W01.ZZZCHGTM = XRYTBL.ZZZCHGTM[sysVar.arrayIndex];
          DQ03W01.ZZZCHGCT = XRYTBL.ZZZCHGCT[sysVar.arrayIndex];
                                                     /* move to map*/
          DQ03M002.XTVCD = XRYTBL.XTVCD[sysVar.arrayIndex];
          DQ03M002.XRYDESC = XRYTBL.XRYDESC[sysVar.arrayIndex];
          DQ03M002.XRYABRV = XRYTBL.XRYABRV[sysVar.arrayIndex];
          DQ03M002.XXXSTAT = XRYTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DQ03W01.XRYGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DQ03W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DQ03W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DQ03M002.XRYCD = DQ03W01.XRYCD; /* move to the map*/
    DQ03M002.XGPCD = DQ03W01.XGPCD;
    DQ03M002.XTVCD = DQ03W01.XTVCD;
    DQ03M002.XRYDESC = DQ03W01.XRYDESC;
    DQ03M002.XRYABRV = DQ03W01.XRYABRV;
    DQ03M002.XXXSTAT = DQ03W01.XXXSTAT;
    DQ03M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DQ03M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DQ03M002.XXXSTAT == "A")
    DQ03M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DQ03M002.XXXSTAT == "I")
    DQ03M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DQ03M002.XXXSTAT == " ")
    DQ03M002.DAW-TBL-STATUS-DESC = " ";
  end

  DQ03P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ03P20 **********************************/
  /* **************/
end // end DQ03P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DQ03P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DQ03W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ03P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ03S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XRYTBL.XRYCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ03P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ03S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DQ03S-TS-ADD-PAGE();
    end

    DQ03M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DQ03M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DQ03P22 **********************************/
  /* **************/
end // end DQ03P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DQ03P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DQ03W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DQ03W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DQ03W01.XRYGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DQ03W02.XRYCD[DAW-ROW-NBR] = DQ03W01.XRYCD;
  DQ03W02.XGPCD[DAW-ROW-NBR] = DQ03W01.XGPCD;
  DQ03W02.XTVCD[DAW-ROW-NBR] = DQ03W01.XTVCD;
  DQ03W02.XRYDESC[DAW-ROW-NBR] = DQ03W01.XRYDESC;
  DQ03W02.XRYABRV[DAW-ROW-NBR] = DQ03W01.XRYABRV;
  DQ03W02.XXXSTAT[DAW-ROW-NBR] = DQ03W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DQ03W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DQ03W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DQ03W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DQ03W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DQ03W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DQ03W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DQ03W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DQ03W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DQ03W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DQ03W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DQ03P221 ******************************/
  /* ***************/
end // end DQ03P23-DB2-TO-TSQ

