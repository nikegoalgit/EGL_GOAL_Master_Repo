// SET MAP 1 FIELD ATTRIBUTES
Function DQ03P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DQ03M001.DAW-PAGE-LIT = "PAGE";
    set DQ03M001.DAW-CURR-PAGE initialAttributes;
    DQ03M001.DAW-OF-LIT = "OF";
    set DQ03M001.DAW-LAST-PAGE initialAttributes;
    DQ03M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DQ03M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DQ03M001.DAW-PAGE-LIT = " ";
    set DQ03M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DQ03M001.DAW-OF-LIT = " ";
    set DQ03M001.DAW-LAST-PAGE skip, invisible;
    DQ03M001.DAW-PF7-LIT = " ";
    DQ03M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DQ03M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DQ03M001.XRYKEY[sysVar.arrayIndex] == " ")
      set DQ03M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DQ03M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DQ03P25 **************************/
  /* **************/
end // end DQ03P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DQ03P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DQ03M002.XRYCD initialAttributes;
    set DQ03M002.XGPCD initialAttributes;
    set DQ03M002.XTVCD initialAttributes;
    set DQ03M002.XRYDESC initialAttributes;
    set DQ03M002.XRYABRV initialAttributes;
    set DQ03M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DQ03M002.XRYCD skip;
    set DQ03M002.XGPCD skip;
    set DQ03M002.XTVCD skip;
    set DQ03M002.XRYDESC skip;
    set DQ03M002.XRYABRV skip;
    set DQ03M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DQ03M002.XRYCD skip;
    set DQ03M002.XGPCD skip;
    set DQ03M002.XTVCD initialAttributes;
    set DQ03M002.XRYDESC initialAttributes;
    set DQ03M002.XRYABRV initialAttributes;
    set DQ03M002.XXXSTAT initialAttributes;
  end

  set DQ03M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DQ03M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ03M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DQ03M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DQ03M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DQ03M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DQ03M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DQ03M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DQ03P25 **************************/
  /* **************/
end // end DQ03P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DQ03P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DQ03P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DQ03M001.VAGen_EZEMSG = " ";
  DQ03M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DQ03P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DQ03P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DQ03P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DQ03P70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DQ03P30 *****************************/
  /* **************/
end // end DQ03P30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DQ03P30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DQ03P40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DQ03M002.VAGen_EZEMSG = " ";
  DQ03M002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DQ03P25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DQ03P50-PF-KEYS-2(); /* Edit pf keys*/

