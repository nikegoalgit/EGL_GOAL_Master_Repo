// BUILD NEW VDATRN01 RECORD
Function DQ03P72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DQ03M002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DQ03M002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DQ03W01.DA-CCAT-KEY-TXT = " ";
  DQ03W01.DQ03W-KEY-XRYCD = DQ03M002.XRYCD;
  DQ03W01.DQ03W-KEY-XGPCD = DQ03M002.XGPCD;
  VDATRN01.DA-CCAT-KEY-TXT = DQ03W01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DQ03W01.XRYCD = DQ03M002.XRYCD;
  DQ03W01.XGPCD = DQ03M002.XGPCD;
  DQ03W01.XTVCD = DQ03M002.XTVCD;
  DQ03W01.XRYDESC = DQ03M002.XRYDESC;
  DQ03W01.XRYABRV = DQ03M002.XRYABRV;
  DQ03W01.XXXSTAT = DQ03M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 043;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DQ03W01.XRYGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DQ03P72 **********************************/
  /* **************/
end // end DQ03P72-NEW-TRAN

