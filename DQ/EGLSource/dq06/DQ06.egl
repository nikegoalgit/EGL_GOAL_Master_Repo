  DQ06P40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DQ06M002.VAGen_EZEMSG = " ";
  DQ06M002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DQ06P25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DQ06P50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DQ06P60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DQ06P70-PROC-MAP-2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DQ06P30 *****************************/
  /* **************/
end // end DQ06P30-MAIN-MAP-2

