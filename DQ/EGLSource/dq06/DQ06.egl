// DRIVER FOR APPLICATION
Function DQ06P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DQ06P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DQ06P20-BUILD-MAP2(); /* Build the second map*/
  else
    DQ06P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DQ06P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DQ06P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DQ06P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DQ06P00 *******************************/
  /* **************/
end // end DQ06P00-DRIVER

// INITIALIZE THE APPLICATION
Function DQ06P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DQ06W01 empty; /* Working storage record*/
  set DQ06W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DQ06M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ06M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ06M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DQ06M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ06M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ06M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DQ06M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DQ06P10 ******************************/
  /* **************/
end // end DQ06P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DQ06P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DQ06P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DQ06M001.DAW-CURR-PAGE)
    DQ06S-TS-READ-PAGE(); /* read temp storage page*/
    DQ06P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DQ06P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ06P20 **********************************/
  /* **************/
end // end DQ06P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DQ06P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DQ06M002.DA-CHNG-USER-ID = sysVar.userID;
  DQ06M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DQ06W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ06M002.DA-PLOG-TXT1 = " ";
    DQ06M002.DA-PLOG-TXT2 = " ";
    DQ06M002.XZCCLNDT = 0;
    DQ06M002.XZCGLPM1 = 0;
    DQ06M002.XZCGLPM2 = 0;
    DQ06M002.XZCGLPM3 = 0;
    DQ06M002.XZCFISMO = 0;
    DQ06M002.XZCGLEDT = 0;
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DQ06W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DQ06W01.XZCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DQ06W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DQ06W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DQ06M002.XZCCLNDT = DQ06W01.XZCCLNDT; /* move to the map*/
      DQ06M002.XZCGLPM1 = DQ06W01.XZCGLPM1;
      DQ06M002.XZCGLPM2 = DQ06W01.XZCGLPM2;
      DQ06M002.XZCGLPM3 = DQ06W01.NL-GEN-LED-PMO3;
      DQ06M002.XZCFISMO = DQ06W01.XZCFISMO;
      DQ06M002.XZCGLEDT = DQ06W01.XZCGLEDT;
      DQ06M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DQ06M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DQ06M002.XZCCLNDT = DQ06W02.XZCCLNDT[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DQ06M002.XZCCLNDT in XZCTBL.XZCCLNDT)
          DQ06W01.XZCCLNDT = XZCTBL.XZCCLNDT[sysVar.arrayIndex]; /* save for a*/
   /* later compare*/
          DQ06W01.XZCGLPM1 = XZCTBL.XZCGLPM1[sysVar.arrayIndex];
          DQ06W01.XZCGLPM2 = XZCTBL.XZCGLPM2[sysVar.arrayIndex];
          DQ06W01.NL-GEN-LED-PMO3 = XZCTBL.NL-GEN-LED-PMO3[sysVar.arrayIndex];
          DQ06W01.XZCFISMO = XZCTBL.XZCFISMO[sysVar.arrayIndex];
          DQ06W01.XZCGLEDT = XZCTBL.XZCGLEDT[sysVar.arrayIndex];
          DQ06W01.ZZZSDT = XZCTBL.ZZZSDT[sysVar.arrayIndex];
          DQ06W01.ZZZCHGDT = XZCTBL.ZZZCHGDT[sysVar.arrayIndex];
          DQ06W01.ZZZCHGTM = XZCTBL.ZZZCHGTM[sysVar.arrayIndex];
          DQ06W01.ZZZCHGCT = XZCTBL.ZZZCHGCT[sysVar.arrayIndex];
                                                           /* move to map*/
          DQ06M002.XZCGLPM1 = XZCTBL.XZCGLPM1[sysVar.arrayIndex];
          DQ06M002.XZCGLPM2 = XZCTBL.XZCGLPM2[sysVar.arrayIndex];
          DQ06M002.XZCGLPM3 = XZCTBL.NL-GEN-LED-PMO3[sysVar.arrayIndex];
          DQ06M002.XZCFISMO = XZCTBL.XZCFISMO[sysVar.arrayIndex];
          DQ06M002.XZCGLEDT = XZCTBL.XZCGLEDT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DQ06W01.XZCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DQ06W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DQ06W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DQ06M002.XZCCLNDT = DQ06W01.XZCCLNDT; /* move to the map*/
    DQ06M002.XZCGLPM1 = DQ06W01.XZCGLPM1;
    DQ06M002.XZCGLPM2 = DQ06W01.XZCGLPM2;
    DQ06M002.XZCGLPM3 = DQ06W01.NL-GEN-LED-PMO3;
    DQ06M002.XZCFISMO = DQ06W01.XZCFISMO;
    DQ06M002.XZCGLEDT = DQ06W01.XZCGLEDT;
    DQ06M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DQ06M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  DQ06P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ06P20 **********************************/
  /* **************/
end // end DQ06P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DQ06P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DQ06W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ06P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ06S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XZCTBL.XZCCLNDT[sysVar.arrayIndex] != 999999999) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ06P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ06S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DQ06S-TS-ADD-PAGE();
    end

    DQ06M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DQ06M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DQ06P22 **********************************/
  /* **************/
end // end DQ06P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DQ06P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DQ06W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DQ06W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DQ06W01.XZCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DQ06W02.XZCCLNDT[DAW-ROW-NBR] = DQ06W01.XZCCLNDT;
  DQ06W02.XZCGLPM1[DAW-ROW-NBR] = DQ06W01.XZCGLPM1;
  DQ06W02.XZCGLPM2[DAW-ROW-NBR] = DQ06W01.XZCGLPM2;
  DQ06W02.NL-GEN-LED-PMO3[DAW-ROW-NBR] = DQ06W01.NL-GEN-LED-PMO3;
  DQ06W02.XZCFISMO[DAW-ROW-NBR] = DQ06W01.XZCFISMO;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DQ06W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DQ06W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DQ06W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DQ06W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DQ06W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DQ06W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DQ06W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DQ06W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DQ06W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DQ06W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DQ06P221 ******************************/
  /* ***************/
end // end DQ06P23-DB2-TO-TSQ

