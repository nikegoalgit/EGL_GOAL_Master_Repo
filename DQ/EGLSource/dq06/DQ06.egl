  TA5REC.TA5APPID = "DQ06"; /* appl identification*/
  TA5REC.TA5FNCCD = "A"; /* function code*/
  DQ06W02.DAW-TEMP-STORAGE-LENGTH = 1096;

  call "TA0050" (DQ06W02, TA5REC) {isNoRefresh = yes, isExternal = yes}; /* temp storage subroutine*/

