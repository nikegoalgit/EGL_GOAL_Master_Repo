// SET MAP 1 FIELD ATTRIBUTES
Function DQ06P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DQ06M001.DAW-PAGE-LIT = "PAGE";
    set DQ06M001.DAW-CURR-PAGE initialAttributes;
    DQ06M001.DAW-OF-LIT = "OF";
    set DQ06M001.DAW-LAST-PAGE initialAttributes;
    DQ06M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DQ06M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DQ06M001.DAW-PAGE-LIT = " ";
    set DQ06M001.DAW-CURR-PAGE skip, invisible;
    DQ06M001.DAW-OF-LIT = " ";
    set DQ06M001.DAW-LAST-PAGE skip, invisible;
    DQ06M001.DAW-PF7-LIT = " ";
    DQ06M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DQ06M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DQ06M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DQ06M001.XZCCLNDT[sysVar.arrayIndex] == 0)
        set DQ06M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
        set DQ06M001.XZCCLNDT[sysVar.arrayIndex] skip, invisible;
        set DQ06M001.XZCGLPM1[sysVar.arrayIndex] skip, invisible;
        set DQ06M001.XZCGLPM2[sysVar.arrayIndex] skip, invisible;
        set DQ06M001.XZCGLPM3[sysVar.arrayIndex] skip, invisible;
        set DQ06M001.XZCFISMO[sysVar.arrayIndex] skip, invisible;
      else
        set DQ06M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
        set DQ06M001.XZCCLNDT[sysVar.arrayIndex] initialAttributes;
        set DQ06M001.XZCGLPM1[sysVar.arrayIndex] initialAttributes;
        set DQ06M001.XZCGLPM2[sysVar.arrayIndex] initialAttributes;
        set DQ06M001.XZCGLPM3[sysVar.arrayIndex] initialAttributes;
        set DQ06M001.XZCFISMO[sysVar.arrayIndex] initialAttributes;
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DQ06P25 **************************/
  /* **************/
end // end DQ06P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DQ06P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DQ06M002.XZCCLNDT initialAttributes;
    set DQ06M002.XZCGLPM1 initialAttributes;
    set DQ06M002.XZCGLPM2 initialAttributes;
    set DQ06M002.XZCGLPM3 initialAttributes;
    set DQ06M002.XZCFISMO initialAttributes;
    set DQ06M002.XZCGLEDT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DQ06M002.XZCCLNDT skip;
    set DQ06M002.XZCGLPM1 skip;
    set DQ06M002.XZCGLPM2 skip;
    set DQ06M002.XZCGLPM3 skip;
    set DQ06M002.XZCFISMO skip;
    set DQ06M002.XZCGLEDT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DQ06M002.XZCCLNDT skip;
    set DQ06M002.XZCGLPM1 initialAttributes;
    set DQ06M002.XZCGLPM2 initialAttributes;
    set DQ06M002.XZCGLPM3 initialAttributes;
    set DQ06M002.XZCFISMO initialAttributes;
    set DQ06M002.XZCGLEDT initialAttributes;
  end

  set DQ06M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DQ06M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ06M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DQ06M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DQ06M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DQ06M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DQ06M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DQ06M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end

  /* **************/
  /* ****************** END DQ06P25 **************************/
  /* **************/
end // end DQ06P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DQ06P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DQ06P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DQ06M001.VAGen_EZEMSG = " ";
  DQ06M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DQ06P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DQ06P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DQ06P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DQ06P70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end

  /* **************/
  /* ****************** END DQ06P30 *****************************/
