   && DQ06W01.DA-PLOG-TXT1 == DQ06M002.DA-PLOG-TXT1
   && DQ06W01.DA-PLOG-TXT2 == DQ06M002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
