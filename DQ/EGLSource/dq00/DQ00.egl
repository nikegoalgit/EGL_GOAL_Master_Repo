// DRIVER FOR APPLICATION
Function DQ00P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DQ00P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DQ00P20-BUILD-MAP2(); /* Build the second map*/
  else
    DQ00P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DQ00P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DQ00P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DQ00P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DQ00P00 *******************************/
  /* **************/
end // end DQ00P00-DRIVER

// INITIALIZE THE APPLICATION
Function DQ00P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DQ00W01 empty; /* Working storage record*/
  set DQ00W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DQ00M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ00M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ00M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DQ00M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ00M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ00M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DQ00M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DQ00P10 ******************************/
  /* **************/
end // end DQ00P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DQ00P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DQ00P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DQ00M001.DAW-CURR-PAGE)
    DQ00S-TS-READ-PAGE(); /* read temp storage page*/
    DQ00P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DQ00P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ00P20 **********************************/
  /* **************/
end // end DQ00P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DQ00P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DQ00M002.DA-CHNG-USER-ID = sysVar.userID;
  DQ00M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DQ00W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ00M002.DA-PLOG-TXT1 = " ";
    DQ00M002.DA-PLOG-TXT2 = " ";
    DQ00M002.YPUCD = " ";
    DQ00M002.YPUDESC = " ";
    DQ00M002.YPUSDESC = " ";
    DQ00M002.YPUABRV = " ";
    DQ00M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DQ00W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DQ00W01.YPUGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DQ00W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DQ00W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DQ00M002.YPUCD = DQ00W01.YPUCD; /* move to the map*/
      DQ00M002.YPUDESC = DQ00W01.YPUDESC;
      DQ00M002.YPUSDESC = DQ00W01.YPUSDESC;
      DQ00M002.YPUABRV = DQ00W01.YPUABRV;
      DQ00M002.XXXSTAT = DQ00W01.XXXSTAT;
      DQ00M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DQ00M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DQ00M002.YPUCD = DQ00W02.YPUCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DQ00M002.YPUCD in YPUTBL.YPUCD)
          DQ00W01.YPUCD = YPUTBL.YPUCD[sysVar.arrayIndex]; /* save for a*/
          DQ00W01.YPUDESC = YPUTBL.YPUDESC[sysVar.arrayIndex]; /* later compare*/
          DQ00W01.YPUSDESC = YPUTBL.YPUSDESC[sysVar.arrayIndex];
          DQ00W01.YPUABRV = YPUTBL.YPUABRV[sysVar.arrayIndex];
          DQ00W01.XXXSTAT = YPUTBL.XXXSTAT[sysVar.arrayIndex];
          DQ00W01.ZZZSDT = YPUTBL.ZZZSDT[sysVar.arrayIndex];
          DQ00W01.ZZZCHGDT = YPUTBL.ZZZCHGDT[sysVar.arrayIndex];
          DQ00W01.ZZZCHGTM = YPUTBL.ZZZCHGTM[sysVar.arrayIndex];
          DQ00W01.ZZZCHGCT = YPUTBL.ZZZCHGCT[sysVar.arrayIndex];

          DQ00M002.YPUDESC = YPUTBL.YPUDESC[sysVar.arrayIndex]; /* move to map*/
          DQ00M002.YPUSDESC = YPUTBL.YPUSDESC[sysVar.arrayIndex];
          DQ00M002.YPUABRV = YPUTBL.YPUABRV[sysVar.arrayIndex];
          DQ00M002.XXXSTAT = YPUTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DQ00W01.YPUGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DQ00W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DQ00W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DQ00M002.YPUCD = DQ00W01.YPUCD; /* move to the map*/
    DQ00M002.YPUDESC = DQ00W01.YPUDESC;
    DQ00M002.YPUABRV = DQ00W01.YPUABRV;
    DQ00M002.XXXSTAT = DQ00W01.XXXSTAT;
    DQ00M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DQ00M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DQ00M002.XXXSTAT == "A")
    DQ00M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DQ00M002.XXXSTAT == "I")
    DQ00M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DQ00M002.XXXSTAT == " ")
    DQ00M002.DAW-TBL-STATUS-DESC = " ";
  end

  DQ00P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ00P20 **********************************/
  /* **************/
end // end DQ00P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DQ00P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DQ00W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ00P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ00S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YPUTBL.YPUCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ00P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ00S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DQ00S-TS-ADD-PAGE();
    end

    DQ00M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DQ00M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DQ00P22 **********************************/
  /* **************/
end // end DQ00P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DQ00P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DQ00W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DQ00W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DQ00W01.YPUGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DQ00W02.YPUCD[DAW-ROW-NBR] = DQ00W01.YPUCD;
  DQ00W02.YPUDESC[DAW-ROW-NBR] = DQ00W01.YPUDESC;
  DQ00W02.YPUABRV[DAW-ROW-NBR] = DQ00W01.YPUABRV;
  DQ00W02.XXXSTAT[DAW-ROW-NBR] = DQ00W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DQ00W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DQ00W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DQ00W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DQ00W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DQ00W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DQ00W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DQ00W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DQ00W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DQ00W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DQ00W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DQ00P221 ******************************/
  /* ***************/
end // end DQ00P23-DB2-TO-TSQ

