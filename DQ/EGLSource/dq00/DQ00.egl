// SET MAP 1 FIELD ATTRIBUTES
Function DQ00P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DQ00M001.DAW-PAGE-LIT = "PAGE";
    set DQ00M001.DAW-CURR-PAGE initialAttributes;
    DQ00M001.DAW-OF-LIT = "OF";
    set DQ00M001.DAW-LAST-PAGE initialAttributes;
    DQ00M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DQ00M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DQ00M001.DAW-PAGE-LIT = " ";
    set DQ00M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DQ00M001.DAW-OF-LIT = " ";
    set DQ00M001.DAW-LAST-PAGE skip, invisible;
    DQ00M001.DAW-PF7-LIT = " ";
    DQ00M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DQ00M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DQ00M001.YPUCD[sysVar.arrayIndex] == " ")
      set DQ00M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DQ00M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DQ00P25 **************************/
  /* **************/
end // end DQ00P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DQ00P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DQ00M002.YPUCD initialAttributes;
    set DQ00M002.YPUDESC initialAttributes;
    set DQ00M002.YPUSDESC initialAttributes;
    set DQ00M002.YPUABRV initialAttributes;
    set DQ00M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DQ00M002.YPUCD skip;
    set DQ00M002.YPUDESC skip;
    set DQ00M002.YPUSDESC skip;
    set DQ00M002.YPUABRV skip;
    set DQ00M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DQ00M002.YPUCD skip;
    set DQ00M002.YPUDESC initialAttributes;
    set DQ00M002.YPUSDESC initialAttributes;
    set DQ00M002.YPUABRV initialAttributes;
    set DQ00M002.XXXSTAT initialAttributes;
  end

  set DQ00M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DQ00M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ00M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DQ00M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DQ00M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DQ00M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DQ00M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DQ00M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DQ00P25 **************************/
  /* **************/
end // end DQ00P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DQ00P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DQ00P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DQ00M001.VAGen_EZEMSG = " ";
  DQ00M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DQ00P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DQ00P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DQ00P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DQ00P70-PROC-MAP1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DQ00P30 *****************************/
  /* **************/
end // end DQ00P30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DQ00P30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DQ00P40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DQ00M002.VAGen_EZEMSG = " ";
  DQ00M002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DQ00P25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DQ00P50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DQ00P60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DQ00P70-PROC-MAP2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DQ00P30 *****************************/
  /* **************/
end // end DQ00P30-MAIN-MAP-2

