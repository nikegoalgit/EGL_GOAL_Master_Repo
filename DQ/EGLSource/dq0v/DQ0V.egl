    DQ0VM001.XXXSTAT[sysVar.arrayIndex] = DQ0VW02.XXXSTAT[sysVar.arrayIndex];
    DQ0VM001.DAW-STATUS-DESC[sysVar.arrayIndex] = DQ0VW02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DQ0VM001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DQ0VW02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DQ0VM001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DQ0VW02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
