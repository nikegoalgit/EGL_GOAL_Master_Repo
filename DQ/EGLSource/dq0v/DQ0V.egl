//*** PROGRAM=DQ0V ****
//   NIKE TABLE HANDLER - TABLE MAINTENANCE - PRODUCTION ORDER
//                                            FORM UNIT
//   OVERVIEW: This application is used to maintain the
//             production order form unit table
// 
//   DB2 TABLE: DCORP.VYPF0001
//   CSP TABLE: YPFTBL
// 
//  DATE CREATED: 10/23/98
//        AUTHOR: Eric Chan
// 
//  MAINTENANCE HISTORY:
// 
//      date     by whom     overview of changes made
//    --------  ----------  -----------------------------------
//    03-29-99  ECHAN       ADD NOHILITE IN DQ0VP25-SET-ATTR-1
//                          CHECK DUPLICATE AND DEADLOCK IN
//                          DQ0VP70-PROC-MAP-2
// *********************
Program DQ0V type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DQ0VW01 DQ0VW01; // record
  DQ0VW02 DQ0VW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YPFTBL {deleteAfterUse = yes}; // table
  use DQ0VM.DQ0VM001,  // forms
      DQ0VM.DQ0VM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DQ0VP00-DRIVER: DQ0VP00-DRIVER();
    exit program;
  end // end main
end // end DQ0V

// DRIVER FOR APPLICATION
Function DQ0VP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DQ0VP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DQ0VP20-BUILD-MAP2(); /* Build the second map*/
  else
    DQ0VP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DQ0VP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DQ0VP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DQ0VP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DQ0VP00 *******************************/
  /* **************/
end // end DQ0VP00-DRIVER

// INITIALIZE THE APPLICATION
Function DQ0VP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DQ0VW01 empty; /* Working storage record*/
  set DQ0VW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DQ0VM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ0VM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ0VM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DQ0VM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ0VM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ0VM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DQ0VM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DQ0VP10 ******************************/
  /* **************/
end // end DQ0VP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DQ0VP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DQ0VP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DQ0VM001.DAW-CURR-PAGE)
    DQ0VS-TS-READ-PAGE(); /* read temp storage page*/
    DQ0VP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DQ0VP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ0VP20 **********************************/
  /* **************/
end // end DQ0VP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DQ0VP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DQ0VM002.DA-CHNG-USER-ID = sysVar.userID;
  DQ0VM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DQ0VW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ0VM002.DA-PLOG-TXT1 = " ";
    DQ0VM002.DA-PLOG-TXT2 = " ";
    DQ0VM002.YPFCD = " ";
    DQ0VM002.YPFDESC = " ";
    DQ0VM002.YPFSDESC = " ";
    DQ0VM002.YPFABRV = " ";
    DQ0VM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DQ0VW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DQ0VW01.YPFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DQ0VW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DQ0VW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DQ0VM002.YPFCD = DQ0VW01.YPFCD; /* move to the map*/
      DQ0VM002.YPFDESC = DQ0VW01.YPFDESC;
      DQ0VM002.YPFSDESC = DQ0VW01.YPFSDESC;
      DQ0VM002.YPFABRV = DQ0VW01.YPFABRV;
      DQ0VM002.XXXSTAT = DQ0VW01.XXXSTAT;
      DQ0VM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DQ0VM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DQ0VM002.YPFCD = DQ0VW02.YPFCD[DAW-ROW-NBR];
                                         /* get the tbl row*/
        if (DQ0VM002.YPFCD in YPFTBL.YPFCD)
          DQ0VW01.YPFCD = YPFTBL.YPFCD[sysVar.arrayIndex]; /* save for a*/
          DQ0VW01.YPFDESC = YPFTBL.YPFDESC[sysVar.arrayIndex]; /* later compare*/
          DQ0VW01.YPFSDESC = YPFTBL.YPFSDESC[sysVar.arrayIndex];
          DQ0VW01.YPFABRV = YPFTBL.YPFABRV[sysVar.arrayIndex];
          DQ0VW01.XXXSTAT = YPFTBL.XXXSTAT[sysVar.arrayIndex];
          DQ0VW01.ZZZSDT = YPFTBL.ZZZSDT[sysVar.arrayIndex];
          DQ0VW01.ZZZCHGDT = YPFTBL.ZZZCHGDT[sysVar.arrayIndex];
          DQ0VW01.ZZZCHGTM = YPFTBL.ZZZCHGTM[sysVar.arrayIndex];
          DQ0VW01.ZZZCHGCT = YPFTBL.ZZZCHGCT[sysVar.arrayIndex];

          DQ0VM002.YPFDESC = YPFTBL.YPFDESC[sysVar.arrayIndex]; /* move to map*/
          DQ0VM002.YPFSDESC = YPFTBL.YPFSDESC[sysVar.arrayIndex];
          DQ0VM002.YPFABRV = YPFTBL.YPFABRV[sysVar.arrayIndex];
          DQ0VM002.XXXSTAT = YPFTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DQ0VW01.YPFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DQ0VW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DQ0VW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DQ0VM002.YPFCD = DQ0VW01.YPFCD; /* move to the map*/
    DQ0VM002.YPFDESC = DQ0VW01.YPFDESC;
    DQ0VM002.YPFSDESC = DQ0VW01.YPFSDESC;
    DQ0VM002.YPFABRV = DQ0VW01.YPFABRV;
    DQ0VM002.XXXSTAT = DQ0VW01.XXXSTAT;
    DQ0VM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DQ0VM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DQ0VM002.XXXSTAT == "A")
    DQ0VM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DQ0VM002.XXXSTAT == "I")
    DQ0VM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DQ0VM002.XXXSTAT == " ")
    DQ0VM002.DAW-TBL-STATUS-DESC = " ";
  end

  DQ0VP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ0VP20 **********************************/
  /* **************/
end // end DQ0VP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DQ0VP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DQ0VW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ0VP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ0VS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YPFTBL.YPFCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ0VP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ0VS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DQ0VS-TS-ADD-PAGE();
    end

    DQ0VM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DQ0VM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DQ0VP22 **********************************/
  /* **************/
end // end DQ0VP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DQ0VP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DQ0VW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DQ0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DQ0VW01.YPFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DQ0VW02.YPFCD[DAW-ROW-NBR] = DQ0VW01.YPFCD;
  DQ0VW02.YPFDESC[DAW-ROW-NBR] = DQ0VW01.YPFDESC;
  DQ0VW02.YPFABRV[DAW-ROW-NBR] = DQ0VW01.YPFABRV;
  DQ0VW02.XXXSTAT[DAW-ROW-NBR] = DQ0VW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DQ0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DQ0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DQ0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DQ0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DQ0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DQ0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DQ0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DQ0VW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DQ0VW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DQ0VW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DQ0VP221 ******************************/
  /* ***************/
end // end DQ0VP23-DB2-TO-TSQ

