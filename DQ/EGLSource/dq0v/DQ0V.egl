  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DQ0VM001.DAW-SELECT-CD[sysVar.arrayIndex] = DQ0VW02.DAW-SELECT-CD[sysVar.arrayIndex];
    DQ0VM001.YPFCD[sysVar.arrayIndex] = DQ0VW02.YPFCD[sysVar.arrayIndex];
    DQ0VM001.YPFDESC[sysVar.arrayIndex] = DQ0VW02.YPFDESC[sysVar.arrayIndex];
