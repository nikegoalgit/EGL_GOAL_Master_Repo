// SET MAP 1 FIELD ATTRIBUTES
Function DQ0TP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DQ0TM001.DAW-PAGE-LIT = "PAGE";
    set DQ0TM001.DAW-CURR-PAGE initialAttributes;
    DQ0TM001.DAW-OF-LIT = "OF";
    set DQ0TM001.DAW-LAST-PAGE initialAttributes;
    DQ0TM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DQ0TM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DQ0TM001.DAW-PAGE-LIT = " ";
    set DQ0TM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DQ0TM001.DAW-OF-LIT = " ";
    set DQ0TM001.DAW-LAST-PAGE skip, invisible;
    DQ0TM001.DAW-PF7-LIT = " ";
    DQ0TM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DQ0TM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DQ0TM001.YOP-CSTM-PORT-CD[sysVar.arrayIndex] == " ")
      set DQ0TM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DQ0TM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DQ0TP25 **************************/
  /* **************/
end // end DQ0TP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DQ0TP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DQ0TM002.YOP-CSTM-PORT-CD initialAttributes;
    set DQ0TM002.YOP-CSTM-PORT-NM initialAttributes;
    set DQ0TM002.XCTCD initialAttributes;
    set DQ0TM002.XSTCD initialAttributes;
    set DQ0TM002.XXXCITY initialAttributes;
    set DQ0TM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DQ0TM002.YOP-CSTM-PORT-CD skip;
    set DQ0TM002.YOP-CSTM-PORT-NM skip;
    set DQ0TM002.XCTCD skip;
    set DQ0TM002.XSTCD skip;
    set DQ0TM002.XXXCITY skip;
    set DQ0TM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DQ0TM002.YOP-CSTM-PORT-CD skip;
    set DQ0TM002.YOP-CSTM-PORT-NM initialAttributes;
    set DQ0TM002.XCTCD initialAttributes;
    set DQ0TM002.XSTCD initialAttributes;
    set DQ0TM002.XXXCITY initialAttributes;
    set DQ0TM002.XXXSTAT initialAttributes;
  end

  set DQ0TM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DQ0TM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ0TM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DQ0TM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DQ0TM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DQ0TM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DQ0TM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DQ0TM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end

  /* **************/
  /* ****************** END DQ0TP25 **************************/
  /* **************/
end // end DQ0TP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DQ0TP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DQ0TP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DQ0TM001.VAGen_EZEMSG = " ";
  DQ0TM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DQ0TP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DQ0TP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DQ0TP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DQ0TP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end

  /* **************/
  /* ****************** END DQ0TP30 *****************************/
  /* **************/
end // end DQ0TP30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DQ0TP30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DQ0TP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DQ0TM002.VAGen_EZEMSG = " ";
  DQ0TM002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DQ0TP25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DQ0TP50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DQ0TP60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

