// BUILD NEW VDATRN01 RECORD
Function DQ0TP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DQ0TM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DQ0TM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DQ0TW01.DA-CCAT-KEY-TXT = " ";
  DQ0TW01.DQ0TW-KEY-YOP-CSTM-PORT-CD = DQ0TM002.YOP-CSTM-PORT-CD;
  VDATRN01.DA-CCAT-KEY-TXT = DQ0TW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DQ0TW01.YOP-CSTM-PORT-CD = DQ0TM002.YOP-CSTM-PORT-CD;
  DQ0TW01.YOP-CSTM-PORT-NM = DQ0TM002.YOP-CSTM-PORT-NM;
  DQ0TW01.XCTCD = DQ0TM002.XCTCD;
  DQ0TW01.XSTCD = DQ0TM002.XSTCD;
  DQ0TW01.XXXCITY = DQ0TM002.XXXCITY;
  DQ0TW01.XXXSTAT = DQ0TM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 79;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DQ0TW01.YOPGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DQ0TP72 **********************************/
  /* **************/
end // end DQ0TP72-NEW-TRAN

