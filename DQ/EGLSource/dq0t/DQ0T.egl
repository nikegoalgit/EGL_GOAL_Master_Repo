Program DQ0T type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DQ0TW01 DQ0TW01; // record
  DQ0TW02 DQ0TW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YOPTBL {deleteAfterUse = yes}; // table
  use XCTTBL {deleteAfterUse = yes}; // table
  use XSTTBL {deleteAfterUse = yes}; // table
  use DQ0TM.DQ0TM001,  // forms
      DQ0TM.DQ0TM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DQ0TP00-DRIVER: DQ0TP00-DRIVER();
    exit program;
  end // end main
end // end DQ0T

// DRIVER FOR APPLICATION
Function DQ0TP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DQ0TP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DQ0TP20-BUILD-MAP2(); /* Build the second map*/
  else
    DQ0TP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DQ0TP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DQ0TP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DQ0TP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DQ0TP00 *******************************/
  /* **************/
end // end DQ0TP00-DRIVER

// INITIALIZE THE APPLICATION
Function DQ0TP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DQ0TW01 empty; /* Working storage record*/
  set DQ0TW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DQ0TM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ0TM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ0TM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DQ0TM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ0TM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ0TM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DQ0TM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DQ0TP10 ******************************/
  /* **************/
end // end DQ0TP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DQ0TP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DQ0TP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DQ0TM001.DAW-CURR-PAGE)
    DQ0TS-TS-READ-PAGE(); /* read temp storage page*/
    DQ0TP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DQ0TP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ0TP20 **********************************/
  /* **************/
end // end DQ0TP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DQ0TP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DQ0TM002.DA-CHNG-USER-ID = sysVar.userID;
  DQ0TM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DQ0TW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DQ0TM002.XCTABRV = " ";
  DQ0TM002.XSTDESC = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ0TM002.DA-PLOG-TXT1 = " ";
    DQ0TM002.DA-PLOG-TXT2 = " ";
    DQ0TM002.YOP-CSTM-PORT-CD = " ";
    DQ0TM002.YOP-CSTM-PORT-NM = " ";
    DQ0TM002.XCTCD = " ";
    DQ0TM002.XSTCD = " ";
    DQ0TM002.XXXCITY = " ";
    DQ0TM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DQ0TW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DQ0TW01.YOPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DQ0TW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DQ0TW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

   /* move to the map*/
      DQ0TM002.YOP-CSTM-PORT-CD = DQ0TW01.YOP-CSTM-PORT-CD;
      DQ0TM002.YOP-CSTM-PORT-NM = DQ0TW01.YOP-CSTM-PORT-NM;
      DQ0TM002.XCTCD = DQ0TW01.XCTCD;
      DQ0TM002.XSTCD = DQ0TW01.XSTCD;
      DQ0TM002.XXXCITY = DQ0TW01.XXXCITY;
      DQ0TM002.XXXSTAT = DQ0TW01.XXXSTAT;
      DQ0TM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DQ0TM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DQ0TM002.YOP-CSTM-PORT-CD = DQ0TW02.YOP-CSTM-PORT-CD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DQ0TM002.YOP-CSTM-PORT-CD in YOPTBL.YOP-CSTM-PORT-CD)
          DQ0TW01.YOP-CSTM-PORT-CD = YOPTBL.YOP-CSTM-PORT-CD[sysVar.arrayIndex]; /* save for a*/
   /* later compare*/
          DQ0TW01.YOP-CSTM-PORT-NM = YOPTBL.YOP-CSTM-PORT-NM[sysVar.arrayIndex];
          DQ0TW01.XCTCD = YOPTBL.XCTCD[sysVar.arrayIndex];
          DQ0TW01.XSTCD = YOPTBL.XSTCD[sysVar.arrayIndex];
          DQ0TW01.XXXCITY = YOPTBL.XXXCITY[sysVar.arrayIndex];
          DQ0TW01.XXXSTAT = YOPTBL.XXXSTAT[sysVar.arrayIndex];
          DQ0TW01.ZZZSDT = YOPTBL.ZZZSDT[sysVar.arrayIndex];
          DQ0TW01.ZZZCHGDT = YOPTBL.ZZZCHGDT[sysVar.arrayIndex];
          DQ0TW01.ZZZCHGTM = YOPTBL.ZZZCHGTM[sysVar.arrayIndex];
          DQ0TW01.ZZZCHGCT = YOPTBL.ZZZCHGCT[sysVar.arrayIndex];

   /* move to map*/
          DQ0TM002.YOP-CSTM-PORT-NM = YOPTBL.YOP-CSTM-PORT-NM[sysVar.arrayIndex];
          DQ0TM002.XCTCD = YOPTBL.XCTCD[sysVar.arrayIndex];
          DQ0TM002.XSTCD = YOPTBL.XSTCD[sysVar.arrayIndex];
          DQ0TM002.XXXCITY = YOPTBL.XXXCITY[sysVar.arrayIndex];
          DQ0TM002.XXXSTAT = YOPTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DQ0TW01.YOPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DQ0TW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DQ0TW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

                                                      /* move to map*/
    DQ0TM002.YOP-CSTM-PORT-CD = DQ0TW01.YOP-CSTM-PORT-CD;
    DQ0TM002.YOP-CSTM-PORT-NM = DQ0TW01.YOP-CSTM-PORT-NM;
    DQ0TM002.XCTCD = DQ0TW01.XCTCD;
    DQ0TM002.XSTCD = DQ0TW01.XSTCD;
    DQ0TM002.XXXCITY = DQ0TW01.XXXCITY;
    DQ0TM002.XXXSTAT = DQ0TW01.XXXSTAT;
    DQ0TM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DQ0TM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DQ0TM002.XXXSTAT == "A")
    DQ0TM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DQ0TM002.XXXSTAT == "I")
    DQ0TM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DQ0TM002.XXXSTAT == " ")
    DQ0TM002.DAW-TBL-STATUS-DESC = " ";
  end

  DQ0TP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ0TP20 **********************************/
  /* **************/
end // end DQ0TP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DQ0TP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DQ0TW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ0TP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ0TS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YOPTBL.YOP-CSTM-PORT-CD[sysVar.arrayIndex] != "99999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ0TP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ0TS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DQ0TS-TS-ADD-PAGE();
    end

    DQ0TM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DQ0TM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DQ0TP22 **********************************/
  /* **************/
end // end DQ0TP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DQ0TP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DQ0TW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DQ0TW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DQ0TW01.YOPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DQ0TW02.YOP-CSTM-PORT-CD[DAW-ROW-NBR] = DQ0TW01.YOP-CSTM-PORT-CD;
  DQ0TW02.YOP-CSTM-PORT-NM[DAW-ROW-NBR] = DQ0TW01.YOP-CSTM-PORT-NM;
  DQ0TW02.XSTCD[DAW-ROW-NBR] = DQ0TW01.XSTCD;
  DQ0TW02.XXXSTAT[DAW-ROW-NBR] = DQ0TW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DQ0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DQ0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DQ0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DQ0TW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DQ0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DQ0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DQ0TW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DQ0TW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DQ0TW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DQ0TW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DQ0TP221 ******************************/
  /* ***************/
end // end DQ0TP23-DB2-TO-TSQ

