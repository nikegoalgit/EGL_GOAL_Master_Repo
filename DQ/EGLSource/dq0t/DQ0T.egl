  /* ************************************************************/

  TA5REC.TA5APPID = "DQ0T"; /* appl identification*/
  TA5REC.TA5FNCCD = "A"; /* function code*/
  DQ0TW02.DAW-TEMP-STORAGE-LENGTH = 1348;

  call "TA0050" (DQ0TW02, TA5REC) {isNoRefresh = yes, isExternal = yes}; /* temp storage subroutine*/

  if (TA5REC.TA5RTNCD == 0)
    DAWREC.DAW-MAX-PAGE = TA5REC.TA5TSQIX;
