  set DQ0RM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DQ0RM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ0RM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
