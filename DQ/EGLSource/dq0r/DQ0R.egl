// SET MAP 1 FIELD ATTRIBUTES
Function DQ0RP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DQ0RM001.DAW-PAGE-LIT = "PAGE";
    set DQ0RM001.DAW-CURR-PAGE initialAttributes;
    DQ0RM001.DAW-OF-LIT = "OF";
    set DQ0RM001.DAW-LAST-PAGE initialAttributes;
    DQ0RM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DQ0RM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DQ0RM001.DAW-PAGE-LIT = " ";
    set DQ0RM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DQ0RM001.DAW-OF-LIT = " ";
    set DQ0RM001.DAW-LAST-PAGE skip, invisible;
    DQ0RM001.DAW-PF7-LIT = " ";
    DQ0RM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DQ0RM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DQ0RM001.YOACD[sysVar.arrayIndex] == " ")
      set DQ0RM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DQ0RM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DQ0RP25 **************************/
  /* **************/
end // end DQ0RP25-SET-ATTR-1

