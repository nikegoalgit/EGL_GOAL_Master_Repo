// DRIVER FOR APPLICATION
Function DQ0RP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DQ0RP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DQ0RP20-BUILD-MAP2(); /* Build the second map*/
  else
    DQ0RP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DQ0RP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DQ0RP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DQ0RP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DQ0RP00 *******************************/
  /* **************/
end // end DQ0RP00-DRIVER

// INITIALIZE THE APPLICATION
Function DQ0RP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DQ0RW01 empty; /* Working storage record*/
  set DQ0RW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DQ0RM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ0RM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ0RM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DQ0RM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DQ0RM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DQ0RM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DQ0RM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DQ0RP10 ******************************/
  /* **************/
end // end DQ0RP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DQ0RP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DQ0RP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DQ0RM001.DAW-CURR-PAGE)
    DQ0RS-TS-READ-PAGE(); /* read temp storage page*/
    DQ0RP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DQ0RP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ0RP20 **********************************/
  /* **************/
end // end DQ0RP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DQ0RP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DQ0RM002.DA-CHNG-USER-ID = sysVar.userID;
  DQ0RM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DQ0RW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DQ0RM002.XGPABRV = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DQ0RM002.DA-PLOG-TXT1 = " ";
    DQ0RM002.DA-PLOG-TXT2 = " ";
    DQ0RM002.YOACD = " ";
    DQ0RM002.XGPCD = " ";
    DQ0RM002.YOADESC = " ";
    DQ0RM002.YOASDESC = " ";
    DQ0RM002.YOAABRV = " ";
    DQ0RM002.YOACMFL = " ";
    DQ0RM002.YOAADJFL = " ";
    DQ0RM002.YOARPTFL = " ";
    DQ0RM002.YOAINQFL = " ";
    DQ0RM002.YOALCFL = " ";
    DQ0RM002.YOA-EDI-SND-IND = " ";
    DQ0RM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DQ0RW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DQ0RW01.YOAGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DQ0RW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DQ0RW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DQ0RM002.YOACD = DQ0RW01.YOACD; /* move to the map*/
      DQ0RM002.XGPCD = DQ0RW01.XGPCD;
      DQ0RM002.YOADESC = DQ0RW01.YOADESC;
      DQ0RM002.YOASDESC = DQ0RW01.YOASDESC;
      DQ0RM002.YOAABRV = DQ0RW01.YOAABRV;
      DQ0RM002.YOACMFL = DQ0RW01.YOACMFL;
      DQ0RM002.YOAADJFL = DQ0RW01.YOAADJFL;
      DQ0RM002.YOARPTFL = DQ0RW01.YOARPTFL;
      DQ0RM002.YOAINQFL = DQ0RW01.YOAINQFL;
      DQ0RM002.YOALCFL = DQ0RW01.YOALCFL;
      DQ0RM002.YOA-EDI-SND-IND = DQ0RW01.YOA-EDI-SND-IND;
      DQ0RM002.XXXSTAT = DQ0RW01.XXXSTAT;
      DQ0RM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DQ0RM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DQ0RM002.YOACD = DQ0RW02.YOACD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DQ0RM002.YOACD in YOATBL.YOACD)
          DQ0RW01.YOACD = YOATBL.YOACD[sysVar.arrayIndex]; /* save for a*/
          DQ0RW01.XGPCD = YOATBL.XGPCD[sysVar.arrayIndex]; /* later compare*/
          DQ0RW01.YOADESC = YOATBL.YOADESC[sysVar.arrayIndex];
          DQ0RW01.YOASDESC = YOATBL.YOASDESC[sysVar.arrayIndex];
          DQ0RW01.YOAABRV = YOATBL.YOAABRV[sysVar.arrayIndex];
          DQ0RW01.YOACMFL = YOATBL.YOACMFL[sysVar.arrayIndex];
          DQ0RW01.YOAADJFL = YOATBL.YOAADJFL[sysVar.arrayIndex];
          DQ0RW01.YOARPTFL = YOATBL.YOARPTFL[sysVar.arrayIndex];
          DQ0RW01.YOAINQFL = YOATBL.YOAINQFL[sysVar.arrayIndex];
          DQ0RW01.YOALCFL = YOATBL.YOALCFL[sysVar.arrayIndex];
          DQ0RW01.YOA-EDI-SND-IND = YOATBL.YOA-EDI-SND-IND[sysVar.arrayIndex];
          DQ0RW01.XXXSTAT = YOATBL.XXXSTAT[sysVar.arrayIndex];
          DQ0RW01.ZZZSDT = YOATBL.ZZZSDT[sysVar.arrayIndex];
          DQ0RW01.ZZZCHGDT = YOATBL.ZZZCHGDT[sysVar.arrayIndex];
          DQ0RW01.ZZZCHGTM = YOATBL.ZZZCHGTM[sysVar.arrayIndex];
          DQ0RW01.ZZZCHGCT = YOATBL.ZZZCHGCT[sysVar.arrayIndex];

          DQ0RM002.XGPCD = YOATBL.XGPCD[sysVar.arrayIndex]; /* move to map*/
          DQ0RM002.YOADESC = YOATBL.YOADESC[sysVar.arrayIndex];
          DQ0RM002.YOASDESC = YOATBL.YOASDESC[sysVar.arrayIndex];
          DQ0RM002.YOAABRV = YOATBL.YOAABRV[sysVar.arrayIndex];
          DQ0RM002.YOACMFL = YOATBL.YOACMFL[sysVar.arrayIndex];
          DQ0RM002.YOAADJFL = YOATBL.YOAADJFL[sysVar.arrayIndex];
          DQ0RM002.YOARPTFL = YOATBL.YOARPTFL[sysVar.arrayIndex];
          DQ0RM002.YOAINQFL = YOATBL.YOAINQFL[sysVar.arrayIndex];
          DQ0RM002.YOALCFL = YOATBL.YOALCFL[sysVar.arrayIndex];
          DQ0RM002.XXXSTAT = YOATBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DQ0RW01.YOAGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DQ0RW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DQ0RW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DQ0RM002.YOACD = DQ0RW01.YOACD; /* move to the map*/
    DQ0RM002.XGPCD = DQ0RW01.XGPCD;
    DQ0RM002.YOADESC = DQ0RW01.YOADESC;
    DQ0RM002.YOASDESC = DQ0RW01.YOASDESC;
    DQ0RM002.YOAABRV = DQ0RW01.YOAABRV;
    DQ0RM002.YOACMFL = DQ0RW01.YOACMFL;
    DQ0RM002.YOAADJFL = DQ0RW01.YOAADJFL;
    DQ0RM002.YOARPTFL = DQ0RW01.YOARPTFL;
    DQ0RM002.YOAINQFL = DQ0RW01.YOAINQFL;
    DQ0RM002.YOALCFL = DQ0RW01.YOALCFL;
    DQ0RM002.YOA-EDI-SND-IND = DQ0RW01.YOA-EDI-SND-IND;
    DQ0RM002.XXXSTAT = DQ0RW01.XXXSTAT;
    DQ0RM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DQ0RM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DQ0RM002.XXXSTAT == "A")
    DQ0RM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DQ0RM002.XXXSTAT == "I")
    DQ0RM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DQ0RM002.XXXSTAT == " ")
    DQ0RM002.DAW-TBL-STATUS-DESC = " ";
  end

  DQ0RP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DQ0RP20 **********************************/
  /* **************/
end // end DQ0RP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DQ0RP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DQ0RW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ0RP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ0RS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YOATBL.YOACD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DQ0RP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DQ0RS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DQ0RS-TS-ADD-PAGE();
    end

    DQ0RM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DQ0RM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DQ0RP22 **********************************/
  /* **************/
end // end DQ0RP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DQ0RP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DQ0RW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DQ0RW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DQ0RW01.YOAGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DQ0RW02.YOACD[DAW-ROW-NBR] = DQ0RW01.YOACD;
  DQ0RW02.XGPCD[DAW-ROW-NBR] = DQ0RW01.XGPCD;
  DQ0RW02.YOASDESC[DAW-ROW-NBR] = DQ0RW01.YOASDESC;
  DQ0RW02.YOACMFL[DAW-ROW-NBR] = DQ0RW01.YOACMFL;
  DQ0RW02.YOAADJFL[DAW-ROW-NBR] = DQ0RW01.YOAADJFL;
  DQ0RW02.YOARPTFL[DAW-ROW-NBR] = DQ0RW01.YOARPTFL;
  DQ0RW02.YOAINQFL[DAW-ROW-NBR] = DQ0RW01.YOAINQFL;
  DQ0RW02.YOALCFL[DAW-ROW-NBR] = DQ0RW01.YOALCFL;
  DQ0RW02.XXXSTAT[DAW-ROW-NBR] = DQ0RW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DQ0RW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DQ0RW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DQ0RW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DQ0RW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DQ0RW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DQ0RW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DQ0RW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DQ0RW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DQ0RW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DQ0RW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DQ0RP221 ******************************/
  /* ***************/
end // end DQ0RP23-DB2-TO-TSQ

