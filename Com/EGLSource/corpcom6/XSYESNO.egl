package corpcom6;
import corpcom2.*;
//*** DATATABLE=XSYESNO ****
// 
// 
// ************************************************************
// *                                                          *
// * This table is used for automatic validation of yes no    *
// *fields.  The valid values are N, Y, NO, and YES.          *
// *                                                          *
// ************************************************************
// 
// 06-06-94  vcowan install using corp table turnover system
// 
// 
// **************************
DataTable XSYESNO type matchValidTable  {
  shared = yes, resident = no
  }
  10 YESNO YESNO; 
   {contents = [
    ["Y  "],
    ["N  "],
    ["YES"],
    ["NO "]
  ]}
end // end XSYESNO

