  OCWREC.OCWXNXCD = OB1REC.XNXCD; /* dept cd*/
  OCWREC.XSOCD = OB1REC.XSOCD; /* sales office id*/
  OCWREC.XDVCD = OB1REC.XDVCD; /* division code*/
