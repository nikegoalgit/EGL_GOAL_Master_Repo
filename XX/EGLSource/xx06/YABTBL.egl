package xx06;
import xx.common.*;
//*** DATATABLE=YABTBL ****
// ************* PRODUCTION ADIVISING BANK TABLE **************
// 
// This table contains the code, description and abbreviation
// that identify a Production advising bank table.   These
// are the banks through which the factories do their monetary
// transactions.
// 
// Corresponding DB2 table is DCORP.VYAB0001.
// 
//  DATE       NAME    DESCRIPTION OF CHANGE
// --------   ------   ----------------------------------------
// 01/07/88   RLESOW   CREATED TABLE LAYOUT
// 09-19-89   JKB      Check out table, move to PRODMSL.
// 02-15-94   vcowan   Install using Corp Table Turnover System
// *************************
DataTable YABTBL type basicTable  {
  shared = yes, resident = no
  }
  10 YABCD YABCD; 
  10 YABDESC YABDESC; 
  10 YABSDESC YABSDESC; 
  10 YABABRV YABABRV; 
  10 XXXSTAT XXXSTAT; 
  10 ZZZSDT ZZZSDT; 
  10 ZZZCHGDT ZZZCHGDT; 
  10 ZZZCHGTM ZZZCHGTM; 
  10 ZZZCHGCT ZZZCHGCT; 
   {contents = [
    ["00", "NOT APPLICABLE                ", "NOT APPLIC  ", "N/A   ", "A", 019880101, 019880101, 1000, 0000],
    ["99", "END OF TABLE                  ", "EOT         ", "EOT   ", "I", 019880101, 019880101, 1000, 0000]
  ]}
end // end YABTBL

