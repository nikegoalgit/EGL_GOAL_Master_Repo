package ne0f;

import corpcom7.XXXSTAT;
//*** DATATABLE=NE0FT02 ****
// ************************************************************
// * NE0FT02 - MANAGEMENT REGION DESCRIPTION TABLE
// * CREATED 1-13-94    BY JEFF DEMARIS
// * MSEVAL  04-19-1999 Included the new management region code
// *                    for Africa region as 'AF'.
// *
// * list of valid management regions for NE0F
// **************************
DataTable NE0FT02 type basicTable  {
  shared = yes, resident = no
  }
  10 NE0FT-02MGT-CD NE0FT-02MGT-CD; 
  10 NE0FT-02MGT-DESC NE0FT-02MGT-DESC; 
  10 XXXSTAT XXXSTAT; 
   {contents = [
    ["XX", "ALL MGT REGIONS               ", "A"],
    ["AP", "ASIA-PACIFIC                  ", "A"],
    ["CA", "CANADA                        ", "A"],
    ["EU", "EUROPE                        ", "A"],
    ["LA", "LAAM                          ", "A"],
    ["AF", "AFRICA                        ", "A"],
    ["ZZ", "END-OF-TABLE-MARKER           ", "Z"]
  ]}
end // end NE0FT02

