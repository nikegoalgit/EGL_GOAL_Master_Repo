// Close CMPNT join csr
Function FK0Y-1900()
   /* ------------------------------------------------------------*/
   /* Close the cursor.*/
   /* ------------------------------------------------------------*/
  ;
  ;
  try
    call "FK1110" ("C1", SQLCA, FKZREC) {isNoRefresh = yes, isExternal = yes};
  end /* Select table row*/
  ;
   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    ;
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      return; /* Return to previous process*/
    end
  end
  ;
   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-REQUEST-DATA-SW = "N";
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
  ;
  FKWREC.XXX-MOD-ID = "1900"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(11); /* Data in use by another*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    converseLib.validationFailed(10); /* Unexpected error occurred*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "CLOSE CMPNT JOIN      ";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FKWREC.XXX-MOD-ID; /* Program number*/
  TA1REC.TA1DBASE = "DCMPNT  "; /* Data base name*/
  TA1REC.TA1TBLVU = "JOIN    "; /* Table view name*/
  TA1REC.TA1TBLKE = FKZREC.FKZREC-GROUP; /* Table key*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
end // end FK0Y-1900

// Format and converse the map
Function FK0Y-2000()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* If error found*/
    FKWREC.XXXMSGNO = converseVar.validationMsgNum; /* Save message number*/
  end
  ;
  ;
   /* ------------------------------------------------------------*/
   /* If requested, and no error, format data to display.*/
   /* ------------------------------------------------------------*/
  ;
  if (FK0YW00.FK0YW-FMAT-MAP-SW == "Y" /* Format requested*/
   && converseVar.validationMsgNum == 0) /* and no error to display*/
    ;
    FK0Y-2100(); /* Format the map*/
  end
  ;
  ;
   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/
  ;
  FK0YM110.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/
  ;
  FK0YM110.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FK0YM110.CAITEM = COMMAREA.CAITEM; /* Application data*/
  ;
   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/
  ;
  if (FKWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/
    ;
    set FK0YM110 alarm; /* Indicate sound alarm*/
    ;
    if (FKWREC.XXXMSGNO != 0) /* Message to display*/
      ;
      if (FKWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(FKWREC.XXXMSGNO);
      end /* Message number*/
    else
      FK0YM110.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/
      ;
    end
    ;
    FK0YM110.XXX-MOD-ID = FKWREC.XXX-MOD-ID; /* Module id*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* Set all detail lines dark if no data to display on them.*/
   /* ------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-MAP-IDX = 1; /* Init map index*/
  ;
  while (FK0YW00.FK0YW-MAP-IDX <= 14) /* Do this for map line*/
    ;
    if (FK0YM110.SY1STNBR[FK0YW-MAP-IDX] == "   ") /* No data*/
      ;
      set FK0YM110.FK0YM-QTY1[FK0YW-MAP-IDX] invisible, skip;
      set FK0YM110.FK0YM-QTY2[FK0YW-MAP-IDX] invisible, skip;
      set FK0YM110.FK0YM-QTY3[FK0YW-MAP-IDX] invisible, skip;
      set FK0YM110.FK0YM-QTY4[FK0YW-MAP-IDX] invisible, skip;
      set FK0YM110.FK0YM-QTY5[FK0YW-MAP-IDX] invisible, skip;
    end
    ;
    FK0YW00.FK0YW-MAP-IDX = FK0YW00.FK0YW-MAP-IDX + 1;
  end
  ;
   /* ------------------------------------------------------------*/
   /* Set map header dark if no data was found to display.*/
   /* ------------------------------------------------------------*/
  ;
  if (FK0YW00.TA5TSQIX == 0) /* If data not found*/
    ;
    FK0YW00.FK0YW-IDX = 1; /* Init index*/
    ;
    while (FK0YW00.FK0YW-IDX <= 5) /* For each date*/
      ;
      set FK0YM110.FK0YM-HDR-DT[FK0YW-IDX] invisible, skip; /* Set dark*/
      FK0YW00.FK0YW-IDX = FK0YW00.FK0YW-IDX + 1;
    end
  end
  ;
  ;
   /* ------------------------------------------------------------*/
   /* Establish cursor position.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum == 0 /* If no message to display*/
   || FKWREC.XXXMSGNO == 1)             /* or update complete*/
    ;
    set FK0YM110.CATOAP cursor;
  end
  ;
  ;
  converse FK0YM110 ;
  ;
   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/
  ;
  FKWREC.XXX-MOD-ID = " "; /* Module identification*/
  FKWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/
  ;
  COMMAREA.CATOAP = FK0YM110.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FK0YM110.CAITEM; /* Application data*/
  ;
  ;
end // end FK0Y-2000

// Format the data to display
Function FK0Y-2100()
   /* --------------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* --------------------------------------------------------------------*/
  ;
  set FK0YM110 initial; /* Initialize map*/
  ;
  FK0YW-FMAT-MAP-SW = "N"; /* Format map switch*/
  ;
  ;
   /* --------------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed, obtain*/
   /* the desired page from temporary storage.*/
   /* --------------------------------------------------------------------*/
  ;
  if (FK0YW00.TA5TSQIX > 0) /* Pages were formatted*/
    ;
    if (FK0YW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/
      ;
      if (FK0YW00.XXXTSPAG > FK0YW00.TA5TSQIX) /* Desireg page > highest*/
        FK0YW00.XXXTSPAG = FK0YW00.TA5TSQIX; /* Defaul to highest*/
      end
      ;
      FK0Y-2110(); /* Obtain temp storage page*/
    end
  end
  ;
  ;
   /* --------------------------------------------------------------------*/
   /* Format header data to the map.*/
   /* --------------------------------------------------------------------*/
  ;
  FK0YM110.FK-VEND-CD = FK0YW01.FK-VEND-CD; /* Vendor cd*/
  FK0YM110.FK-MAT-NBR = FK0YW01.FK-MAT-NBR; /* Material nbr*/
  FK0YM110.FK-DEPT-MAT-DESC = FK0YW01.FK-DEPT-MAT-DESC; /* Mtrl desc*/
  FK0YM110.XTM-MAT-THCK-DESC = FK0YW01.XTM-MAT-THCK-DESC; /* Thk desc*/
  FK0YM110.XCLDESC = FK0YW01.XCLDESC; /* Color desc*/
  ;
  FK0YM110.XXXTSPAG = FK0YW00.XXXTSPAG; /* Current page*/
  FK0YM110.TA5TSQIX = FK0YW00.TA5TSQIX; /* Max page*/
  ;
   /* --------------------------------------------------------------------*/
   /* If requested, format the left header*/
   /* --------------------------------------------------------------------*/
  ;
  if (FK0YW00.FK0YW-SIDE-TO-DISP-SW == "L") /* If left page to display*/
    ;
    FK0YW00.FK0YW-IDX = 1; /* Initialize idx*/
    ;
    while (FK0YW00.FK0YW-IDX <= 5) /* For each buy date*/
      ;
      FK0YW00.FK0YW-BUY-DT = FK0YW00.FK0YW-MAP-DATE[FK0YW-IDX]; /* Breakout date*/
      ;
      FK0YW00.FK0YW-FORMAT-YY = FK0YW00.FK0YW-BUY-YY; /* Format year*/
      FK0YW00.FK0YW-FORMAT-MM = FK0YW00.FK0YW-BUY-MM; /* Format month*/
      ;
      FK0YM110.FK0YM-HDR-DT[FK0YW-IDX] = FK0YW00.FK0YW-FORMAT-DT;
      ;
      FK0YM110.FK0YM-HDR-TYPE[FK0YW-IDX] = FK0YW00.FK0YM-HDR-TYPE[FK0YW-IDX]; /* Header type*/
      ;
      FK0YW00.FK0YW-IDX = FK0YW00.FK0YW-IDX + 1;
    end
  end
  ;
  ;
   /* --------------------------------------------------------------------*/
   /* If requested, format the right header*/
   /* --------------------------------------------------------------------*/
  ;
  if (FK0YW00.FK0YW-SIDE-TO-DISP-SW != "L") /* If right page to display*/
    ;
    FK0YW00.FK0YW-IDX = 6; /* Initialize idx*/
    FK0YW00.FK0YW-IDX2 = 1; /* Initialize another idx*/
    ;
    while (FK0YW00.FK0YW-IDX <= 10 /* For each buy date*/
     && FK0YW00.FK0YW-IDX2 <= 5) /* and no more than 5 columns*/
      ;
      FK0YW00.FK0YW-BUY-DT = FK0YW00.FK0YW-MAP-DATE[FK0YW-IDX]; /* Breakout date*/
      ;
      FK0YW00.FK0YW-FORMAT-YY = FK0YW00.FK0YW-BUY-YY; /* Format year*/
      FK0YW00.FK0YW-FORMAT-MM = FK0YW00.FK0YW-BUY-MM; /* Format month*/
      ;
      FK0YM110.FK0YM-HDR-DT[FK0YW-IDX2] = FK0YW00.FK0YW-FORMAT-DT;
      ;
      FK0YM110.FK0YM-HDR-TYPE[FK0YW-IDX2] = FK0YW00.FK0YM-HDR-TYPE[FK0YW-IDX]; /* Header type*/
      ;
      FK0YW00.FK0YW-IDX = FK0YW00.FK0YW-IDX + 1;
      FK0YW00.FK0YW-IDX2 = FK0YW00.FK0YW-IDX2 + 1;
    end
  end
  ;
  ;
   /* --------------------------------------------------------------------*/
   /* If requested, format the left detail data*/
   /* --------------------------------------------------------------------*/
  ;
  if (FK0YW00.FK0YW-SIDE-TO-DISP-SW == "L") /* If left page to display*/
    ;
    FK0YW00.FK0YW-MAP-IDX = 1; /* Initialize idx*/
    ;
    while (FK0YW00.FK0YW-MAP-IDX <= 14) /* For each map line*/
      ;
      FK0YM110.SY1STNBR[FK0YW-MAP-IDX] = FK0YW01.SY1STNBR[FK0YW-MAP-IDX]; /* Style*/
      FK0YM110.SY2CLRID[FK0YW-MAP-IDX] = FK0YW01.SY2CLRID[FK0YW-MAP-IDX]; /* Color*/
      FK0YM110.XDMCD[FK0YW-MAP-IDX] = FK0YW01.XDMCD[FK0YW-MAP-IDX]; /* Dimension*/
      FK0YM110.SY1SSTNM[FK0YW-MAP-IDX] = FK0YW01.SY1SSTNM[FK0YW-MAP-IDX]; /* Short desc*/
      FK0YM110.SY2CLRAB[FK0YW-MAP-IDX] = FK0YW01.SY2CLRAB[FK0YW-MAP-IDX]; /* Color abrev*/
      FK0YM110.FK0YM-QTY1[FK0YW-MAP-IDX] = FK0YW01.FK0YM-QTY1[FK0YW-MAP-IDX]; /* Qty 1*/
      FK0YM110.FK0YM-QTY2[FK0YW-MAP-IDX] = FK0YW01.FK0YM-QTY2[FK0YW-MAP-IDX]; /* Qty 2*/
      FK0YM110.FK0YM-QTY3[FK0YW-MAP-IDX] = FK0YW01.FK0YM-QTY3[FK0YW-MAP-IDX]; /* Qty 3*/
      FK0YM110.FK0YM-QTY4[FK0YW-MAP-IDX] = FK0YW01.FK0YM-QTY4[FK0YW-MAP-IDX]; /* Qty 4*/
      FK0YM110.FK0YM-QTY5[FK0YW-MAP-IDX] = FK0YW01.FK0YM-QTY5[FK0YW-MAP-IDX]; /* Qty 5*/
      ;
      FK0YW00.FK0YW-MAP-IDX = FK0YW00.FK0YW-MAP-IDX + 1;
    end
  end
  ;
   /* --------------------------------------------------------------------*/
   /* If requested, format the right detail data*/
   /* --------------------------------------------------------------------*/
  ;
  if (FK0YW00.FK0YW-SIDE-TO-DISP-SW != "L") /* If right page to display*/
    ;
    FK0YW00.FK0YW-MAP-IDX = 1; /* Initialize idx*/
    ;
    while (FK0YW00.FK0YW-MAP-IDX <= 14) /* For each map line*/
      ;
