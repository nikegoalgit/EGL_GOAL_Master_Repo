      FK0YM110.SY1SSTNM[FK0YW-MAP-IDX] = FK0YW01.SY1SSTNM[FK0YW-MAP-IDX]; /* Short desc*/
      FK0YM110.SY2CLRAB[FK0YW-MAP-IDX] = FK0YW01.SY2CLRAB[FK0YW-MAP-IDX]; /* Color abbrev*/
