    )                             
      FK0YW00.SY1SSTNM = "************"; /* Short name*/
      FK0YW00.SY2CLRAB = "*************"; /* Color abbrev*/
      ;
      return;
    end
  end
  ;
   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
