//*** PROGRAM=FK0Y ****
// THIS PROGRAM IS PART OF THE COMPONENTS FORECASTING SYSTEM.
// 
// THIS APPLICATION PROVIDES INQUIRY CAPABILITY FOR LRPS BY
// VENDOR, MATERIAL, COLOR, AND STYLE/COLOR/DIMENSION.  THIS
// APPLICATION WILL BE NETERED FROM APPLICATION FK0X,
// BRINGING VENDOR, MATERIAL NUMBER, MATERIAL COLOR, LRPS
// NUMBER AND LRPS TYPE ALONG WITH THE TRANSFER.  THE
// APPLICATION WILL THEN OBTAIN ALL STYLE/COLOR/DIMENSION
// COMBINATIONS FOR THE KEY DATA PASSED AND WILL ACCUMULATE
// LRPS QUANTITIES FOR EACH OF THEM.  THE DATA WILL BE
// FORMATTED TO TEMPORARY STORAGE AND DISPLAYED ON THE MAP.
// USERS WILL BE ALLOWED TO USE PF7 AND PF8 SCROLLING LOGIC
// TO VIEW MULTIPLE PAGES OF DATA.  PF10 AND PF11 WILL ALSO
// BE PROVIDED TO DISPLAY LEFT AND RIGHT PAGES FOR MULTIPLE
// MONTH VIEWING.
// 
// THE USER MAY EXIT TO ANY ALLOWED APPLICATION VIA FASTPATH
// OR PFKEYS, WITH THE OPTION FIELD AVAILABLE FOR USE WITH ANY
// APPLICATION TRANSFER.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      03/16/92  JALLE1  New application.
//          08/10/93  JALLE1  Remove references to FBDMDTYP
//                            for Aug '93 table changes.
// y2kfix   11-09-97  oricha  y2k modifications:  fk0ym-hdr-dt
//                            set to zero fill on map fk0ym110
//                            for year 2000.
// *********************
Program FK0Y type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "FK00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  FK0YW00 FK0YW00; // record
  FK0YW01 FK0YW01; // record
  FKWREC FKWREC; // record
  FKZREC FKZREC; // record
  PG2REC PG2REC; // record
  PG4REC PG4REC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VLRPSH01 VLRPSH01; // record
  VLRSTY01 VLRSTY01; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XCLTBL; // table
  use XTMTBL; // table
  use FK0YM.FK0YM110 // form
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPFK001: XPFK001();
     /* ------------------------------------------------------------*/
     /* Flow to perform application initialization.*/
     /* ------------------------------------------------------------*/
    ;
    goto FK0Y-001; /* Application initialization*/
    ;
    ;
    FK0Y-001: FK0Y-001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/
    ;
    goto FK0Y-002; /* Converse the main process*/
    ;
    ;
    FK0Y-002: FK0Y-002();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/
    ;
    goto FK0Y-002; /* Converse the main process*/
    ;
    ;
  end // end main
end // end FK0Y

// Application initialization
Function FK0Y-001()
   /* ------------------------------------------------------------*/
   /* Initalize working storage and display storage areas.*/
   /* ------------------------------------------------------------*/
  ;
  set FK0YM110 initial; /* M110 input/output*/
  set FK0YW00 empty; /* Application working storage*/
  set FK0YW01 empty; /* M110 display storage*/
  set SQLCA empty; /* SQL communication area*/
  set TAEREC empty; /* Error diagnosis information*/
  set TA1REC empty; /* Error diagnosis parameters*/
  set TA5REC empty; /* Temporary storage parameters*/
  ;
  ;
   /* ------------------------------------------------------------*/
   /* Verify application entered via fk0v, w, or x.  If not, set*/
   /* error and exit to display the map.*/
   /* ------------------------------------------------------------*/
  ;
  if (COMMAREA.CAFROMAP != "FK0X" /* If from app. not = FK0X*/
   && COMMAREA.CAFROMAP != "FK0Y") /* and from app. not = FK0Y*/
    ;
    converseLib.validationFailed(72); /* Cannot fastpath to this application*/
    FKWREC.XXX-MOD-ID = "0001"; /* Module identification*/
    set FK0YM110.CATOAP cursor; /* Set cursor*/
    FK0YW00.FK0YW-REQUEST-DATA-SW = "N"; /* Don't request data*/
    FK0YW00.FK0YW-DATA-FOUND-SW = "N"; /* No data was found*/
    FK0YW00.FK0YW-FMAT-MAP-SW = "N"; /* Don't format the map*/
    ;
    return; /* Exit to display the map*/
  end
  ;
  ;
   /* ------------------------------------------------------------*/
   /* Use data passed in the COMMAREA as the initial key.*/
   /* ------------------------------------------------------------*/
  ;
  FK0YM110.FK-VEND-CD = FKWREC.FK-VEND-CD[1]; /* Vendor code*/
  FK0YM110.FK-MAT-NBR = FKWREC.FK-MAT-NBR[1]; /* Material nbr*/
  FK0YW00.XMG-COLR-GRP-CD = FKWREC.XMG-COLR-GRP-CD[1]; /* Color grp*/
  FK0YW00.XCLCD = FKWREC.XCLCD[1]; /* Color*/
  FK0YW00.FBAMDNBR = FKWREC.FBAMDNBR; /* Model number*/
  ;
  set FKWREC empty; /* Initialize fkwrec*/
  ;
  ;
  ;
  FK0YW00.FK0YW-REQUEST-DATA-SW = "Y"; /* Request data*/
  FK0YW00.FK0YW-DATA-FOUND-SW = "N"; /* No data found (yet)*/
  FK0YW00.FK0YW-FMAT-MAP-SW = "N"; /* Don't format the map*/
  FK0YW00.FK0YW-SIDE-TO-DISP-SW = "L"; /* Format left page*/
  ;
  ;
end // end FK0Y-001

// Main process to display a map
Function FK0Y-002()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/
  ;
   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/
  ;
   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/
  ;
  FK0Y-1000(); /* Obtain the data to display*/
  ;
  FK0Y-2000(); /* Format and display the map*/
  ;
  FK0Y-3000(); /* Check attention identifier*/
  ;
  FK0Y-4000(); /* Reset fields to normal intensity*/
  ;
  FK0Y-5000(); /* Verify the entered data*/
  ;
  FK0Y-6000(); /* Process valid input request*/
  ;
  ;
end // end FK0Y-002

// Obtain data to display
Function FK0Y-1000()
   /* --------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* --------------------------------------------------------------*/
  ;
  if (FK0YW00.FK0YW-REQUEST-DATA-SW != "Y" /* Data not requested*/
   || converseVar.validationMsgNum != 0)   /* or an error was found*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  ;
  FK0YW00.FK0YW-DATA-FOUND-SW = "N"; /* Init data found switch*/
  ;
  ;
   /* --------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* --------------------------------------------------------------*/
  ;
  FK0YW00.XXXTSLEN = 1854; /* Temporary storage page length*/
  ;
  ;
   /* --------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* --------------------------------------------------------------*/
  ;
  if (FKWREC.XXXMSGNO == 0) /* No message to display*/
    FK0YW00.XXXTSPAG = 1; /* Page number to display*/
  end
  ;
  ;
   /* --------------------------------------------------------------*/
   /* Purge any temporary storage for this application.*/
   /* --------------------------------------------------------------*/
  ;
  FK0Y-1700(); /* Purge temp storage*/
  ;
  ;
   /* --------------------------------------------------------------*/
   /* Obtain the lrps header record.*/
   /* --------------------------------------------------------------*/
  ;
  FK0Y-1100(); /* Get lrps header*/
  ;
  ;
   /* --------------------------------------------------------------*/
   /* Calculate dates to be used on the map.*/
   /* --------------------------------------------------------------*/
  ;
  FK0Y-1200(); /* Calculate dates*/
  ;
  ;
   /* --------------------------------------------------------------*/
   /* Declare and process a cursor to do a join of the vendor,*/
   /* material, color group, color, and material usage tables.*/
   /* --------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-END-CMPNT-SW = "N"; /* Init end of cursor switch*/
  FK0YW00.FK0YW-MAP-IDX = 1; /* Init map index*/
  ;
  FK0Y-1400(); /* Declare cursor to join cmpnt tables*/
  ;
  FK0Y-1500(); /* Fetch cmpnt join cursor*/
  ;
   /* --------------------------------------------------------------*/
   /* Initialize the working storage hold area and header variables.*/
   /* --------------------------------------------------------------*/
  ;
  FK0YW00.FK0YM-QTY1 = 0; /* Column 1 quantity*/
  FK0YW00.FK0YM-QTY2 = 0; /* Column 2 quantity*/
  FK0YW00.FK0YM-QTY3 = 0; /* Column 3 quantity*/
  FK0YW00.FK0YM-QTY4 = 0; /* Column 4 quantity*/
  FK0YW00.FK0YM-QTY5 = 0; /* Column 5 quantity*/
  FK0YW00.FK0YM-QTY6 = 0; /* Column 6 quantity*/
  FK0YW00.FK0YM-QTY7 = 0; /* Column 7 quantity*/
  FK0YW00.FK0YM-QTY8 = 0; /* Column 8 quantity*/
  FK0YW00.FK0YM-QTY9 = 0; /* Column 9 quantity*/
  FK0YW00.FK0YM-QTY10 = 0; /* Column 10 quantity*/
  ;
  if (FK0YW00.FK0YW-END-CMPNT-SW != "Y") /* If not end of cursor*/
    ;
    FK0YW00.FK-DEPT-MAT-DESC = FKZREC.FK-DEPT-MAT-DESC; /* Desc*/
    FK0YW00.XTM-MAT-THCK-CD = FKZREC.XTM-MAT-THCK-CD; /* Thickness*/
    ;
    if (FK0YW00.XTM-MAT-THCK-CD in XTMTBL.XTM-MAT-THCK-CD
     && XTMTBL.XXXSTAT[sysVar.arrayIndex] == "A")
      ;
      FK0YW00.XTM-MAT-THCK-DESC = XTMTBL.XTM-MAT-THCK-DESC[sysVar.arrayIndex]; /* Thickness desc*/
    else
      FK0YW00.XTM-MAT-THCK-DESC = "************"; /* Thickness desc*/
    end
    ;
    FK0YW00.XMG-COLR-GRP-CD = FKZREC.XMG-COLR-GRP-CD; /* Color grp*/
    FK0YW00.XCLCD = FKZREC.XCLCD; /* Color*/
    ;
    if (FK0YW00.XCLCD in XCLTBL.XCLCD
     && XCLTBL.XXXSTAT[sysVar.arrayIndex] == "A")
      ;
      FK0YW00.XCLDESC = XCLTBL.XCLDESC[sysVar.arrayIndex]; /* Color desc*/
    else
      ;
      FK0YW00.XCLDESC = "************"; /* Color desc*/
    end
    FK0YW00.SY1STNBR = FKZREC.SY1STNBR; /* Style*/
    FK0YW00.SY2CLRID = FKZREC.SY2CLRID; /* Color*/
    FK0YW00.XDMCD = FKZREC.XDMCD; /* Dimension*/
  else
    ;
    FK0YW00.FK-DEPT-MAT-DESC = " "; /* Desc*/
    FK0YW00.XTM-MAT-THCK-CD = " "; /* Thickness*/
    FK0YW00.XTM-MAT-THCK-DESC = " "; /* Thickness desc*/
    FK0YW00.XMG-COLR-GRP-CD = " "; /* Color group*/
    FK0YW00.XCLCD = " "; /* Color*/
    FK0YW00.XCLDESC = " "; /* Color desc*/
    FK0YW00.SY1STNBR = " "; /* Style*/
    FK0YW00.SY2CLRID = " "; /* Color*/
    FK0YW00.XDMCD = " "; /* Dimension*/
  end
  ;
  while (FK0YW00.FK0YW-END-CMPNT-SW != "Y") /* Until end of cursor*/
    ;
    FK0Y-1600(); /* Obtain lrps quantities*/
    ;
    FK0Y-1500(); /* Fetch cmpnt join cursor*/
    ;
    if (FK0YW00.FK0YW-END-CMPNT-SW == "Y"   /* If end of cursor*/
     || FKZREC.SY1STNBR != FK0YW00.SY1STNBR /* or style change*/
     || FKZREC.SY2CLRID != FK0YW00.SY2CLRID /* or color change*/
     || FKZREC.XDMCD != FK0YW00.XDMCD)      /* or dimension change*/
      ;
      /* --------------------------------------------------------------*/
      /* If lrps data was found,*/
      /* Format a detail line to temporary storage.  If necessary, add*/
      /* the temporary storage page to the temporary storage page queue.*/
      /* --------------------------------------------------------------*/
      ;
      if (FK0YW00.FK0YW-DATA-FOUND-SW == "Y") /* If data was found*/
        ;
        ;
        FK0YS-01(); /* Obtain product descriptions*/
        ;
        ;
        FK0YW01.FK-VEND-CD = FK0YM110.FK-VEND-CD; /* Vendor*/
        FK0YW01.FK-MAT-NBR = FK0YM110.FK-MAT-NBR; /* Material*/
        FK0YW01.FK-DEPT-MAT-DESC = FK0YW00.FK-DEPT-MAT-DESC; /* Material desc*/
        FK0YW01.XTM-MAT-THCK-CD = FK0YW00.XTM-MAT-THCK-CD; /* Thickness*/
        FK0YW01.XTM-MAT-THCK-DESC = FK0YW00.XTM-MAT-THCK-DESC; /* Thickness desc*/
        FK0YW01.XMG-COLR-GRP-CD = FK0YW00.XMG-COLR-GRP-CD; /* Color group*/
        FK0YW01.XCLCD = FK0YW00.XCLCD; /* Color*/
        FK0YW01.XCLDESC = FK0YW00.XCLDESC; /* Color desc*/
        FK0YW01.SY1STNBR[FK0YW-MAP-IDX] = FK0YW00.SY1STNBR; /* Style*/
        FK0YW01.SY2CLRID[FK0YW-MAP-IDX] = FK0YW00.SY2CLRID; /* Color*/
        FK0YW01.XDMCD[FK0YW-MAP-IDX] = FK0YW00.XDMCD; /* Dimension*/
        FK0YW01.SY1SSTNM[FK0YW-MAP-IDX] = FK0YW00.SY1SSTNM;
        FK0YW01.SY2CLRAB[FK0YW-MAP-IDX] = FK0YW00.SY2CLRAB;
        ;
        FK0YW01.FK0YM-QTY1[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY1;
        FK0YW01.FK0YM-QTY2[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY2;
        FK0YW01.FK0YM-QTY3[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY3;
        FK0YW01.FK0YM-QTY4[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY4;
        FK0YW01.FK0YM-QTY5[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY5;
        FK0YW01.FK0YM-QTY6[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY6;
        FK0YW01.FK0YM-QTY7[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY7;
        FK0YW01.FK0YM-QTY8[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY8;
        FK0YW01.FK0YM-QTY9[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY9;
        FK0YW01.FK0YM-QTY10[FK0YW-MAP-IDX] = FK0YW00.FK0YM-QTY10;
        ;
        FK0YW00.FK0YW-MAP-IDX = FK0YW00.FK0YW-MAP-IDX + 1; /* Increment*/
        ;
        if (FK0YW00.FK0YW-MAP-IDX > 14) /* If temp storage page full*/
          ;
          FK0Y-1800(); /* Add page to temp storage page queue*/
          ;
          FK0YW00.FK0YW-MAP-IDX = 1; /* Reset map index*/
        end
        ;
      end
      ;
      /* --------------------------------------------------------------*/
      /* Initialize the working storage hold area.*/
      /* Only do this if not at the end of the components cursor.*/
      /* --------------------------------------------------------------*/
      ;
      if (FK0YW00.FK0YW-END-CMPNT-SW != "Y") /* If not end of cursor*/
        ;
        FK0YW00.FK0YW-DATA-FOUND-SW = "N"; /* Init data found switch*/
        ;
        FK0YW00.SY1STNBR = FKZREC.SY1STNBR; /* Style*/
        FK0YW00.SY2CLRID = FKZREC.SY2CLRID; /* Color*/
        FK0YW00.XDMCD = FKZREC.XDMCD; /* Dimension*/
        FK0YW00.FK0YM-QTY1 = 0; /* Column 1 quantity*/
        FK0YW00.FK0YM-QTY2 = 0; /* Column 2 quantity*/
        FK0YW00.FK0YM-QTY3 = 0; /* Column 3 quantity*/
        FK0YW00.FK0YM-QTY4 = 0; /* Column 4 quantity*/
        FK0YW00.FK0YM-QTY5 = 0; /* Column 5 quantity*/
        FK0YW00.FK0YM-QTY6 = 0; /* Column 6 quantity*/
        FK0YW00.FK0YM-QTY7 = 0; /* Column 7 quantity*/
        FK0YW00.FK0YM-QTY8 = 0; /* Column 8 quantity*/
        FK0YW00.FK0YM-QTY9 = 0; /* Column 9 quantity*/
        FK0YW00.FK0YM-QTY10 = 0; /* Column 10 quantity*/
      end
    end
  end
  ;
  FK0Y-1900(); /* Close cmpnt join cursor*/
  ;
   /* --------------------------------------------------------------*/
   /* Add any partially formatted page to temporary storage.*/
   /* --------------------------------------------------------------*/
  ;
  if (FK0YW00.FK0YW-MAP-IDX > 1) /* Data formatted*/
    FK0Y-1800(); /* Add page to temporary storage*/
  end
  ;
  ;
  ;
   /* --------------------------------------------------------------*/
   /* If data was found, save map header information.  Otherwise,*/
   /* set up 'no data to display' message.*/
   /* --------------------------------------------------------------*/
  ;
  if (FK0YW00.TA5TSQIX == 0) /* If data was not found*/
    ;
    converseLib.validationFailed(18); /* Request processed - no data*/
    FKWREC.XXX-MOD-ID = "1000"; /* Module identification*/
    set FK0YM110.FK-VEND-CD bold;
    set FK0YM110.FK-MAT-NBR bold;
    set FK0YM110.XCLDESC bold;
    set FK0YM110.CATOAP cursor; /* Set cursor*/
  end
  ;
  ;
   /* --------------------------------------------------------------*/
   /* Indicate request for data is complete.*/
   /* --------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-REQUEST-DATA-SW = "N";
  FK0YW00.FK0YW-FMAT-MAP-SW = "Y";
  ;
  ;
end // end FK0Y-1000

// Select unique DCMPNT.VLRPSH01
Function FK0Y-1100()
   /* ------------------------------------------------------------*/
   /* Select the requested row from the table.*/
   /* ------------------------------------------------------------*/
  ;
  set VLRPSH01 empty; /* Clear row storage area*/
  ;
  VLRPSH01.FBAMDNBR = FK0YW00.FBAMDNBR; /* LRPS number*/
  ;
  try
    call "IO6250" ("S ", SQLCA, VLRPSH01) {isNoRefresh = yes, isExternal = yes};
  end /* Select table row*/
  ;
   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    ;
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      return; /* Return to previous process*/
    end
    ;
    if (SQLCA.VAGen_SQLCODE == 100) /* Row not found*/
      ;
      FKWREC.XXX-MOD-ID = "1100"; /* module id*/
      converseLib.validationFailed(15); /* No row for specified key value*/
      ;
      exit stack; /* Exit to display the map*/
      ;
    end
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-REQUEST-DATA-SW = "N";
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
  ;
  FKWREC.XXX-MOD-ID = "1100"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(11); /* Data in use by another*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    converseLib.validationFailed(10); /* Unexpected error occurred*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT DCMPNT.VLRPSH01";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FKWREC.XXX-MOD-ID; /* Program number*/
  TA1REC.TA1DBASE = "DCMPNT  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VLRPSH01"; /* Table view name*/
  TA1REC.TA1TBLKE = VLRPSH01.VLRPSH01-GROUP; /* Table key*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
end // end FK0Y-1100

// Calculate map dates
Function FK0Y-1200()
   /* ------------------------------------------------------------*/
   /* Calculate the prelim date.*/
   /* Prelim date = vlrpsh01.fb-buy-dt - 1 month.*/
   /* ------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-BUY-DT = VLRPSH01.FB8BUYDT;
  ;
  if (FK0YW00.FK0YW-BUY-MM == 1) /* If year end*/
    ;
    FK0YW00.FK0YW-BUY-MM = 12; /* move December to month*/
    FK0YW00.FK0YW-BUY-CCYY = FK0YW00.FK0YW-BUY-CCYY - 1; /* subract 1 yy*/
  else
    ;
    FK0YW00.FK0YW-BUY-MM = FK0YW00.FK0YW-BUY-MM - 1; /* subtract 1 mm*/
  end
  ;
  FK0YW00.FK0YW-MAP-DATE[4] = FK0YW00.FK0YW-BUY-DT; /* Prelim*/
  FK0YW00.FK0YM-HDR-TYPE[4] = "PRELIM"; /* Header type*/
  ;
  ;
   /* ------------------------------------------------------------*/
   /* Calculate the actual dates.*/
   /* Actual dates are the 3 months prior to the prelim date.*/
   /* ------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-BUY-DT = FK0YW00.FK0YW-MAP-DATE[4];
  ;
  FK0YW00.FK0YW-IDX = 3; /* Initialize index*/
  ;
  while (FK0YW00.FK0YW-IDX > 0) /* Calculate 3 months*/
    ;
    if (FK0YW00.FK0YW-BUY-MM == 1) /* If year end*/
      ;
      FK0YW00.FK0YW-BUY-MM = 12; /* move December to month*/
      FK0YW00.FK0YW-BUY-CCYY = FK0YW00.FK0YW-BUY-CCYY - 1; /* subract 1 yy*/
    else
      ;
      FK0YW00.FK0YW-BUY-MM = FK0YW00.FK0YW-BUY-MM - 1; /* subtract 1 mm*/
    end
    ;
    FK0YW00.FK0YW-MAP-DATE[FK0YW-IDX] = FK0YW00.FK0YW-BUY-DT;
    FK0YW00.FK0YM-HDR-TYPE[FK0YW-IDX] = "ACTUAL"; /* Header type*/
    ;
    FK0YW00.FK0YW-IDX = FK0YW00.FK0YW-IDX - 1; /* Decrement index*/
  end
  ;
  ;
   /* ------------------------------------------------------------*/
   /* Calculate the forecast dates.*/
   /* Forecast dates are the 6 months following the prelim date.*/
   /* ------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-BUY-DT = FK0YW00.FK0YW-MAP-DATE[4];
  ;
  FK0YW00.FK0YW-IDX = 5; /* Initialize index*/
  ;
  while (FK0YW00.FK0YW-IDX <= 10) /* Calculate 6 months*/
    ;
    if (FK0YW00.FK0YW-BUY-MM == 12 /* If year end*/
    )                             
      FK0YW00.FK0YW-BUY-MM = 1; /* move January to month*/
      FK0YW00.FK0YW-BUY-CCYY = FK0YW00.FK0YW-BUY-CCYY + 1; /* subract 1 yy*/
    else
      ;
      FK0YW00.FK0YW-BUY-MM = FK0YW00.FK0YW-BUY-MM + 1; /* subtract 1 mm*/
    end
    ;
    FK0YW00.FK0YW-MAP-DATE[FK0YW-IDX] = FK0YW00.FK0YW-BUY-DT;
    FK0YW00.FK0YM-HDR-TYPE[FK0YW-IDX] = "FORCST"; /* Header type*/
    ;
    FK0YW00.FK0YW-IDX = FK0YW00.FK0YW-IDX + 1; /* Decrement index*/
  end
  ;
  ;
end // end FK0Y-1200

