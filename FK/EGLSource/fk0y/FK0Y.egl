// Obtain data to display
Function FK0Y-1600()
   /* --------------------------------------------------------------*/
   /* Accumulate lrps quantities into a working storage hold area.*/
   /* --------------------------------------------------------------*/
  ;
  FK0YW00.FK0YW-END-LRSTY-SW = "N"; /* Init end of cursor switch*/
  FK0YW00.FK0YW-TEMP-QTY1 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY2 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY3 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY4 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY5 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY6 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY7 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY8 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY9 = 0; /* Init qty*/
  FK0YW00.FK0YW-TEMP-QTY10 = 0; /* Init qty*/
  ;
  FK0Y-1610(); /* Declare cursor for DCMPNT.VLRSTY01*/
  ;
  FK0Y-1620(); /* Fetch DCMPNT.VLRSTY01 cursor*/
  ;
  ;
  while (FK0YW00.FK0YW-END-LRSTY-SW != "Y") /* Until end of cursor*/
    ;
       /* ==== Add quantities to working storage according to buy date ===*/
    ;
    if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[1])
      FK0YW00.FK0YW-TEMP-QTY1 = FK0YW00.FK0YW-TEMP-QTY1 + VLRSTY01.FB8FTQTB;
    else
      ;
      if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[2])
        FK0YW00.FK0YW-TEMP-QTY2 = FK0YW00.FK0YW-TEMP-QTY2 + VLRSTY01.FB8FTQTB;
      else
        ;
        if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[3])
          FK0YW00.FK0YW-TEMP-QTY3 = FK0YW00.FK0YW-TEMP-QTY3 + VLRSTY01.FB8FTQTB;
        else
          ;
          if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[4])
            FK0YW00.FK0YW-TEMP-QTY4 = FK0YW00.FK0YW-TEMP-QTY4 + VLRSTY01.FB8FTQTB;
          else
            ;
            if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[5])
              FK0YW00.FK0YW-TEMP-QTY5 = FK0YW00.FK0YW-TEMP-QTY5 + VLRSTY01.FB8FTQTB;
            else
              ;
              if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[6])
                FK0YW00.FK0YW-TEMP-QTY6 = FK0YW00.FK0YW-TEMP-QTY6 + VLRSTY01.FB8FTQTB;
              else
                ;
                if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[7])
                  FK0YW00.FK0YW-TEMP-QTY7 = FK0YW00.FK0YW-TEMP-QTY7 + VLRSTY01.FB8FTQTB;
                else
                  ;
                  if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[8])
                    FK0YW00.FK0YW-TEMP-QTY8 = FK0YW00.FK0YW-TEMP-QTY8 + VLRSTY01.FB8FTQTB;
                  else
                    ;
                    if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[9])
                      FK0YW00.FK0YW-TEMP-QTY9 = FK0YW00.FK0YW-TEMP-QTY9 + VLRSTY01.FB8FTQTB;
                    else
                      ;
                      if (VLRSTY01.FB8BUYDT == FK0YW00.FK0YW-MAP-DATE[10])
                        FK0YW00.FK0YW-TEMP-QTY10 = FK0YW00.FK0YW-TEMP-QTY10 + VLRSTY01.FB8FTQTB;
                      end
                    end
                  end
                end
              end
            end
          end
        end
      end
    end
    ;
    ;
    FK0Y-1620(); /* Fetch DCMPNT.VLRSTY01 cursor*/
  end
  ;
  ;
     /* ======= Determine usage ========================================*/
  ;
  if (FKZREC.FK-MAT-FINL-USE != 0) /* Final usage*/
    FK0YW00.FK0YW-USAGE = FKZREC.FK-MAT-FINL-USE; /* Save usage*/
  else
    if (FKZREC.FK-MAT-PROD-USE != 0) /* Prod usage*/
      FK0YW00.FK0YW-USAGE = FKZREC.FK-MAT-PROD-USE; /* Save usage*/
    else
      FK0YW00.FK0YW-USAGE = FKZREC.FK-MAT-DEVL-USE; /* Save usage*/
    end
  end
  ;
  ;
     /* ======= convert lrps qty to manufacturing usage quantity =======*/
  ;
  FK0YW00.FK0YW-TEMP-QTY1 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY1   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY1));
  ;
  FK0YW00.FK0YW-TEMP-QTY2 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY2   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY2));
  ;
  FK0YW00.FK0YW-TEMP-QTY3 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY3   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY3));
  ;
  FK0YW00.FK0YW-TEMP-QTY4 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY4   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY4));
  ;
  FK0YW00.FK0YW-TEMP-QTY5 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY5   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY5));
  ;
  FK0YW00.FK0YW-TEMP-QTY6 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY6   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY6));
  ;
  FK0YW00.FK0YW-TEMP-QTY7 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY7   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY7));
  ;
  FK0YW00.FK0YW-TEMP-QTY8 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY8   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY8));
  ;
  FK0YW00.FK0YW-TEMP-QTY9 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY9   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY9));
  ;
  FK0YW00.FK0YW-TEMP-QTY10 = mathLib.round(FK0YW00.FK0YW-TEMP-QTY10   * FKZREC.FK-MAT-MFG-PCT   * FK0YW00.FK0YW-USAGE, -mathLib.decimals(FK0YW00.FK0YW-TEMP-QTY10));
  ;
  ;
  FK0YW00.FK0YM-QTY1 = FK0YW00.FK0YM-QTY1 + FK0YW00.FK0YW-TEMP-QTY1;
  FK0YW00.FK0YM-QTY2 = FK0YW00.FK0YM-QTY2 + FK0YW00.FK0YW-TEMP-QTY2;
  FK0YW00.FK0YM-QTY3 = FK0YW00.FK0YM-QTY3 + FK0YW00.FK0YW-TEMP-QTY3;
  FK0YW00.FK0YM-QTY4 = FK0YW00.FK0YM-QTY4 + FK0YW00.FK0YW-TEMP-QTY4;
  FK0YW00.FK0YM-QTY5 = FK0YW00.FK0YM-QTY5 + FK0YW00.FK0YW-TEMP-QTY5;
  FK0YW00.FK0YM-QTY6 = FK0YW00.FK0YM-QTY6 + FK0YW00.FK0YW-TEMP-QTY6;
  FK0YW00.FK0YM-QTY7 = FK0YW00.FK0YM-QTY7 + FK0YW00.FK0YW-TEMP-QTY7;
  FK0YW00.FK0YM-QTY8 = FK0YW00.FK0YM-QTY8 + FK0YW00.FK0YW-TEMP-QTY8;
  FK0YW00.FK0YM-QTY9 = FK0YW00.FK0YM-QTY9 + FK0YW00.FK0YW-TEMP-QTY9;
  FK0YW00.FK0YM-QTY10 = FK0YW00.FK0YM-QTY10 + FK0YW00.FK0YW-TEMP-QTY10;
  ;
  ;
  FK0Y-1630(); /* Close DCMPNT.VLRSTY01 cursor*/
  ;
  ;
end // end FK0Y-1600

