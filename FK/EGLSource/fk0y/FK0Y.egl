// Check attention identifier
Function FK0Y-3000()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, fastpath to current application*/
   /* and map.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    ;
    set FKWREC empty; /* Initialize fkwrec*/
    ;
    FKWREC.FK-VEND-CD[1] = FK0YM110.FK-VEND-CD; /* Vendor*/
    FKWREC.FK-MAT-NBR[1] = FK0YM110.FK-MAT-NBR; /* Material*/
    FKWREC.XMG-COLR-GRP-CD[1] = FK0YW00.XMG-COLR-GRP-CD; /* Clr grp*/
    FKWREC.XCLCD[1] = FK0YW00.XCLCD; /* Color*/
    FKWREC.FBAMDNBR = FK0YW00.FBAMDNBR; /* Model number*/
    ;
    COMMAREA.CATOAP = "FK0Y"; /* to applicaiton*/
    COMMAREA.CAITEM = " "; /* item*/
    ;
    XSFKS01(); /* Exit to another application*/
    ;
    set FK0YM110.CATOAP cursor, bold; /* Highlight*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/
  ;
  if (FK0YM110.CATOAP > " ") /* FAST PATH was requested*/
    ;
    if (converseVar.eventKey is enter) /* ENTER was pressed*/
      ;
      set FKWREC empty; /* Initialize fkwrec*/
      ;
      FKWREC.FK-VEND-CD[1] = FK0YM110.FK-VEND-CD; /* Vendor*/
      FKWREC.FK-MAT-NBR[1] = FK0YM110.FK-MAT-NBR; /* Material*/
      FKWREC.XMG-COLR-GRP-CD[1] = FK0YW00.XMG-COLR-GRP-CD; /* Clr grp*/
      FKWREC.XCLCD[1] = FK0YW00.XCLCD; /* Color*/
      FKWREC.FBAMDNBR = FK0YW00.FBAMDNBR; /* Model number*/
      ;
      COMMAREA.CATOAP = FK0YM110.CATOAP; /* to applicaiton*/
      COMMAREA.CAITEM = FK0YM110.CAITEM; /* item*/
      ;
      XSFKS01(); /* Exit to another application*/
      ;
      set FK0YM110.CATOAP cursor, bold; /* Highlight*/
      ;
    else
      ;
      FKWREC.XXX-MOD-ID = "3000"; /* Module identification*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/
      ;
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If requested, process PF key.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf1  /* HELP requested*/
   || converseVar.eventKey is pf3  /* FK0X requested*/
   || converseVar.eventKey is pf4  /* MSTR MN requested*/
   || converseVar.eventKey is pf12 /* FK0P requested*/
   || converseVar.eventKey is pf14 /* FK0R requested*/
   || converseVar.eventKey is pf16) /* FK0T requested*/
    ;
    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CATOAP = "GD01"; /* TO application*/
      COMMAREA.CAITEM = "FK0YM110"; /* Application find code*/
    end
    ;
    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FK0X"; /* TO application*/
    end
    ;
    if (converseVar.eventKey is pf4) /* PF4 was pressed*/
      COMMAREA.CATOAP = "MN0M"; /* TO application*/
      COMMAREA.CAITEM = "    "; /* Application find code*/
    end
    ;
    if (converseVar.eventKey is pf12) /* PF12 was pressed*/
      COMMAREA.CATOAP = "FK0P"; /* TO application*/
    end
    ;
    if (converseVar.eventKey is pf14) /* PF14 was pressed*/
      COMMAREA.CATOAP = "FK0R"; /* TO application*/
    end
    ;
    if (converseVar.eventKey is pf16) /* PF16 was pressed*/
      COMMAREA.CATOAP = "FK0T"; /* TO application*/
    end
    ;
    set FKWREC empty; /* Initialize fkwrec*/
    ;
    FKWREC.FK-VEND-CD[1] = FK0YM110.FK-VEND-CD; /* Vendor*/
    FKWREC.FK-MAT-NBR[1] = FK0YM110.FK-MAT-NBR; /* Material*/
    FKWREC.XMG-COLR-GRP-CD[1] = FK0YW00.XMG-COLR-GRP-CD; /* Clr grp*/
    FKWREC.XCLCD[1] = FK0YW00.XCLCD; /* Color*/
    FKWREC.FBAMDNBR = FK0YW00.FBAMDNBR; /* Model number*/
    COMMAREA.CAITEM = FK0YM110.CAITEM; /* Application find code*/
    ;
    XSFKS01(); /* Exit to another application*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* Back*/
   && converseVar.eventKey not pf8   /* Fwrd*/
   && converseVar.eventKey not pf10  /* Left*/
   && converseVar.eventKey not pf11) /* Rght*/
    ;
    FKWREC.XXX-MOD-ID = "3000"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  ;
end // end FK0Y-3000

// Reset Highlighted fields
Function FK0Y-4000()
   /* --------------------------------------------------------------------*/
   /* Reset all modifiable field attributes to normal intensity.*/
   /* --------------------------------------------------------------------*/
  ;
  set FK0YM110.FK-VEND-CD initialAttributes; /* Vender code*/
  set FK0YM110.FK-MAT-NBR initialAttributes; /* Material nbr*/
  set FK0YM110.XCLDESC initialAttributes; /* Color desc*/
  ;
  FK0YW00.FK0YW-IDX = 1; /* Initialize index*/
  ;
  while (FK0YW00.FK0YW-IDX <= 5) /* For each date*/
    set FK0YM110.FK0YM-HDR-DT[FK0YW-IDX] initialAttributes; /* Header dates*/
    FK0YW00.FK0YW-IDX = FK0YW00.FK0YW-IDX + 1;
  end
  ;
  ;
  FK0YW00.FK0YW-MAP-IDX = 1; /* Initialize index*/
  ;
  while (FK0YW00.FK0YW-MAP-IDX <= 14) /* For each line*/
    ;
    set FK0YM110.FK0YM-QTY1[FK0YW-MAP-IDX] initialAttributes; /* Qty 1*/
    set FK0YM110.FK0YM-QTY2[FK0YW-MAP-IDX] initialAttributes; /* Qty 2*/
    set FK0YM110.FK0YM-QTY3[FK0YW-MAP-IDX] initialAttributes; /* Qty 3*/
    set FK0YM110.FK0YM-QTY4[FK0YW-MAP-IDX] initialAttributes; /* Qty 4*/
    set FK0YM110.FK0YM-QTY5[FK0YW-MAP-IDX] initialAttributes; /* Qty 5*/
    FK0YW00.FK0YW-MAP-IDX = FK0YW00.FK0YW-MAP-IDX + 1;
  end
  ;
  set FK0YM110.CATOAP initialAttributes; /* To application*/
  set FK0YM110.CAITEM initialAttributes; /* Application data*/
  ;
  ;
end // end FK0Y-4000

// Verify entered fields
Function FK0Y-5000()
   /* ---------------------------------------------------------------*/
   /* Verify application was not entered via fastpath.  If it was,*/
   /* set an error and exit to display the map.*/
   /* ---------------------------------------------------------------*/
  ;
  if (FK0YM110.FK-VEND-CD == "  ") /* Vendor never entered*/
    ;
    converseLib.validationFailed(72); /* Can't fastpath into this application*/
    FKWREC.XXX-MOD-ID = "5000"; /* Module identification*/
    set FK0YM110.CATOAP cursor; /* Set cursor*/
    ;
    exit stack; /* Exit to display the map*/
  end
  ;
  ;
   /* ---------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ---------------------------------------------------------------*/
  ;
  if (FK0YM110.CAITEM > " ") /* Application data was entered*/
    ;
    set FK0YM110.CAITEM cursor, bold; /* Highlight*/
    FKWREC.XXX-MOD-ID = "5000"; /* Module identification*/
    converseLib.validationFailed(7); /* Only use this with fastpath*/
  end
  ;
  ;
   /* ---------------------------------------------------------------*/
   /* Verify scroll request.*/
   /* ---------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7  /* Pf7 pressed*/
   || converseVar.eventKey is pf8  /* Pf8 pressed*/
   || converseVar.eventKey is pf10 /* Pf10 pressed*/
   || converseVar.eventKey is pf11) /* Pf11 pressed*/
    ;
    FK0Y-5100(); /* Verify scroll request*/
  end
  ;
  ;
   /* ---------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ---------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack;
  end
  ;
  ;
end // end FK0Y-5000

// Verify scroll request
Function FK0Y-5100()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && FK0YW00.XXXTSPAG < 2)       /* and no previous pages*/
    ;
    FKWREC.XXX-MOD-ID = "5100"; /* Module identification*/
    converseLib.validationFailed(2); /* No previous page to display*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && FK0YW00.XXXTSPAG >= FK0YW00.TA5TSQIX) /* and no more pages*/
    ;
    FKWREC.XXX-MOD-ID = "5100"; /* Module identification*/
    converseLib.validationFailed(3); /* No more pages to display*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If scroll left was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf10 /* Scroll left request*/
   && FK0YW00.FK0YW-SIDE-TO-DISP-SW == "L") /* And already on left page*/
    ;
    FKWREC.XXX-MOD-ID = "5100"; /* Module identification*/
    converseLib.validationFailed(56); /* No more columns to left to display*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If scroll right was requested, endure that pages exist.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf11 /* Scroll right request*/
   && FK0YW00.FK0YW-SIDE-TO-DISP-SW == "R") /* And already on right page*/
    ;
    FKWREC.XXX-MOD-ID = "5100"; /* Module identification*/
    converseLib.validationFailed(57); /* No more columns to right to display*/
    ;
  end
  ;
  ;
end // end FK0Y-5100

// Process valid request
Function FK0Y-6000()
   /* ------------------------------------------------------------*/
   /* If scroll backward was requested, decrement the page number*/
   /* and exit to display the map.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7) /* Back*/
    ;
    FK0YW00.FK0YW-FMAT-MAP-SW = "Y"; /* Format the map*/
    FK0YW00.XXXTSPAG = FK0YW00.XXXTSPAG - 1; /* Decrement page to display*/
    ;
    return;
  end
  ;
  ;
   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, increment the page number*/
   /* and exit to display the map.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf8) /* Forward*/
    ;
    FK0YW00.FK0YW-FMAT-MAP-SW = "Y"; /* Format the map*/
    FK0YW00.XXXTSPAG = FK0YW00.XXXTSPAG + 1; /* Decrement page to display*/
    ;
    return;
  end
  ;
  ;
   /* ------------------------------------------------------------*/
   /* If scroll left was requested, set side to display switch*/
   /* and exit to display the map.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf10) /* left*/
    ;
    FK0YW00.FK0YW-FMAT-MAP-SW = "Y"; /* Format the map*/
    FK0YW00.FK0YW-SIDE-TO-DISP-SW = "L"; /* Display the left page*/
    ;
    return;
  end
  ;
  ;
   /* ------------------------------------------------------------*/
   /* If scroll right was requested, set side to display switch*/
   /* and exit to display the map.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf11) /* Right*/
    ;
    FK0YW00.FK0YW-FMAT-MAP-SW = "Y"; /* Format the map*/
    FK0YW00.FK0YW-SIDE-TO-DISP-SW = "R"; /* Display the right page*/
    ;
    return;
  end
  ;
  ;
end // end FK0Y-6000

// Get product data
Function FK0YS-01()
   /* ------------------------------------------------------------*/
   /* Set up key*/
   /* ------------------------------------------------------------*/
  ;
  set PG2REC empty;
  ;
  PG2REC.XGPCD = "02"; /* Group code*/
  PG2REC.SY1STNBR = FK0YW00.SY1STNBR; /* Style number*/
  PG2REC.SY2CLRID = FK0YW00.SY2CLRID; /* Color id*/
  PG2REC.XDMCD = FK0YW00.XDMCD; /* Dimension code*/
  ;
   /* ------------------------------------------------------------*/
   /* Select the requested row from the table.*/
   /* ------------------------------------------------------------*/
  ;
  try
    call "IO3120" ("S ", SQLCA, PG2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select row*/
  ;
   /* ------------------------------------------------------------*/
   /* If the call was successful, return to the previous process.*/
   /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    ;
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      ;
      FK0YW00.SY1SSTNM = PG2REC.SY1SSTNM; /* Short style name*/
      FK0YW00.SY2CLRAB = PG2REC.SY2CLRAB; /* Color abbreviation*/
      ;
      return;
      ;
    end
    ;
    if (SQLCA.VAGen_SQLCODE == 100) /* Not found*/
      ;
      FK0YS-02(); /* Obtain style development desc*/
      ;
      return;
    end
  end
  ;
   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that an unexpected error has occurred.*/
   /* ------------------------------------------------------------*/
  ;
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL Commarea*/
  ;
  FKWREC.XXX-MOD-ID = "S-01"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(11); /* Data was not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    converseLib.validationFailed(10); /* Unexpected error occurred*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "S-01 GET PRODUCT DATA           ";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = "FK0YM110"; /* Program number*/
  TA1REC.TA1DBASE = "DPRODG  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VSYCLR01"; /* Table view name*/
  TA1REC.TA1TBLKE = PG2REC.PG2KEY; /* Key*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
  ;
end // end FK0YS-01

