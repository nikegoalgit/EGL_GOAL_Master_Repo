   /* --------------------------------------------------------------------*/

  set FK0XM110 initial; /* Initialize map*/

  FK0XW-FMAT-MAP-SW = "N"; /* Format map switch*/


   /* --------------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed, obtain*/
   /* the desired page from temporary storage.*/
   /* --------------------------------------------------------------------*/

  if (FK0XW00.TA5TSQIX > 0) /* Pages were formatted*/

    if (FK0XW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      if (FK0XW00.XXXTSPAG > FK0XW00.TA5TSQIX) /* Desireg page > highest*/
        FK0XW00.XXXTSPAG = FK0XW00.TA5TSQIX; /* Defaul to highest*/
      end

      FK0X-2110(); /* Obtain temp storage page*/
    end
  end


   /* --------------------------------------------------------------------*/
