// Obtain data to display
Function FK0X-1600()
   /* --------------------------------------------------------------*/
   /* Accumulate lrps quantities into a working storage hold area.*/
   /* --------------------------------------------------------------*/

  FK0XW00.FK0XW-END-LRSTY-SW = "N"; /* Init end of cursor switch*/
  FK0XW00.FK0XW-TEMP-QTY1 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY2 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY3 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY4 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY5 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY6 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY7 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY8 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY9 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY10 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY11 = 0; /* Init qty*/
  FK0XW00.FK0XW-TEMP-QTY12 = 0; /* Init qty*/

  FK0X-1610(); /* Declare cursor for DCMPNT.VLRSTY01*/

  FK0X-1620(); /* Fetch DCMPNT.VLRSTY01 cursor*/


  while (FK0XW00.FK0XW-END-LRSTY-SW != "Y") /* Until end of cursor*/

       /* ==== Add quantities to working storage according to buy date ===*/

    if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[1])
      FK0XW00.FK0XW-TEMP-QTY1 = FK0XW00.FK0XW-TEMP-QTY1 + VLRSTY01.FB8FTQTB;
    else

      if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[2])
        FK0XW00.FK0XW-TEMP-QTY2 = FK0XW00.FK0XW-TEMP-QTY2 + VLRSTY01.FB8FTQTB;
      else

        if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[3])
          FK0XW00.FK0XW-TEMP-QTY3 = FK0XW00.FK0XW-TEMP-QTY3 + VLRSTY01.FB8FTQTB;
        else

          if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[4])
            FK0XW00.FK0XW-TEMP-QTY4 = FK0XW00.FK0XW-TEMP-QTY4 + VLRSTY01.FB8FTQTB;
          else

            if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[5])
              FK0XW00.FK0XW-TEMP-QTY5 = FK0XW00.FK0XW-TEMP-QTY5 + VLRSTY01.FB8FTQTB;
            else

              if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[6])
                FK0XW00.FK0XW-TEMP-QTY6 = FK0XW00.FK0XW-TEMP-QTY6 + VLRSTY01.FB8FTQTB;
              else

                if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[7])
                  FK0XW00.FK0XW-TEMP-QTY7 = FK0XW00.FK0XW-TEMP-QTY7 + VLRSTY01.FB8FTQTB;
                else

                  if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[8])
                    FK0XW00.FK0XW-TEMP-QTY8 = FK0XW00.FK0XW-TEMP-QTY8 + VLRSTY01.FB8FTQTB;
                  else

                    if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[9])
                      FK0XW00.FK0XW-TEMP-QTY9 = FK0XW00.FK0XW-TEMP-QTY9 + VLRSTY01.FB8FTQTB;
                    else

                      if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[10])
                        FK0XW00.FK0XW-TEMP-QTY10 = FK0XW00.FK0XW-TEMP-QTY10 + VLRSTY01.FB8FTQTB;
                      else

                        if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[11])
                          FK0XW00.FK0XW-TEMP-QTY11 = FK0XW00.FK0XW-TEMP-QTY11 + VLRSTY01.FB8FTQTB;
                        else

                          if (VLRSTY01.FB8BUYDT == FK0XW00.FK0XW-MAP-DATE[12])
                            FK0XW00.FK0XW-TEMP-QTY12 = FK0XW00.FK0XW-TEMP-QTY12 + VLRSTY01.FB8FTQTB;
                          end
                        end
                      end
                    end
                  end
                end
              end
            end
          end
        end
      end
    end


    FK0X-1620(); /* Fetch DCMPNT.VLRSTY01 cursor*/
  end


     /* ======= Determine usage ========================================*/

  if (FKZREC.FK-MAT-FINL-USE != 0) /* Final usage*/
    FK0XW00.FK0XW-USAGE = FKZREC.FK-MAT-FINL-USE; /* Save usage*/
  else
    if (FKZREC.FK-MAT-PROD-USE != 0) /* Prod usage*/
      FK0XW00.FK0XW-USAGE = FKZREC.FK-MAT-PROD-USE; /* Save usage*/
    else
      FK0XW00.FK0XW-USAGE = FKZREC.FK-MAT-DEVL-USE; /* Save usage*/
    end
  end


     /* ======= convert lrps qty to manufacturing usage quantity =======*/

  FK0XW00.FK0XW-TEMP-QTY1 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY1   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY1));

  FK0XW00.FK0XW-TEMP-QTY2 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY2   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY2));

  FK0XW00.FK0XW-TEMP-QTY3 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY3   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY3));

  FK0XW00.FK0XW-TEMP-QTY4 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY4   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY4));

  FK0XW00.FK0XW-TEMP-QTY5 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY5   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY5));

  FK0XW00.FK0XW-TEMP-QTY6 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY6   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY6));

  FK0XW00.FK0XW-TEMP-QTY7 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY7   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY7));

  FK0XW00.FK0XW-TEMP-QTY8 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY8   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY8));

  FK0XW00.FK0XW-TEMP-QTY9 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY9   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY9));

  FK0XW00.FK0XW-TEMP-QTY10 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY10   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY10));

  FK0XW00.FK0XW-TEMP-QTY11 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY11   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY11));

  FK0XW00.FK0XW-TEMP-QTY12 = mathLib.round(FK0XW00.FK0XW-TEMP-QTY12   * FKZREC.FK-MAT-MFG-PCT   * FK0XW00.FK0XW-USAGE, -mathLib.decimals(FK0XW00.FK0XW-TEMP-QTY12));


  FK0XW00.FK0XM-QTY1 = FK0XW00.FK0XM-QTY1 + FK0XW00.FK0XW-TEMP-QTY1;
  FK0XW00.FK0XM-QTY2 = FK0XW00.FK0XM-QTY2 + FK0XW00.FK0XW-TEMP-QTY2;
  FK0XW00.FK0XM-QTY3 = FK0XW00.FK0XM-QTY3 + FK0XW00.FK0XW-TEMP-QTY3;
  FK0XW00.FK0XM-QTY4 = FK0XW00.FK0XM-QTY4 + FK0XW00.FK0XW-TEMP-QTY4;
  FK0XW00.FK0XM-QTY5 = FK0XW00.FK0XM-QTY5 + FK0XW00.FK0XW-TEMP-QTY5;
  FK0XW00.FK0XM-QTY6 = FK0XW00.FK0XM-QTY6 + FK0XW00.FK0XW-TEMP-QTY6;
  FK0XW00.FK0XM-QTY7 = FK0XW00.FK0XM-QTY7 + FK0XW00.FK0XW-TEMP-QTY7;
  FK0XW00.FK0XM-QTY8 = FK0XW00.FK0XM-QTY8 + FK0XW00.FK0XW-TEMP-QTY8;
  FK0XW00.FK0XM-QTY9 = FK0XW00.FK0XM-QTY9 + FK0XW00.FK0XW-TEMP-QTY9;
