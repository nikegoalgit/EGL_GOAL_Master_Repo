Program DI04 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DI04W01 DI04W01; // record
  DI04W02 DI04W02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use ZRTTBL {deleteAfterUse = yes}; // table
  use DI04M.DI04M001,  // forms
      DI04M.DI04M002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DI04P00-DRIVER: DI04P00-DRIVER();
    exit program;
  end // end main
end // end DI04

// DRIVER FOR APPLICATION
Function DI04P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DI04P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DI04P20-BUILD-MAP2(); /* Build the second map*/
  else
    DI04P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DI04P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DI04P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DI04P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DI04P00 *******************************/
  /* **************/
end // end DI04P00-DRIVER

// INITIALIZE THE APPLICATION
Function DI04P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DI04W01 empty; /* Working storage record*/
  set DI04W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DI04M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DI04M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI04M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DI04M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DI04M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI04M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DI04M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DI04P10 ******************************/
  /* **************/
end // end DI04P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DI04P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DI04P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DI04M001.DAW-CURR-PAGE)
    DI04S-TS-READ-PAGE(); /* read temp storage page*/
    DI04P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DI04P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI04P20 **********************************/
  /* **************/
end // end DI04P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DI04P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DI04M002.DA-CHNG-USER-ID = sysVar.userID;
  DI04M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DI04W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI04M002.DA-PLOG-TXT1 = " ";
    DI04M002.DA-PLOG-TXT2 = " ";
    DI04M002.ZRTCD = " ";
    DI04M002.ZRTABRV = " ";
    DI04M002.ZRTDESC = " ";
    DI04M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DI04W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DI04W01.ZRTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DI04W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DI04W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DI04M002.ZRTCD = DI04W01.ZRTCD; /* move to the map*/
      DI04M002.ZRTABRV = DI04W01.ZRTABRV;
      DI04M002.ZRTDESC = DI04W01.ZRTDESC;
      DI04M002.XXXSTAT = DI04W01.XXXSTAT;
      DI04M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DI04M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DI04M002.ZRTCD = DI04W02.ZRTCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DI04M002.ZRTCD in ZRTTBL.ZRTCD)
          DI04W01.ZRTCD = ZRTTBL.ZRTCD[sysVar.arrayIndex]; /* save for a*/
          DI04W01.ZRTABRV = ZRTTBL.ZRTABRV[sysVar.arrayIndex]; /* later compare*/
          DI04W01.ZRTDESC = ZRTTBL.ZRTDESC[sysVar.arrayIndex];
          DI04W01.XXXSTAT = ZRTTBL.XXXSTAT[sysVar.arrayIndex];
          DI04W01.ZZZSDT = ZRTTBL.ZZZSDT[sysVar.arrayIndex];
          DI04W01.ZZZCHGDT = ZRTTBL.ZZZCHGDT[sysVar.arrayIndex];
          DI04W01.ZZZCHGTM = ZRTTBL.ZZZCHGTM[sysVar.arrayIndex];
          DI04W01.ZZZCHGCT = ZRTTBL.ZZZCHGCT[sysVar.arrayIndex];

          DI04M002.ZRTABRV = ZRTTBL.ZRTABRV[sysVar.arrayIndex]; /* *//*move to map*/
          DI04M002.ZRTDESC = ZRTTBL.ZRTDESC[sysVar.arrayIndex];
          DI04M002.XXXSTAT = ZRTTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DI04W01.ZRTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DI04W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DI04W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DI04M002.ZRTCD = DI04W01.ZRTCD; /* move to the map*/
    DI04M002.ZRTABRV = DI04W01.ZRTABRV;
    DI04M002.ZRTDESC = DI04W01.ZRTDESC;
    DI04M002.XXXSTAT = DI04W01.XXXSTAT;
    DI04M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DI04M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DI04M002.XXXSTAT == "A")
    DI04M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DI04M002.XXXSTAT == "I")
    DI04M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DI04M002.XXXSTAT == " ")
    DI04M002.DAW-TBL-STATUS-DESC = " ";
  end

  DI04P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI04P20 **********************************/
  /* **************/
end // end DI04P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DI04P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DI04W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI04P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI04S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (ZRTTBL.ZRTCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI04P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI04S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DI04S-TS-ADD-PAGE();
    end

    DI04M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DI04M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DI04P22 **********************************/
  /* **************/
end // end DI04P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DI04P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DI04W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DI04W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DI04W01.ZRTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DI04W02.ZRTCD[DAW-ROW-NBR] = DI04W01.ZRTCD;
  DI04W02.ZRTABRV[DAW-ROW-NBR] = DI04W01.ZRTABRV;
  DI04W02.ZRTDESC[DAW-ROW-NBR] = DI04W01.ZRTDESC;
  DI04W02.XXXSTAT[DAW-ROW-NBR] = DI04W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DI04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DI04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DI04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DI04W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DI04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DI04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DI04W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DI04W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DI04W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DI04W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DI04P221 ******************************/
  /* ***************/
end // end DI04P23-DB2-TO-TSQ

