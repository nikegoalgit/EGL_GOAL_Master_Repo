  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
  /* ***************** END DI04P71 **********************************/
  /* **************/
end // end DI04P71-COMPARE

// MOVE TEMP STORAGE PAGE TO MAP
Function DI04P71-TSQ-TO-MAP()
  /* ************************************************************/
  /* Move temp storage page to map*/
  /* ************************************************************/

  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DI04M001.DAW-SELECT-CD[sysVar.arrayIndex] = DI04W02.DAW-SELECT-CD[sysVar.arrayIndex];
    DI04M001.ZRTCD[sysVar.arrayIndex] = DI04W02.ZRTCD[sysVar.arrayIndex];
    DI04M001.ZRTABRV[sysVar.arrayIndex] = DI04W02.ZRTABRV[sysVar.arrayIndex];
    DI04M001.ZRTDESC[sysVar.arrayIndex] = DI04W02.ZRTDESC[sysVar.arrayIndex];
    DI04M001.XXXSTAT[sysVar.arrayIndex] = DI04W02.XXXSTAT[sysVar.arrayIndex];
    DI04M001.DAW-STATUS-DESC[sysVar.arrayIndex] = DI04W02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DI04M001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DI04W02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DI04M001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DI04W02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  /* **************/
  /* ****************** END DI04P71 *****************************/
  /* **************/
end // end DI04P71-TSQ-TO-MAP

// BUILD NEW VDATRN01 RECORD
Function DI04P72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DI04M002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DI04M002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DI04W01.DA-CCAT-KEY-TXT = " ";
  DI04W01.DI04W-KEY-ZRTCD = DI04M002.ZRTCD;
  VDATRN01.DA-CCAT-KEY-TXT = DI04W01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DI04W01.ZRTCD = DI04M002.ZRTCD;
  DI04W01.ZRTABRV = DI04M002.ZRTABRV;
  DI04W01.ZRTDESC = DI04M002.ZRTDESC;
  DI04W01.XXXSTAT = DI04M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 36;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DI04W01.ZRTGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DI04P72 **********************************/
  /* **************/
end // end DI04P72-NEW-TRAN

