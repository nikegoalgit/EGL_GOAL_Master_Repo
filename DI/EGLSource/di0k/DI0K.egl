// SET MAP 1 FIELD ATTRIBUTES
Function DI0KP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DI0KM001.DAW-PAGE-LIT = "PAGE";
    set DI0KM001.DAW-CURR-PAGE initialAttributes;
    DI0KM001.DAW-OF-LIT = "OF";
    set DI0KM001.DAW-LAST-PAGE initialAttributes;
    DI0KM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DI0KM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DI0KM001.DAW-PAGE-LIT = " ";
    set DI0KM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DI0KM001.DAW-OF-LIT = " ";
    set DI0KM001.DAW-LAST-PAGE skip, invisible;
    DI0KM001.DAW-PF7-LIT = " ";
    DI0KM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DI0KM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DI0KM001.XGSCD[sysVar.arrayIndex] == " ")
      set DI0KM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DI0KM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DI0KP25 **************************/
  /* **************/
end // end DI0KP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DI0KP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DI0KM002.XGSCD initialAttributes;
    set DI0KM002.XLCCD initialAttributes;
    set DI0KM002.XQACD initialAttributes;
    set DI0KM002.YSMXPR initialAttributes;
    set DI0KM002.XGSDESC initialAttributes;
    set DI0KM002.XGSSDESC initialAttributes;
    set DI0KM002.XGSABRV initialAttributes;
    set DI0KM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DI0KM002.XGSCD skip;
    set DI0KM002.XLCCD skip;
    set DI0KM002.XQACD skip;
    set DI0KM002.YSMXPR skip;
    set DI0KM002.XGSDESC skip;
    set DI0KM002.XGSSDESC skip;
    set DI0KM002.XGSABRV skip;
    set DI0KM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DI0KM002.XGSCD skip;
    set DI0KM002.XLCCD skip;
    set DI0KM002.XQACD skip;
    set DI0KM002.YSMXPR initialAttributes;
    set DI0KM002.XGSDESC initialAttributes;
    set DI0KM002.XGSSDESC initialAttributes;
    set DI0KM002.XGSABRV initialAttributes;
    set DI0KM002.XXXSTAT initialAttributes;
  end

  set DI0KM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DI0KM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI0KM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DI0KM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DI0KM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DI0KM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DI0KM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DI0KM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DI0KP25 **************************/
