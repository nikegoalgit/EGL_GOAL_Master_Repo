   && DI0KW01.XXXSTAT == DI0KM002.XXXSTAT
   && DI0KW01.DA-PLOG-TXT1 == DI0KM002.DA-PLOG-TXT1
   && DI0KW01.DA-PLOG-TXT2 == DI0KM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
