// BUILD NEW VDATRN01 RECORD
Function DI0KP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DI0KM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DI0KM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DI0KW01.DA-CCAT-KEY-TXT = " ";
  DI0KW01.DI0KW-KEY-XGSCD = DI0KM002.XGSCD;
  DI0KW01.DI0KW-KEY-XLCCD = DI0KM002.XLCCD;
  DI0KW01.DI0KW-KEY-XQACD = DI0KM002.XQACD;
  VDATRN01.DA-CCAT-KEY-TXT = DI0KW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DI0KW01.XGSCD = DI0KM002.XGSCD;
  DI0KW01.XLCCD = DI0KM002.XLCCD;
  DI0KW01.XQACD = DI0KM002.XQACD;
  DI0KW01.YSMXPR = DI0KM002.YSMXPR;
  DI0KW01.XGSDESC = DI0KM002.XGSDESC;
  DI0KW01.XGSSDESC = DI0KM002.XGSSDESC;
  DI0KW01.XGSABRV = DI0KM002.XGSABRV;
  DI0KW01.XXXSTAT = DI0KM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 69;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DI0KW01.XGSGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DI0KP72 **********************************/
  /* **************/
end // end DI0KP72-NEW-TRAN

