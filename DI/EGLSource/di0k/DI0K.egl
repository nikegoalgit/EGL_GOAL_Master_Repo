//   NIKE TABLE HANDLER - TABLE MAINTENANCE - PRODUCT GROUP
// 
//   OVERVIEW: This application is used to maintain the Product
//             group table.
// 
//   DB2 TABLE: DCORP.VXGS0001
//   CSP TABLE: XGSTBL
// 
//  DATE CREATED: 01/28/98
//        AUTHOR: Eric Chan
// 
//  MAINTENANCE HISTORY:
// 
//      date     by whom     overview of changes made
//    --------  ----------  -----------------------------------
//    04-28-99  ECHAN       ADD NOHILITE IN DI0KP25-SET-ATTR-1
//                          CHG TRAMSACTION TO TRANSACTION IN
//                          DI0KP70-PROC-MAP-1
//                          CHECK DUPLICATE AND DEADLOCK IN
//                          DI0KP70-PROC-MAP-2
// *********************
Program DI0K type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DI0KW01 DI0KW01; // record
  DI0KW02 DI0KW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XGSTBL {deleteAfterUse = yes}; // table
  use XLCTBL {deleteAfterUse = yes}; // table
  use XPRTBL {deleteAfterUse = yes}; // table
  use XQATBL {deleteAfterUse = yes}; // table
  use DI0KM.DI0KM001,  // forms
      DI0KM.DI0KM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DI0KP00-DRIVER: DI0KP00-DRIVER();
    exit program;
  end // end main
end // end DI0K

// DRIVER FOR APPLICATION
Function DI0KP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DI0KP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DI0KP20-BUILD-MAP2(); /* Build the second map*/
  else
    DI0KP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DI0KP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DI0KP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DI0KP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DI0KP00 *******************************/
  /* **************/
end // end DI0KP00-DRIVER

// INITIALIZE THE APPLICATION
Function DI0KP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DI0KW01 empty; /* Working storage record*/
  set DI0KW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DI0KM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DI0KM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI0KM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DI0KM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DI0KM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI0KM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DI0KM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DI0KP10 ******************************/
  /* **************/
end // end DI0KP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DI0KP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DI0KP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DI0KM001.DAW-CURR-PAGE)
    DI0KS-TS-READ-PAGE(); /* read temp storage page*/
    DI0KP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DI0KP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI0KP20 **********************************/
  /* **************/
end // end DI0KP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DI0KP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DI0KM002.DA-CHNG-USER-ID = sysVar.userID;
  DI0KM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DI0KW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DI0KM002.XLCABRV = " ";
  DI0KM002.XQAABRV = " ";
  DI0KM002.XPRABRV = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI0KM002.DA-PLOG-TXT1 = " ";
    DI0KM002.DA-PLOG-TXT2 = " ";
    DI0KM002.XGSCD = " ";
    DI0KM002.XLCCD = " ";
    DI0KM002.XQACD = " ";
    DI0KM002.YSMXPR = " ";
    DI0KM002.XGSDESC = " ";
    DI0KM002.XGSSDESC = " ";
    DI0KM002.XGSABRV = " ";
    DI0KM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DI0KW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DI0KW01.XGSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DI0KW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DI0KW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DI0KM002.XGSCD = DI0KW01.XGSCD; /* move to the map*/
      DI0KM002.XLCCD = DI0KW01.XLCCD;
      DI0KM002.XQACD = DI0KW01.XQACD;
      DI0KM002.YSMXPR = DI0KW01.YSMXPR;
      DI0KM002.XGSDESC = DI0KW01.XGSDESC;
      DI0KM002.XGSSDESC = DI0KW01.XGSSDESC;
      DI0KM002.XGSABRV = DI0KW01.XGSABRV;
      DI0KM002.XXXSTAT = DI0KW01.XXXSTAT;
      DI0KM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DI0KM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DI0KM002.XGSCD = DI0KW02.XGSCD[DAW-ROW-NBR];
        DI0KM002.XLCCD = DI0KW02.XLCCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        DI0KW01.DI0KW-FND-FL = "N";
        sysVar.arrayIndex = 1;
        while (DI0KW01.DI0KW-FND-FL != "Y"
         && XGSTBL.XGSCD[sysVar.arrayIndex] != "99")
          if (XGSTBL.XGSCD[sysVar.arrayIndex] == DI0KM002.XGSCD
           && XGSTBL.XLCCD[sysVar.arrayIndex] == DI0KM002.XLCCD)
            DI0KW01.DI0KW-FND-FL = "Y";
          else
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
          end
        end

        if (DI0KW01.DI0KW-FND-FL == "Y")
          DI0KW01.XGSCD = XGSTBL.XGSCD[sysVar.arrayIndex]; /* save for a*/
          DI0KW01.XLCCD = XGSTBL.XLCCD[sysVar.arrayIndex];
          DI0KW01.XQACD = XGSTBL.XQACD[sysVar.arrayIndex];
          DI0KW01.YSMXPR = XGSTBL.YSMXPR[sysVar.arrayIndex];
          DI0KW01.XGSDESC = XGSTBL.XGSDESC[sysVar.arrayIndex];
          DI0KW01.XGSSDESC = XGSTBL.XGSSDESC[sysVar.arrayIndex];
          DI0KW01.XGSABRV = XGSTBL.XGSABRV[sysVar.arrayIndex]; /* later compare*/
          DI0KW01.XXXSTAT = XGSTBL.XXXSTAT[sysVar.arrayIndex];
          DI0KW01.ZZZSDT = XGSTBL.ZZZSDT[sysVar.arrayIndex];
          DI0KW01.ZZZCHGDT = XGSTBL.ZZZCHGDT[sysVar.arrayIndex];
          DI0KW01.ZZZCHGTM = XGSTBL.ZZZCHGTM[sysVar.arrayIndex];
          DI0KW01.ZZZCHGCT = XGSTBL.ZZZCHGCT[sysVar.arrayIndex];

          DI0KM002.XQACD = XGSTBL.XQACD[sysVar.arrayIndex]; /* move to map*/
          DI0KM002.YSMXPR = XGSTBL.YSMXPR[sysVar.arrayIndex];
          DI0KM002.XGSDESC = XGSTBL.XGSDESC[sysVar.arrayIndex];
          DI0KM002.XGSSDESC = XGSTBL.XGSSDESC[sysVar.arrayIndex];
          DI0KM002.XGSABRV = XGSTBL.XGSABRV[sysVar.arrayIndex];
          DI0KM002.XXXSTAT = XGSTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DI0KW01.XGSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DI0KW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DI0KW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DI0KM002.XGSCD = DI0KW01.XGSCD; /* move to the map*/
    DI0KM002.XLCCD = DI0KW01.XLCCD;
    DI0KM002.XQACD = DI0KW01.XQACD;
    DI0KM002.YSMXPR = DI0KW01.YSMXPR;
    DI0KM002.XGSDESC = DI0KW01.XGSDESC;
    DI0KM002.XGSSDESC = DI0KW01.XGSSDESC;
    DI0KM002.XGSABRV = DI0KW01.XGSABRV;
    DI0KM002.XXXSTAT = DI0KW01.XXXSTAT;
    DI0KM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DI0KM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DI0KM002.XXXSTAT == "A")
    DI0KM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DI0KM002.XXXSTAT == "I")
    DI0KM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DI0KM002.XXXSTAT == " ")
    DI0KM002.DAW-TBL-STATUS-DESC = " ";
  end

  DI0KP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI0KP20 **********************************/
  /* **************/
end // end DI0KP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DI0KP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DI0KW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI0KP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI0KS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XGSTBL.XGSCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI0KP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI0KS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DI0KS-TS-ADD-PAGE();
    end

    DI0KM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DI0KM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DI0KP22 **********************************/
  /* **************/
end // end DI0KP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DI0KP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DI0KW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DI0KW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DI0KW01.XGSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DI0KW02.XGSCD[DAW-ROW-NBR] = DI0KW01.XGSCD;
  DI0KW02.XLCCD[DAW-ROW-NBR] = DI0KW01.XLCCD;
  DI0KW02.XQACD[DAW-ROW-NBR] = DI0KW01.XQACD;
  DI0KW02.YSMXPR[DAW-ROW-NBR] = DI0KW01.YSMXPR;
  DI0KW02.XGSSDESC[DAW-ROW-NBR] = DI0KW01.XGSSDESC;
  DI0KW02.XGSABRV[DAW-ROW-NBR] = DI0KW01.XGSABRV;
  DI0KW02.XXXSTAT[DAW-ROW-NBR] = DI0KW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DI0KW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DI0KW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DI0KW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DI0KW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DI0KW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DI0KW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DI0KW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DI0KW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DI0KW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DI0KW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DI0KP221 ******************************/
  /* ***************/
end // end DI0KP23-DB2-TO-TSQ

