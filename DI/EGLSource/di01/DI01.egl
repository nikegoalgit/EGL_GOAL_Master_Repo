// DRIVER FOR APPLICATION
Function DI01P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DI01P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DI01P20-BUILD-MAP2(); /* Build the second map*/
  else
    DI01P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DI01P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DI01P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DI01P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DI01P00 *******************************/
  /* **************/
end // end DI01P00-DRIVER

// INITIALIZE THE APPLICATION
Function DI01P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DI01W01 empty; /* Working storage record*/
  set DI01W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DI01M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DI01M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI01M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DI01M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DI01M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI01M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DI01M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DI01P10 ******************************/
  /* **************/
end // end DI01P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DI01P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DI01P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DI01M001.DAW-CURR-PAGE)
    DI01S-TS-READ-PAGE(); /* read temp storage page*/
    DI01P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DI01P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI01P20 **********************************/
  /* **************/
end // end DI01P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DI01P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DI01M002.DA-CHNG-USER-ID = sysVar.userID;
  DI01M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DI01W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI01M002.DA-PLOG-TXT1 = " ";
    DI01M002.DA-PLOG-TXT2 = " ";
    DI01M002.ZITCD = " ";
    DI01M002.ZITABRV = " ";
    DI01M002.ZITDESC = " ";
    DI01M002.ZITSDESC = " ";
    DI01M002.ZITFDESC = " ";
    DI01M002.ZITPOLFL = " ";
    DI01M002.ZITPODFL = " ";
    DI01M002.ZITWHSFL = " ";
    DI01M002.ZITCONFL = " ";
    DI01M002.ZITFACFL = " ";
    DI01M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DI01W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DI01W01.ZITGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DI01W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DI01W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DI01M002.ZITCD = DI01W01.ZITCD; /* move to the map*/
      DI01M002.ZITABRV = DI01W01.ZITABRV;
      DI01M002.ZITDESC = DI01W01.ZITDESC;
      DI01M002.ZITSDESC = DI01W01.ZITSDESC;
      DI01M002.ZITFDESC = DI01W01.ZITFDESC;
      DI01M002.ZITPOLFL = DI01W01.ZITPOLFL;
      DI01M002.ZITPODFL = DI01W01.ZITPODFL;
      DI01M002.ZITWHSFL = DI01W01.ZITWHSFL;
      DI01M002.ZITCONFL = DI01W01.ZITCONFL;
      DI01M002.ZITFACFL = DI01W01.ZITFACFL;
      DI01M002.XXXSTAT = DI01W01.XXXSTAT;
      DI01M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DI01M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DI01M002.ZITCD = DI01W02.ZITCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DI01M002.ZITCD in ZITTBL.ZITCD)
          DI01W01.ZITCD = ZITTBL.ZITCD[sysVar.arrayIndex]; /* save for a*/
          DI01W01.ZITABRV = ZITTBL.ZITABRV[sysVar.arrayIndex]; /* later compare*/
          DI01W01.ZITDESC = ZITTBL.ZITDESC[sysVar.arrayIndex];
          DI01W01.ZITSDESC = ZITTBL.ZITSDESC[sysVar.arrayIndex];
          DI01W01.ZITFDESC = ZITTBL.ZITFDESC[sysVar.arrayIndex];
          DI01W01.ZITPOLFL = ZITTBL.ZITPOLFL[sysVar.arrayIndex];
          DI01W01.ZITPODFL = ZITTBL.ZITPODFL[sysVar.arrayIndex];
          DI01W01.ZITWHSFL = ZITTBL.ZITWHSFL[sysVar.arrayIndex];
          DI01W01.ZITCONFL = ZITTBL.ZITCONFL[sysVar.arrayIndex];
          DI01W01.ZITFACFL = ZITTBL.ZITFACFL[sysVar.arrayIndex];
          DI01W01.XXXSTAT = ZITTBL.XXXSTAT[sysVar.arrayIndex];
          DI01W01.ZZZSDT = ZITTBL.ZZZSDT[sysVar.arrayIndex];
          DI01W01.ZZZCHGDT = ZITTBL.ZZZCHGDT[sysVar.arrayIndex];
          DI01W01.ZZZCHGTM = ZITTBL.ZZZCHGTM[sysVar.arrayIndex];
          DI01W01.ZZZCHGCT = ZITTBL.ZZZCHGCT[sysVar.arrayIndex];

          DI01M002.ZITABRV = ZITTBL.ZITABRV[sysVar.arrayIndex]; /* move to map*/
          DI01M002.ZITDESC = ZITTBL.ZITDESC[sysVar.arrayIndex];
          DI01M002.ZITSDESC = ZITTBL.ZITSDESC[sysVar.arrayIndex];
          DI01M002.ZITFDESC = ZITTBL.ZITFDESC[sysVar.arrayIndex];
          DI01M002.ZITPOLFL = ZITTBL.ZITPOLFL[sysVar.arrayIndex];
          DI01M002.ZITPODFL = ZITTBL.ZITPODFL[sysVar.arrayIndex];
          DI01M002.ZITWHSFL = ZITTBL.ZITWHSFL[sysVar.arrayIndex];
          DI01M002.ZITCONFL = ZITTBL.ZITCONFL[sysVar.arrayIndex];
          DI01M002.ZITFACFL = ZITTBL.ZITFACFL[sysVar.arrayIndex];
          DI01M002.XXXSTAT = ZITTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DI01W01.ZITGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DI01W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DI01W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DI01M002.ZITCD = DI01W01.ZITCD; /* move to the map*/
    DI01M002.ZITABRV = DI01W01.ZITABRV;
    DI01M002.ZITDESC = DI01W01.ZITDESC;
    DI01M002.ZITSDESC = DI01W01.ZITSDESC;
    DI01M002.ZITFDESC = DI01W01.ZITFDESC;
    DI01M002.ZITPOLFL = DI01W01.ZITPOLFL;
    DI01M002.ZITPODFL = DI01W01.ZITPODFL;
    DI01M002.ZITWHSFL = DI01W01.ZITWHSFL;
    DI01M002.ZITCONFL = DI01W01.ZITCONFL;
    DI01M002.ZITFACFL = DI01W01.ZITFACFL;
    DI01M002.XXXSTAT = DI01W01.XXXSTAT;
    DI01M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DI01M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DI01M002.XXXSTAT == "A")
    DI01M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DI01M002.XXXSTAT == "I")
    DI01M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DI01M002.XXXSTAT == " ")
    DI01M002.DAW-TBL-STATUS-DESC = " ";
  end

  DI01P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI01P20 **********************************/
  /* **************/
end // end DI01P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DI01P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DI01W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI01P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI01S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (ZITTBL.ZITCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI01P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI01S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DI01S-TS-ADD-PAGE();
    end

    DI01M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DI01M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DI01P22 **********************************/
  /* **************/
end // end DI01P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DI01P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DI01W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DI01W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DI01W01.ZITGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DI01W02.ZITCD[DAW-ROW-NBR] = DI01W01.ZITCD;
  DI01W02.ZITDESC[DAW-ROW-NBR] = DI01W01.ZITDESC;
  DI01W02.ZITPOLFL[DAW-ROW-NBR] = DI01W01.ZITPOLFL;
  DI01W02.ZITPODFL[DAW-ROW-NBR] = DI01W01.ZITPODFL;
  DI01W02.ZITWHSFL[DAW-ROW-NBR] = DI01W01.ZITWHSFL;
  DI01W02.ZITCONFL[DAW-ROW-NBR] = DI01W01.ZITCONFL;
  DI01W02.ZITFACFL[DAW-ROW-NBR] = DI01W01.ZITFACFL;
  DI01W02.XXXSTAT[DAW-ROW-NBR] = DI01W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DI01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DI01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DI01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DI01W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DI01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DI01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DI01W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DI01W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DI01W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DI01W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DI01P221 ******************************/
  /* ***************/
end // end DI01P23-DB2-TO-TSQ

