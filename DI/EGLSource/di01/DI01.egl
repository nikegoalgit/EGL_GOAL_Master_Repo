    set DI01M002.ZITWHSFL skip;
    set DI01M002.ZITCONFL skip;
    set DI01M002.ZITFACFL skip;
    set DI01M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DI01M002.ZITCD skip;
    set DI01M002.ZITABRV initialAttributes;
    set DI01M002.ZITDESC initialAttributes;
    set DI01M002.ZITSDESC initialAttributes;
    set DI01M002.ZITFDESC initialAttributes;
    set DI01M002.ZITPOLFL initialAttributes;
    set DI01M002.ZITPODFL initialAttributes;
    set DI01M002.ZITWHSFL initialAttributes;
    set DI01M002.ZITCONFL initialAttributes;
    set DI01M002.ZITFACFL initialAttributes;
    set DI01M002.XXXSTAT initialAttributes;
  end

  set DI01M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DI01M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI01M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
