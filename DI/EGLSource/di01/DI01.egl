    set DI01M002.ZITWHSFL initialAttributes;
    set DI01M002.ZITCONFL initialAttributes;
    set DI01M002.ZITFACFL initialAttributes;
    set DI01M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DI01M002.ZITCD skip;
    set DI01M002.ZITABRV skip;
    set DI01M002.ZITDESC skip;
    set DI01M002.ZITSDESC skip;
    set DI01M002.ZITFDESC skip;
    set DI01M002.ZITPOLFL skip;
