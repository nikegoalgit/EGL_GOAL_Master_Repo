// SET MAP 1 FIELD ATTRIBUTES
Function DI01P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DI01M001.DAW-PAGE-LIT = "PAGE";
    set DI01M001.DAW-CURR-PAGE initialAttributes;
    DI01M001.DAW-OF-LIT = "OF";
    set DI01M001.DAW-LAST-PAGE initialAttributes;
    DI01M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DI01M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DI01M001.DAW-PAGE-LIT = " ";
    set DI01M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DI01M001.DAW-OF-LIT = " ";
    set DI01M001.DAW-LAST-PAGE skip, invisible;
    DI01M001.DAW-PF7-LIT = " ";
    DI01M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DI01M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DI01M001.ZITCD[sysVar.arrayIndex] == " ")
      set DI01M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DI01M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DI01P25 **************************/
  /* **************/
end // end DI01P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DI01P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
