// BUILD NEW VDATRN01 RECORD
Function DI01P72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DI01M002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DI01M002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DI01W01.DA-CCAT-KEY-TXT = " ";
  DI01W01.DI01W-KEY-ZITCD = DI01M002.ZITCD;
  VDATRN01.DA-CCAT-KEY-TXT = DI01W01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DI01W01.ZITCD = DI01M002.ZITCD;
  DI01W01.ZITABRV = DI01M002.ZITABRV;
  DI01W01.ZITDESC = DI01M002.ZITDESC;
  DI01W01.ZITSDESC = DI01M002.ZITSDESC;
  DI01W01.ZITFDESC = DI01M002.ZITFDESC;
  DI01W01.ZITPOLFL = DI01M002.ZITPOLFL;
  DI01W01.ZITPODFL = DI01M002.ZITPODFL;
  DI01W01.ZITWHSFL = DI01M002.ZITWHSFL;
  DI01W01.ZITCONFL = DI01M002.ZITCONFL;
  DI01W01.ZITFACFL = DI01M002.ZITFACFL;
  DI01W01.XXXSTAT = DI01M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 86;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DI01W01.ZITGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DI01P72 **********************************/
  /* **************/
end // end DI01P72-NEW-TRAN

