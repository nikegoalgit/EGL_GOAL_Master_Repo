//   05-07-99  ECHAN       ADD NOHILITE IN DI07P25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DI07P70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DI07P70-PROC-MAP-2
// *********************
Program DI07 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DI07W01 DI07W01; // record
  DI07W02 DI07W02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XPPTBL {deleteAfterUse = yes}; // table
  use DI07M.DI07M001,  // forms
      DI07M.DI07M002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DI07P00-DRIVER: DI07P00-DRIVER();
    exit program;
  end // end main
end // end DI07

// DRIVER FOR APPLICATION
Function DI07P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DI07P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DI07P20-BUILD-MAP2(); /* Build the second map*/
  else
    DI07P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DI07P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DI07P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DI07P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DI07P00 *******************************/
  /* **************/
end // end DI07P00-DRIVER

// INITIALIZE THE APPLICATION
Function DI07P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DI07W01 empty; /* Working storage record*/
  set DI07W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DI07M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DI07M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI07M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DI07M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DI07M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI07M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DI07M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DI07P10 ******************************/
  /* **************/
end // end DI07P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DI07P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DI07P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DI07M001.DAW-CURR-PAGE)
    DI07S-TS-READ-PAGE(); /* read temp storage page*/
    DI07P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DI07P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI07P20 **********************************/
  /* **************/
end // end DI07P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DI07P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DI07M002.DA-CHNG-USER-ID = sysVar.userID;
  DI07M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DI07W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI07M002.DA-PLOG-TXT1 = " ";
    DI07M002.DA-PLOG-TXT2 = " ";
    DI07M002.XPPCD = " ";
    DI07M002.XPPDESC = " ";
    DI07M002.XPPABRV = " ";
    DI07M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DI07W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DI07W01.XPPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DI07W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DI07W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DI07M002.XPPCD = DI07W01.XPPCD; /* move to map*/
      DI07M002.XPPDESC = DI07W01.XPPDESC;
      DI07M002.XPPABRV = DI07W01.XPPABRV;
      DI07M002.XXXSTAT = DI07W01.XXXSTAT;
      DI07M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DI07M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DI07M002.XPPCD = DI07W02.XPPCD[DAW-ROW-NBR];
                                         /* get the table row*/
        if (DI07M002.XPPCD in XPPTBL.XPPCD)
   /* save for*/
          DI07W01.XPPCD = XPPTBL.XPPCD[sysVar.arrayIndex]; /* later compare*/
          DI07W01.XPPDESC = XPPTBL.XPPDESC[sysVar.arrayIndex];
          DI07W01.XPPABRV = XPPTBL.XPPABRV[sysVar.arrayIndex];
          DI07W01.XXXSTAT = XPPTBL.XXXSTAT[sysVar.arrayIndex];
          DI07W01.ZZZSDT = XPPTBL.ZZZSDT[sysVar.arrayIndex];
          DI07W01.ZZZCHGDT = XPPTBL.ZZZCHGDT[sysVar.arrayIndex];
          DI07W01.ZZZCHGTM = XPPTBL.ZZZCHGTM[sysVar.arrayIndex];
          DI07W01.ZZZCHGCT = XPPTBL.ZZZCHGCT[sysVar.arrayIndex];

          DI07M002.XPPDESC = XPPTBL.XPPDESC[sysVar.arrayIndex]; /* move to map*/
          DI07M002.XPPABRV = XPPTBL.XPPABRV[sysVar.arrayIndex];
          DI07M002.XXXSTAT = XPPTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DI07W01.XPPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DI07W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DI07W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DI07M002.XPPCD = DI07W01.XPPCD; /* move to the map*/
    DI07M002.XPPDESC = DI07W01.XPPDESC;
    DI07M002.XPPABRV = DI07W01.XPPABRV;
    DI07M002.XXXSTAT = DI07W01.XXXSTAT;
    DI07M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DI07M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DI07M002.XXXSTAT == "A")
    DI07M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DI07M002.XXXSTAT == "I")
    DI07M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DI07M002.XXXSTAT == " ")
    DI07M002.DAW-TBL-STATUS-DESC = " ";
  end

  DI07P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI07P20 **********************************/
  /* **************/
end // end DI07P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DI07P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DI07W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI07P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI07S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XPPTBL.XPPCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI07P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI07S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DI07S-TS-ADD-PAGE();
    end

    DI07M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DI07M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DI07P22 **********************************/
  /* **************/
end // end DI07P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DI07P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DI07W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DI07W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DI07W01.XPPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DI07W02.XPPCD[DAW-ROW-NBR] = DI07W01.XPPCD;
  DI07W02.XPPDESC[DAW-ROW-NBR] = DI07W01.XPPDESC;
  DI07W02.XPPABRV[DAW-ROW-NBR] = DI07W01.XPPABRV;
  DI07W02.XXXSTAT[DAW-ROW-NBR] = DI07W01.XXXSTAT;
  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DI07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DI07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DI07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DI07W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DI07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DI07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DI07W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DI07W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DI07W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DI07W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DI07P221 ******************************/
  /* ***************/
end // end DI07P23-DB2-TO-TSQ

