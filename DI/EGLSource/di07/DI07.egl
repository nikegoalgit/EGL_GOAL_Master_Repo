// SET MAP 1 FIELD ATTRIBUTES
Function DI07P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DI07M001.DAW-PAGE-LIT = "PAGE";
    set DI07M001.DAW-CURR-PAGE initialAttributes;
    DI07M001.DAW-OF-LIT = "OF";
    set DI07M001.DAW-LAST-PAGE initialAttributes;
    DI07M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DI07M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DI07M001.DAW-PAGE-LIT = " ";
    set DI07M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DI07M001.DAW-OF-LIT = " ";
    set DI07M001.DAW-LAST-PAGE skip, invisible;
    DI07M001.DAW-PF7-LIT = " ";
    DI07M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DI07M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DI07M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DI07M001.XPPCD[sysVar.arrayIndex] == " "
       && DI07M001.XXXSTAT[sysVar.arrayIndex] == " ")
        set DI07M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
      else
        set DI07M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
      end
      sysVar.arrayIndex = sysVar.arrayIndex - 1;
    end
  end

  /* **************/
  /* ****************** END DI07P25 **************************/
  /* **************/
end // end DI07P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DI07P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DI07M002.XPPCD initialAttributes;
    set DI07M002.XPPDESC initialAttributes;
    set DI07M002.XPPABRV initialAttributes;
    set DI07M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DI07M002.XPPCD skip;
    set DI07M002.XPPDESC skip;
    set DI07M002.XPPABRV skip;
    set DI07M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DI07M002.XPPCD skip;
    set DI07M002.XPPDESC initialAttributes;
    set DI07M002.XPPABRV initialAttributes;
    set DI07M002.XXXSTAT initialAttributes;
  end

  set DI07M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DI07M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI07M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DI07M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DI07M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DI07M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DI07M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DI07M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DI07P25 **************************/
  /* **************/
end // end DI07P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DI07P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DI07P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DI07M001.VAGen_EZEMSG = " ";
  DI07M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DI07P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DI07P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DI07P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DI07P70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DI07P30 *****************************/
  /* **************/
end // end DI07P30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DI07P30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DI07P40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DI07M002.VAGen_EZEMSG = " ";
  DI07M002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DI07P25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DI07P50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DI07P60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DI07P70-PROC-MAP-2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DI07P30 *****************************/
  /* **************/
end // end DI07P30-MAIN-MAP-2

