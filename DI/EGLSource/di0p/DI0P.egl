// SET MAP 1 FIELD ATTRIBUTES
Function DI0PP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DI0PM001.DAW-PAGE-LIT = "PAGE";
    set DI0PM001.DAW-CURR-PAGE initialAttributes;
    DI0PM001.DAW-OF-LIT = "OF";
    set DI0PM001.DAW-LAST-PAGE initialAttributes;
    DI0PM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DI0PM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DI0PM001.DAW-PAGE-LIT = " ";
    set DI0PM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DI0PM001.DAW-OF-LIT = " ";
    set DI0PM001.DAW-LAST-PAGE skip, invisible;
    DI0PM001.DAW-PF7-LIT = " ";
    DI0PM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DI0PM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DI0PM001.YFFCD[sysVar.arrayIndex] == " ")
      set DI0PM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DI0PM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DI0PP25 **************************/
  /* **************/
end // end DI0PP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DI0PP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DI0PM002.YFFCD initialAttributes;
    set DI0PM002.XGPCD initialAttributes;
    set DI0PM002.YFFDESC initialAttributes;
    set DI0PM002.YFFSDESC initialAttributes;
    set DI0PM002.YFFABRV initialAttributes;
    set DI0PM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DI0PM002.YFFCD skip;
    set DI0PM002.XGPCD skip;
    set DI0PM002.YFFDESC skip;
    set DI0PM002.YFFSDESC skip;
    set DI0PM002.YFFABRV skip;
    set DI0PM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DI0PM002.YFFCD skip;
    set DI0PM002.XGPCD skip;
    set DI0PM002.YFFDESC initialAttributes;
    set DI0PM002.YFFSDESC initialAttributes;
    set DI0PM002.YFFABRV initialAttributes;
    set DI0PM002.XXXSTAT initialAttributes;
  end

  set DI0PM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DI0PM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI0PM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DI0PM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DI0PM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DI0PM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DI0PM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DI0PM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DI0PP25 **************************/
  /* **************/
end // end DI0PP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DI0PP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DI0PP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DI0PM001.VAGen_EZEMSG = " ";
  DI0PM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DI0PP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DI0PP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DI0PP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DI0PP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DI0PP30 *****************************/
  /* **************/
