// EDIT TABLE ROW KEY
Function DI0PP69-EDIT-KEY()
  /* ****************************************************************/
  /* Edit the table key (YFFcd)*/
  /* a) required - can't be spaces*/
  /* b) can't exist if current function is add or copy*/
  /* field is protected when current function is update*/
  /* c) can't exist in transaction table with todays date*/
  /* in any status except 'c' complete*/
  /* ****************************************************************/

  if (DI0PM002.YFFCD == " ")
    converseLib.validationFailed(530); /* table key is required*/
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    set DI0PM002.YFFCD cursor, bold;
  end

  /* Can't exist in current table (no matter what the status is)*/

  DI0PW01.DI0PW-FND-FL = "N";
  sysVar.arrayIndex = 1;
  while (DI0PW01.DI0PW-FND-FL != "Y"
   && YFFTBL.YFF-FIN-LVL4-CD[sysVar.arrayIndex] != "999")
    if (YFFTBL.YFF-FIN-LVL4-CD[sysVar.arrayIndex] == DI0PM002.YFFCD
     && YFFTBL.XGPCD[sysVar.arrayIndex] == DI0PM002.XGPCD)
      DI0PW01.DI0PW-FND-FL = "Y";
    else
      sysVar.arrayIndex = sysVar.arrayIndex + 1;
    end
  end

  if (DI0PW01.DI0PW-FND-FL == "Y")
    if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
      converseLib.validationFailed(529); /* table key already exists*/
      DI0PM002.DAW-INFO-MESSAGE = "REVIEW TABLE LIST AND SELECT ROW TO UPDATE";
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DI0PM002.YFFCD cursor, bold;
      set DI0PM002.XGPCD bold;
    end
    if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
      converseLib.validationFailed(529); /* table key already exists*/
      DI0PM002.DAW-INFO-MESSAGE = "TABLE KEY MUST BE CHANGED WHEN COPYING A ROW TO BE ADDED";
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DI0PM002.YFFCD cursor, bold;
      set DI0PM002.XGPCD bold;
    end
  end

  /* **  **  **  **  **  **  **  **  **  **  **  **  **  **  ***/
  /* WARNING - Add any new edits for the key prior to this line*/
  /* **  **  **  **  **  **  **  **  **  **  **  **  **  **  ***/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    return; /* row is a transaction that is being*/
  end /* updated - skip the next edit*/

  /* Can't exist in transaction table - must be the last edit*/

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;
  DI0PW01.DA-CCAT-KEY-TXT = " ";
  DI0PW01.DI0PW-KEY-YFFCD = DI0PM002.YFFCD;
  DI0PW01.DI0PW-KEY-XGPCD = DI0PM002.XGPCD;
  VDATRN01.DA-CCAT-KEY-TXT = DI0PW01.DA-CCAT-KEY-TXT;
  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;

  XSDAS-DA1140-SEL-2();

  if (SQLCA.VAGen_SQLCODE != 100) /* row found*/
    converseLib.validationFailed(017); /* table key already added or changed*/
    DI0PM002.DAW-INFO-MESSAGE = "SELECT TRANSACTION TO UPDATE OR CALL DATA ADMIN IF APPROVED/PROCESSING";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    set DI0PM002.YFFCD cursor, bold;
    set DI0PM002.XGPCD bold;
  end

  /* **************/
  /* ***************** END DI0PP69 **********************************/
  /* **************/
end // end DI0PP69-EDIT-KEY

// DO PROCESSING FOR THE MAP 1
Function DI0PP70-PROC-MAP-1()
  /* ****************************************************************/
  /* Do any processing for MAP 001*/
  /* ****************************************************************/

  /* *****************************************************************/
  /* Display new page*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-PAGE != DI0PM001.DAW-CURR-PAGE)
    DI0PS-TS-READ-PAGE();
    DI0PP71-TSQ-TO-MAP();
    DI0PP25-SET-ATTR-1();
  end

  /* *****************************************************************/
  /* set all in-process rows on the current page to pending*/
  /* *****************************************************************/

  if (DAWREC.DAW-SET-PENDING-ALL-FL == "Y")

    DAWREC.DAW-ROW-NBR = 1;
    while (DAWREC.DAW-ROW-NBR <= 14) /* check each row on the MAP 001*/
      if (DI0PM001.DAW-STATUS-DESC[DAW-ROW-NBR] == "EDIT")
        set VDATRN01 empty;
        VDATRN01.DAW-VDATRN01-KEY = DI0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
        XSDAS-SET-STATUS(); /* set the status to 'pending'*/
      end
      DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    end

  end

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-ADD-ROW-FL == "Y")

    DAWREC.DAW-RECORD-TYPE = " ";
    DAWREC.DAW-CURR-FUNCTION-CD = "ADD";
    DAWREC.DAW-ROW-NBR = 0;

    DI0PP20-BUILD-MAP2();

    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    while (DAWREC.DAW-DONE-WITH-ROW-FL != "Y" /* process row until done*/
     && DAWREC.DAW-TERM-APPL-FL == "N") /* with it or exit from appl*/
      DI0PP30-MAIN-MAP-2(); /* main loop for MAP 002*/
    end

    DAWREC.DAW-CURR-FUNCTION-CD = "   ";

  end

  /* *****************************************************************/
  /* Select, copy or delete rows;*/
  /* *****************************************************************/

  if (DAWREC.DAW-SELECT-ROW-FL == "Y"
   || DAWREC.DAW-COPY-ROW-FL == "Y"
   || DAWREC.DAW-DELETE-ROW-FL == "Y")

    DAWREC.DAW-ROW-NBR = 1;
    while (DAWREC.DAW-ROW-NBR <= 14 /* check each row on the MAP 001*/
     && DAWREC.DAW-TERM-APPL-FL == "N") /* until done or exit from appl*/

      DAWREC.DAW-RECORD-TYPE = DI0PW02.DAW-RECORD-TYPE[DAW-ROW-NBR];

      if (DI0PM001.DAW-SELECT-CD[DAW-ROW-NBR] == "S"
       || DI0PM001.DAW-SELECT-CD[DAW-ROW-NBR] == "C"
       || DI0PM001.DAW-SELECT-CD[DAW-ROW-NBR] == "D")

        if (DI0PM001.DAW-SELECT-CD[DAW-ROW-NBR] == "S")
          DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
        end
        if (DI0PM001.DAW-SELECT-CD[DAW-ROW-NBR] == "C")
          DAWREC.DAW-CURR-FUNCTION-CD = "CPY";
        end
        if (DI0PM001.DAW-SELECT-CD[DAW-ROW-NBR] == "D")
          DAWREC.DAW-CURR-FUNCTION-CD = "DEL";
          DAWREC.DAW-OK-TO-DELETE-FL = "N";
        end

        DAWREC.DAW-DONE-WITH-ROW-FL = "N";
        DI0PP20-BUILD-MAP2();
        while (DAWREC.DAW-DONE-WITH-ROW-FL != "Y" /* process row until done*/
         && DAWREC.DAW-TERM-APPL-FL == "N") /* with it or exit from appl*/
          DI0PP30-MAIN-MAP-2(); /* main loop for MAP 002*/
        end

      end

      DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
      DAWREC.DAW-CURR-FUNCTION-CD = "   ";

    end
  end

  /* *****************************************************************/
  /* Any maintenance selections are now completed so rebuild*/
  /* temp storage*/
  /* *****************************************************************/

  if (DAWREC.DAW-TERM-APPL-FL == "N") /* not ready to exit this application*/
    if (DAWREC.DAW-SELECT-ROW-FL == "Y"
     || DAWREC.DAW-COPY-ROW-FL == "Y"
     || DAWREC.DAW-DELETE-ROW-FL == "Y"
     || DAWREC.DAW-ADD-ROW-FL == "Y"
     || DAWREC.DAW-SET-PENDING-ALL-FL == "Y")
      DI0PS-TS-PURGE-FIL(); /* purge temp storage*/
      DI0PP20-BUILD-MAP1(); /* and then rebuilt it*/
      if (DAWREC.DAW-SET-PENDING-ALL-FL == "Y")
        DI0PM001.DAW-INFO-MESSAGE = "ALL TRANSACTIONS IN EDIT STATUS SET TO PENDING - PLEASE REVIEW LIST";
      else
        DI0PM001.DAW-INFO-MESSAGE = "ALL MAINTENANCE SELECTIONS HAVE BEEN COMPLETED - PLEASE REVIEW LIST";
      end
    end
  end

  /* **************/
  /* ***************** END DI0PP70 **********************************/
  /* **************/
end // end DI0PP70-PROC-MAP-1

// DO PROCESSING NEEDED FOR MAP 2
Function DI0PP70-PROC-MAP-2()
  /* ****************************************************************/
  /* Do any processing for MAP 002*/
  /* ****************************************************************/

  /* ****************************************************************/
  /* User hit enter instead of PF6 or PF10, so there's nothing to*/
  /* do except give them a message indicating what to do*/
  /* ****************************************************************/

  if (DAWREC.DAW-APPLY-FL == "N"
   && DAWREC.DAW-SET-PENDING-FL == "N")
    DI0PM002.DAW-INFO-MESSAGE = "USE PF6 TO CREATE TRANSACTION OR PF10 TO CREATE AND SET TO PENDING";
    DI0PM002.VAGen_EZEMSG = "OR PF3 TO RETURN TO LIST";
    return;
  end


  /* *****************************************************************/
  /* See if anything was changed- Compare map to table record*/
  /* *****************************************************************/

  DI0PP71-COMPARE();


  /* *****************************************************************/
  /* Set status to pending if no change to row*/
  /* *****************************************************************/

  if (DAWREC.DAW-SET-PENDING-FL == "Y"
   && DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-DATA-CHANGED-FL == "N") /* no change to row*/
    set VDATRN01 empty;
    if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
      VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    else
      VDATRN01.DAW-VDATRN01-KEY = DI0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
    end
    XSDAS-SET-STATUS(); /* set the status to 'pending'*/
    DI0PM002.DAW-INFO-MESSAGE = "TRANSACTION STATUS CHANGED TO PENDING: PF3 TO CONTINUE ";
    return;
  end


  /* *****************************************************************/
  /* Create a transaction to add a table row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   && DAWREC.DAW-RECORD-TYPE == " "
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* row was added or changed*/

    DI0PP72-NEW-TRAN(); /* build new transaction row*/
    XSDAS-DA1140-ADD();
    DI0PM002.DAW-INFO-MESSAGE = "NEW TABLE ROW WILL BE ADDED - PF3 TO CONTINUE OR RETURN TO LIST";
    return;

  end


  /* *****************************************************************/
  /* Create a transaction to copy a table row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   && DAWREC.DAW-RECORD-TYPE == "TBL "
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* row was copied or updated*/

    DI0PP72-NEW-TRAN(); /* build new transaction row*/
    XSDAS-DA1140-ADD();
    DI0PM002.DAW-INFO-MESSAGE = "NEW TABLE ROW WILL BE ADDED - PF3 TO CONTINUE OR RETURN TO LIST";
    return;
  end


  /* *****************************************************************/
  /* Create a transaction to update a table row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   && DAWREC.DAW-RECORD-TYPE == "TBL "
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* row was copied or updated*/

    DI0PP72-NEW-TRAN(); /* build new transaction row*/
    XSDAS-DA1140-ADD();
    DI0PM002.DAW-INFO-MESSAGE = "TABLE ROW WILL BE UPDATED - PF3 TO CONTINUE OR RETURN TO LIST";
    return;
  end


  /* *****************************************************************/
  /* Create a transaction to delete a table row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL"
   && DAWREC.DAW-RECORD-TYPE == "TBL "
   && DAWREC.DAW-DATA-CHANGED-FL == "Y")

    if (DAWREC.DAW-OK-TO-DELETE-FL == "Y")
      DI0PP72-NEW-TRAN(); /* build new transaction row*/
      XSDAS-DA1140-ADD();
      DI0PM002.DAW-INFO-MESSAGE = "TABLE ROW WILL BE DELETED - PF3 TO CONTINUE OR RETURN TO LIST";
    else
      DI0PM002.DAW-INFO-MESSAGE = "USE PF6 TO DELETE OR PF3 TO CANCEL THE DELETE AND RETURN TO LIST";
      DAWREC.DAW-OK-TO-DELETE-FL = "Y";
    end
    return;
  end


  /* *****************************************************************/
  /* Update a transaction*/
  /* *****************************************************************/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* something was changed*/

    DI0PP73-OLD-TRAN(); /* build old transaction row*/
    XSDAS-DA1140-UPD();
    DI0PM002.DAW-INFO-MESSAGE = "TRANSACTION UPDATED - PF3 TO CONTINUE OR RETURN TO LIST";
    return;

  end


  /* *****************************************************************/
  /* copy a transaction*/
  /* *****************************************************************/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* something was changed*/

    DI0PP72-NEW-TRAN(); /* build new transaction row*/
    XSDAS-DA1140-ADD();
    DI0PM002.DAW-INFO-MESSAGE = "NEW TABLE ROW WILL BE ADDED - PF3 TO CONTINUE OR RETURN TO LIST";
    return;

  end


  /* *****************************************************************/
  /* Delete  a transaction*/
  /* *****************************************************************/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-OK-TO-DELETE-FL == "Y")
      set VDATRN01 empty;
      VDATRN01.DAW-VDATRN01-KEY = DI0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-DEL();
      DI0PM002.DAW-INFO-MESSAGE = "TRANSACTION DELETED - PF3 TO CONTINUE OR RETURN TO LIST";
      DAWREC.DAW-OK-TO-DELETE-FL = "N";
      return;
    else
      DI0PM002.DAW-INFO-MESSAGE = "USE PF6 TO DELETE OR PF3 TO CANCEL THE DELETE AND RETURN TO LIST";
      DAWREC.DAW-OK-TO-DELETE-FL = "Y";
      return;
    end
  end


  /* still here !! Then nothing changed and there's nothing to process*/

  DI0PM002.DAW-INFO-MESSAGE = "NOTHING ADDED OR CHANGED..... ARE YOU JUST LOOKING ?? ";
  DI0PM002.VAGen_EZEMSG = "USE PF3 TO RETURN TO LIST WHEN YOU ARE READY";

  /* **************/
  /* ***************** END DI0PP70 **********************************/
  /* **************/
end // end DI0PP70-PROC-MAP-2

// COMPARE TABLE ROW TO RECORD
Function DI0PP71-COMPARE()
  /* *****************************************************************/
  /* See if anything changed*/
  /* *****************************************************************/

  if (DI0PW01.YFF-FIN-LVL4-CD == DI0PM002.YFFCD
   && DI0PW01.XGPCD == DI0PM002.XGPCD
   && DI0PW01.YFF-FIN-LVL4-ABRV == DI0PM002.YFFABRV
   && DI0PW01.YFF-FIN-LVL4-DESC == DI0PM002.YFFDESC
   && DI0PW01.YFF-FIN-LVL4-SDESC == DI0PM002.YFFSDESC
   && DI0PW01.XXXSTAT == DI0PM002.XXXSTAT
   && DI0PW01.DA-PLOG-TXT1 == DI0PM002.DA-PLOG-TXT1
   && DI0PW01.DA-PLOG-TXT2 == DI0PM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
  /* ***************** END DI0PP71 **********************************/
  /* **************/
end // end DI0PP71-COMPARE

// MOVE TEMP STORAGE PAGE TO MAP
Function DI0PP71-TSQ-TO-MAP()
  /* ************************************************************/
  /* Move temp storage page to map*/
  /* ************************************************************/

  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DI0PM001.DAW-SELECT-CD[sysVar.arrayIndex] = DI0PW02.DAW-SELECT-CD[sysVar.arrayIndex];
    DI0PM001.YFFCD[sysVar.arrayIndex] = DI0PW02.YFF-FIN-LVL4-CD[sysVar.arrayIndex];
    DI0PM001.XGPCD[sysVar.arrayIndex] = DI0PW02.XGPCD[sysVar.arrayIndex];
    DI0PM001.YFFABRV[sysVar.arrayIndex] = DI0PW02.YFF-FIN-LVL4-ABRV[sysVar.arrayIndex];
    DI0PM001.YFFDESC[sysVar.arrayIndex] = DI0PW02.YFF-FIN-LVL4-DESC[sysVar.arrayIndex];
    DI0PM001.XXXSTAT[sysVar.arrayIndex] = DI0PW02.XXXSTAT[sysVar.arrayIndex];
    DI0PM001.DAW-STATUS-DESC[sysVar.arrayIndex] = DI0PW02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DI0PM001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DI0PW02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DI0PM001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DI0PW02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  /* **************/
  /* ****************** END DI0PP71 *****************************/
  /* **************/
end // end DI0PP71-TSQ-TO-MAP

// BUILD NEW VDATRN01 RECORD
Function DI0PP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DI0PM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DI0PM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DI0PW01.DA-CCAT-KEY-TXT = " ";
  DI0PW01.DI0PW-KEY-YFFCD = DI0PM002.YFFCD;
  DI0PW01.DI0PW-KEY-XGPCD = DI0PM002.XGPCD;
  VDATRN01.DA-CCAT-KEY-TXT = DI0PW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DI0PW01.YFF-FIN-LVL4-CD = DI0PM002.YFFCD;
  DI0PW01.XGPCD = DI0PM002.XGPCD;
  DI0PW01.YFF-FIN-LVL4-ABRV = DI0PM002.YFFABRV;
  DI0PW01.YFF-FIN-LVL4-DESC = DI0PM002.YFFDESC;
  DI0PW01.YFF-FIN-LVL4-SDESC = DI0PM002.YFFSDESC;
  DI0PW01.XXXSTAT = DI0PM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 56;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DI0PW01.YFFGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DI0PP72 **********************************/
  /* **************/
end // end DI0PP72-NEW-TRAN

