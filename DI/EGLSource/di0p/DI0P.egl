// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   05-13-99  ECHAN       ADD NOHILITE IN DI0PP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DI0PP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DI0PP70-PROC-MAP-2
//                         DISPLAY STATUS IN DI0PM001
// *********************
Program DI0P type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DI0PW01 DI0PW01; // record
  DI0PW02 DI0PW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YFFTBL {deleteAfterUse = yes}; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use DI0PM.DI0PM001,  // forms
      DI0PM.DI0PM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DI0PP00-DRIVER: DI0PP00-DRIVER();
    exit program;
  end // end main
end // end DI0P

// DRIVER FOR APPLICATION
Function DI0PP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DI0PP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DI0PP20-BUILD-MAP2(); /* Build the second map*/
  else
    DI0PP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DI0PP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DI0PP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DI0PP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DI0PP00 *******************************/
  /* **************/
end // end DI0PP00-DRIVER

// INITIALIZE THE APPLICATION
Function DI0PP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DI0PW01 empty; /* Working storage record*/
  set DI0PW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DI0PM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DI0PM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI0PM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DI0PM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DI0PM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DI0PM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DI0PM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DI0PP10 ******************************/
  /* **************/
end // end DI0PP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DI0PP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DI0PP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DI0PM001.DAW-CURR-PAGE)
    DI0PS-TS-READ-PAGE(); /* read temp storage page*/
    DI0PP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DI0PP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI0PP20 **********************************/
  /* **************/
end // end DI0PP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DI0PP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DI0PM002.DA-CHNG-USER-ID = sysVar.userID;
  DI0PM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DI0PW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DI0PM002.XGPABRV = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DI0PM002.DA-PLOG-TXT1 = " ";
    DI0PM002.DA-PLOG-TXT2 = " ";
    DI0PM002.YFFCD = " ";
    DI0PM002.XGPCD = " ";
    DI0PM002.YFFABRV = " ";
    DI0PM002.YFFDESC = " ";
    DI0PM002.YFFSDESC = " ";
    DI0PM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DI0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DI0PW01.YFFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DI0PW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DI0PW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DI0PM002.YFFCD = DI0PW01.YFF-FIN-LVL4-CD; /* move to the map*/
      DI0PM002.XGPCD = DI0PW01.XGPCD;
      DI0PM002.YFFABRV = DI0PW01.YFF-FIN-LVL4-ABRV;
      DI0PM002.YFFDESC = DI0PW01.YFF-FIN-LVL4-DESC;
      DI0PM002.YFFSDESC = DI0PW01.YFF-FIN-LVL4-SDESC;
      DI0PM002.XXXSTAT = DI0PW01.XXXSTAT;
      DI0PM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DI0PM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DI0PM002.YFFCD = DI0PW02.YFF-FIN-LVL4-CD[DAW-ROW-NBR];
        DI0PM002.XGPCD = DI0PW02.XGPCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        DI0PW01.DI0PW-FND-FL = "N";
        sysVar.arrayIndex = 1;
        while (DI0PW01.DI0PW-FND-FL != "Y"
         && YFFTBL.YFF-FIN-LVL4-CD[sysVar.arrayIndex] != "999")
          if (YFFTBL.YFF-FIN-LVL4-CD[sysVar.arrayIndex] == DI0PM002.YFFCD
           && YFFTBL.XGPCD[sysVar.arrayIndex] == DI0PM002.XGPCD)
            DI0PW01.DI0PW-FND-FL = "Y";
          else
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
          end
        end
        if (DI0PW01.DI0PW-FND-FL == "Y")
   /* save for*/
          DI0PW01.YFF-FIN-LVL4-CD = YFFTBL.YFF-FIN-LVL4-CD[sysVar.arrayIndex]; /* later compare*/
          DI0PW01.YFF-FIN-LVL4-SDESC = YFFTBL.YFF-FIN-LVL4-SDESC[sysVar.arrayIndex];
          DI0PW01.YFF-FIN-LVL4-ABRV = YFFTBL.YFF-FIN-LVL4-ABRV[sysVar.arrayIndex];
          DI0PW01.YFF-FIN-LVL4-DESC = YFFTBL.YFF-FIN-LVL4-DESC[sysVar.arrayIndex];
          DI0PW01.XXXSTAT = YFFTBL.XXXSTAT[sysVar.arrayIndex];
          DI0PW01.ZZZSDT = YFFTBL.ZZZSDT[sysVar.arrayIndex];
          DI0PW01.ZZZCHGDT = YFFTBL.ZZZCHGDT[sysVar.arrayIndex];
          DI0PW01.ZZZCHGTM = YFFTBL.ZZZCHGTM[sysVar.arrayIndex];
          DI0PW01.ZZZCHGCT = YFFTBL.ZZZCHGCT[sysVar.arrayIndex];
                                                     /* move to map*/
          DI0PM002.YFFABRV = YFFTBL.YFF-FIN-LVL4-ABRV[sysVar.arrayIndex];
          DI0PM002.YFFDESC = YFFTBL.YFF-FIN-LVL4-DESC[sysVar.arrayIndex];
          DI0PM002.YFFSDESC = YFFTBL.YFF-FIN-LVL4-SDESC[sysVar.arrayIndex];
          DI0PM002.XXXSTAT = YFFTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DI0PW01.YFFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DI0PW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DI0PW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DI0PM002.YFFCD = DI0PW01.YFF-FIN-LVL4-CD; /* move to the map*/
    DI0PM002.XGPCD = DI0PW01.XGPCD;
    DI0PM002.YFFABRV = DI0PW01.YFF-FIN-LVL4-ABRV;
    DI0PM002.YFFDESC = DI0PW01.YFF-FIN-LVL4-DESC;
    DI0PM002.YFFSDESC = DI0PW01.YFF-FIN-LVL4-SDESC;
    DI0PM002.XXXSTAT = DI0PW01.XXXSTAT;
    DI0PM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DI0PM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DI0PM002.XXXSTAT == "A")
    DI0PM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DI0PM002.XXXSTAT == "I")
    DI0PM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DI0PM002.XXXSTAT == " ")
    DI0PM002.DAW-TBL-STATUS-DESC = " ";
  end

  DI0PP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DI0PP20 **********************************/
  /* **************/
end // end DI0PP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DI0PP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DI0PW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI0PP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI0PS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YFFTBL.YFF-FIN-LVL4-CD[sysVar.arrayIndex] != "999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DI0PP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DI0PS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DI0PS-TS-ADD-PAGE();
    end

    DI0PM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DI0PM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DI0PP22 **********************************/
  /* **************/
end // end DI0PP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DI0PP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DI0PW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DI0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DI0PW01.YFFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DI0PW02.YFF-FIN-LVL4-CD[DAW-ROW-NBR] = DI0PW01.YFF-FIN-LVL4-CD;
  DI0PW02.XGPCD[DAW-ROW-NBR] = DI0PW01.XGPCD;
  DI0PW02.YFF-FIN-LVL4-ABRV[DAW-ROW-NBR] = DI0PW01.YFF-FIN-LVL4-ABRV;
  DI0PW02.YFF-FIN-LVL4-DESC[DAW-ROW-NBR] = DI0PW01.YFF-FIN-LVL4-DESC;
  DI0PW02.XXXSTAT[DAW-ROW-NBR] = DI0PW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DI0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DI0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DI0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DI0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DI0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DI0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DI0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DI0PW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DI0PW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DI0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DI0PP221 ******************************/
  /* ***************/
end // end DI0PP23-DB2-TO-TSQ

