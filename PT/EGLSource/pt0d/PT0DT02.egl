package pt0d;
//*** DATATABLE=PT0DT02 ****
// ------------------------------------------------------------
// This table contains the Period Order Month number and Period
// Month text to be used in conjunction with the Period Receipt
// Month extracted from PT3YYMM (PT_PERIOD_MO).
// ------------------------------------------------------------
// **************************
DataTable PT0DT02 type basicTable  {
  shared = no, resident = no
  }
  10 PT0DTPRM PT0DTPRM; 
  10 PT0DTPOM PT0DTPOM; 
  10 PT0DTPMO PT0DTPMO; 
   {contents = [
    [01, 11, "JAN"],
    [02, 12, "FEB"],
    [03, 01, "MAR"],
    [04, 02, "APR"],
    [05, 03, "MAY"],
    [06, 04, "JUN"],
    [07, 05, "JUL"],
    [08, 06, "AUG"],
    [09, 07, "SEP"],
    [10, 08, "OCT"],
    [11, 09, "NOV"],
    [12, 10, "DEC"],
    [99, 00, "FUT"]
  ]}
end // end PT0DT02

