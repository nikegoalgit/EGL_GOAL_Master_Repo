package pt0c;
//*** DATATABLE=PT0CT02 ****
// ------------------------------------------------------------
// This table contains the Period Order Month number and Period
// Month text to be used in conjunction with the Period Receipt
// Month extracted from PT3YYMM (PT_PERIOD_MO).
// ------------------------------------------------------------
// **************************
DataTable PT0CT02 type basicTable  {
  shared = no, resident = no
  }
  10 PT0CTPRM PT0CTPRM; 
  10 PT0CTPMO PT0CTPMO; 
   {contents = [
    [01, "JAN"],
    [02, "FEB"],
    [03, "MAR"],
    [04, "APR"],
    [05, "MAY"],
    [06, "JUN"],
    [07, "JUL"],
    [08, "AUG"],
    [09, "SEP"],
    [10, "OCT"],
    [11, "NOV"],
    [12, "DEC"],
    [99, "FUT"]
  ]}
end // end PT0CT02

