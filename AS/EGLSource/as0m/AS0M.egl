  ;
  if (AS0MW01.AS0MW-TRANS-CD > " ")
    VASDTL01.AB-TRN-TYP-CD = AS0MW01.AS0MW-TRANS-CD; /* Move Transaction Code*/
  else
    VASDTL01.AB-TRN-TYP-CD = "*"; /* Wildcard Transaction Code*/
  end
  ;
  VASDTL01.AB-PRT-PLN-VER-NBR = -1;
  ;
end // end AS0M-S008

