     && AS0MM001.AS0MM-TRANS-CD != "FUI" /* Fnshd Unallocated Inv*/
     && AS0MM001.AS0MM-TRANS-CD != "BOO" /* Blank Open Orders*/
     && AS0MM001.AS0MM-TRANS-CD != "FIB" /* Fnshd (w/intm) bal to rcv*/
     && AS0MM001.AS0MM-TRANS-CD != "IAI" /* Intm Alternate Inventory*/
     && AS0MM001.AS0MM-TRANS-CD != "FWS" /* Fnshd Shipped Orders(Wknd)*/
