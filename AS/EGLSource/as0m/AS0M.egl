// Validate Season CD/YR
Function AS0M-5110()
   /* ------------------------------------------------------------*/
   /* VERIFY SESN & YR*/
   /* ------------------------------------------------------------*/
  ;
  if (AS0MM001.XSECD == "  "
   && AS0MM001.XSNYR == 0)
    AS0MW01.XSECD = AS0MM001.XSECD;
    AS0MW01.XSNYR = AS0MM001.XSNYR;
    AS1REC.XSECD = AS0MM001.XSECD;
    AS1REC.XSNYR = AS0MM001.XSNYR;
    AS1REC.AS1-XSN-CC-YY = 0;
    AS0MW01.AS1-XSN-YY = 0;
    return;
  end
  ;
  if (AS0MM001.XSECD == "  "
   && AS0MM001.XSNYR > 0)
    set AS0MM001.XSECD cursor, bold;
    converseLib.validationFailed(16); /* not a valid season yr*/
    AS0MM001.XXXPRCNM = "AS0M-5110";
    return;
  end
  ;
  if (AS0MM001.XSECD > "  "
   && AS0MM001.XSNYR == 0)
    set AS0MM001.XSNYR cursor, bold;
    converseLib.validationFailed(26); /* not a valid season yr*/
    AS0MM001.XXXPRCNM = "AS0M-5110";
    return;
  end
  ;
  AS1REC.AS1-VALID-XSE-FL = "0";
  AS1REC.AS1-VALID-XSN-FL = "0";
  AS1REC.AS1-VALID-XSE-XSN-FL = "0";
  ;
  AS1REC.XSECD = AS0MM001.XSECD;
  ;
  XSASS-XSE(); /* perform 'pop-up window' processing*/
  if (AS1REC.AS1-VALID-XSE-FL == "0")
    AS0MM001.XSECD = AS1REC.XSECD;
  end
  ;
  AS1REC.AS1-XSN-CC-YY = AS0MM001.XSNYR;
  ;
  XPAS-51200();
  ;
  if (AS1REC.AS1-VALID-XSE-XSN-FL != "0")
    set AS0MM001.XSECD cursor, bold;
    set AS0MM001.XSNYR bold;
    AS1REC.AS1-MSG-NO = 71; /* invalid sesn/yr msg*/
    AS0MM001.XXXPRCNM = "AS0M-5110";
  end
  if (AS1REC.AS1-VALID-XSN-FL != "0")
    set AS0MM001.XSNYR cursor, bold;
    AS1REC.AS1-MSG-NO = 26; /* invalid yr msg;*/
    AS0MM001.XXXPRCNM = "AS0M-5110";
  end
  ;
  if (AS1REC.AS1-VALID-XSE-FL != "0")
    set AS0MM001.XSECD cursor, bold;
    AS1REC.AS1-MSG-NO = 16; /* invalid sesn msg;*/
    AS0MM001.XXXPRCNM = "AS0M-5110";
  end
  ;
  if (AS1REC.AS1-VALID-XSE-XSN-FL != "0"
   || AS1REC.AS1-VALID-XSN-FL != "0"
   || AS1REC.AS1-VALID-XSE-FL != "0")
    if (AS1REC.AS1-MSG-NO == 9999)
      converseLib.validationFailed();
    else
      converseLib.validationFailed(AS1REC.AS1-MSG-NO);
    end /* invalid sesn msg;*/
    return;
  end
  AS0MW01.XSECD = AS0MM001.XSECD;
  AS0MW01.XSNYR = AS0MM001.XSNYR;
  AS0MW01.AS1-XSN-YY = AS0MM001.XSNYR;
  AS1REC.XSECD = AS0MM001.XSECD;
  AS1REC.AS1-XSN-YY = AS0MM001.XSNYR;
  ;
  if (AS1REC.AS1-XSN-YY > 75)
    AS1REC.AS1-XSN-CC = 19;
    AS0MW01.XSNYR = AS0MW01.XSNYR + 1900;
  else
    AS1REC.AS1-XSN-CC = 20;
    AS0MW01.XSNYR = AS0MW01.XSNYR + 2000;
  end
  ;
  ;
   /* ****************** End of AS0M-5110 *************************/
end // end AS0M-5110

// Validate Division
Function AS0M-5120()
  if (AS0MM001.XDVCD == " ")
    AS0MW01.XDVCD = "  ";
    AS1REC.XDVCD = "  ";
    return;
  end
  ;
