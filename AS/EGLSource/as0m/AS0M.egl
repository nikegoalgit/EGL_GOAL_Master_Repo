  AS0M-1440(); /* Close Print PLan Cursor*/
  ;
  if (AS0MW-TSQ-MAX > 0)
    AS0M-1432(); /* ADD LAST PAGE*/
  end
  ;
   /* ********************** End of AS0M-1400 ********************/
end // end AS0M-1400

