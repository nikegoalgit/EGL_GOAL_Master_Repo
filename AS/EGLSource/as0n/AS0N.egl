// Move Select Fields For Fcty
Function AS0N-S008()
  ;
    /* Move Selection Data to vprtpl01*/
  ;
  if (AS0NW01.FY1FCTYC > " ")
    VPRTPL01.FY1FCTYC = AS0NW01.FY1FCTYC; /* get ONE factory*/
  else
    VPRTPL01.FY1FCTYC = "*"; /* get ALL factory*/
  end
  ;
  if (AS0NW01.SY1STNBR > " ")
    VPRTPL01.SY1STNBR = AS0NW01.SY1STNBR; /* Move Style*/
    if (AS0NW01.SY2CLRID > " ")
      VPRTPL01.SY2CLRID = AS0NW01.SY2CLRID; /* Move Color*/
    else
      VPRTPL01.SY2CLRID = "*"; /* Wildcard Color*/
    end
  else
    VPRTPL01.SY1STNBR = "*"; /* Wildcard Style*/
    VPRTPL01.SY2CLRID = "*"; /* Wildcard Color*/
  end
  ;
  if (AS0NW01.XOWCD > " ")
    VPRTPL01.XOWCD = AS0NW01.XOWCD; /* Move Owner Group*/
  else
    VPRTPL01.XOWCD = "*"; /* Wildcard Owner Group*/
  end
  ;
  if (AS0NW01.XDVCD > " ")
    VPRTPL01.XDVCD = AS0NW01.XDVCD; /* Move Division Code*/
  else
    VPRTPL01.XDVCD = "*"; /* Wildcard Owner Division Code*/
  end
  ;
  ;
  VPRTPL01.XSECD = "*";
  VPRTPL01.XSNYR = -1;
  VPRTPL01.AB-PRT-PLN-VER-NBR = -1;
  ;
end // end AS0N-S008

// Lock Unprotected fields
Function AS0N-S009()
    /* Set all fields to lock so changes can be verified.*/
  ;
   /* ------------------------------------------------------------*/
   /* Reset selection field attributes to protect*/
   /* ------------------------------------------------------------*/
   /*  */
  set AS0NM001.FY1FCTYC protect, cursor; /* Factory*/
  set AS0NM001.SY1STNBR protect; /* Style*/
  set AS0NM001.SY2CLRID protect; /* Style Color*/
  set AS0NM001.XOWCD protect; /* Owner Group*/
  set AS0NM001.XDVCD protect; /* Division code*/
  set AS0NM001.XSECD protect; /* Season Code*/
  set AS0NM001.XSNYR protect; /* Season Year*/
   /*  */
  ASWREC.ASW-IDX2 = 1;
  ;
  while (ASWREC.ASW-IDX2 <= 10)
    if (AS0NM001.AS0NM-STYLE[ASW-IDX2] != " ")
     /* SET AS0NM001.AS0NM-FCTY(ASW-IDX2) PROTECT;*/
   /* ELSE;*/
      set AS0NM001.AS0NM-STYLE[ASW-IDX2] invisible, protect;
    end
    ASWREC.ASW-IDX2 = ASWREC.ASW-IDX2 + 1;
  end
  ;
  ;
  ;
  ;
end // end AS0N-S009

// compute line total
Function AS0N-S012()
  AS0NW03.AS0NW-TSQ-MAX = 1;
  ;
  while (AS0NW03.AS0NW-TSQ-MAX <= 10
   && AS0NW03.SY1STNBR[AS0NW-TSQ-MAX] != " ") /* an entry this line*/
    AS0NW03.AS0NM-MNTH-TOTL[AS0NW-TSQ-MAX] = AS0NW-PRT-PLN-QTY1[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY2[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY3[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY4[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY5[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY6[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY7[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY8[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY9[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY10[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY11[AS0NW-TSQ-MAX] +     AS0NW-PRT-PLN-QTY12[AS0NW-TSQ-MAX];
    AS0NW03.AS0NW-TSQ-MAX = AS0NW-TSQ-MAX + 1;
  end
end // end AS0N-S012

// Process Style Total Line
Function AS0N-S013()
  ;
  if (AS0NW01.AS0NW-SCREEN-DISPLAYED == 1 /* screen 1*/
   && AS0NW01.AS0NW-CURRENT-PAGE == AS0NW01.AS0NW-MAX-PAGE) /* lst style Pg*/
    AS0NM001.AS0NM-STY-TOT1 = AS0NW02.AS0NW-STYLE-TOTAL[1]; /* Move*/
    AS0NM001.AS0NM-STY-TOT2 = AS0NW02.AS0NW-STYLE-TOTAL[2]; /* style*/
    AS0NM001.AS0NM-STY-TOT3 = AS0NW02.AS0NW-STYLE-TOTAL[3]; /* Totals*/
    AS0NM001.AS0NM-STY-TOT4 = AS0NW02.AS0NW-STYLE-TOTAL[4];
    AS0NM001.AS0NM-STY-TTOT = 0;
    ASWREC.ASW-IDX2 = 1;
    ;
    while (ASWREC.ASW-IDX2 <= 12)
      AS0NM001.AS0NM-STY-TTOT = AS0NM001.AS0NM-STY-TTOT +       AS0NW02.AS0NW-STYLE-TOTAL[ASW-IDX2];
      ASWREC.ASW-IDX2 = ASWREC.ASW-IDX2 + 1;
    end
    ;
    return;
  end
  ;
  if (AS0NW01.AS0NW-SCREEN-DISPLAYED == 2 /* screen 2*/
   && AS0NW01.AS0NW-CURRENT-PAGE == AS0NW01.AS0NW-MAX-PAGE) /* lst style Pg*/
    AS0NM001.AS0NM-STY-TOT1 = AS0NW02.AS0NW-STYLE-TOTAL[5]; /* Move*/
    AS0NM001.AS0NM-STY-TOT2 = AS0NW02.AS0NW-STYLE-TOTAL[6]; /* style*/
    AS0NM001.AS0NM-STY-TOT3 = AS0NW02.AS0NW-STYLE-TOTAL[7]; /* Totals*/
    AS0NM001.AS0NM-STY-TOT4 = AS0NW02.AS0NW-STYLE-TOTAL[8];
    AS0NM001.AS0NM-STY-TTOT = 0;
    ASWREC.ASW-IDX2 = 1;
    ;
    while (ASWREC.ASW-IDX2 <= 12)
      AS0NM001.AS0NM-STY-TTOT = AS0NM001.AS0NM-STY-TTOT +       AS0NW02.AS0NW-STYLE-TOTAL[ASW-IDX2];
      ASWREC.ASW-IDX2 = ASWREC.ASW-IDX2 + 1;
    end
    ;
    return;
  end
  ;
  if (AS0NW01.AS0NW-SCREEN-DISPLAYED == 3 /* screen 2*/
   && AS0NW01.AS0NW-CURRENT-PAGE == AS0NW01.AS0NW-MAX-PAGE) /* lst style Pg*/
    AS0NM001.AS0NM-STY-TOT1 = AS0NW02.AS0NW-STYLE-TOTAL[9]; /* Totals*/
    AS0NM001.AS0NM-STY-TOT2 = AS0NW02.AS0NW-STYLE-TOTAL[10];
    AS0NM001.AS0NM-STY-TOT3 = AS0NW02.AS0NW-STYLE-TOTAL[11];
    AS0NM001.AS0NM-STY-TOT4 = AS0NW02.AS0NW-STYLE-TOTAL[12];
    AS0NM001.AS0NM-STY-TTOT = 0;
    ASWREC.ASW-IDX2 = 1;
    ;
    while (ASWREC.ASW-IDX2 <= 12)
      AS0NM001.AS0NM-STY-TTOT = AS0NM001.AS0NM-STY-TTOT +       AS0NW02.AS0NW-STYLE-TOTAL[ASW-IDX2];
      ASWREC.ASW-IDX2 = ASWREC.ASW-IDX2 + 1;
    end
    ;
    return;
  end
  ;
  AS0NM001.AS0NM-STY-TTOT = 0;
  ASWREC.ASW-IDX2 = 1;
  ;
  while (ASWREC.ASW-IDX2 <= 12)
    AS0NM001.AS0NM-STY-TTOT = AS0NM001.AS0NM-STY-TTOT +     AS0NW02.AS0NW-STYLE-TOTAL[ASW-IDX2];
    ASWREC.ASW-IDX2 = ASWREC.ASW-IDX2 + 1;
  end
  ;
  set AS0NM001.AS0NM-STYLE-LINE invisible, protect;
  set AS0NM001.AS0NM-STY-TOT1 invisible, protect;
  set AS0NM001.AS0NM-STY-TOT2 invisible, protect;
  set AS0NM001.AS0NM-STY-TOT3 invisible, protect;
  set AS0NM001.AS0NM-STY-TOT4 invisible, protect;
  set AS0NM001.AS0NM-STY-TTOT invisible, protect;
end // end AS0N-S013

