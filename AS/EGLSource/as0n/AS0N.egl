  else
    VPRTPL01.SY2CLRID = "*"; /* Wildcard Color*/
  end
   /* ELSE;*/
   /* MOVE '*' TO VPRTPL01.SY1STNBR; /* Wildcard Style*/
   /* MOVE '*' TO VPRTPL01.SY2CLRID; /* Wildcard Color*/
   /* END;*/
  ;
  if (AS0NW01.XOWCD > " ")
    VPRTPL01.XOWCD = AS0NW01.XOWCD; /* Move Owner Group*/
  else
    VPRTPL01.XOWCD = "*"; /* Wildcard Owner Group*/
  end
  ;
  if (AS0NW01.XSECD > " ")
    VPRTPL01.XSECD = AS0NW01.XSECD; /* Move season Code*/
  else
    VPRTPL01.XSECD = "*"; /* Wildcard season Code*/
  end
