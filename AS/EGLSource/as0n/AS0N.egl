// Process input request
Function AS0N-6000()
   /* ------------------------------------------------------------*/
   /*  */
   /* PROCESS VALID INPUT REQUESTS*/
   /*  */
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum > 0) /* ===> errors have occurred*/
    return; /* ===> return to previous process*/
  end
  ;
  if (converseVar.eventKey is pf7  /* ===> Pf7 was pressed*/
   || converseVar.eventKey is pf8  /* ===> Pf8 was pressed*/
   || converseVar.eventKey is pf10 /* ===> Pf10 was pressed*/
   || converseVar.eventKey is pf11 /* ===> Pf11 was pressed*/
   || converseVar.eventKey is pf19 /* ===> Pf19 was pressed*/
   || converseVar.eventKey is pf20) /* ===> Pf20 was pressed*/
    /* ------------------------------------------------------------*/
    /* If a scroll backward was requested, decrement the page*/
    /* number and return to the previous process.*/
    /* If no prev pages check prev factories.*/
    /* ------------------------------------------------------------*/
    ;
    if (AS0NW-VALID-UPD-FL == "Y")
      AS0NM001.XXXPRCNM = "AS0N-6000";
      converseLib.clearScreen();
      
      ASWREC.ASW-MSG-NO = 14; /* no scroll allowed*/
      exit stack;
    end
    ;
    if (converseVar.eventKey is pf7) /* ===> Scroll back requested*/
      if (AS0NW01.AS0NW-CURRENT-PAGE > 1)
        AS0NW01.AS0NW-CURRENT-PAGE = AS0NW01.AS0NW-CURRENT-PAGE - 1;
      else
        if (AS0NW02.AS0NW-FCTY-IDX > 1)
          AS0NW02.AS0NW-FCTY-IDX = AS0NW02.AS0NW-FCTY-IDX - 1;
          AS0NW01.AS0NW-NEW-FCTY-DATA = "Y";
        else
          AS0NM001.XXXPRCNM = "AS0N-6000";
          converseLib.validationFailed(127); /* no scroll forward*/
        end
      end
      return; /* ===> Return to previous process*/
    end
    ;
    /* ------------------------------------------------------------*/
    /* If a scroll forward was requested, increment the page*/
    /* number and return to the previous process.*/
    /* If no Next pages check Next factories.*/
    /* ------------------------------------------------------------*/
    ;
    if (converseVar.eventKey is pf8) /* ===> Scroll forward requested*/
      if (AS0NW01.AS0NW-CURRENT-PAGE < AS0NW01.AS0NW-MAX-PAGE)
        AS0NW01.AS0NW-CURRENT-PAGE = AS0NW01.AS0NW-CURRENT-PAGE + 1;
      else
        if (AS0NW02.AS0NW-FCTY-IDX < AS0NW02.AS0NW-MAX-FCTY)
          AS0NW02.AS0NW-FCTY-IDX = AS0NW02.AS0NW-FCTY-IDX + 1;
          AS0NW01.AS0NW-NEW-FCTY-DATA = "Y";
        else
          AS0NM001.XXXPRCNM = "AS0N-6000";
          converseLib.validationFailed(128); /* no scroll forward*/
        end
      end
      return; /* ===> Return to previous process*/
    end
    ;
    if (converseVar.eventKey is pf19) /* ===> Scroll fcty back requested*/
      if (AS0NW02.AS0NW-FCTY-IDX > 1)
        AS0NW02.AS0NW-FCTY-IDX = AS0NW02.AS0NW-FCTY-IDX - 1;
        AS0NW01.AS0NW-NEW-FCTY-DATA = "Y";
      else
        AS0NM001.XXXPRCNM = "AS0N-6000";
        converseLib.validationFailed(129); /* no scroll forward*/
      end
      return; /* ===> Return to previous process*/
    end
    ;
    if (converseVar.eventKey is pf20) /* ===> Scroll fcty forward requested*/
      if (AS0NW02.AS0NW-FCTY-IDX < AS0NW02.AS0NW-MAX-FCTY)
        AS0NW02.AS0NW-FCTY-IDX = AS0NW02.AS0NW-FCTY-IDX + 1;
        AS0NW01.AS0NW-NEW-FCTY-DATA = "Y";
      else
        AS0NM001.XXXPRCNM = "AS0N-6000";
        converseLib.validationFailed(130); /* no scroll forward*/
      end
      return; /* ===> Return to previous process*/
    end
    ;
    if (converseVar.eventKey is pf10) /* ===> Scroll left requested*/
      if (AS0NW01.AS0NW-SCREEN-DISPLAYED == 3)
        AS0NW01.AS0NW-SCREEN-DISPLAYED = 2;
      else
        if (AS0NW01.AS0NW-SCREEN-DISPLAYED == 2)
          AS0NW01.AS0NW-SCREEN-DISPLAYED = 1;
        else
          AS0NM001.XXXPRCNM = "AS0N-6000";
          converseLib.validationFailed(49); /* no scroll forward*/
        end
      end
      return; /* ===> Return to previous process*/
    end
    ;
    if (converseVar.eventKey is pf11) /* ===> Scroll right requested*/
      if (AS0NW01.AS0NW-SCREEN-DISPLAYED == 1)
        AS0NW01.AS0NW-SCREEN-DISPLAYED = 2;
      else
        if (AS0NW01.AS0NW-SCREEN-DISPLAYED == 2)
          AS0NW01.AS0NW-SCREEN-DISPLAYED = 3;
        else
          AS0NM001.XXXPRCNM = "AS0N-6000";
          converseLib.validationFailed(50); /* no scroll forward*/
        end
      end
      return; /* ===> Return to previous process*/
    end
    ;
  end
  ;
  if (converseVar.validationMsgNum != 0)
    exit stack;
  end
  ;
  if (AS0NW01.AS0NW-VALID-UPD-FL == "Y"
   && AS0NW01.AS0NW-CONFIRM-UPD-FL != "Y")
    AS0N-S009();
    AS0NW-CONFIRM-UPD-FL = "Y"; /* GET A CONFIRMATION*/
    converseLib.validationFailed(89);
    return;
  end
  ;
   /* IF AS0NW01.AS0NW-VALID-UPD-FL = 'Y';*/
    /* AND AS0NW01.AS0NW-CONFIRM-UPD-FL = 'Y';*/
    /* MOVE 'N' TO AS0NW01.AS0NW-VALID-UPD-FL;*/
    /* MOVE 'N' TO AS0NW01.AS0NW-CONFIRM-UPD-FL;*/
    /* MOVE 'N' TO AS0NW01.AS0NW-UPDATES-MADE;*/
    /* PERFORM AS0N-6100;             /* UPDATE PROCESS*/
   /* END;*/
  ;
  ;
  ;
end // end AS0N-6000

// Setup Month buckets
Function AS0N-S000()
  ;
  ASW-DATE = VGVar.currentShortGregorianDate;
  ;
  AS0NW01.AS0NW-WORK-DATE-YYMM = ASWREC.ASW-YY-MM;
  ;
  if (ASWREC.ASW-YY > 75)
    AS0NW01.AS0NW-WORK-DATE-CC = 19;
  else
    AS0NW01.AS0NW-WORK-DATE-CC = 20;
  end
  ;
  ASWREC.ASW-IDX1 = 1;
  while (ASWREC.ASW-IDX1 < 12)
    AS0NW01.AS0NW-MONTH-BUCKET[ASW-IDX1] = AS0NW01.AS0NW-WORK-DATE-A;
    ASWREC.ASW-IDX1 = ASWREC.ASW-IDX1 + 1;
    AS0NW01.AS0NW-WORK-DATE-MM = AS0NW01.AS0NW-WORK-DATE-MM + 1;
    if (AS0NW01.AS0NW-WORK-DATE-MM == 13)
      AS0NW01.AS0NW-WORK-DATE-MM = 1;
      if (AS0NW01.AS0NW-WORK-DATE-YY == 99)
        AS0NW01.AS0NW-WORK-DATE-CC = 20;
        AS0NW01.AS0NW-WORK-DATE-YY = 0;
      else
        AS0NW01.AS0NW-WORK-DATE-YY = AS0NW01.AS0NW-WORK-DATE-YY + 1;
      end
    end
  end
end // end AS0N-S000

