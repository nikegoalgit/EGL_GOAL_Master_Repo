  ;
  if (AS0NW01.XSECD > " ") /* Season Entered*/
    AS0N-1431();
    if (AS0NW01.AS0NW-VALID-SEASON-ROW == "N")
      return;
    end
  end
  ;
  set PG2REC empty;
  PG2REC.SY1STNBR = VPRTPL01.SY1STNBR;
  PG2REC.SY2CLRID = VPRTPL01.SY2CLRID;
  PG2REC.XGPCD = "03";
  PG2REC.XDMCD = "00";
  AS0N-1433(); /* get scheduler, blank sty nbr*/
  ;
   /* valid print plan and season row.  Add row to WS*/
  ;
  if (AS0NW-TSQ-MAX > 0)
    /* IF VPRTPL01.FY1FCTYC NE AS0NW03.FY1FCTYC(AS0NW-TSQ-MAX);*/
    if (VPRTPL01.FY1FCTYC != AS0NW02.AS0NW-CURR-FCTY
     || VPRTPL01.SY1STNBR != AS0NW03.SY1STNBR[AS0NW-TSQ-MAX]
     || VPRTPL01.SY2CLRID != AS0NW03.SY2CLRID[AS0NW-TSQ-MAX]
     || VPRTPL01.XOWCD != AS0NW03.XOWCD[AS0NW-TSQ-MAX]
     || VPRTPL01.XDVCD != AS0NW03.XDVCD[AS0NW-TSQ-MAX])
      if (AS0NW-TSQ-MAX == 10)
        AS0N-S012(); /* build line totals*/
        AS0N-1432(); /* add data to temp tsq*/
        AS0NW-TSQ-MAX = 1; /* start new page*/
        AS0N-S003();
      else
        AS0NW03.AS0NW-TSQ-MAX = AS0NW-TSQ-MAX + 1;
        AS0N-S003();
      end
    end
  else
    AS0NW-TSQ-MAX = 1;
    AS0N-S003();
  end
  ;
  AS0N-S004();
  ;
end // end AS0N-1430

// Select Unique/DACAST.VAFCLR01
Function AS0N-1431()
   /* --------------------------------------------------------------------*/
   /* - Select Unique From DACAST.VAFCLR01 To Verify Season For Style    -*/
   /* --------------------------------------------------------------------*/
  ;
   /* *=====================**/
   /* *  Build Access Keys  **/
   /* *=====================**/
  ;
  AS0N-S002();
  ;
   /* *=============================**/
   /* *  Access DACAST.VAFCLR01 DB  **/
   /* *=============================**/
  ;
  try
    call "IO3520" ("S ", SQLCA, AF5REC) {isNoRefresh = yes, isExternal = yes};
  end
  ;
   /* ------------------------------------------------------------*/
    /* If successful call, save desc to working storage*/
   /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* Normal return code*/
    AS0NW01.AS0NW-VALID-SEASON-ROW = "Y"; /* row is valid for season*/
    return; /* Return to calling process*/
  end
  ;
  AS0NW01.AS0NW-VALID-SEASON-ROW = "N"; /* row is valid for season*/
  ;
   /* ------------------------------------------------------------*/
   /* If data not available, return error condition.*/
   /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    ;
    if (SQLCA.VAGen_SQLCODE == 100) /* No data found*/
      return; /* Return to calling process*/
    end
    if (SQLCA.VAGen_SQLCODE == -911) /* Resource not available*/
      converseLib.validationFailed(11); /* Indicate not available*/
      return; /* Termination function*/
    else
      converseLib.validationFailed(10); /* Indicate system error*/
      TA1REC.TA1FUNC = "RETN"; /* Termination function*/
      converseLib.clearScreen();
       /* Clear screen before redisplay*/
    end
    ;
  end
  ;
  TA1REC.TA1MAP = sysVar.errorCode; /* Save CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* Save SQL commarea*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  AS0NM001.XXXPRCNM = "AS0N-1431"; /* Process number*/
  TA1REC.TA1PGMNM = "AS0N"; /* Error location*/
  TA1REC.TA1LOC = "GET DACAST.VAFCLR01   ";
  TA1REC.TA1DBASE = "DACAST  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VAFCLR01"; /* Table view name*/
  call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis rtn*/
  converseLib.validationFailed(10); /* Indicate system error*/
  exit stack; /* Exit to redisplay map*/
