package ka0m;
//*** DATATABLE=KA0MT ****
// this table contains the selection number, description and
// four digit mnemonic for each entry on the menu screen.
// ************************
DataTable KA0MT type basicTable  {
  shared = yes, resident = no
  }
  10 KA0MTOCD KA0MTOCD; 
  10 KA0MTCDD KA0MTCDD; 
  10 KA0MTTAP KA0MTTAP; 
   {contents = [
    [01, "ADD COMPANY                   ", "KA0C"],
    [02, "ADD STORE                     ", "KA0S"],
    [03, "ADD CUSTOMER-UNIT             ", "KA0D"],
    [04, "ADD PARENT                    ", "KA0P"],
    [99, "END OF TABLE MARKER           ", "ZZZZ"]
  ]}
end // end KA0MT

