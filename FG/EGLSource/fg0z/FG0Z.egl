// verify modifiable fields
Function FG0ZS08()
   /* -----------------------------------------------------*/
   /* verify header modifiable fields*/
   /* -----------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWIDX = 1;
  while (FG0ZW00.FG0ZWIDX <= 12
   && FG0ZW01.FG0ZWHD4[FG0ZWIDX] <= FFWREC.FFAENDMO)
    ;
    if (FG0ZM001.FG0ZMIX4[FG0ZWIDX] != FG0ZW01.FG0ZWIX4[FG0ZWIDX])
      set FG0ZM001.FG0ZMIX4[FG0ZWIDX] cursor, bold;
      converseLib.validationFailed(111);
      FFWREC.XXXMODID = "S08";
    end
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
    ;
  end
  ;
  FG0ZW00.FG0ZWIDX = 1;
  while (FG0ZW00.FG0ZWIDX <= 4)
    ;
    if (FG0ZM001.FG0ZMSEL[FG0ZWIDX] != FG0ZW01.FG0ZWSEL[FG0ZWIDX])
      set FG0ZM001.FG0ZMSEL[FG0ZWIDX] cursor, bold;
      converseLib.validationFailed(111);
      FFWREC.XXXMODID = "S08";
    end
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
    ;
  end
  ;
  if (converseVar.eventKey not enter)
    ;
    if (FG0ZM001.XPCCD != FFWREC.XPCCD)
      set FG0ZM001.XPCCD cursor, bold;
      converseLib.validationFailed(130);
      FFWREC.XXXMODID = "S08";
    end
    ;
    if (FG0ZM001.YPGCD != FFWREC.YPGCD)
      set FG0ZM001.YPGCD cursor, bold;
      converseLib.validationFailed(130);
      FFWREC.XXXMODID = "S08";
    end
    ;
    FG0ZW00.FG0ZWWED = FFWREC.FFAENDMO;
    if (FG0ZM001.FG0ZMERG != FG0ZW00.FG0ZWEYM)
      set FG0ZM001.FG0ZMERG cursor, bold;
      converseLib.validationFailed(130);
      FFWREC.XXXMODID = "S08";
    end
    ;
    FG0ZW00.FG0ZWWBD = FFWREC.FFABEGMO;
    if (FG0ZM001.FG0ZMBRG != FG0ZW00.FG0ZWBYM)
      set FG0ZM001.FG0ZMBRG cursor, bold;
      converseLib.validationFailed(130);
      FFWREC.XXXMODID = "S08";
    end
  end
  ;
  ;
end // end FG0ZS08

