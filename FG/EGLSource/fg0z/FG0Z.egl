// load user requested data
Function FG0Z150()
  ;
   /* ------------------------------------------------------------*/
   /* obtain monthly headings and quantities for user created*/
   /* data*/
   /* ------------------------------------------------------------*/
  ;
  set FFZREC empty;
  ;
  FFZREC.FFZBYYMM = FG0ZW01.FG0ZWHBY[4]; /* user beg date*/
  FFZREC.FFZEYYMM = FG0ZW01.FG0ZWHEY[4]; /* user end date*/
  ;
  FG0ZS01(); /* get months within range*/
  ;
  FG0ZW00.FG0ZWIDX = 1; /* init index*/
  while (FG0ZW00.FG0ZWIDX <= FFZREC.FFZNUMMO) /* process all months in range*/
    ;
    FG0ZW01.FG0ZWHD4[FG0ZWIDX] = FFZREC.FFZINDMO[FG0ZWIDX];
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* verify dfcast.vrcat001 exists and if the future ship*/
   /* indicator equals 'u' (user created), if so,  select all*/
   /* dfcast.vrcmth01 for the user requested date range.*/
   /* ------------------------------------------------------------*/
  ;
  set FFCREC empty;
  ;
  FFCREC.FFABEGMO = FFWREC.FFABEGMO;
  FFCREC.FFAENDMO = FFWREC.FFAENDMO;
  ;
  FG0ZS02(); /* select dfcast.vrcat001*/
  ;
  if (SQLCA.VAGen_SQLCODE == 0)
    if (FFCREC.FFCPSIND == "1" /* history 1 yr ago*/
     && FG0ZW00.FG0ZWH1E != "Y")
      FG0ZW01.FG0ZWSEL[1] = "S"; /* data exists*/
    end
    if (FFCREC.FFCPSIND == "2" /* history 2 yr ago*/
     && FG0ZW00.FG0ZWH2E != "Y")
      FG0ZW01.FG0ZWSEL[2] = "S"; /* data exists*/
    end
    if (FFCREC.FFCPSIND == "3" /* history 3 yr ago*/
     && FG0ZW00.FG0ZWH3E != "Y")
      FG0ZW01.FG0ZWSEL[3] = "S"; /* data exists*/
    end
    if (FFCREC.FFCPSIND == "U") /* user created*/
      ;
      FG0ZW01.FG0ZWSEL[4] = "S"; /* data exists*/
      ;
      set FFDREC empty;
      ;
      FFFREC.FFFBEGMO = FFWREC.FFABEGMO;
      FFFREC.FFFENDMO = FFWREC.FFAENDMO;
      ;
      FFDREC.FFABEGMO = FFWREC.FFABEGMO;
      FFDREC.FFAENDMO = FFWREC.FFAENDMO;
      ;
      FG0ZS03(); /* select 's1' dfcast.vrcmth01*/
      FG0ZS04(); /* fetch 'n1' dfcast.vrcmth01*/
      ;
      FG0ZW00.FG0ZWIDX = 1; /* init index*/
      while (SQLCA.VAGen_SQLCODE == 0) /* process all dfcast.vrcmth01*/
        ;
        FG0ZW01.FG0ZWIX4[FG0ZWIDX] = mathLib.round(FFDREC.FFDPSPCT + 0, -mathLib.decimals(FG0ZW01.FG0ZWIX4[FG0ZWIDX]));
        ;
        FG0ZS04(); /* fetch 'n1' dfcast.vrcmth01*/
        ;
        FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
        ;
      end
      ;
      FG0ZS05(); /* close 'c1' dfcast.vrcmth01*/
      ;
    end
  end
  ;
  ;
end // end FG0Z150

