    ;
  else
    FFWREC.XXXMSGNO = converseVar.validationMsgNum; /* load message number*/
    ;
  end
  ;
  FG0ZM001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/
  ;
  FG0ZM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FG0ZM001.CAITEM = COMMAREA.CAITEM; /* Application data*/
  ;
   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/
  ;
  if (FFWREC.XXXMSGNO != 0) /* Message to display*/
    if (FFWREC.XXXMSGNO == 9999)
      converseLib.validationFailed();
    else
      converseLib.validationFailed(FFWREC.XXXMSGNO);
    end /* error message number*/
    FG0ZM001.XXXMODID = FFWREC.XXXMODID; /* Module identification*/
    set FG0ZM001 alarm; /* Indicate sound alarm*/
  end
  ;
  converse FG0ZM001 ;
  ;
   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/
  FG0ZM001.FG0ZMHED[1] = " ";
  FG0ZM001.FG0ZMHED[2] = " ";
  FG0ZM001.FG0ZMHED[3] = " ";
  ;
  FFWREC.XXXMODID = " "; /* Module identification*/
  FFWREC.XXXMSGNO = 0;
  ;
  COMMAREA.CATOAP = FG0ZM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FG0ZM001.CAITEM; /* Application data*/
  ;
  ;
end // end FG0Z200

// Format the display data
Function FG0Z210()
  ;
   /* ------------------------------------------------------------*/
   /* init map fg0zm001*/
   /* ------------------------------------------------------------*/
  ;
  set FG0ZM001 initial; /* m001 input/ouput area*/
  ;
  FG0ZW00.FG0ZWSWF = "N"; /* init map switch*/
  ;
   /* ------------------------------------------------------------*/
   /* Format fg0zm001 using passed data*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWWDT = FFWREC.FFABEGMO; /* w/s date field*/
  FG0ZM001.FG0ZMBRG = FG0ZW00.FG0ZWWYM; /* begin month range*/
  FG0ZW00.FG0ZWWDT = FFWREC.FFAENDMO; /* w/s date field*/
  FG0ZM001.FG0ZMERG = FG0ZW00.FG0ZWWYM; /* end month range*/
  ;
  FG0ZM001.YPGCD = FFWREC.YPGCD; /* product type group*/
  FG0ZM001.YPGABRV = FFWREC.YPGABRV; /* product type group abrv*/
  ;
  FG0ZM001.XPCCD = FFWREC.XPCCD; /* category code*/
  FG0ZM001.XPCABRV = FFWREC.XPCABRV; /* category code abrv*/
  ;
   /* ------------------------------------------------------------*/
   /* if data has not been loaded bypass load map fiecds*/
   /* ------------------------------------------------------------*/
  ;
  if (FG0ZW01.CACHGFL != "Y")
    return;
  end
  ;
   /* ------------------------------------------------------------*/
   /* load monthly headings/index/quantities/total for*/
   /* historical data 1 year ago*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWIDX = 1;
  while (FG0ZW00.FG0ZWIDX <= 12) /* nbr of mos loaded*/
    ;
    FG0ZM001.FG0ZMHD1[FG0ZWIDX] = FG0ZW01.FG0ZWDH1[FG0ZWIDX];
    if (FG0ZW00.FG0ZWH1E != "Y")
      FG0ZM001.FG0ZMIX1[FG0ZWIDX] = FG0ZW01.FG0ZWIX1[FG0ZWIDX];
      FG0ZM001.FG0ZMQT1[FG0ZWIDX] = FG0ZW01.FG0ZWQT1[FG0ZWIDX];
    end
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1; /* increment index*/
    ;
  end
  ;
  if (FG0ZW00.FG0ZWH1E == "Y")
    FG0ZW00.FG0ZWEBD = FG0ZW01.FG0ZWPBD[1];
    FG0ZW00.FG0ZWEED = FG0ZW01.FG0ZWPED[1];
    FG0ZM001.FG0ZMHED[1] = FG0ZW00.FG0ZWHEM;
  else
    FG0ZM001.FG0ZMTOT[1] = FG0ZW01.FG0ZWQT1[FG0ZWIDX];
    FG0ZM001.FG0ZMSEL[1] = FG0ZW01.FG0ZWSEL[1];
    set FG0ZM001.FG0ZMSEL[1] normal;
  end
  ;
   /* ------------------------------------------------------------*/
   /* load monthly headings/index/quantities/total for*/
   /* historical data 2 year ago*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWIDX = 1;
  while (FG0ZW00.FG0ZWIDX <= 12) /* nbr of mos loaded*/
    ;
    FG0ZM001.FG0ZMHD2[FG0ZWIDX] = FG0ZW01.FG0ZWDH2[FG0ZWIDX];
    if (FG0ZW00.FG0ZWH2E != "Y")
      FG0ZM001.FG0ZMIX2[FG0ZWIDX] = FG0ZW01.FG0ZWIX2[FG0ZWIDX];
      FG0ZM001.FG0ZMQT2[FG0ZWIDX] = FG0ZW01.FG0ZWQT2[FG0ZWIDX];
    end
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1; /* increment index*/
    ;
  end
  ;
  if (FG0ZW00.FG0ZWH2E == "Y")
    FG0ZW00.FG0ZWEBD = FG0ZW01.FG0ZWPBD[2];
    FG0ZW00.FG0ZWEED = FG0ZW01.FG0ZWPED[2];
    FG0ZM001.FG0ZMHED[2] = FG0ZW00.FG0ZWHEM;
  else
    FG0ZM001.FG0ZMTOT[2] = FG0ZW01.FG0ZWQT2[FG0ZWIDX];
    FG0ZM001.FG0ZMSEL[2] = FG0ZW01.FG0ZWSEL[2];
    set FG0ZM001.FG0ZMSEL[2] normal;
  end
  ;
   /* ------------------------------------------------------------*/
   /* load monthly headings/index/quantities/total for*/
   /* historical data 3 year ago*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWIDX = 1;
  while (FG0ZW00.FG0ZWIDX <= 12) /* nbr of mos loaded*/
    ;
    FG0ZM001.FG0ZMHD3[FG0ZWIDX] = FG0ZW01.FG0ZWDH3[FG0ZWIDX];
    if (FG0ZW00.FG0ZWH3E != "Y")
      FG0ZM001.FG0ZMIX3[FG0ZWIDX] = FG0ZW01.FG0ZWIX3[FG0ZWIDX];
      FG0ZM001.FG0ZMQT3[FG0ZWIDX] = FG0ZW01.FG0ZWQT3[FG0ZWIDX];
    end
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1; /* increment index*/
    ;
  end
  ;
  if (FG0ZW00.FG0ZWH3E == "Y")
    FG0ZW00.FG0ZWEBD = FG0ZW01.FG0ZWPBD[3];
    FG0ZW00.FG0ZWEED = FG0ZW01.FG0ZWPED[3];
    FG0ZM001.FG0ZMHED[3] = FG0ZW00.FG0ZWHEM;
  else
    FG0ZM001.FG0ZMTOT[3] = FG0ZW01.FG0ZWQT3[FG0ZWIDX];
    FG0ZM001.FG0ZMSEL[3] = FG0ZW01.FG0ZWSEL[3];
    set FG0ZM001.FG0ZMSEL[3] normal;
  end
  ;
   /* ------------------------------------------------------------*/
   /* load user created monthly headings*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWIDX = 1;
  while (FG0ZW00.FG0ZWIDX <= 12) /* nbr of mos loaded*/
    ;
    FG0ZM001.FG0ZMHD4[FG0ZWIDX] = FG0ZW01.FG0ZWDH4[FG0ZWIDX];
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1; /* increment index*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* if dfcast.vrcmth01 data has been loaded only allow the*/
   /* user to update existing data, if not, allow the user to*/
   /* add up to 12 indexs*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWIDX = 1;
  while (FG0ZW00.FG0ZWIDX <= 12
   && FG0ZW01.FG0ZWHD4[FG0ZWIDX] <= FFWREC.FFAENDMO)
    ;
    FG0ZM001.FG0ZMIX4[FG0ZWIDX] = FG0ZW01.FG0ZWIX4[FG0ZWIDX];
    set FG0ZM001.FG0ZMIX4[FG0ZWIDX] normal;
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1; /* increment index*/
    ;
  end
  ;
  FG0ZM001.FG0ZMSEL[4] = FG0ZW01.FG0ZWSEL[4];
  set FG0ZM001.FG0ZMSEL[4] normal;
  ;
  ;
end // end FG0Z210

// Check attention identifier
Function FG0Z300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    ;
    converseLib.clearScreen();
     /* refresh map*/
    ;
    FG0ZW00.FG0ZWSWF = "Y"; /* format screen m001*/
    FG0ZW00.FG0ZWSWR = "N"; /* request for data*/
    ;
    COMMAREA.CATOAP = " "; /* to application*/
    COMMAREA.CAITEM = " "; /* passed data*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/
  ;
  if (FG0ZM001.CATOAP > " ") /* FAST PATH was requested*/
    ;
    if (converseVar.eventKey is enter) /* ENTER was pressed*/
      ;
      XSFFS01(); /* Exit to another application*/
      ;
      FG0ZW00.FG0ZWSWF = "Y"; /* format screen m001*/
      FG0ZW00.FG0ZWSWR = "N"; /* request for data*/
      ;
      set FG0ZM001.CATOAP cursor, bold; /* Highlight*/
      ;
    else
      ;
      FFWREC.XXXMODID = "300"; /* Module identification*/
      converseLib.validationFailed(36); /* Indicate PF key not valid*/
      ;
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf5) /* or PF5 was pressed*/
    ;
    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FG0ZM001"; /* Application find code*/
    end
    ;
    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FG0A"; /* TO application*/
    end
    ;
    XSFFS01(); /* Exit to another application*/
    ;
    FG0ZW00.FG0ZWSWF = "Y"; /* format screen m001*/
    FG0ZW00.FG0ZWSWR = "N"; /* request for data*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf13) /* pf13 was not pressed*/
    ;
    FFWREC.XXXMODID = "300"; /* Module identification*/
    converseLib.validationFailed(3); /* Indicate invalid attention key*/
    ;
    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  ;
end // end FG0Z300

// Reset highlighted fields
Function FG0Z400()
   /* ------------------------------------------------------------*/
   /* Reset field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/
  ;
  set FG0ZM001.FG0ZMBRG initialAttributes; /* begin range*/
  set FG0ZM001.FG0ZMERG initialAttributes; /* end range*/
  set FG0ZM001.XPCCD initialAttributes; /* category code*/
  set FG0ZM001.YPGCD initialAttributes; /* product type group*/
  ;
  if (FG0ZW01.CACHGFL == "Y")
    ;
    set FG0ZM001.FG0ZMSEL[1] normal;
    set FG0ZM001.FG0ZMSEL[2] normal;
    set FG0ZM001.FG0ZMSEL[3] normal;
    set FG0ZM001.FG0ZMSEL[4] normal;
    ;
    FG0ZW00.FG0ZWIDX = 1;
    while (FG0ZW00.FG0ZWIDX <= 12
     && FG0ZW01.FG0ZWHD4[FG0ZWIDX] <= FFWREC.FFAENDMO)
      ;
      set FG0ZM001.FG0ZMIX4[FG0ZWIDX] normal;
      ;
      FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
      ;
    end
  end
  ;
  set FG0ZM001.CATOAP initialAttributes;
  set FG0ZM001.CAITEM initialAttributes;
  ;
   /* ------------------------------------------------------------*/
   /* If no lists were requested, return to the previous process.*/
   /* ------------------------------------------------------------*/
  ;
  if (FG0ZM001.XPCCD == "?" /* categroy code*/
   || FG0ZM001.YPGCD == "?") /* product type group*/
    ;
    if (converseVar.eventKey not enter) /* ENTER was not pressed*/
      ;
      FFWREC.XXXMODID = "400"; /* Module identification*/
      converseLib.validationFailed(19); /* Indicate PF key not valid*/
      ;
      exit stack; /* Exit to display map*/
      ;
    end
    ;
  else
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If requested, display the list of category codes*/
   /* ------------------------------------------------------------*/
  ;
  if (FG0ZM001.XPCCD == "?") /* product category list*/
    ;
    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "CATEGORY CODES "; /* Heading*/
    ;
    XX0XW02.XGPCD = COMMAREA.XGPCD;
    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXPC01(); /* Display the list*/
    end
    ;
    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/
      ;
      FG0ZM001.XPCCD = FFWREC.XPCCD; /* product category*/
      XPCREC.XPCCD = FFWREC.XPCCD; /* product category*/
      XPCREC.XGPCD = COMMAREA.XGPCD; /* gpc code*/
      if (XPCREC.XPCKEY in XPCTBL.XPCKEY)
        FG0ZM001.XPCABRV = XPCTBL.XPCABRV[sysVar.arrayIndex];
      end
    else
      FG0ZM001.XPCCD = XPCTBL.XPCCD[XX0XWPIX]; /* Selected code*/
      FG0ZM001.XPCABRV = XPCTBL.XPCABRV[XX0XWPIX]; /* Selected code*/
      ;
    end
    ;
    set FG0ZM001.XPCCD cursor; /* Establish cursor position*/
    ;
  end
   /* ------------------------------------------------------------*/
   /* If requested, display the list of product type group codes*/
   /* ------------------------------------------------------------*/
  ;
  if (FG0ZM001.YPGCD == "?") /* product type grp list*/
    ;
    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "PRODUCT TYPE GROUP"; /* Heading*/
    ;
    XX0XW02.YPGFFFL = "Y"; /* init flag*/
    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPYPG01(); /* Display the list*/
    end
    ;
    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/
      ;
      FG0ZM001.YPGCD = FFWREC.YPGCD; /* Restore prior code*/
      FG0ZM001.YPGABRV = FFWREC.YPGABRV; /* Restore prior code*/
    else
      FG0ZM001.YPGCD = YPGTBL.YPGCD[XX0XWPIX]; /* Selected code*/
      FG0ZM001.YPGABRV = YPGTBL.YPGABRV[XX0XWPIX]; /* Selected code*/
      ;
    end
    ;
    set FG0ZM001.YPGCD cursor; /* Establish cursor position*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* Indicate that the requested lists have been displayed and*/
   /* that ENTER must be pressed to process any changes.*/
   /* ------------------------------------------------------------*/
  ;
  converseLib.clearScreen();
   /* Clear screen before display*/
  ;
  FFWREC.XXXMODID = "400"; /* Module identification*/
  converseLib.validationFailed(97); /* Indicate press ENTER*/
  ;
  exit stack; /* Exit to display map*/
  ;
  ;
end // end FG0Z400

// Verify the entered data
Function FG0Z500()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/
  ;
  if (COMMAREA.CAITEM > " ") /* Application data was entered*/
    ;
    FFWREC.XXXMODID = "500"; /* Module identification*/
    set FG0ZM001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(40); /* Enter only with FAST PATH*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* request for transfer*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf13)
    ;
    FG0ZS08(); /* verify modifiable fields*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* validate entry fields*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is enter)
    ;
    FG0Z510(); /* validate entry fields*/
    ;
    /* ------------------------------------------------------*/
     /* if key data changed check modifiable fields*/
    /* ------------------------------------------------------*/
    ;
    FG0ZW00.FG0ZWWBD = FFWREC.FFABEGMO;
    FG0ZW00.FG0ZWWED = FFWREC.FFAENDMO;
    ;
    if (FG0ZM001.YPGCD != FFWREC.YPGCD        /* product type grp chg*/
     || FG0ZM001.YPGCD == " "                 /* product type grp chg*/
     || FG0ZM001.XPCCD != FFWREC.XPCCD        /* product category chg*/
     || FG0ZM001.XPCCD == " "                 /* product category chg*/
     || FG0ZM001.FG0ZMBRG != FG0ZW00.FG0ZWBYM /* beg range changed*/
     || FG0ZM001.FG0ZMBRG == 0                /* beg range changed*/
     || FG0ZM001.FG0ZMERG != FG0ZW00.FG0ZWEYM /* end range changed*/
     || FG0ZM001.FG0ZMERG == 0)               /* end range changed*/
      ;
      if (FG0ZW01.CACHGFL == "Y")
        FG0ZS08(); /* verify modifiable fields*/
      end
      ;
      if (converseVar.validationMsgNum == 0)
        FG0ZW00.FG0ZWSWR = "Y"; /* request data request*/
      end
      ;
    end
  end
  ;
  if (converseVar.validationMsgNum != 0)
    exit stack; /* display error message*/
  end
  ;
  ;
end // end FG0Z500

// validate modifiable data
Function FG0Z510()
  ;
   /* ------------------------------------------------------------*/
   /* validate user created index*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW01.FG0ZWUVQ = 0;
  FG0ZW00.FG0ZWIDX = 12;
  while (FG0ZW00.FG0ZWIDX > 0)
    ;
    FG0ZW01.FG0ZWUVQ = FG0ZW01.FG0ZWUVQ + FG0ZM001.FG0ZMIX4[FG0ZWIDX];
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX - 1;
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* if user created was selected and the percent total does*/
   /* not equal 100 display variance qty and error message*/
   /* ------------------------------------------------------------*/
  ;
  if (FG0ZW01.FG0ZWUVQ != 100
   && FG0ZW01.FG0ZWUVQ != 0)
    converseLib.validationFailed(159);
    FFWREC.XXXMODID = "510";
    FG0ZW01.FG0ZWUVQ = 100 - FG0ZW01.FG0ZWUVQ;
    FG0ZM001.FG0ZMVQT = FG0ZW01.FG0ZWUVQ;
    set FG0ZM001.FG0ZMIX4[1] cursor;
  else
    if (FG0ZW01.FG0ZWUVQ == 0
     && FG0ZM001.FG0ZMSEL[4] == "S")
      converseLib.validationFailed(159);
      FFWREC.XXXMODID = "510";
      FG0ZW01.FG0ZWUVQ = 100 - FG0ZW01.FG0ZWUVQ;
      FG0ZM001.FG0ZMVQT = FG0ZW01.FG0ZWUVQ;
      set FG0ZM001.FG0ZMIX4[1] cursor;
    end
  end
  ;
   /* ------------------------------------------------------------*/
   /* validate product category code*/
   /* ------------------------------------------------------------*/
  ;
  XPCREC.XGPCD = COMMAREA.XGPCD;
  XPCREC.XPCCD = FG0ZM001.XPCCD;
  if (XPCREC.XPCKEY in XPCTBL.XPCKEY
   && XPCTBL.XXXSTAT[sysVar.arrayIndex] == "A")
    FG0ZM001.XPCABRV = XPCTBL.XPCABRV[sysVar.arrayIndex];
  else
    FG0ZM001.XPCABRV = " ";
    converseLib.validationFailed(118);
    FFWREC.XXXMODID = "510";
    set FG0ZM001.XPCCD cursor, bold;
  end
  ;
   /* ------------------------------------------------------------*/
   /* validate product type group*/
   /* ------------------------------------------------------------*/
  ;
  if (FG0ZM001.YPGCD != "*")
    if (FG0ZM001.YPGCD in YPGTBL.YPGCD
     && YPGTBL.XXXSTAT[sysVar.arrayIndex] == "A")
      FG0ZM001.YPGABRV = YPGTBL.YPGABRV[sysVar.arrayIndex];
    else
      FG0ZM001.YPGABRV = " ";
      converseLib.validationFailed(118);
      FFWREC.XXXMODID = "510";
      set FG0ZM001.YPGCD cursor, bold;
    end
  end
  ;
   /* ------------------------------------------------------------*/
   /* validate end yymm range and begin yymm range*/
   /* ------------------------------------------------------------*/
  ;
  set FFZREC empty;
  ;
  FFZREC.FFZMONTH = 6;
  FFZREC.FFZBYYMM = FG0ZM001.FG0ZMBRG;
  FFZREC.FFZEYYMM = FG0ZM001.FG0ZMERG;
  ;
  XSFFS35(); /* edit date range*/
  ;
  if (FFZREC.FFZENDRC == "1")
    set FG0ZM001.FG0ZMERG cursor, bold; /* Highlight*/
    converseLib.validationFailed(8); /* Indicate missing or invalid*/
    FFWREC.XXXMODID = "510";
  else
    if (FFZREC.FFZENDRC == "2")
      set FG0ZM001.FG0ZMERG cursor, bold; /* Highlight*/
      converseLib.validationFailed(17); /* Indicate missing or invalid*/
      FFWREC.XXXMODID = "510";
    else
      if (FFZREC.FFZENDRC == "3")
        set FG0ZM001.FG0ZMERG cursor, bold; /* Highlight*/
        converseLib.validationFailed(151); /* out of range*/
        FFWREC.XXXMODID = "510";
      end
    end
  end
  ;
  if (FFZREC.FFZBEGRC == "1")
    set FG0ZM001.FG0ZMBRG cursor, bold; /* Highlight*/
    converseLib.validationFailed(8); /* Indicate missing or invalid*/
    FFWREC.XXXMODID = "510";
  else
    if (FFZREC.FFZBEGRC == "2")
      set FG0ZM001.FG0ZMBRG cursor, bold; /* Highlight*/
      converseLib.validationFailed(17); /* Indicate missing or invalid*/
      FFWREC.XXXMODID = "510";
    else
      if (FFZREC.FFZBEGRC == "3")
        set FG0ZM001.FG0ZMBRG cursor, bold; /* Highlight*/
        converseLib.validationFailed(151); /* out of range*/
        FFWREC.XXXMODID = "510";
      end
    end
  end
   /* ------------------------------------------------------------*/
   /* validate 's'election field*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWSLF = "N"; /* init selection flag*/
  ;
  FG0ZW00.FG0ZWIDX = 4; /* init index*/
  while (FG0ZW00.FG0ZWIDX >= 1)
    ;
    if (FG0ZM001.FG0ZMSEL[FG0ZWIDX] != " "
     && FG0ZM001.FG0ZMSEL[FG0ZWIDX] != "S")
      converseLib.validationFailed(41);
      FFWREC.XXXMODID = "510";
