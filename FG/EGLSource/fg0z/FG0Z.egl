//*** PROGRAM=FG0Z ****
// This application provides the user with the ability to
// create a prop ship index by referring to the historical
// data or the user can pick a specific historical prop ship
// index(1 year ago, 2 year ago, or 3 years ago) for a given
// range and specific category.
// 
// The displayed shipment data may be actual shipment or
// forecasted shipment.  If the requested range is before the
// current month -1 month, the actual prop shipped quantity
// will be shown.  If the range is between the current month
// and the next 5 months, the forecasted shipped quantity
// will be shown.  This program needs to use the invoice
// cut off date to determine if the prop shipped quantity
// should be used or the actual shipped quantity from the
// the history database should be used.
// 
// The monthly quantities and indices will be displayed.
// If the user created indices didn't add up to 100, then
// the variance will be shown on the top of the screen.  The
// quantites are all in thousand.
// 
// The user may exit to any allowed application, via FAST PATH
// or PF key, from either of the screens.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
//          02/21/90  rgilli  New application.
// FF1016   08/01/90  JJUCH   Disallowed to fcst current main
//                            buy month and all prior months.
// FF175    12/12/96  DSMITH  Regenerated for yr 2000.
//          05/13/99  CWHITE  set date to 0 fill
// *********************
Program FG0Z type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "FF00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  FF1REC FF1REC; // record
  FF7REC FF7REC; // record
  FFCREC FFCREC; // record
  FFDREC FFDREC; // record
  FFFREC FFFREC; // record
  FFWREC FFWREC; // record
  FFXREC FFXREC; // record
  FFZREC FFZREC; // record
  FG0ZW00 FG0ZW00; // record
  FG0ZW01 FG0ZW01; // record
  FH6REC FH6REC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  XPCREC XPCREC; // record
  XX0XW01 XX0XW01; // record
  XX0XW02 XX0XW02; // record
  YSMREC YSMREC; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XPCTBL; // table
  use YPGTBL; // table
  use FG0ZM.FG0ZM001, // form
  		FG0ZM.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPFF001: XPFF001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/
    ;
    set FFFREC empty;
    set FG0ZM001 initial; /* M001 input/output*/
    set FG0ZW00 empty; /* Working storage*/
    set FG0ZW01 empty; /* M001 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set TA2REC empty; /* date routine work area*/
    set TA5REC empty; /* temp storage queue area*/
    set XX0XW01 empty; /* Date/time subroutine parameters*/
    ;
    FG0ZW00.FG0ZWSWR = "N"; /* Request for data switch*/
    FG0ZW00.FG0ZWSWF = "Y"; /* format screen data switch*/
    FG0ZW00.FG0ZWDES = "INDEX HAS NOT BEEN DEFINED";
    ;
     /* ---------------------------------------------------*/
     /* Clear unused fields*/
     /* ---------------------------------------------------*/
    ;
    FFWREC.XPLCD = " "; /* Clear product line code*/
    FFWREC.XPLABRV = " "; /* Clear product line abrv*/
    FFWREC.SYBSTGNB = " "; /* Clear style group number*/
    FFWREC.SYBSTGNM = " "; /* Clear style group name*/
    FFWREC.SY1STNBR = " "; /* Clear style number*/
    FFWREC.SY2CLRID = " "; /* Clear color id*/
    FFWREC.XDMCD = " "; /* Clear dimension code*/
    FFWREC.SY1SSTNM = " "; /* Clear short style name*/
    FFWREC.SY2CLRAB = " "; /* Clear color abrv*/
    FFWREC.XDMABRV = " "; /* Clear dimension abrv*/
    FFWREC.ZRGCD = " "; /* Clear region code*/
    FFWREC.ZRGABRV = " "; /* Clear region abrv*/
    FFWREC.XCTCD = " "; /* Clear country code*/
    FFWREC.XCTABRV = " "; /* Clear country abrv*/
    ;
     /* ------------------------------------------------------------*/
     /* determine default data to be loaded*/
     /* ------------------------------------------------------------*/
    ;
     /* ------------------------------------------------------------*/
     /* product type group*/
     /* ------------------------------------------------------------*/
    ;
    if (FFWREC.YPGCD == " ")
      FFWREC.YPGCD = "01";
      if (FFWREC.YPGCD in YPGTBL.YPGCD)
        FFWREC.YPGABRV = YPGTBL.YPGABRV[sysVar.arrayIndex];
      end
    end
    ;
     /* ------------------------------------------------------------*/
     /* determine if data is to be loaded*/
     /* ------------------------------------------------------------*/
    ;
    if (FFWREC.FFABEGMO != 0
     && FFWREC.FFAENDMO != 0
     && FFWREC.XPCCD != " ")
      FFZREC.FFZBEGMO = FFWREC.FFABEGMO;
      FFZREC.FFZENDMO = FFWREC.FFAENDMO;
      FG0ZW00.FG0ZWSWR = "Y"; /* set data request flag*/
    end
    ;
     /* ------------------------------------------------------------*/
     /* Flow to first time through date validation*/
     /* ------------------------------------------------------------*/
    ;
    goto FG0Z001; /* Converse the main process*/
    ;
    FG0Z001: FG0Z001();
     /* ------------------------------------------------------------*/
     /* flow to converse the main process used to display a map*/
     /* ------------------------------------------------------------*/
    ;
    goto FG0Z001; /* converse the main process*/
    ;
  end // end main
end // end FG0Z

// maint. historical prop shp idx
Function FG0Z001()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/
  ;
   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/
  ;
   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/
  ;
  FG0Z100(); /* Obtain the data to display*/
  ;
  FG0Z200(); /* Format and display the map*/
  ;
  FG0Z300(); /* Check attention identifier*/
  ;
  FG0Z400(); /* Reset fields to normal intensity*/
  ;
  FG0Z500(); /* Verify the entered data*/
  ;
  FG0Z600(); /* Process valid input request*/
  ;
  ;
end // end FG0Z001

// obtain data for display
Function FG0Z100()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/
  ;
  if (FG0ZW00.FG0ZWSWR != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* init working storage*/
   /* ------------------------------------------------------------*/
  ;
  set FG0ZW01 empty; /* init w/s*/
  ;
   /* ------------------------------------------------------------*/
   /* obtain current date using routine xx0xs01, calculate*/
   /* current date minus one month and current date plus five mos*/
   /* ------------------------------------------------------------*/
   /* ------------------------*/
   /* get current date*/
   /* ------------------------*/
  ;
  XX0XS01();
  ;
  FG0ZW00.FG0ZWCWD = XX0XW01.XX0XWDAT; /* current work dt ccyymmdd*/
  FG0ZW00.FG0ZWCRD = XX0XW01.XX0XWCYM; /* current date ccyymm*/
  FG0ZW00.FG0ZWWDT = XX0XW01.XX0XWCYM; /* work date*/
  ;
  if (FG0ZW00.FG0ZWWMM == 1)
    FG0ZW00.FG0ZWWMM = 12;
    if (FG0ZW00.FG0ZWWYY == 00)
      FG0ZW00.FG0ZWWYY = 99;
      FG0ZW00.FG0ZWWCC = FG0ZW00.FG0ZWWCC - 1;
    else
      FG0ZW00.FG0ZWWYY = FG0ZW00.FG0ZWWYY - 1;
    end
  else
    FG0ZW00.FG0ZWWMM = FG0ZW00.FG0ZWWMM - 1;
  end
  FG0ZW00.FG0ZWCD1 = FG0ZW00.FG0ZWWDT; /* current date minus 1 month*/
  ;
  FG0ZW00.FG0ZWWDT = XX0XW01.XX0XWCYM; /* work date*/
  FG0ZW00.FG0ZWWMM = FG0ZW00.FG0ZWWMM + 5;
  if (FG0ZW00.FG0ZWWMM > 12)
    FG0ZW00.FG0ZWWMM = FG0ZW00.FG0ZWWMM - 12;
    if (FG0ZW00.FG0ZWWYY == 99)
      FG0ZW00.FG0ZWWCC = FG0ZW00.FG0ZWWCC + 1;
      FG0ZW00.FG0ZWWYY = 0;
    else
      FG0ZW00.FG0ZWWYY = FG0ZW00.FG0ZWWYY + 1;
    end
  end
  FG0ZW00.FG0ZWCD5 = FG0ZW00.FG0ZWWDT; /* current date plus 5 months*/
  ;
   /* ------------------------------------------------------------*/
   /* calculate "user created" heading date range (12 months)*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWWBD = FFWREC.FFABEGMO; /* user requested begin range*/
  FG0ZW00.FG0ZWWED = FFWREC.FFABEGMO; /* begin range + 11 months*/
  ;
  FG0ZW00.FG0ZWEMM = FG0ZW00.FG0ZWEMM + 11;
  if (FG0ZW00.FG0ZWEMM > 12)
    FG0ZW00.FG0ZWEMM = FG0ZW00.FG0ZWEMM - 12;
    if (FG0ZW00.FG0ZWEYY == 99)
      FG0ZW00.FG0ZWECC = FG0ZW00.FG0ZWECC + 1;
      FG0ZW00.FG0ZWEYY = 0;
    else
      FG0ZW00.FG0ZWEYY = FG0ZW00.FG0ZWEYY + 1;
    end
  end
  FG0ZW01.FG0ZWHBD[4] = FG0ZW00.FG0ZWWBD; /* user hdg beg date*/
  FG0ZW01.FG0ZWHED[4] = FG0ZW00.FG0ZWWED; /* user hdg end date*/
  ;
   /* ------------------------------------------------------------*/
   /* calculate "historical" heading date ranges (12 months)*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWIDX = 1; /* init index*/
  while (FG0ZW00.FG0ZWIDX <= 3) /* calculate historical date ranges*/
    ;
    if (FG0ZW00.FG0ZWBYY == 00)
      FG0ZW00.FG0ZWBYY = 99;
      FG0ZW00.FG0ZWBCC = FG0ZW00.FG0ZWBCC - 1;
    else
      FG0ZW00.FG0ZWBYY = FG0ZW00.FG0ZWBYY - 1;
    end
    FG0ZW01.FG0ZWHBD[FG0ZWIDX] = FG0ZW00.FG0ZWWBD; /* prior beg date*/
    ;
    if (FG0ZW00.FG0ZWEYY == 00)
      FG0ZW00.FG0ZWEYY = 99;
      FG0ZW00.FG0ZWECC = FG0ZW00.FG0ZWECC - 1;
    else
      FG0ZW00.FG0ZWEYY = FG0ZW00.FG0ZWEYY - 1;
    end
    FG0ZW01.FG0ZWHED[FG0ZWIDX] = FG0ZW00.FG0ZWWED; /* prior end date*/
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* calculate historical date ranges*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWWBD = FFWREC.FFABEGMO; /* user req begin range*/
  FG0ZW00.FG0ZWWED = FFWREC.FFAENDMO; /* user req end range*/
  ;
  FG0ZW00.FG0ZWIDX = 1; /* init index*/
  while (FG0ZW00.FG0ZWIDX <= 3) /* calculate historical date ranges*/
    ;
    if (FG0ZW00.FG0ZWBYY == 00)
      FG0ZW00.FG0ZWBYY = 99;
      FG0ZW00.FG0ZWBCC = FG0ZW00.FG0ZWBCC - 1;
    else
      FG0ZW00.FG0ZWBYY = FG0ZW00.FG0ZWBYY - 1;
    end
    FG0ZW01.FG0ZWPBD[FG0ZWIDX] = FG0ZW00.FG0ZWWBD; /* prior beg date*/
    ;
    if (FG0ZW00.FG0ZWEYY == 00)
      FG0ZW00.FG0ZWEYY = 99;
      FG0ZW00.FG0ZWECC = FG0ZW00.FG0ZWECC - 1;
    else
      FG0ZW00.FG0ZWEYY = FG0ZW00.FG0ZWEYY - 1;
    end
    FG0ZW01.FG0ZWPED[FG0ZWIDX] = FG0ZW00.FG0ZWWED; /* prior end date*/
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* save user requested date range calculated by xsffs30*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWWBD = FFZREC.FFZBEGMO; /* save begin range*/
  FG0ZW00.FG0ZWWED = FFZREC.FFZENDMO; /* save end range*/
  ;
   /* ------------------------------------------------------------*/
   /* obtain invoice date from dfcast.vffctl01. this is used*/
   /* when gathering historical sales information.*/
   /* ------------------------------------------------------------*/
  ;
  FG0Z110(); /* get invoice cutoff date*/
  FG0ZW00.FG0ZWICD = FF1REC.FF1IVCDT;
  ;
   /* ------------------------------------------------------------*/
   /* obtain monthly headings and quantities for historical*/
   /* data 1 year ago*/
   /* ------------------------------------------------------------*/
  ;
  FG0Z120();
  ;
   /* ------------------------------------------------------------*/
   /* obtain monthly headings and quantities for historical*/
   /* data 2 year ago*/
   /* ------------------------------------------------------------*/
  ;
  FG0Z130();
  ;
   /* ------------------------------------------------------------*/
   /* obtain monthly headings and quantities for historical*/
   /* data 3 year ago*/
   /* ------------------------------------------------------------*/
  ;
  FG0Z140();
  ;
   /* ------------------------------------------------------------*/
   /* obtain monthly headings and qtys for user created data*/
   /* ------------------------------------------------------------*/
  ;
  FG0Z150();
  ;
   /* ------------------------------------------------------------*/
   /* reload user requested dates*/
   /* ------------------------------------------------------------*/
  ;
  FFZREC.FFZBEGMO = FG0ZW00.FG0ZWWBD;
  FFZREC.FFZENDMO = FG0ZW00.FG0ZWWED;
  ;
   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW01.CACHGFL = "Y"; /* set data loaded flag*/
  FG0ZW00.FG0ZWSWR = "N"; /* Request for data switch*/
  ;
end // end FG0Z100

// Select dfcast.vffctl01 row
Function FG0Z110()
   /* ------------------------------------------------------------*/
   /* obtain next work sheet number dfcast.vffctl01*/
   /* ------------------------------------------------------------*/
  ;
  set FF1REC empty; /* Clear row storage area*/
  ;
  try
    call "IO3660" ("S ", SQLCA, FF1REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select set*/
  ;
   /* ------------------------------------------------------------*/
   /* If successful return to the previous process.*/
   /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      ;
      return;
      ;
    end
  end
  ;
   /* ------------------------------------------------------------*/
   /* indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/
  ;
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL Commarea*/
  ;
  FFWREC.XXXMODID = "110"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(47); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    converseLib.validationFailed(1); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT DFCAST.VFFCTL01 ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FFWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFCAST  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VFFCTL01"; /* Table view name*/
  TA1REC.TA1TBLKE = FF1REC.FF1GROUP; /* Table key*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
end // end FG0Z110

// load historical data 1 yr ago
Function FG0Z120()
   /* ------------------------------------------------------------*/
   /* obtain monthly headings for historical data 1 yr ago*/
   /* ------------------------------------------------------------*/
  ;
  set FFZREC empty;
  ;
  FFZREC.FFZBYYMM = FG0ZW01.FG0ZWHBY[1];
  FFZREC.FFZEYYMM = FG0ZW01.FG0ZWHEY[1];
  ;
  FG0ZS01(); /* get months within range*/
  ;
  FG0ZW00.FG0ZWIDX = 1; /* init index*/
  while (FG0ZW00.FG0ZWIDX <= FFZREC.FFZNUMMO) /* process all months in range*/
    ;
    FG0ZW01.FG0ZWHD1[FG0ZWIDX] = FFZREC.FFZINDMO[FG0ZWIDX];
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
  end
  ;
   /* ------------------------------------------------------------*/
   /* obtain historical actual/forecast sales 1 yr ago*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW00.FG0ZWH1L = "N"; /* init load flag*/
  FG0ZW00.FG0ZWH1E = "N"; /* init error flag*/
  ;
  FG0ZW00.FG0ZWUTQ = 0; /* init total quantity*/
  FG0ZW00.FG0ZWIDX = 1; /* init index*/
  ;
  while (FG0ZW00.FG0ZWH1L != "Y" /* data load flag*/
   && FG0ZW00.FG0ZWH1E != "Y" /* data error flag*/
   && FG0ZW00.FG0ZWIDX <= FFZREC.FFZNUMMO
   && FFZREC.FFZINDMO[FG0ZWIDX] <= FG0ZW01.FG0ZWPED[1])
    ;
    if (FFZREC.FFZINDMO[FG0ZWIDX] < FG0ZW00.FG0ZWCD1)
      FG0ZS06(); /* get actuals*/
      ;
      FG0ZW01.FG0ZWUQ1[FG0ZWIDX] = FG0ZW00.FG0ZWUPS;
      FFXREC.FFXNUMB = FG0ZW00.FG0ZWUPS;
      XSFFS10();
      FG0ZW01.FG0ZWQT1[FG0ZWIDX] = FFXREC.FFXCHAR;
      ;
      FG0ZW00.FG0ZWUTQ = FG0ZW00.FG0ZWUPS + FG0ZW00.FG0ZWUTQ;
      ;
    else
      if (FFZREC.FFZINDMO[FG0ZWIDX] == FG0ZW00.FG0ZWCD1
       && FG0ZW00.FG0ZWCYM == FG0ZW00.FG0ZWIYM
       && FG0ZW00.FG0ZWCWD >= FG0ZW00.FG0ZWICD)
        ;
        FG0ZS06(); /* get actuals*/
        ;
        FG0ZW01.FG0ZWUQ1[FG0ZWIDX] = FG0ZW00.FG0ZWUPS;
        FFXREC.FFXNUMB = FG0ZW00.FG0ZWUPS;
        XSFFS10();
        FG0ZW01.FG0ZWQT1[FG0ZWIDX] = FFXREC.FFXCHAR;
        ;
        FG0ZW00.FG0ZWUTQ = FG0ZW00.FG0ZWUPS + FG0ZW00.FG0ZWUTQ;
        ;
      else
        ;
        set FFCREC empty;
        FFCREC.FFABEGMO = FFZREC.FFZINDMO[FG0ZWIDX];
        FFCREC.FFAENDMO = FG0ZW01.FG0ZWPED[1];
        ;
        FG0ZS02(); /* select dfcast.vrcat001*/
        ;
        if (SQLCA.VAGen_SQLCODE == 100
         && FFZREC.FFZINDMO[FG0ZWIDX] > FG0ZW00.FG0ZWCD5)
          FG0ZW00.FG0ZWH1E = "Y";
        else
          if (SQLCA.VAGen_SQLCODE == 0)
            ;
            FG0ZW00.FG0ZWSIX = FG0ZW00.FG0ZWIDX;
            FG0ZW00.FG0ZWUPS = 0;
            while (FG0ZW00.FG0ZWIDX <= FFZREC.FFZNUMMO
             && FFZREC.FFZINDMO[FG0ZWIDX] <= FG0ZW01.FG0ZWPED[1])
              ;
              FG0ZS07(); /* select dfcast.vmcat001*/
              ;
              FG0ZW00.FG0ZWNM1 = FF7REC.FF6SLFCT * FF7REC.FF7FURAT / 100;
              FG0ZW00.FG0ZWNM1 = FF7REC.FF6SLFCT - FG0ZW00.FG0ZWNM1;
              FG0ZW00.FG0ZWUPS = FG0ZW00.FG0ZWNM1 + FG0ZW00.FG0ZWUPS;
              ;
              FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
              ;
            end
            FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWSIX;
            ;
            set FFDREC empty;
            ;
            FFFREC.FFFBEGMO = FFCREC.FFABEGMO;
            FFFREC.FFFENDMO = FFCREC.FFAENDMO;
            ;
            FFDREC.FFABEGMO = FFCREC.FFABEGMO;
            FFDREC.FFAENDMO = FFCREC.FFAENDMO;
            ;
            FG0ZS03(); /* select dfcast.vrcmth01*/
            FG0ZS04(); /* fetch dfcast.vrcmth01*/
            while (SQLCA.VAGen_SQLCODE == 0)
              ;
              FG0ZW01.FG0ZWUQ1[FG0ZWIDX] = mathLib.round(FG0ZW00.FG0ZWUPS * FFDREC.FFDPSPCT / 100, -mathLib.decimals(FG0ZW01.FG0ZWUQ1[FG0ZWIDX]));
              ;
              FFXREC.FFXNUMB = FG0ZW01.FG0ZWUQ1[FG0ZWIDX];
              XSFFS10();
              FG0ZW01.FG0ZWQT1[FG0ZWIDX] = FFXREC.FFXCHAR;
              ;
              FG0ZW00.FG0ZWUTQ = FG0ZW01.FG0ZWUQ1[FG0ZWIDX] + FG0ZW00.FG0ZWUTQ;
              ;
              FG0ZS04();
              ;
              FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
              ;
            end
            FG0ZS05(); /* close dfcast.vrcmth01*/
