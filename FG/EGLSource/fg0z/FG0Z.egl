   /* ------------------------------------------------------------*/
  ;
  FFWREC.XPCCD = FG0ZM001.XPCCD; /* product category code*/
  FFWREC.XPCABRV = FG0ZM001.XPCABRV; /* product category abrv desc*/
  ;
  FFWREC.YPGCD = FG0ZM001.YPGCD; /* product type grp code*/
  FFWREC.YPGABRV = FG0ZM001.YPGABRV; /* product type grp abrv desc*/
  ;
  FFWREC.FFABEGMO = FFZREC.FFZBEGMO;
  FFWREC.FFAENDMO = FFZREC.FFZENDMO;
  ;
  if (FG0ZW00.FG0ZWSWR == "Y") /* KEY DATA MODIFIED*/
    return;
  end
  ;
   /* ------------------------------------------------------------*/
   /* if user data changed or historical data selected*/
   /* update/add dfcast.vrcat001 dfcast.vrcmth01*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is enter
   && FG0ZW00.FG0ZWSLF == "Y") /* data selected for update/add*/
    ;
    XX0XS01(); /* get current date and time*/
    ;
    set FFCREC empty;
    ;
    FFCREC.FFABEGMO = FFWREC.FFABEGMO;
    FFCREC.FFAENDMO = FFWREC.FFAENDMO;
    ;
    FG0ZS02(); /* select dfcast.vrcat001 row*/
    ;
    if (SQLCA.VAGen_SQLCODE == 0)
      ;
      FG0Z610(); /* update dfcast.vrcat001 row*/
      ;
      set FFDREC empty;
      ;
      FFFREC.FFFBEGMO = FFWREC.FFABEGMO;
      FFFREC.FFFENDMO = FFWREC.FFAENDMO;
      ;
      FFDREC.FFABEGMO = FFWREC.FFABEGMO;
      FFDREC.FFAENDMO = FFWREC.FFAENDMO;
      ;
      FG0ZS03(); /* select 's1' dfcast.vrcmth01*/
      FG0ZS04(); /* fetch 'n1' dfcast.vrcmth01*/
      ;
      FG0ZW00.FG0ZWIDX = 1; /* init index*/
      while (SQLCA.VAGen_SQLCODE == 0)
        ;
        FG0Z620(); /* update dfcast.vrcmth01*/
        ;
        FG0ZS04(); /* fetch 'n1' dfcast.vrcmth01*/
        ;
        FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
        ;
      end
    else
      ;
      FG0Z630(); /* add dfcast.vrcat001 row*/
      ;
      set FFDREC empty;
      ;
      FFDREC.FFABEGMO = FFWREC.FFABEGMO;
      FFDREC.FFAENDMO = FFWREC.FFAENDMO;
      FFDREC.XPCCD = FFWREC.XPCCD;
      FFDREC.YPGCD = FFWREC.YPGCD;
      ;
      FG0ZW00.FG0ZWIDX = 1; /* init index*/
      while (FFDREC.FF4FCTMO < FFDREC.FFAENDMO)
        ;
        FG0Z640(); /* add dfcast.vrcmth01 row*/
        ;
        FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
        ;
      end
    end
    ;
    VGVar.handleSysLibraryErrors = 0;
    sysLib.commit();
    ;
    FFWREC.XXXMSGNO = 33;
    FFWREC.XXXMODID = "600";
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* if pf13 transfer to application fg0x*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf13)
    ;
    COMMAREA.CATOAP = "FG0X";
    ;
    XSFFS01(); /* Exit to another application*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* load map work record*/
   /* ------------------------------------------------------------*/
  ;
  FG0ZW01.FG0ZWSEL[1] = FG0ZM001.FG0ZMSEL[1];
  FG0ZW01.FG0ZWSEL[2] = FG0ZM001.FG0ZMSEL[2];
  FG0ZW01.FG0ZWSEL[3] = FG0ZM001.FG0ZMSEL[3];
  FG0ZW01.FG0ZWSEL[4] = FG0ZM001.FG0ZMSEL[4];
  ;
  FG0ZW00.FG0ZWIDX = 1; /* init index*/
  while (FG0ZW00.FG0ZWIDX <= 12
   && FG0ZW01.FG0ZWHD4[FG0ZWIDX] <= FFWREC.FFAENDMO)
    ;
    FG0ZW01.FG0ZWIX4[FG0ZWIDX] = FG0ZM001.FG0ZMIX4[FG0ZWIDX];
    ;
    FG0ZW00.FG0ZWIDX = FG0ZW00.FG0ZWIDX + 1;
    ;
  end
  ;
  ;
end // end FG0Z600

