// BUILD NEW VDATRN01 RECORD
Function DH0PP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DH0PM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DH0PM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DH0PW01.DA-CCAT-KEY-TXT = " ";
  DH0PW01.DH0PW-KEY-XOTCD = DH0PM002.XOTCD;
  DH0PW01.DH0PW-KEY-XODBENDT = DH0PM002.XODBENDT;
  VDATRN01.DA-CCAT-KEY-TXT = DH0PW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DH0PW01.XOTCD = DH0PM002.XOTCD;
  DH0PW01.XODBENDT = DH0PM002.XODBENDT;
  DH0PW01.XODEENDT = DH0PM002.XODEENDT;
  DH0PW01.XPTCD = DH0PM002.XPTCD;
  DH0PW01.XTDCD = DH0PM002.XTDCD;
  DH0PW01.XODTDBKP = DH0PM002.XODTDBKP;
  DH0PW01.XODTDFCP = DH0PM002.XODTDFCP;
  DH0PW01.XAACD = DH0PM002.XAACD;
  DH0PW01.XODAABKP = DH0PM002.XODAABKP;
