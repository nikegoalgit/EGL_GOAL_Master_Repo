// SET MAP 1 FIELD ATTRIBUTES
Function DH0PP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DH0PM001.DAW-PAGE-LIT = "PAGE";
    set DH0PM001.DAW-CURR-PAGE initialAttributes;
    DH0PM001.DAW-OF-LIT = "OF";
    set DH0PM001.DAW-LAST-PAGE initialAttributes;
    DH0PM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DH0PM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DH0PM001.DAW-PAGE-LIT = " ";
    set DH0PM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DH0PM001.DAW-OF-LIT = " ";
    set DH0PM001.DAW-LAST-PAGE skip, invisible;
    DH0PM001.DAW-PF7-LIT = " ";
    DH0PM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DH0PM001.XODEENDT[sysVar.arrayIndex] == " "
       && DH0PM001.XODBENDT[sysVar.arrayIndex] == " ")
        set DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
      else
        set DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DH0PP25 **************************/
  /* **************/
end // end DH0PP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DH0PP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DH0PM002.XOTCD initialAttributes;
    set DH0PM002.XODBENDT initialAttributes;
    set DH0PM002.XODEENDT initialAttributes;
    set DH0PM002.XPTCD initialAttributes;
    set DH0PM002.XTDCD initialAttributes;
    set DH0PM002.XODTDBKP initialAttributes;
    set DH0PM002.XODTDFCP initialAttributes;
    set DH0PM002.XAACD initialAttributes;
    set DH0PM002.XODAABKP initialAttributes;
    set DH0PM002.XODAAFCP initialAttributes;
    set DH0PM002.XCPCD initialAttributes;
    set DH0PM002.XODMXDSC initialAttributes;
    set DH0PM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DH0PM002.XOTCD skip;
    set DH0PM002.XODBENDT skip;
    set DH0PM002.XODEENDT skip;
    set DH0PM002.XPTCD skip;
    set DH0PM002.XTDCD skip;
    set DH0PM002.XODTDBKP skip;
    set DH0PM002.XODTDFCP skip;
    set DH0PM002.XAACD skip;
    set DH0PM002.XODAABKP skip;
    set DH0PM002.XODAAFCP skip;
    set DH0PM002.XCPCD skip;
    set DH0PM002.XODMXDSC skip;
    set DH0PM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DH0PM002.XOTCD skip;
    set DH0PM002.XODBENDT skip;
    set DH0PM002.XODEENDT initialAttributes;
    set DH0PM002.XPTCD initialAttributes;
    set DH0PM002.XTDCD initialAttributes;
    set DH0PM002.XODTDBKP initialAttributes;
    set DH0PM002.XODTDFCP initialAttributes;
    set DH0PM002.XAACD initialAttributes;
    set DH0PM002.XODAABKP initialAttributes;
    set DH0PM002.XODAAFCP initialAttributes;
    set DH0PM002.XCPCD initialAttributes;
    set DH0PM002.XODMXDSC initialAttributes;
    set DH0PM002.XXXSTAT initialAttributes;
  end

  DH0PM002.XOTABRV = " ";
  DH0PM002.XPTABRV = " ";
  DH0PM002.XCPABRV = " ";

  set DH0PM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DH0PM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0PM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DH0PM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DH0PM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DH0PM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DH0PM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DH0PM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DH0PP25 **************************/
  /* **************/
end // end DH0PP25-SET-ATTR-2

