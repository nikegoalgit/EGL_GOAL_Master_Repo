// EDIT MAP PAGE NUMBER
Function DH0PP61-EDIT-PAGE()
  /* ***********************************************************/
  /* Edit map current page number*/
  /* ***********************************************************/

  if (DH0PM001.DAW-CURR-PAGE == DAWREC.DAW-CURR-PAGE)
    return;
  end

  /* if select codes entered and page number changed*/
  /* then reset page number*/

  if (DAWREC.DAW-ADD-ROW-FL == "Y"
   || DAWREC.DAW-DELETE-ROW-FL == "Y"
   || DAWREC.DAW-SELECT-ROW-FL == "Y"
   || DAWREC.DAW-COPY-ROW-FL == "Y")
    set DH0PM001.DAW-CURR-PAGE bold;
    DH0PM001.VAGen_EZEMSG = "CANNOT MAINTAIN OR ADD A ROW AND CHANGE PAGE AT THE SAME TIME";
    DH0PM001.DAW-CURR-PAGE = DAWREC.DAW-CURR-PAGE;
    DH0PM001.DAW-INFO-MESSAGE = "PAGE NUMBER HAS BEEN RESET - HIT ENTER OR PF11 TO CONTINUE";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
  end

  if (DH0PM001.DAW-CURR-PAGE > DH0PM001.DAW-LAST-PAGE)
    set DH0PM001.DAW-CURR-PAGE cursor, bold;
    converseLib.validationFailed(006); /* page number entered is greater than*/
    DAWREC.DAW-EDIT-ERROR-FL = "Y"; /* last page available*/
  end

  if (DH0PM001.DAW-CURR-PAGE == 0)
    set DH0PM001.DAW-CURR-PAGE cursor, bold;
    converseLib.validationFailed(007); /* page number entered is zero - page*/
    DAWREC.DAW-EDIT-ERROR-FL = "Y"; /* does not exist*/
  end

  /* **************/
  /* ****************** END DH0PP62 ****************************/
  /* **************/
end // end DH0PP61-EDIT-PAGE

// EDIT MAP PREFIX OPTION
Function DH0PP61-EDIT-SEL()
  /* ***********************************************************/
  /* Edit map select codes - user may enter multiple selects*/
  /* on a single page. Each will be processed as long as there*/
  /* are no errors.*/
  /* ***********************************************************/

  sysVar.arrayIndex = 14;

  while (sysVar.arrayIndex != 0)

    if (DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] == " "
     || DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
    ) /* CONTINUE*/
    else
      if (DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] == "S")
        DAWREC.DAW-SELECT-ROW-FL = "Y";
      else
        if (DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] == "C")
          DAWREC.DAW-COPY-ROW-FL = "Y";
        else
          if (DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] == "D")
            if (DH0PM001.DAW-STATUS-DESC[sysVar.arrayIndex] == "EDIT    "
             || DH0PM001.DAW-STATUS-DESC[sysVar.arrayIndex] == "PENDING "
             || DH0PM001.DAW-STATUS-DESC[sysVar.arrayIndex] == "REJECTED")
              DAWREC.DAW-DELETE-ROW-FL = "Y";
            else
              converseLib.validationFailed(009); /* only edit, wait or rejected*/
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
                                   /* status rows may be deleted*/
              set DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, bold;
            end
          else
            if (DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] == "A")
              converseLib.validationFailed(010); /* invalid select - use PF11 to add a*/
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
                                   /* new code instead of 'A'*/
              set DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, bold;
            else
              converseLib.validationFailed(011); /* invalid select - use 's' to select*/
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
                                   /* 'd' to delete or 'c' copy*/
              set DH0PM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, bold;
            end
          end
        end
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DH0PP61 ****************************/
  /* **************/
end // end DH0PP61-EDIT-SEL

Function DH0PP62-EDIT-DATES()
  /* ********************************************************************/
  /* Edit DATES*/
  /* ********************************************************************/

  if (DH0PM002.XODEENDT == 0)
    converseLib.validationFailed(339);
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    set DH0PM002.XODEENDT cursor, bold;
  else
    set TA2REC empty;
    TA2REC.TA2GRGG = DH0PM002.XODEENDT;
    TA2REC.TA2DTOPT = "GREGGREG";
    TA2REC.TA2GRTYP = "G";
    call "TA0040" (TA2REC) {isNoRefresh = yes, isExternal = yes};
    if (TA2REC.TA2RTNCD != 0)
      converseLib.validationFailed(015); /* Date is not a valid date*/
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DH0PM002.XODEENDT cursor, bold;
    end
  end


  if (DH0PM002.XODBENDT == 0)
    converseLib.validationFailed(338);
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    set DH0PM002.XODBENDT cursor, bold;
  else
    set TA2REC empty;
    TA2REC.TA2GRGG = DH0PM002.XODBENDT;
    TA2REC.TA2DTOPT = "GREGGREG";
    TA2REC.TA2GRTYP = "G";
    call "TA0040" (TA2REC) {isNoRefresh = yes, isExternal = yes};
    if (TA2REC.TA2RTNCD != 0)
      converseLib.validationFailed(015); /* Date is not a valid date*/
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DH0PM002.XODBENDT cursor, bold;
    end
  end

  if (DH0PM002.XODBENDT > DH0PM002.XODEENDT)
    converseLib.validationFailed(337); /* Date is not a valid date*/
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    set DH0PM002.XODBENDT cursor, bold;
  end

  /* **************/
  /* ****************** END DH0PP62 **************************************/
  /* **************/
end // end DH0PP62-EDIT-DATES

