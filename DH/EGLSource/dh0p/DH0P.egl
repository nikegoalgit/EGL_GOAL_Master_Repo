//*** PROGRAM=DH0P ****
// NIKE TABLE HANDLER - TABLE MAINTENANCE - ORDER TYPE DISCOUNT
// 
// OVERVIEW: This application is used to maintain the ORDER
//           TYPE DISCOUNT table
// 
// DB2 TABLE: DCORP.VXOD0001
// CSP TABLE: XODTBL
// 
// DATE CREATED: 05/26/97
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   06/18/97  lraine      fix key edit
//   01/09/98  Echan       fix key edit to avoid dulplicate row
//   05/05/99  Echan       ADD NOHILITE IN DH0P25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DH0P70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DH0P70-PROC-MAP-2
//                         DISPLAY XTDCD, XAACD, XCPCD, STATUS
//                         IN DH0PM001
// *********************
Program DH0P type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DH0PW01 DH0PW01; // record
  DH0PW02 DH0PW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XODTBL {deleteAfterUse = yes}; // table
  use XTDTBL {deleteAfterUse = yes}; // table
  use XOTTBL {deleteAfterUse = yes}; // table
  use XAATBL {deleteAfterUse = yes}; // table
  use XPTTBL {deleteAfterUse = yes}; // table
  use XCPTBL {deleteAfterUse = yes}; // table
  use DH0PM.DH0PM001,  // forms
      DH0PM.DH0PM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DH0PP00-DRIVER: DH0PP00-DRIVER();
    exit program;
  end // end main
end // end DH0P

// DRIVER FOR APPLICATION
Function DH0PP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH0PP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH0PP20-BUILD-MAP2(); /* Build the second map*/
  else
    DH0PP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH0PP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH0PP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH0PP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH0PP00 *******************************/
  /* **************/
end // end DH0PP00-DRIVER

// INITIALIZE THE APPLICATION
Function DH0PP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH0PW01 empty; /* Working storage record*/
  set DH0PW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH0PM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0PM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0PM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH0PM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0PM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0PM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH0PM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH0PP10 ******************************/
  /* **************/
end // end DH0PP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH0PP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH0PP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH0PM001.DAW-CURR-PAGE)
    DH0PS-TS-READ-PAGE(); /* read temp storage page*/
    DH0PP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH0PP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0PP20 **********************************/
  /* **************/
end // end DH0PP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DH0PP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DH0PM002.DA-CHNG-USER-ID = sysVar.userID;
  DH0PM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DH0PW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0PM002.DA-PLOG-TXT1 = " ";
    DH0PM002.DA-PLOG-TXT2 = " ";
    DH0PM002.XOTCD = " ";
    DH0PM002.XODBENDT = 0;
    DH0PM002.XODEENDT = 0;
    DH0PM002.XPTCD = " ";
    DH0PM002.XTDCD = " ";
    DH0PM002.XODTDBKP = 0;
    DH0PM002.XODTDFCP = 0;
    DH0PM002.XAACD = " ";
    DH0PM002.XODAABKP = 0;
    DH0PM002.XODAAFCP = 0;
    DH0PM002.XCPCD = " ";
    DH0PM002.XODMXDSC = 0;
    DH0PM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DH0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DH0PW01.XODGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DH0PW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DH0PW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DH0PM002.XOTCD = DH0PW01.XOTCD;
      DH0PM002.XODBENDT = DH0PW01.XODBENDT; /* move to map*/
      DH0PM002.XODEENDT = DH0PW01.XODEENDT; /* move to the map*/
      DH0PM002.XPTCD = DH0PW01.XPTCD;
      DH0PM002.XTDCD = DH0PW01.XTDCD;
      DH0PM002.XODTDBKP = DH0PW01.XODTDBKP;
      DH0PM002.XODTDFCP = DH0PW01.XODTDFCP;
      DH0PM002.XAACD = DH0PW01.XAACD;
      DH0PM002.XODAABKP = DH0PW01.XODAABKP;
      DH0PM002.XODAAFCP = DH0PW01.XODAAFCP;
      DH0PM002.XCPCD = DH0PW01.XCPCD;
      DH0PM002.XODMXDSC = DH0PW01.XODMXDSC;
      DH0PM002.XXXSTAT = DH0PW01.XXXSTAT;
      DH0PM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DH0PM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DH0PM002.XOTCD = DH0PW02.XOTCD[DAW-ROW-NBR];
        DH0PM002.XODBENDT = DH0PW02.XODBENDT[DAW-ROW-NBR];
        DH0PM002.XODEENDT = DH0PW02.XODEENDT[DAW-ROW-NBR];
        DH0PM002.XPTCD = DH0PW02.XPTCD[DAW-ROW-NBR];

        if (DH0PM002.XOTCD in XODTBL.XOTCD
         && DH0PM002.XODEENDT == XODTBL.XODEENDT[sysVar.arrayIndex]
         && DH0PM002.XODBENDT == XODTBL.XODBENDT[sysVar.arrayIndex]
         && DH0PM002.XPTCD == XODTBL.XPTCD[sysVar.arrayIndex])
          DH0PW01.DH0PW-FND-FL = "Y";
        else
          while (DH0PW01.DH0PW-FND-FL != "Y"
           && XODTBL.XOTCD[sysVar.arrayIndex] != "99")
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
            if (DH0PM002.XODEENDT == XODTBL.XODEENDT[sysVar.arrayIndex]
             && DH0PM002.XODBENDT == XODTBL.XODBENDT[sysVar.arrayIndex]
             && DH0PM002.XPTCD == XODTBL.XPTCD[sysVar.arrayIndex])
              DH0PW01.DH0PW-FND-FL = "Y";
            end
          end
        end
        if (DH0PW01.DH0PW-FND-FL == "Y")
          DH0PW01.XOTCD = XODTBL.XOTCD[sysVar.arrayIndex];
          DH0PW01.XODBENDT = XODTBL.XODBENDT[sysVar.arrayIndex];
          DH0PW01.XODEENDT = XODTBL.XODEENDT[sysVar.arrayIndex];
          DH0PW01.XPTCD = XODTBL.XPTCD[sysVar.arrayIndex];

          DH0PW01.XTDCD = XODTBL.XTDCD[sysVar.arrayIndex];
          DH0PW01.XODTDBKP = XODTBL.XODTDBKP[sysVar.arrayIndex];
          DH0PW01.XODTDFCP = XODTBL.XODTDFCP[sysVar.arrayIndex];
          DH0PW01.XAACD = XODTBL.XAACD[sysVar.arrayIndex];
          DH0PW01.XODAABKP = XODTBL.XODAABKP[sysVar.arrayIndex];
          DH0PW01.XODAAFCP = XODTBL.XODAAFCP[sysVar.arrayIndex];
          DH0PW01.XCPCD = XODTBL.XCPCD[sysVar.arrayIndex];
          DH0PW01.XODMXDSC = XODTBL.XODMXDSC[sysVar.arrayIndex];
          DH0PW01.XXXSTAT = XODTBL.XXXSTAT[sysVar.arrayIndex];
          DH0PW01.ZZZSDT = XODTBL.ZZZSDT[sysVar.arrayIndex];
          DH0PW01.ZZZCHGTM = XODTBL.ZZZCHGTM[sysVar.arrayIndex];
          DH0PW01.ZZZCHGDT = XODTBL.ZZZCHGDT[sysVar.arrayIndex];
          DH0PW01.ZZZCHGCT = XODTBL.ZZZCHGCT[sysVar.arrayIndex];

          DH0PM002.XTDCD = XODTBL.XTDCD[sysVar.arrayIndex];
          DH0PM002.XODTDBKP = XODTBL.XODTDBKP[sysVar.arrayIndex];
          DH0PM002.XODTDFCP = XODTBL.XODTDFCP[sysVar.arrayIndex];
          DH0PM002.XAACD = XODTBL.XAACD[sysVar.arrayIndex];
          DH0PM002.XODAABKP = XODTBL.XODAABKP[sysVar.arrayIndex];
          DH0PM002.XODAAFCP = XODTBL.XODAAFCP[sysVar.arrayIndex];
          DH0PM002.XCPCD = XODTBL.XCPCD[sysVar.arrayIndex];
          DH0PM002.XODMXDSC = XODTBL.XODMXDSC[sysVar.arrayIndex];
          DH0PM002.XXXSTAT = XODTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DH0PW01.XODGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DH0PW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DH0PW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DH0PM002.XOTCD = DH0PW01.XOTCD;
    DH0PM002.XODBENDT = DH0PW01.XODBENDT;
    DH0PM002.XODEENDT = DH0PW01.XODEENDT; /* move to the map*/
    DH0PM002.XPTCD = DH0PW01.XPTCD;
    DH0PM002.XTDCD = DH0PW01.XTDCD;
    DH0PM002.XODTDBKP = DH0PW01.XODTDBKP;
    DH0PM002.XODTDFCP = DH0PW01.XODTDFCP;
    DH0PM002.XAACD = DH0PW01.XAACD;
    DH0PM002.XODAABKP = DH0PW01.XODAABKP;
    DH0PM002.XCPCD = DH0PW01.XCPCD;
    DH0PM002.XODMXDSC = DH0PW01.XODMXDSC;
    DH0PM002.XXXSTAT = DH0PW01.XXXSTAT;
    DH0PM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DH0PM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DH0PM002.XXXSTAT == "A")
    DH0PM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DH0PM002.XXXSTAT == "I")
    DH0PM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DH0PM002.XXXSTAT == " ")
    DH0PM002.DAW-TBL-STATUS-DESC = " ";
  end

  DH0PP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0PP20 **********************************/
  /* **************/
end // end DH0PP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DH0PP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DH0PW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0PP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0PS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XODTBL.XOTCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0PP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0PS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DH0PS-TS-ADD-PAGE();
    end

    DH0PM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DH0PM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DH0PP22 **********************************/
  /* **************/
end // end DH0PP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH0PP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DH0PW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DH0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DH0PW01.XODGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DH0PW02.XOTCD[DAW-ROW-NBR] = DH0PW01.XOTCD;
  DH0PW02.XODBENDT[DAW-ROW-NBR] = DH0PW01.XODBENDT;
  DH0PW02.XODEENDT[DAW-ROW-NBR] = DH0PW01.XODEENDT;
  DH0PW02.XPTCD[DAW-ROW-NBR] = DH0PW01.XPTCD;
  DH0PW02.XTDCD[DAW-ROW-NBR] = DH0PW01.XTDCD;
  DH0PW02.XAACD[DAW-ROW-NBR] = DH0PW01.XAACD;
  DH0PW02.XCPCD[DAW-ROW-NBR] = DH0PW01.XCPCD;
  DH0PW02.XXXSTAT[DAW-ROW-NBR] = DH0PW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DH0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DH0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DH0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DH0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DH0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DH0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DH0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DH0PW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DH0PW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DH0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DH0PP221 ******************************/
  /* ***************/
end // end DH0PP23-DB2-TO-TSQ

