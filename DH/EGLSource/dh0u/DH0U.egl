// BUILD NEW VDATRN01 RECORD
Function DH0UP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DH0UM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DH0UM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DH0UW01.DA-CCAT-KEY-TXT = " ";
  DH0UW01.DH0UW-KEY-XOCCD = DH0UM002.XOCCD;
  VDATRN01.DA-CCAT-KEY-TXT = DH0UW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DH0UW01.XOCCD = DH0UM002.XOCCD;
  DH0UW01.XOCABRV = DH0UM002.XOCABRV;
  DH0UW01.XOCDESC = DH0UM002.XOCDESC;
  DH0UW01.XDVCD = DH0UM002.XDVCD;
  DH0UW01.XOCFUTRF = DH0UM002.XOCFUTRF;
  DH0UW01.XOCPROPF = DH0UM002.XOCPROPF;
  DH0UW01.XLCCD1 = DH0UM002.XLCCD1;
  DH0UW01.XLCCD2 = DH0UM002.XLCCD2;
  DH0UW01.XPPCD = DH0UM002.XPPCD;
  DH0UW01.XPKCD = DH0UM002.XPKCD;
  DH0UW01.XXXSTAT = DH0UM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 53;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DH0UW01.XOCGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DH0UP72 **********************************/
  /* **************/
end // end DH0UP72-NEW-TRAN

