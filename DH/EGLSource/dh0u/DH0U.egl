// SET MAP 1 FIELD ATTRIBUTES
Function DH0UP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DH0UM001.DAW-PAGE-LIT = "PAGE";
    set DH0UM001.DAW-CURR-PAGE initialAttributes;
    DH0UM001.DAW-OF-LIT = "OF";
    set DH0UM001.DAW-LAST-PAGE initialAttributes;
    DH0UM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DH0UM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DH0UM001.DAW-PAGE-LIT = " ";
    set DH0UM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DH0UM001.DAW-OF-LIT = " ";
    set DH0UM001.DAW-LAST-PAGE skip, invisible;
    DH0UM001.DAW-PF7-LIT = " ";
    DH0UM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DH0UM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DH0UM001.XOCCD[sysVar.arrayIndex] == " ")
      set DH0UM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DH0UM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DH0UP25 **************************/
  /* **************/
end // end DH0UP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DH0UP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DH0UM002.XOCCD initialAttributes;
  else
    set DH0UM002.XOCCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DH0UM002.XOCABRV skip;
    set DH0UM002.XOCDESC skip;
    set DH0UM002.XDVCD skip;
    set DH0UM002.XOCFUTRF skip;
    set DH0UM002.XOCPROPF skip;
    set DH0UM002.XLCCD1 skip;
    set DH0UM002.XLCCD2 skip;
    set DH0UM002.XPPCD skip;
    set DH0UM002.XPKCD skip;
    set DH0UM002.XXXSTAT skip;
  else
    set DH0UM002.XOCABRV initialAttributes;
    set DH0UM002.XOCDESC initialAttributes;
    set DH0UM002.XDVCD initialAttributes;
    set DH0UM002.XOCFUTRF initialAttributes;
    set DH0UM002.XOCPROPF initialAttributes;
    set DH0UM002.XLCCD1 initialAttributes;
    set DH0UM002.XLCCD2 initialAttributes;
    set DH0UM002.XPPCD initialAttributes;
    set DH0UM002.XPKCD initialAttributes;
    set DH0UM002.XXXSTAT initialAttributes;
  end

  DH0UM002.XDVABRV = " ";
  DH0UM002.XPKABRV = " ";
  DH0UM002.XPPABRV = " ";

  set DH0UM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DH0UM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0UM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DH0UM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DH0UM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DH0UM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DH0UM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DH0UM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DH0UP25 **************************/
  /* **************/
end // end DH0UP25-SET-ATTR-2

