// NIKE TABLE HANDLER - TABLE MAINTENANCE - ORDER CLASS
// 
// OVERVIEW: This application is the maintenance application
//           for the ORDER CLASS table
// 
// DB2 TBL: DCORP.VXOC0001
// CSP TBL: XOCTBL
// 
// DATE CREATED: 05/31/97
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   05-03-99  ECHAN       ADD NOHILITE IN DH0UP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DH0UP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DH0UP70-PROC-MAP-2
//                         DISPLAY FUTRF, PROPF, STATUS IN
//                         DH0UM001
// *********************
Program DH0U type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DH0UW01 DH0UW01; // record
  DH0UW02 DH0UW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XOCTBL {deleteAfterUse = yes}; // table
  use XLCTBL {deleteAfterUse = yes}; // table
  use XDVTBL {deleteAfterUse = yes}; // table
  use XPKTBL {deleteAfterUse = yes}; // table
  use XPPTBL {deleteAfterUse = yes}; // table
  use DH0UM.DH0UM001,
      DH0UM.DH0UM002 // form
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DH0UP00-DRIVER: DH0UP00-DRIVER();
    exit program;
  end // end main
end // end DH0U

// DRIVER FOR APPLICATION
Function DH0UP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH0UP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH0UP20-BUILD-MAP2(); /* Build the second map*/
  else
    DH0UP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH0UP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH0UP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH0UP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH0UP00 *******************************/
  /* **************/
end // end DH0UP00-DRIVER

// INITIALIZE THE APPLICATION
Function DH0UP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH0UW01 empty; /* Working storage record*/
  set DH0UW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH0UM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0UM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0UM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH0UM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0UM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0UM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH0UM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH0UP10 ******************************/
  /* **************/
end // end DH0UP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH0UP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH0UP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH0UM001.DAW-CURR-PAGE)
    DH0US-TS-READ-PAGE(); /* read temp storage page*/
    DH0UP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH0UP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0UP20 **********************************/
  /* **************/
end // end DH0UP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DH0UP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DH0UM002.DA-CHNG-USER-ID = sysVar.userID;
  DH0UM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DH0UW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0UM002.DA-PLOG-TXT1 = " ";
    DH0UM002.DA-PLOG-TXT2 = " ";
    DH0UM002.XOCCD = " ";
    DH0UM002.XOCABRV = " ";
    DH0UM002.XOCDESC = " ";
    DH0UM002.XDVCD = " ";
    DH0UM002.XOCFUTRF = " ";
    DH0UM002.XOCPROPF = " ";
    DH0UM002.XLCCD1 = " ";
    DH0UM002.XLCCD2 = " ";
    DH0UM002.XPPCD = " ";
    DH0UM002.XPKCD = " ";
    DH0UM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DH0UW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DH0UW01.XOCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DH0UW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DH0UW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DH0UM002.XOCCD = DH0UW01.XOCCD; /* move to the map*/
      DH0UM002.XOCABRV = DH0UW01.XOCABRV;
      DH0UM002.XOCDESC = DH0UW01.XOCDESC;
      DH0UM002.XDVCD = DH0UW01.XDVCD;
      DH0UM002.XOCFUTRF = DH0UW01.XOCFUTRF;
      DH0UM002.XOCPROPF = DH0UW01.XOCPROPF;
      DH0UM002.XLCCD1 = DH0UW01.XLCCD1;
      DH0UM002.XLCCD2 = DH0UW01.XLCCD2;
      DH0UM002.XPPCD = DH0UW01.XPPCD;
      DH0UM002.XPKCD = DH0UW01.XPKCD;
      DH0UM002.XXXSTAT = DH0UW01.XXXSTAT;
      DH0UM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DH0UM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DH0UM002.XOCCD = DH0UW02.XOCCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DH0UM002.XOCCD in XOCTBL.XOCCD)
          DH0UW01.XOCCD = XOCTBL.XOCCD[sysVar.arrayIndex]; /* save for a*/
          DH0UW01.XOCABRV = XOCTBL.XOCABRV[sysVar.arrayIndex]; /* later compare*/
          DH0UW01.XOCDESC = XOCTBL.XOCDESC[sysVar.arrayIndex];
          DH0UW01.XDVCD = XOCTBL.XDVCD[sysVar.arrayIndex];
          DH0UW01.XOCFUTRF = XOCTBL.XOCFUTRF[sysVar.arrayIndex];
          DH0UW01.XOCPROPF = XOCTBL.XOCPROPF[sysVar.arrayIndex];
          DH0UW01.XLCCD1 = XOCTBL.XLCCD1[sysVar.arrayIndex];
          DH0UW01.XLCCD2 = XOCTBL.XLCCD2[sysVar.arrayIndex];
          DH0UW01.XPPCD = XOCTBL.XPPCD[sysVar.arrayIndex];
          DH0UW01.XPKCD = XOCTBL.XPKCD[sysVar.arrayIndex];
          DH0UW01.XXXSTAT = XOCTBL.XXXSTAT[sysVar.arrayIndex];
          DH0UW01.ZZZSDT = XOCTBL.ZZZSDT[sysVar.arrayIndex];
          DH0UW01.ZZZCHGDT = XOCTBL.ZZZCHGDT[sysVar.arrayIndex];
          DH0UW01.ZZZCHGTM = XOCTBL.ZZZCHGTM[sysVar.arrayIndex];
          DH0UW01.ZZZCHGCT = XOCTBL.ZZZCHGCT[sysVar.arrayIndex];

          DH0UM002.XOCABRV = XOCTBL.XOCABRV[sysVar.arrayIndex]; /* move to map*/
          DH0UM002.XOCDESC = XOCTBL.XOCDESC[sysVar.arrayIndex];
          DH0UM002.XDVCD = XOCTBL.XDVCD[sysVar.arrayIndex];
          DH0UM002.XOCFUTRF = XOCTBL.XOCFUTRF[sysVar.arrayIndex];
          DH0UM002.XOCPROPF = XOCTBL.XOCPROPF[sysVar.arrayIndex];
          DH0UM002.XLCCD1 = XOCTBL.XLCCD1[sysVar.arrayIndex];
          DH0UM002.XLCCD2 = XOCTBL.XLCCD2[sysVar.arrayIndex];
          DH0UM002.XPPCD = XOCTBL.XPPCD[sysVar.arrayIndex];
          DH0UM002.XPKCD = XOCTBL.XPKCD[sysVar.arrayIndex];
          DH0UM002.XXXSTAT = XOCTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DH0UW01.XOCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DH0UW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DH0UW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DH0UM002.XOCCD = DH0UW01.XOCCD; /* move to the map*/
    DH0UM002.XOCABRV = DH0UW01.XOCABRV;
    DH0UM002.XOCDESC = DH0UW01.XOCDESC;
    DH0UM002.XDVCD = DH0UW01.XDVCD;
    DH0UM002.XOCFUTRF = DH0UW01.XOCFUTRF;
    DH0UM002.XOCPROPF = DH0UW01.XOCPROPF;
    DH0UM002.XLCCD1 = DH0UW01.XLCCD1;
    DH0UM002.XLCCD2 = DH0UW01.XLCCD2;
    DH0UM002.XPPCD = DH0UW01.XPPCD;
    DH0UM002.XPKCD = DH0UW01.XPKCD;
    DH0UM002.XXXSTAT = DH0UW01.XXXSTAT;
    DH0UM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DH0UM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DH0UM002.XXXSTAT == "A")
    DH0UM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DH0UM002.XXXSTAT == "I")
    DH0UM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DH0UM002.XXXSTAT == " ")
    DH0UM002.DAW-TBL-STATUS-DESC = " ";
  end

  DH0UP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0UP20 **********************************/
  /* **************/
end // end DH0UP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DH0UP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DH0UW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0UP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0US-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XOCTBL.XOCCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0UP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0US-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DH0US-TS-ADD-PAGE();
    end

    DH0UM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DH0UM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DH0UP22 **********************************/
  /* **************/
end // end DH0UP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH0UP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DH0UW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DH0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DH0UW01.XOCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DH0UW02.XOCCD[DAW-ROW-NBR] = DH0UW01.XOCCD;
  DH0UW02.XOCABRV[DAW-ROW-NBR] = DH0UW01.XOCABRV;
  DH0UW02.XOCDESC[DAW-ROW-NBR] = DH0UW01.XOCDESC;
  DH0UW02.XOCFUTRF[DAW-ROW-NBR] = DH0UW01.XOCFUTRF;
  DH0UW02.XOCPROPF[DAW-ROW-NBR] = DH0UW01.XOCPROPF;
  DH0UW02.XXXSTAT[DAW-ROW-NBR] = DH0UW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DH0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DH0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DH0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DH0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DH0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DH0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DH0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DH0UW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DH0UW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DH0UW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DH0UP221 ******************************/
  /* ***************/
end // end DH0UP23-DB2-TO-TSQ

