// EDIT TABLE ROW KEY
Function DH0EP69-EDIT-KEY()
  /* ****************************************************************/
  /* Edit the table key (code)*/
  /* a) required - can't be spaces*/
  /* b) can't exist if current function is add or copy*/
  /* field is protected when current function is update*/
  /* c) can't exist in transaction table with todays date*/
  /* in any status except 'c' complete*/
  /* ****************************************************************/

  if (DH0EM002.YSMTBLPF == " ")
    converseLib.validationFailed(373); /* Table prefix is required*/
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    set DH0EM002.YSMTBLPF cursor, bold;
  end

  /* Can't exist in current table (no matter what the status is)*/

  DH0EW01.DH0EW-FND-FL = "N";
  sysVar.arrayIndex = 1;
  while (DH0EW01.DH0EW-FND-FL != "Y"
   && YSMTBL.YSMTBLPF[sysVar.arrayIndex] != "999")
    if (YSMTBL.YSMTBLPF[sysVar.arrayIndex] == DH0EM002.YSMTBLPF
     && YSMTBL.YSMSYSPF[sysVar.arrayIndex] == DH0EM002.YSMSYSPF
     && YSMTBL.YSMSUMID[sysVar.arrayIndex] == DH0EM002.YSMSUMID
     && YSMTBL.YSMSUMCD[sysVar.arrayIndex] == DH0EM002.YSMSUMCD
     && YSMTBL.YSMSRCVL[sysVar.arrayIndex] == DH0EM002.YSMSRCVL)
      DH0EW01.DH0EW-FND-FL = "Y";
    else
      sysVar.arrayIndex = sysVar.arrayIndex + 1;
    end
  end
  if (DH0EW01.DH0EW-FND-FL == "Y")
    if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
      converseLib.validationFailed(372); /* table key already exists*/
      DH0EM002.DAW-INFO-MESSAGE = "REVIEW TABLE LIST AND SELECT ROW TO UPDATE";
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DH0EM002.YSMTBLPF cursor, bold;
      set DH0EM002.YSMSYSPF bold;
      set DH0EM002.YSMSUMID bold;
      set DH0EM002.YSMSUMCD bold;
      set DH0EM002.YSMSRCVL bold;
    end
    if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
      converseLib.validationFailed(372); /* table key already exists*/
      DH0EM002.DAW-INFO-MESSAGE = "TABLE KEY MUST BE CHANGED WHEN COPYING A ROW TO BE ADDED";
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DH0EM002.YSMTBLPF cursor, bold;
      set DH0EM002.YSMSYSPF bold;
      set DH0EM002.YSMSUMID bold;
      set DH0EM002.YSMSUMCD bold;
      set DH0EM002.YSMSRCVL bold;
    end
  end

  /* **  **  **  **  **  **  **  **  **  **  **  **  **  **  ***/
  /* WARNING - Add any new edits for the key prior to this line*/
  /* **  **  **  **  **  **  **  **  **  **  **  **  **  **  ***/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
     || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
      return; /* row is a transaction that is being*/
    end /* updated - skip the next edit*/
  end

  /* Can't exist in transaction table - must be the last edit*/

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;
  DH0EW01.DA-CCAT-KEY-TXT = " ";
  DH0EW01.DH0EW-KEY-TBLPF = DH0EM002.YSMTBLPF;
  DH0EW01.DH0EW-KEY-SYSPF = DH0EM002.YSMSYSPF;
  DH0EW01.DH0EW-KEY-SUMID = DH0EM002.YSMSUMID;
  DH0EW01.DH0EW-KEY-SUMCD = DH0EM002.YSMSUMCD;
  DH0EW01.DH0EW-KEY-SRCVL = DH0EM002.YSMSRCVL;
  VDATRN01.DA-CCAT-KEY-TXT = DH0EW01.DA-CCAT-KEY-TXT;

  XSDAS-DA1140-SEL-2();

  if (SQLCA.VAGen_SQLCODE != 100) /* row found*/
    converseLib.validationFailed(017); /* table key already added or changed*/
    DH0EM002.DAW-INFO-MESSAGE = "SELECT TRANSACTION TO UPDATE OR CALL DATA ADMIN IF APPROVED/PROCESSING";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";

    if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
     || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
                                   /* Field is autoskipped*/
    else
      set DH0EM002.YSMTBLPF cursor, bold;
    end
  end

  /* **************/
  /* ***************** END DH0EP69 **********************************/
  /* **************/
end // end DH0EP69-EDIT-KEY

// DO PROCESSING FOR THE MAP 1
Function DH0EP70-PROC-MAP-1()
  /* ****************************************************************/
  /* Do any processing for MAP 001*/
