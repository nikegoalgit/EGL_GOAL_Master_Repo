// SET MAP 1 FIELD ATTRIBUTES
Function DH0EP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DH0EM001.DAW-PAGE-LIT = "PAGE";
    set DH0EM001.DAW-CURR-PAGE initialAttributes;
    DH0EM001.DAW-OF-LIT = "OF";
    set DH0EM001.DAW-LAST-PAGE initialAttributes;
    DH0EM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DH0EM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DH0EM001.DAW-PAGE-LIT = " ";
    set DH0EM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DH0EM001.DAW-OF-LIT = " ";
    set DH0EM001.DAW-LAST-PAGE skip, invisible;
    DH0EM001.DAW-PF7-LIT = " ";
    DH0EM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DH0EM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DH0EM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DH0EM001.YSMTBLPF[sysVar.arrayIndex] == " ")
        set DH0EM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
        set DH0EM001.YSMSUMID[sysVar.arrayIndex] skip, invisible;
      else
        set DH0EM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
        set DH0EM001.YSMSUMID[sysVar.arrayIndex] initialAttributes;
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DH0EP25 **************************/
  /* **************/
end // end DH0EP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DH0EP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DH0EM002.YSMTBLPF initialAttributes;
    set DH0EM002.YSMSYSPF initialAttributes;
    set DH0EM002.YSMSUMID initialAttributes;
    set DH0EM002.YSMSUMCD initialAttributes;
    set DH0EM002.YSMSRCVL initialAttributes;
    set DH0EM002.YSMSMDSC initialAttributes;
    set DH0EM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DH0EM002.YSMTBLPF skip;
    set DH0EM002.YSMSYSPF skip;
    set DH0EM002.YSMSUMID skip;
    set DH0EM002.YSMSUMCD skip;
    set DH0EM002.YSMSRCVL skip;
    set DH0EM002.YSMSMDSC skip;
    set DH0EM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DH0EM002.YSMTBLPF skip;
    set DH0EM002.YSMSYSPF skip;
    set DH0EM002.YSMSUMID skip;
    set DH0EM002.YSMSUMCD skip;
    set DH0EM002.YSMSRCVL skip;
    set DH0EM002.YSMSMDSC initialAttributes;
    set DH0EM002.XXXSTAT initialAttributes;
  end

  set DH0EM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DH0EM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0EM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DH0EM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DH0EM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DH0EM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DH0EM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DH0EM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DH0EP25 **************************/
  /* **************/
end // end DH0EP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DH0EP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DH0EP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DH0EM001.VAGen_EZEMSG = " ";
  DH0EM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DH0EP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DH0EP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DH0EP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DH0EP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DH0EP30 *****************************/
  /* **************/
end // end DH0EP30-MAIN-MAP-1

