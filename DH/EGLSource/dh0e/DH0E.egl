Program DH0E type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DH0EW01 DH0EW01; // record
  DH0EW02 DH0EW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YSMTBL {deleteAfterUse = yes}; // table
  use DH0EM.DH0EM001,  // forms
      DH0EM.DH0EM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DH0EP00-DRIVER: DH0EP00-DRIVER();
    exit program;
  end // end main
end // end DH0E

// DRIVER FOR APPLICATION
Function DH0EP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH0EP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH0EP20-BUILD-MAP2(); /* Build the second map*/
  else
    DH0EP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH0EP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH0EP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH0EP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH0EP00 *******************************/
  /* **************/
end // end DH0EP00-DRIVER

// INITIALIZE THE APPLICATION
Function DH0EP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH0EW01 empty; /* Working storage record*/
  set DH0EW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH0EM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0EM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0EM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH0EM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0EM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0EM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH0EM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH0EP10 ******************************/
  /* **************/
end // end DH0EP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH0EP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH0EP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH0EM001.DAW-CURR-PAGE)
    DH0ES-TS-READ-PAGE(); /* read temp storage page*/
    DH0EP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH0EP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0EP20 **********************************/
  /* **************/
end // end DH0EP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DH0EP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DH0EM002.DA-CHNG-USER-ID = sysVar.userID;
  DH0EM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DH0EW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0EM002.DA-PLOG-TXT1 = " ";
    DH0EM002.DA-PLOG-TXT2 = " ";
    DH0EM002.YSMTBLPF = " ";
    DH0EM002.YSMSYSPF = " ";
    DH0EM002.YSMSUMID = " ";
    DH0EM002.YSMSUMCD = " ";
    DH0EM002.YSMSRCVL = " ";
    DH0EM002.YSMSMDSC = " ";
    DH0EM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DH0EW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DH0EW01.YSMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DH0EW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DH0EW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DH0EM002.YSMTBLPF = DH0EW01.YSMTBLPF;
                                   /* move to the map*/
      DH0EM002.YSMSYSPF = DH0EW01.YSMSYSPF;
      DH0EM002.YSMSUMID = DH0EW01.YSMSUMID;
      DH0EM002.YSMSUMCD = DH0EW01.YSMSUMCD;
      DH0EM002.YSMSRCVL = DH0EW01.YSMSRCVL;
      DH0EM002.YSMSMDSC = DH0EW01.YSMSMDSC;
      DH0EM002.XXXSTAT = DH0EW01.XXXSTAT;
      DH0EM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DH0EM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/
                                              /* get the tbl row*/
        DH0EM002.YSMTBLPF = DH0EW02.YSMTBLPF[DAW-ROW-NBR];
        DH0EM002.YSMSYSPF = DH0EW02.YSMSYSPF[DAW-ROW-NBR];
        DH0EM002.YSMSUMID = DH0EW02.YSMSUMID[DAW-ROW-NBR];
        DH0EM002.YSMSUMCD = DH0EW02.YSMSUMCD[DAW-ROW-NBR];
        DH0EM002.YSMSRCVL = DH0EW02.YSMSRCVL[DAW-ROW-NBR];
        DH0EW01.DH0EW-FND-FL = "N";
        sysVar.arrayIndex = 1;
        while (DH0EW01.DH0EW-FND-FL != "Y"
         && YSMTBL.YSMTBLPF[sysVar.arrayIndex] != "999")
          if (YSMTBL.YSMTBLPF[sysVar.arrayIndex] == DH0EM002.YSMTBLPF
           && YSMTBL.YSMSYSPF[sysVar.arrayIndex] == DH0EM002.YSMSYSPF
           && YSMTBL.YSMSUMID[sysVar.arrayIndex] == DH0EM002.YSMSUMID
           && YSMTBL.YSMSUMCD[sysVar.arrayIndex] == DH0EM002.YSMSUMCD
           && YSMTBL.YSMSRCVL[sysVar.arrayIndex] == DH0EM002.YSMSRCVL)
            DH0EW01.DH0EW-FND-FL = "Y";
          else
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
          end
        end
        if (DH0EW01.DH0EW-FND-FL == "Y")
          DH0EW01.YSMSYSPF = YSMTBL.YSMSYSPF[sysVar.arrayIndex]; /* save for a*/
          DH0EW01.YSMSUMID = YSMTBL.YSMSUMID[sysVar.arrayIndex]; /* later compare*/
          DH0EW01.YSMSUMCD = YSMTBL.YSMSUMCD[sysVar.arrayIndex];
          DH0EW01.YSMSRCVL = YSMTBL.YSMSRCVL[sysVar.arrayIndex];
          DH0EW01.YSMSMDSC = YSMTBL.YSMSMDSC[sysVar.arrayIndex];
          DH0EW01.XXXSTAT = YSMTBL.XXXSTAT[sysVar.arrayIndex];
          DH0EW01.ZZZSDT = YSMTBL.ZZZSDT[sysVar.arrayIndex];
          DH0EW01.ZZZCHGDT = YSMTBL.ZZZCHGDT[sysVar.arrayIndex];
          DH0EW01.ZZZCHGTM = YSMTBL.ZZZCHGTM[sysVar.arrayIndex];
          DH0EW01.ZZZCHGCT = YSMTBL.ZZZCHGCT[sysVar.arrayIndex];
          DH0EM002.YSMSMDSC = YSMTBL.YSMSMDSC[sysVar.arrayIndex]; /* move to map*/
          DH0EM002.XXXSTAT = YSMTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DH0EW01.YSMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DH0EW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DH0EW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DH0EM002.YSMTBLPF = DH0EW01.YSMTBLPF;
                                   /* move to the map*/
    DH0EM002.YSMSYSPF = DH0EW01.YSMSYSPF;
    DH0EM002.YSMSUMID = DH0EW01.YSMSUMID;
    DH0EM002.YSMSUMCD = DH0EW01.YSMSUMCD;
    DH0EM002.YSMSRCVL = DH0EW01.YSMSRCVL;
    DH0EM002.YSMSMDSC = DH0EW01.YSMSMDSC;
    DH0EM002.XXXSTAT = DH0EW01.XXXSTAT;
    DH0EM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DH0EM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DH0EM002.XXXSTAT == "A")
    DH0EM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DH0EM002.XXXSTAT == "I")
    DH0EM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DH0EM002.XXXSTAT == " ")
    DH0EM002.DAW-TBL-STATUS-DESC = " ";
  end

  DH0EP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0EP20 **********************************/
  /* **************/
end // end DH0EP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DH0EP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DH0EW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0EP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0ES-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YSMTBL.YSMTBLPF[sysVar.arrayIndex] != "999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0EP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0ES-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DH0ES-TS-ADD-PAGE();
    end

    DH0EM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DH0EM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DH0EP22 **********************************/
  /* **************/
end // end DH0EP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH0EP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DH0EW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DH0EW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DH0EW01.YSMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DH0EW02.YSMTBLPF[DAW-ROW-NBR] = DH0EW01.YSMTBLPF;
  DH0EW02.YSMSYSPF[DAW-ROW-NBR] = DH0EW01.YSMSYSPF;
  DH0EW02.YSMSUMID[DAW-ROW-NBR] = DH0EW01.YSMSUMID;
  DH0EW02.YSMSUMCD[DAW-ROW-NBR] = DH0EW01.YSMSUMCD;
  DH0EW02.YSMSRCVL[DAW-ROW-NBR] = DH0EW01.YSMSRCVL;
  DH0EW02.XXXSTAT[DAW-ROW-NBR] = DH0EW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DH0EW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DH0EW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DH0EW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DH0EW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DH0EW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DH0EW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DH0EW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DH0EW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DH0EW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DH0EW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DH0EP221 ******************************/
  /* ***************/
end // end DH0EP23-DB2-TO-TSQ

