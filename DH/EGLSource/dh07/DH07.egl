// DRIVER FOR APPLICATION
Function DH07P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH07P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH07P20-BUILD-MAP2(); /* Build the second map*/
  else
    DH07P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH07P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH07P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH07P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH07P00 *******************************/
  /* **************/
end // end DH07P00-DRIVER

// INITIALIZE THE APPLICATION
Function DH07P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH07W01 empty; /* Working storage record*/
  set DH07W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH07M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH07M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH07M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH07M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH07M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH07M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH07M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH07P10 ******************************/
  /* **************/
end // end DH07P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH07P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH07P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH07M001.DAW-CURR-PAGE)
    DH07S-TS-READ-PAGE(); /* read temp storage page*/
    DH07P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH07P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH07P20 **********************************/
  /* **************/
end // end DH07P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DH07P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DH07M002.DA-CHNG-USER-ID = sysVar.userID;
  DH07M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DH07W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH07M002.DA-PLOG-TXT1 = " ";
    DH07M002.DA-PLOG-TXT2 = " ";
    DH07M002.YSE-SLS-CAT1-CD = " ";
    DH07M002.YSE-SLS-CAT1-ABRV = " ";
    DH07M002.XGPCD = " ";
    DH07M002.XGPABRV = " ";
    DH07M002.YSE-SRT-SEQ-NBR = " ";
    DH07M002.YSE-SLS-CAT1-SDESC = " ";
    DH07M002.YSE-SLS-CAT1-DESC = " ";
    DH07M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DH07W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DH07W01.YSEGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DH07W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DH07W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DH07M002.YSE-SLS-CAT1-CD = DH07W01.YSE-SLS-CAT1-CD;
                                   /* move to the map*/
      DH07M002.XGPCD = DH07W01.XGPCD;
      DH07M002.XGPABRV = " ";
      DH07M002.YSE-SRT-SEQ-NBR = DH07W01.YSE-SRT-SEQ-NBR;
      DH07M002.YSE-SLS-CAT1-SDESC = DH07W01.YSE-SLS-CAT1-SDESC;
      DH07M002.YSE-SLS-CAT1-ABRV = DH07W01.YSE-SLS-CAT1-ABRV;
      DH07M002.YSE-SLS-CAT1-DESC = DH07W01.YSE-SLS-CAT1-DESC;
      DH07M002.XXXSTAT = DH07W01.XXXSTAT;
      DH07M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DH07M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DH07M002.YSE-SLS-CAT1-CD = DH07W02.YSE-SLS-CAT1-CD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DH07M002.YSE-SLS-CAT1-CD in YSETBL.YSE-SLS-CAT1-CD)
          DH07W01.YSE-SLS-CAT1-CD = YSETBL.YSE-SLS-CAT1-CD[sysVar.arrayIndex];
                                   /* save for a*/
          DH07W01.XGPCD = YSETBL.XGPCD[sysVar.arrayIndex]; /* later compare*/
          DH07W01.YSE-SRT-SEQ-NBR = YSETBL.YSE-SRT-SEQ-NBR[sysVar.arrayIndex];
          DH07W01.YSE-SLS-CAT1-SDESC = YSETBL.YSE-SLS-CAT1-SDESC[sysVar.arrayIndex];
          DH07W01.YSE-SLS-CAT1-ABRV = YSETBL.YSE-SLS-CAT1-ABRV[sysVar.arrayIndex]; /* /*/
          DH07W01.YSE-SLS-CAT1-DESC = YSETBL.YSE-SLS-CAT1-DESC[sysVar.arrayIndex];
          DH07W01.XXXSTAT = YSETBL.XXX-STAT[sysVar.arrayIndex];
          DH07W01.ZZZSDT = YSETBL.ZZ-SDT[sysVar.arrayIndex];
          DH07W01.ZZZCHGDT = YSETBL.ZZ-CHNG-DT[sysVar.arrayIndex];
          DH07W01.ZZZCHGTM = YSETBL.ZZ-CHNG-HHMM[sysVar.arrayIndex];
          DH07W01.ZZZCHGCT = YSETBL.ZZ-CHNG-CNT[sysVar.arrayIndex];

          DH07M002.XGPCD = YSETBL.XGPCD[sysVar.arrayIndex]; /* later compare*/
          DH07M002.XGPABRV = " ";
          DH07M002.YSE-SRT-SEQ-NBR = YSETBL.YSE-SRT-SEQ-NBR[sysVar.arrayIndex];
          DH07M002.YSE-SLS-CAT1-SDESC = YSETBL.YSE-SLS-CAT1-SDESC[sysVar.arrayIndex];
          DH07M002.YSE-SLS-CAT1-ABRV = YSETBL.YSE-SLS-CAT1-ABRV[sysVar.arrayIndex]; /* move to map*/
          DH07M002.YSE-SLS-CAT1-DESC = YSETBL.YSE-SLS-CAT1-DESC[sysVar.arrayIndex];
          DH07M002.XXXSTAT = YSETBL.XXX-STAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DH07W01.YSEGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DH07W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DH07W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DH07M002.YSE-SLS-CAT1-CD = DH07W01.YSE-SLS-CAT1-CD;
                                   /* move to the map*/
    DH07M002.XGPCD = DH07W01.XGPCD;
    DH07M002.XGPABRV = " ";
    DH07M002.YSE-SRT-SEQ-NBR = DH07W01.YSE-SRT-SEQ-NBR;
    DH07M002.YSE-SLS-CAT1-SDESC = DH07W01.YSE-SLS-CAT1-SDESC;
    DH07M002.YSE-SLS-CAT1-ABRV = DH07W01.YSE-SLS-CAT1-ABRV;
    DH07M002.YSE-SLS-CAT1-DESC = DH07W01.YSE-SLS-CAT1-DESC;
    DH07M002.XXXSTAT = DH07W01.XXXSTAT;
    DH07M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DH07M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DH07M002.XXXSTAT == "A")
    DH07M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DH07M002.XXXSTAT == "I")
    DH07M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DH07M002.XXXSTAT == " ")
    DH07M002.DAW-TBL-STATUS-DESC = " ";
  end

  DH07P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH07P20 **********************************/
  /* **************/
end // end DH07P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DH07P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DH07W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH07P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH07S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YSETBL.YSE-SLS-CAT1-CD[sysVar.arrayIndex] != "999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH07P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH07S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DH07S-TS-ADD-PAGE();
    end

    DH07M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DH07M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DH07P22 **********************************/
  /* **************/
end // end DH07P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH07P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DH07W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DH07W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DH07W01.YSEGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DH07W02.YSE-SLS-CAT1-CD[DAW-ROW-NBR] = DH07W01.YSE-SLS-CAT1-CD;
  DH07W02.YSE-SLS-CAT1-ABRV[DAW-ROW-NBR] = DH07W01.YSE-SLS-CAT1-ABRV;
  DH07W02.YSE-SLS-CAT1-DESC[DAW-ROW-NBR] = DH07W01.YSE-SLS-CAT1-DESC;
  DH07W02.XXXSTAT[DAW-ROW-NBR] = DH07W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DH07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DH07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DH07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DH07W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DH07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DH07W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DH07W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DH07W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DH07W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DH07W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DH07P221 ******************************/
  /* ***************/
end // end DH07P23-DB2-TO-TSQ

