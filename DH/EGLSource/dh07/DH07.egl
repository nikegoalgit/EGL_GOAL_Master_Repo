// SET MAP 1 FIELD ATTRIBUTES
Function DH07P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DH07M001.DAW-PAGE-LIT = "PAGE";
    set DH07M001.DAW-CURR-PAGE initialAttributes;
    DH07M001.DAW-OF-LIT = "OF";
    set DH07M001.DAW-LAST-PAGE initialAttributes;
    DH07M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DH07M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DH07M001.DAW-PAGE-LIT = " ";
    set DH07M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DH07M001.DAW-OF-LIT = " ";
    set DH07M001.DAW-LAST-PAGE skip, invisible;
    DH07M001.DAW-PF7-LIT = " ";
    DH07M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DH07M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DH07M001.YSE-SLS-CAT1-CD[sysVar.arrayIndex] == " ")
      set DH07M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DH07M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DH07P25 **************************/
  /* **************/
end // end DH07P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DH07P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DH07M002.YSE-SLS-CAT1-CD initialAttributes;
    set DH07M002.XGPCD initialAttributes;
    set DH07M002.YSE-SLS-CAT1-SDESC initialAttributes;
    set DH07M002.YSE-SLS-CAT1-ABRV initialAttributes;
    set DH07M002.YSE-SLS-CAT1-DESC initialAttributes;
    set DH07M002.YSE-SRT-SEQ-NBR initialAttributes;
    set DH07M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DH07M002.YSE-SLS-CAT1-CD skip;
    set DH07M002.XGPCD skip;
    set DH07M002.YSE-SLS-CAT1-SDESC skip;
    set DH07M002.YSE-SLS-CAT1-ABRV skip;
    set DH07M002.YSE-SLS-CAT1-DESC skip;
    set DH07M002.YSE-SRT-SEQ-NBR skip;
    set DH07M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DH07M002.YSE-SLS-CAT1-CD skip;
    set DH07M002.XGPCD initialAttributes;
    set DH07M002.YSE-SLS-CAT1-SDESC initialAttributes;
    set DH07M002.YSE-SLS-CAT1-ABRV initialAttributes;
    set DH07M002.YSE-SLS-CAT1-DESC initialAttributes;
    set DH07M002.YSE-SRT-SEQ-NBR initialAttributes;
    set DH07M002.XXXSTAT initialAttributes;
  end

  set DH07M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DH07M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH07M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DH07M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DH07M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DH07M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DH07M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DH07M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DH07P25 **************************/
  /* **************/
end // end DH07P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DH07P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DH07P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DH07M001.VAGen_EZEMSG = " ";
  DH07M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DH07P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DH07P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DH07P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DH07P70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

