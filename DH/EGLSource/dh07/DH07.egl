// BUILD NEW VDATRN01 RECORD
Function DH07P72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DH07M002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DH07M002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DH07W01.DA-CCAT-KEY-TXT = " ";
  DH07W01.DH07W-KEY-YSECD = DH07M002.YSE-SLS-CAT1-CD;
  VDATRN01.DA-CCAT-KEY-TXT = DH07W01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DH07W01.YSE-SLS-CAT1-CD = DH07M002.YSE-SLS-CAT1-CD;
  DH07W01.XGPCD = DH07M002.XGPCD;
  DH07W01.YSE-SRT-SEQ-NBR = DH07M002.YSE-SRT-SEQ-NBR;
  DH07W01.YSE-SLS-CAT1-SDESC = DH07M002.YSE-SLS-CAT1-SDESC;
  DH07W01.YSE-SLS-CAT1-ABRV = DH07M002.YSE-SLS-CAT1-ABRV;
  DH07W01.YSE-SLS-CAT1-DESC = DH07M002.YSE-SLS-CAT1-DESC;
  DH07W01.XXXSTAT = DH07M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 68;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DH07W01.YSEGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DH07P72 **********************************/
  /* **************/
end // end DH07P72-NEW-TRAN

