// SET MAP 1 FIELD ATTRIBUTES
Function DH05P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DH05M001.DAW-PAGE-LIT = "PAGE";
    set DH05M001.DAW-CURR-PAGE initialAttributes;
    DH05M001.DAW-OF-LIT = "OF";
    set DH05M001.DAW-LAST-PAGE initialAttributes;
    DH05M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DH05M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DH05M001.DAW-PAGE-LIT = " ";
    set DH05M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DH05M001.DAW-OF-LIT = " ";
    set DH05M001.DAW-LAST-PAGE skip, invisible;
    DH05M001.DAW-PF7-LIT = " ";
    DH05M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DH05M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DH05M001.XFCCD[sysVar.arrayIndex] == " ")
      set DH05M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DH05M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DH05P25 **************************/
  /* **************/
end // end DH05P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DH05P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DH05M002.XFCCD initialAttributes;
    set DH05M002.XGPCD initialAttributes;
    set DH05M002.XFCDESC initialAttributes;
    set DH05M002.XFCABRV initialAttributes;
    set DH05M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DH05M002.XFCCD skip;
    set DH05M002.XGPCD skip;
    set DH05M002.XFCDESC skip;
    set DH05M002.XFCABRV skip;
    set DH05M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DH05M002.XFCCD skip;
    set DH05M002.XGPCD skip;
    set DH05M002.XFCDESC initialAttributes;
    set DH05M002.XFCABRV initialAttributes;
    set DH05M002.XXXSTAT initialAttributes;
  end

  set DH05M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DH05M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH05M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DH05M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DH05M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DH05M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DH05M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DH05M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DH05P25 **************************/
  /* **************/
end // end DH05P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DH05P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DH05P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DH05M001.VAGen_EZEMSG = " ";
  DH05M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DH05P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DH05P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DH05P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DH05P70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DH05P30 *****************************/
  /* **************/
end // end DH05P30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DH05P30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DH05P40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DH05M002.VAGen_EZEMSG = " ";
  DH05M002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DH05P25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DH05P50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DH05P60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

