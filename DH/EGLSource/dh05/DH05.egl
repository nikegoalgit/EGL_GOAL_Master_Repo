// NIKE TABLE HANDLER - TABLE MAINTENANCE - SPECIAL FEATURE
// 
// OVERVIEW: This application is used to maintain the customer
//           order type profile table.
// 
// DB2 TABLE: DCORP.VXFC0001
// CSP TABLE: XFCTBL
// 
// DATE CREATED: 10/30/97
//       AUTHOR: Eric Chan
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   04-27-99  ECHAN       ADD NOHILITE IN DH05P25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DH05P70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DH05P70-PROC-MAP-2
//                         DISPLAY STATUS IN DH05PM001
//                         CORRECT ABRV TO CHAR(10) IN DH05M002
// *********************
Program DH05 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DH05W01 DH05W01; // record
  DH05W02 DH05W02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XFCTBL {deleteAfterUse = yes}; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use DH05M.DH05M001,  // forms
      DH05M.DH05M002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DH05P00-DRIVER: DH05P00-DRIVER();
    exit program;
  end // end main
end // end DH05

// DRIVER FOR APPLICATION
Function DH05P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH05P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH05P20-BUILD-MAP2(); /* Build the second map*/
  else
    DH05P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH05P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH05P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH05P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH05P00 *******************************/
  /* **************/
end // end DH05P00-DRIVER

// INITIALIZE THE APPLICATION
Function DH05P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH05W01 empty; /* Working storage record*/
  set DH05W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH05M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH05M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH05M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH05M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH05M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH05M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH05M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH05P10 ******************************/
  /* **************/
end // end DH05P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH05P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH05P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH05M001.DAW-CURR-PAGE)
    DH05S-TS-READ-PAGE(); /* read temp storage page*/
    DH05P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH05P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH05P20 **********************************/
  /* **************/
end // end DH05P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DH05P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DH05M002.DA-CHNG-USER-ID = sysVar.userID;
  DH05M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DH05W01 empty;
  set VDATRN01 empty;
  DH05M002.XGPABRV = " ";

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DH05M002.XGPABRV = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH05M002.DA-PLOG-TXT1 = " ";
    DH05M002.DA-PLOG-TXT2 = " ";
    DH05M002.XFCCD = " ";
    DH05M002.XGPCD = " ";
    DH05M002.XFCDESC = " ";
    DH05M002.XFCABRV = " ";
    DH05M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DH05W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DH05W01.XFCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DH05W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DH05W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DH05M002.XFCCD = DH05W01.XFCCD; /* move to the map*/
      DH05M002.XGPCD = DH05W01.XGPCD;
      DH05M002.XFCDESC = DH05W01.XFCDESC;
      DH05M002.XFCABRV = DH05W01.XFCABRV;
      DH05M002.XXXSTAT = DH05W01.XXXSTAT;
      DH05M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DH05M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DH05M002.XFCCD = DH05W02.XFCCD[DAW-ROW-NBR];
        DH05M002.XGPCD = DH05W02.XGPCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        DH05W01.DH05W-FND-FL = "N";
        sysVar.arrayIndex = 1;
        while (DH05W01.DH05W-FND-FL != "Y"
         && XFCTBL.XFCKEY[sysVar.arrayIndex] != "9999")
          if (XFCTBL.XFCCD[sysVar.arrayIndex] == DH05M002.XFCCD
           && XFCTBL.XGPCD[sysVar.arrayIndex] == DH05M002.XGPCD)
            DH05W01.DH05W-FND-FL = "Y";
          else
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
          end
        end
        if (DH05W01.DH05W-FND-FL == "Y")
          DH05W01.XFCCD = XFCTBL.XFCCD[sysVar.arrayIndex];
          DH05W01.XGPCD = XFCTBL.XGPCD[sysVar.arrayIndex]; /* save for*/
          DH05W01.XFCDESC = XFCTBL.XFCDESC[sysVar.arrayIndex]; /* later compare*/
          DH05W01.XFCABRV = XFCTBL.XFCABRV[sysVar.arrayIndex]; /* later compare*/
          DH05W01.XXXSTAT = XFCTBL.XXXSTAT[sysVar.arrayIndex];
          DH05W01.ZZZSDT = XFCTBL.ZZZSDT[sysVar.arrayIndex];
          DH05W01.ZZZCHGDT = XFCTBL.ZZZCHGDT[sysVar.arrayIndex];
          DH05W01.ZZZCHGTM = XFCTBL.ZZZCHGTM[sysVar.arrayIndex];
          DH05W01.ZZZCHGCT = XFCTBL.ZZZCHGCT[sysVar.arrayIndex];

          DH05M002.XFCDESC = XFCTBL.XFCDESC[sysVar.arrayIndex]; /* move to map*/
          DH05M002.XFCABRV = XFCTBL.XFCABRV[sysVar.arrayIndex];
          DH05M002.XXXSTAT = XFCTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DH05W01.XFCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DH05W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DH05W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DH05M002.XFCCD = DH05W01.XFCCD; /* move to the map*/
    DH05M002.XGPCD = DH05W01.XGPCD;
    DH05M002.XFCDESC = DH05W01.XFCDESC;
    DH05M002.XFCABRV = DH05W01.XFCABRV;
    DH05M002.XXXSTAT = DH05W01.XXXSTAT;
    DH05M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DH05M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DH05M002.XXXSTAT == "A")
    DH05M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DH05M002.XXXSTAT == "I")
    DH05M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DH05M002.XXXSTAT == " ")
    DH05M002.DAW-TBL-STATUS-DESC = " ";
  end

  DH05P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH05P20 **********************************/
  /* **************/
end // end DH05P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DH05P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DH05W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH05P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH05S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XFCTBL.XFCKEY[sysVar.arrayIndex] != "9999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH05P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH05S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DH05S-TS-ADD-PAGE();
    end

    DH05M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DH05M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DH05P22 **********************************/
  /* **************/
end // end DH05P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH05P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DH05W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DH05W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DH05W01.XFCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DH05W02.XFCCD[DAW-ROW-NBR] = DH05W01.XFCCD;
  DH05W02.XGPCD[DAW-ROW-NBR] = DH05W01.XGPCD;
  DH05W02.XFCDESC[DAW-ROW-NBR] = DH05W01.XFCDESC;
  DH05W02.XXXSTAT[DAW-ROW-NBR] = DH05W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DH05W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DH05W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DH05W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DH05W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DH05W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DH05W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DH05W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DH05W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DH05W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DH05W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DH05P221 ******************************/
  /* ***************/
end // end DH05P23-DB2-TO-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH05P24-CSP-TO-TSQ()
  /* **************************************************************/
