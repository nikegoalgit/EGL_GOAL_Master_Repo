  DH05W01.XFCABRV = DH05M002.XFCABRV;
  DH05W01.XXXSTAT = DH05M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 52;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DH05W01.XFCGROUP;

