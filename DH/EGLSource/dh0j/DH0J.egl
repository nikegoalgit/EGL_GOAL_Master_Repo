// OVERVIEW: This application is used to maintain the NATURE
//           OF DEFECT table
// 
// DB2 TABLE: DCORP.VYDF0001
// CSP TABLE: YDFTBL
// 
// DATE CREATED: 05/13/97
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   05-13-99  ECHAN       ADD NOHILITE IN DH0JP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DH0JP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DH0JP70-PROC-MAP-2
//                         DISPLAY STATUS IN DH0JM001
// *********************
Program DH0J type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DH0JW01 DH0JW01; // record
  DH0JW02 DH0JW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YDFTBL {deleteAfterUse = yes}; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use YDCTBL {deleteAfterUse = yes}; // table
  use DH0JM.DH0JM001,  // forms
      DH0JM.DH0JM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DH0JP00-DRIVER: DH0JP00-DRIVER();
    exit program;
  end // end main
end // end DH0J

// DRIVER FOR APPLICATION
Function DH0JP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH0JP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH0JP20-BUILD-MAP2(); /* Build the second map*/
  else
    DH0JP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH0JP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH0JP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH0JP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH0JP00 *******************************/
  /* **************/
end // end DH0JP00-DRIVER

// INITIALIZE THE APPLICATION
Function DH0JP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH0JW01 empty; /* Working storage record*/
  set DH0JW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH0JM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0JM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0JM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH0JM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0JM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0JM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH0JM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH0JP10 ******************************/
  /* **************/
end // end DH0JP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH0JP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH0JP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH0JM001.DAW-CURR-PAGE)
    DH0JS-TS-READ-PAGE(); /* read temp storage page*/
    DH0JP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH0JP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0JP20 **********************************/
  /* **************/
end // end DH0JP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DH0JP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DH0JM002.DA-CHNG-USER-ID = sysVar.userID;
  DH0JM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DH0JW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0JM002.DA-PLOG-TXT1 = " ";
    DH0JM002.DA-PLOG-TXT2 = " ";
    DH0JM002.XGPCD = " ";
    DH0JM002.YDFCD = " ";
    DH0JM002.YDCCD = " ";
    DH0JM002.YDFDESC = " ";
    DH0JM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DH0JW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DH0JW01.YDFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DH0JW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DH0JW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DH0JM002.XGPCD = DH0JW01.XGPCD; /* move to the map*/
      DH0JM002.YDFCD = DH0JW01.YDFCD;
                                   /* move to the map*/
      DH0JM002.YDCCD = DH0JW01.YDCCD;
      DH0JM002.YDFDESC = DH0JW01.YDFDESC;
      DH0JM002.XXXSTAT = DH0JW01.XXXSTAT;
      DH0JM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DH0JM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DH0JM002.XGPCD = DH0JW02.XGPCD[DAW-ROW-NBR];
        DH0JM002.YDFCD = DH0JW02.YDFCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DH0JM002.XGPCD in YDFTBL.XGPCD
         && DH0JM002.YDFCD in YDFTBL.YDFCD
        ) /* AND DH0JM002.XLS-SCAN-STAT-CD EQ XLSTBL.XLS-SCAN-STAT-CD(EZETST);*/
          DH0JW01.XGPCD = YDFTBL.XGPCD[sysVar.arrayIndex];
          DH0JW01.YDFCD = YDFTBL.YDFCD[sysVar.arrayIndex];
                                   /* save for*/
          DH0JW01.YDCCD = YDFTBL.YDCCD[sysVar.arrayIndex];
          DH0JW01.YDFDESC = YDFTBL.YDFDESC[sysVar.arrayIndex];
          DH0JW01.XXXSTAT = YDFTBL.XXXSTAT[sysVar.arrayIndex];
          DH0JW01.ZZZSDT = YDFTBL.ZZZSDT[sysVar.arrayIndex];
          DH0JW01.ZZZCHGDT = YDFTBL.ZZZCHGDT[sysVar.arrayIndex];
          DH0JW01.ZZZCHGTM = YDFTBL.ZZZCHGTM[sysVar.arrayIndex];
          DH0JW01.ZZZCHGCT = YDFTBL.ZZZCHGCT[sysVar.arrayIndex];

          DH0JM002.YDCCD = YDFTBL.YDCCD[sysVar.arrayIndex];
          DH0JM002.YDFDESC = YDFTBL.YDFDESC[sysVar.arrayIndex];
          DH0JM002.XXXSTAT = YDFTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DH0JW01.YDFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DH0JW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DH0JW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DH0JM002.XGPCD = DH0JW01.XGPCD; /* move to the map*/
    DH0JM002.YDFCD = DH0JW01.YDFCD;
                                   /* move to the map*/
    DH0JM002.YDCCD = DH0JW01.YDCCD;
    DH0JM002.YDFDESC = DH0JW01.YDFDESC;
    DH0JM002.XXXSTAT = DH0JW01.XXXSTAT;
    DH0JM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DH0JM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DH0JM002.XXXSTAT == "A")
    DH0JM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DH0JM002.XXXSTAT == "I")
    DH0JM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DH0JM002.XXXSTAT == " ")
    DH0JM002.DAW-TBL-STATUS-DESC = " ";
  end

  DH0JP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0JP20 **********************************/
  /* **************/
end // end DH0JP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DH0JP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DH0JW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0JP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0JS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YDFTBL.YDFCD[sysVar.arrayIndex] != "999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0JP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0JS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DH0JS-TS-ADD-PAGE();
    end

    DH0JM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DH0JM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DH0JP22 **********************************/
  /* **************/
end // end DH0JP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH0JP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DH0JW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DH0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DH0JW01.YDFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DH0JW02.YDFCD[DAW-ROW-NBR] = DH0JW01.YDFCD;
  DH0JW02.XGPCD[DAW-ROW-NBR] = DH0JW01.XGPCD;
  DH0JW02.YDFDESC[DAW-ROW-NBR] = DH0JW01.YDFDESC;
  DH0JW02.XXXSTAT[DAW-ROW-NBR] = DH0JW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DH0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DH0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DH0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DH0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DH0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DH0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DH0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DH0JW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DH0JW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DH0JW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DH0JP221 ******************************/
  /* ***************/
end // end DH0JP23-DB2-TO-TSQ

