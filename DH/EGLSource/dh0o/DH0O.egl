// EDIT TABLE ROW KEY
Function DH0OP69-EDIT-KEY()
  /* ****************************************************************/
  /* Edit the table key*/
  /* a) required - can't be spaces*/
  /* b) can't exist if current function is add or copy*/
  /* field is protected when current function is update*/
  /* c) can't exist in transaction table with todays date*/
  /* in any status except 'c' complete*/
  /* ****************************************************************/

  if (DH0OM002.XGPCD == " "
   || DH0OM002.XDR-PROD-REG-CD == " "
   || DH0OM002.XSECD == " "
   || DH0OM002.XSNYR == 0)
    return;
  end

  /* Can't exist in current table (no matter what the status is)*/

  DH0OW01.DH0OW-FND-FL = "N";
  sysVar.arrayIndex = 1;
  if (DH0OM002.XGPCD in YSNTBL.XGPCD
   && DH0OM002.XDR-PROD-REG-CD == YSNTBL.XDR-PROD-REG-CD[sysVar.arrayIndex]
   && DH0OM002.XSECD == YSNTBL.XSECD[sysVar.arrayIndex]
   && DH0OM002.XSNYR == YSNTBL.XSNYR[sysVar.arrayIndex])
    DH0OW01.DH0OW-FND-FL = "Y";
  else
  /* cnaman  -- changes begin 20030612 -- added check for EZETST = 0 to avoid subscript error 4038*/
    if (sysVar.arrayIndex == 0)
      sysVar.arrayIndex = 1;
    end
  /* cnaman  -- changes end 20030612 -- added check for EZETST = 0 to avoid subscript error 4038*/
    while (DH0OW01.DH0OW-FND-FL != "Y"
     && YSNTBL.XSECD[sysVar.arrayIndex] != "99")
      sysVar.arrayIndex = sysVar.arrayIndex + 1;
      if (DH0OM002.XGPCD == YSNTBL.XGPCD[sysVar.arrayIndex]
       && DH0OM002.XDR-PROD-REG-CD == YSNTBL.XDR-PROD-REG-CD[sysVar.arrayIndex]
       && DH0OM002.XSECD == YSNTBL.XSECD[sysVar.arrayIndex]
       && DH0OM002.XSNYR == YSNTBL.XSNYR[sysVar.arrayIndex]
       && YSNTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and is a active status*/
        DH0OW01.DH0OW-FND-FL = "Y";
      end
    end
  end


  if (DH0OW01.DH0OW-FND-FL == "Y")
    if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
      converseLib.validationFailed(016); /* table key already exists*/
      DH0OM002.DAW-INFO-MESSAGE = "REVIEW TABLE LIST AND SELECT ROW TO UPDATE";
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DH0OM002.XDR-PROD-REG-CD cursor, bold;
      set DH0OM002.XGPCD bold;
      set DH0OM002.XSECD bold;
      set DH0OM002.XSNYR bold;
    end
    if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
      converseLib.validationFailed(016); /* table key already exists*/
      DH0OM002.DAW-INFO-MESSAGE = "TABLE KEY MUST BE CHANGED WHEN COPYING A ROW TO BE ADDED";
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DH0OM002.XDR-PROD-REG-CD cursor, bold;
      set DH0OM002.XSECD bold;
      set DH0OM002.XGPCD bold;
      set DH0OM002.XSNYR bold;
    end
  end


  /* **  **  **  **  **  **  **  **  **  **  **  **  **  **  ***/
  /* WARNING - Add any new edits for the key prior to this line*/
  /* **  **  **  **  **  **  **  **  **  **  **  **  **  **  ***/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    return; /* row is a transaction that is being*/
  end /* updated - skip the next edit*/

  /* Can't exist in transaction table - must be the last edit*/

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;
  DH0OW01.DA-CCAT-KEY-TXT = " ";
  DH0OW01.XDR-PROD-REG-CD = DH0OM002.XDR-PROD-REG-CD;
  DH0OW01.DH0OW-KEY-XGPCD = DH0OM002.XGPCD;
  DH0OW01.DH0OW-KEY-XDRCD = DH0OM002.XDR-PROD-REG-CD;
  DH0OW01.DH0OW-KEY-XSECD = DH0OM002.XSECD;
  DH0OW01.DH0OW-KEY-XSNYR = DH0OM002.XSNYR;
  VDATRN01.DA-CCAT-KEY-TXT = DH0OW01.DA-CCAT-KEY-TXT;
  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;

  XSDAS-DA1140-SEL-2();

  if (SQLCA.VAGen_SQLCODE != 100) /* row found*/
    converseLib.validationFailed(017); /* table key already added or changed*/
    DH0OM002.DAW-INFO-MESSAGE = "SELECT TRANSACTION TO UPDATE OR CALL DATA ADMIN IF APPROVED/PROCESSING";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    set DH0OM002.XGPCD cursor, bold;
  end

  /* **************/
  /* ***************** END DH0OP69 **********************************/
  /* **************/
end // end DH0OP69-EDIT-KEY

// DO PROCESSING FOR THE MAP 1
Function DH0OP70-PROC-MAP-1()
  /* ****************************************************************/
  /* Do any processing for MAP 001*/
  /* ****************************************************************/

  /* *****************************************************************/
  /* Display new page*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-PAGE != DH0OM001.DAW-CURR-PAGE)
    DH0OS-TS-READ-PAGE();
    DH0OP71-TSQ-TO-MAP();
    DH0OP25-SET-ATTR-1();
  end

  /* *****************************************************************/
  /* set all in-process rows on the current page to pending*/
  /* *****************************************************************/

  if (DAWREC.DAW-SET-PENDING-ALL-FL == "Y")

    DAWREC.DAW-ROW-NBR = 1;
    while (DAWREC.DAW-ROW-NBR <= 14) /* check each row on the MAP 001*/
      if (DH0OM001.DAW-STATUS-DESC[DAW-ROW-NBR] == "EDIT")
        set VDATRN01 empty;
        VDATRN01.DAW-VDATRN01-KEY = DH0OW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
        XSDAS-SET-STATUS(); /* set the status to 'pending'*/
      end
      DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    end

  end

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-ADD-ROW-FL == "Y")

    DAWREC.DAW-RECORD-TYPE = " ";
    DAWREC.DAW-CURR-FUNCTION-CD = "ADD";
    DAWREC.DAW-ROW-NBR = 0;

    DH0OP20-BUILD-MAP2();

    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    while (DAWREC.DAW-DONE-WITH-ROW-FL != "Y" /* process row until done*/
     && DAWREC.DAW-TERM-APPL-FL == "N") /* with it or exit from appl*/
      DH0OP30-MAIN-MAP-2(); /* main loop for MAP 002*/
    end

    DAWREC.DAW-CURR-FUNCTION-CD = "   ";

  end

  /* *****************************************************************/
  /* Select, copy or delete rows;*/
  /* *****************************************************************/

  if (DAWREC.DAW-SELECT-ROW-FL == "Y"
   || DAWREC.DAW-COPY-ROW-FL == "Y"
   || DAWREC.DAW-DELETE-ROW-FL == "Y")

    DAWREC.DAW-ROW-NBR = 1;
    while (DAWREC.DAW-ROW-NBR <= 14 /* check each row on the MAP 001*/
     && DAWREC.DAW-TERM-APPL-FL == "N") /* until done or exit from appl*/

      DAWREC.DAW-RECORD-TYPE = DH0OW02.DAW-RECORD-TYPE[DAW-ROW-NBR];

      if (DH0OM001.DAW-SELECT-CD[DAW-ROW-NBR] == "S"
       || DH0OM001.DAW-SELECT-CD[DAW-ROW-NBR] == "C"
       || DH0OM001.DAW-SELECT-CD[DAW-ROW-NBR] == "D")

        if (DH0OM001.DAW-SELECT-CD[DAW-ROW-NBR] == "S")
          DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
        end
        if (DH0OM001.DAW-SELECT-CD[DAW-ROW-NBR] == "C")
          DAWREC.DAW-CURR-FUNCTION-CD = "CPY";
        end
        if (DH0OM001.DAW-SELECT-CD[DAW-ROW-NBR] == "D")
          DAWREC.DAW-CURR-FUNCTION-CD = "DEL";
          DAWREC.DAW-OK-TO-DELETE-FL = "N";
        end

        DAWREC.DAW-DONE-WITH-ROW-FL = "N";
        DH0OP20-BUILD-MAP2();
        while (DAWREC.DAW-DONE-WITH-ROW-FL != "Y" /* process row until done*/
         && DAWREC.DAW-TERM-APPL-FL == "N") /* with it or exit from appl*/
          DH0OP30-MAIN-MAP-2(); /* main loop for MAP 002*/
        end

      end

      DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
      DAWREC.DAW-CURR-FUNCTION-CD = "   ";

    end
  end

  /* *****************************************************************/
  /* Any maintenance selections are now completed so rebuild*/
  /* temp storage*/
  /* *****************************************************************/

  if (DAWREC.DAW-TERM-APPL-FL == "N") /* not ready to exit this application*/
    if (DAWREC.DAW-SELECT-ROW-FL == "Y"
     || DAWREC.DAW-COPY-ROW-FL == "Y"
     || DAWREC.DAW-DELETE-ROW-FL == "Y"
     || DAWREC.DAW-ADD-ROW-FL == "Y"
     || DAWREC.DAW-SET-PENDING-ALL-FL == "Y")
      DH0OS-TS-PURGE-FIL(); /* purge temp storage*/
      DH0OP20-BUILD-MAP1(); /* and then rebuilt it*/
      if (DAWREC.DAW-SET-PENDING-ALL-FL == "Y")
        DH0OM001.DAW-INFO-MESSAGE = "ALL TRANSACTIONS IN EDIT STATUS SET TO PENDING - PLEASE REVIEW LIST";
      else
        DH0OM001.DAW-INFO-MESSAGE = "ALL MAINTENANCE SELECTIONS HAVE BEEN COMPLETED - PLEASE REVIEW LIST";
      end
    end
  end

  /* **************/
  /* ***************** END DH0OP70 **********************************/
  /* **************/
end // end DH0OP70-PROC-MAP-1

// DO PROCESSING NEEDED FOR MAP 2
Function DH0OP70-PROC-MAP-2()
  /* ****************************************************************/
  /* Do any processing for MAP 002*/
  /* ****************************************************************/

  /* ****************************************************************/
  /* User hit enter instead of PF6 or PF10, so there's nothing to*/
  /* do except give them a message indicating what to do*/
  /* ****************************************************************/

  if (DAWREC.DAW-APPLY-FL == "N"
   && DAWREC.DAW-SET-PENDING-FL == "N")
    DH0OM002.DAW-INFO-MESSAGE = "USE PF6 TO CREATE TRANSACTION OR PF10 TO CREATE AND SET TO PENDING";
    DH0OM002.VAGen_EZEMSG = "OR PF3 TO RETURN TO LIST";
    return;
  end


  /* *****************************************************************/
  /* See if anything was changed- Compare map to table record*/
  /* *****************************************************************/

  DH0OP71-COMPARE();


  /* *****************************************************************/
  /* Set status to pending if no change to row*/
  /* *****************************************************************/

  if (DAWREC.DAW-SET-PENDING-FL == "Y"
   && DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-DATA-CHANGED-FL == "N") /* no change to row*/
    set VDATRN01 empty;
    if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
      VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    else
      VDATRN01.DAW-VDATRN01-KEY = DH0OW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
    end
    XSDAS-SET-STATUS(); /* set the status to 'pending'*/
    DH0OM002.DAW-INFO-MESSAGE = "TRANSACTION STATUS CHANGED TO PENDING: PF3 TO CONTINUE ";
    return;
  end


  /* *****************************************************************/
  /* Create a transaction to add a table row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   && DAWREC.DAW-RECORD-TYPE == " "
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* row was added or changed*/

    DH0OP72-NEW-TRAN(); /* build new transaction row*/
    XSDAS-DA1140-ADD();
    if (SQLCA.VAGen_SQLCODE == -803 /* check duplicate*/
     || SQLCA.VAGen_SQLCODE == -911) /* check deadlock*/
      DH0OM002.DAW-INFO-MESSAGE = DAWREC.DAW-INFO-MESSAGE;
    else
      DH0OM002.DAW-INFO-MESSAGE = "NEW TABLE ROW WILL BE ADDED - PF3 TO CONTINUE OR RETURN TO LIST";
    end
    return;

  end


  /* *****************************************************************/
  /* Create a transaction to copy a table row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   && DAWREC.DAW-RECORD-TYPE == "TBL "
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* row was copied or updated*/

    DH0OP72-NEW-TRAN(); /* build new transaction row*/
    XSDAS-DA1140-ADD();
    if (SQLCA.VAGen_SQLCODE == -803 /* check duplicate*/
     || SQLCA.VAGen_SQLCODE == -911) /* check deadlock*/
      DH0OM002.DAW-INFO-MESSAGE = DAWREC.DAW-INFO-MESSAGE;
    else
      DH0OM002.DAW-INFO-MESSAGE = "NEW TABLE ROW WILL BE ADDED - PF3 TO CONTINUE OR RETURN TO LIST";
    end
    return;
  end


  /* *****************************************************************/
  /* Create a transaction to update a table row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   && DAWREC.DAW-RECORD-TYPE == "TBL "
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* row was copied or updated*/

    DH0OP72-NEW-TRAN(); /* build new transaction row*/
    XSDAS-DA1140-ADD();
    if (SQLCA.VAGen_SQLCODE == -803 /* check duplicate*/
     || SQLCA.VAGen_SQLCODE == -911) /* check deadlock*/
      DH0OM002.DAW-INFO-MESSAGE = DAWREC.DAW-INFO-MESSAGE;
    else
      DH0OM002.DAW-INFO-MESSAGE = "TABLE ROW WILL BE UPDATED - PF3 TO CONTINUE OR RETURN TO LIST";
    end
    return;
  end


  /* *****************************************************************/
  /* Create a transaction to delete a table row*/
  /* *****************************************************************/

  /* This function has not been used*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL"
   && DAWREC.DAW-RECORD-TYPE == "TBL "
   && DAWREC.DAW-DATA-CHANGED-FL == "Y")

    if (DAWREC.DAW-OK-TO-DELETE-FL == "Y")
      DH0OP72-NEW-TRAN(); /* build new transaction row*/
      XSDAS-DA1140-ADD();
      DH0OM002.DAW-INFO-MESSAGE = "TABLE ROW WILL BE DELETED - PF3 TO CONTINUE OR RETURN TO LIST";
    else
      DH0OM002.DAW-INFO-MESSAGE = "USE PF6 TO DELETE OR PF3 TO CANCEL THE DELETE AND RETURN TO LIST";
      DAWREC.DAW-OK-TO-DELETE-FL = "Y";
    end
    return;
  end


  /* *****************************************************************/
  /* Update a transaction*/
  /* *****************************************************************/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* something was changed*/

    DH0OP73-OLD-TRAN(); /* build old transaction row*/
    XSDAS-DA1140-UPD();
    if (SQLCA.VAGen_SQLCODE == -911) /* check deadlock*/
      DH0OM002.DAW-INFO-MESSAGE = DAWREC.DAW-INFO-MESSAGE;
    else
      DH0OM002.DAW-INFO-MESSAGE = "TRANSACTION UPDATED - PF3 TO CONTINUE OR RETURN TO LIST";
    end
    return;

  end


  /* *****************************************************************/
  /* copy a transaction*/
  /* *****************************************************************/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   && DAWREC.DAW-DATA-CHANGED-FL == "Y") /* something was changed*/

    DH0OP72-NEW-TRAN(); /* build new transaction row*/
    XSDAS-DA1140-ADD();
    if (SQLCA.VAGen_SQLCODE == -803 /* check duplicate*/
     || SQLCA.VAGen_SQLCODE == -911) /* check deadlock*/
      DH0OM002.DAW-INFO-MESSAGE = DAWREC.DAW-INFO-MESSAGE;
    else
      DH0OM002.DAW-INFO-MESSAGE = "NEW TABLE ROW WILL BE ADDED - PF3 TO CONTINUE OR RETURN TO LIST";
    end
    return;

  end


  /* *****************************************************************/
  /* Delete  a transaction*/
  /* *****************************************************************/

  if (DAWREC.DAW-RECORD-TYPE == "TRAN"
   && DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-OK-TO-DELETE-FL == "Y")
      set VDATRN01 empty;
      VDATRN01.DAW-VDATRN01-KEY = DH0OW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-DEL();
      if (SQLCA.VAGen_SQLCODE == -911) /* check deadlock*/
        DH0OM002.DAW-INFO-MESSAGE = DAWREC.DAW-INFO-MESSAGE;
      else
        DH0OM002.DAW-INFO-MESSAGE = "TRANSACTION DELETED - PF3 TO CONTINUE OR RETURN TO LIST";
        DAWREC.DAW-OK-TO-DELETE-FL = "N";
      end
      return;
    else
      DH0OM002.DAW-INFO-MESSAGE = "USE PF6 TO DELETE OR PF3 TO CANCEL THE DELETE AND RETURN TO LIST";
      DAWREC.DAW-OK-TO-DELETE-FL = "Y";
      return;
    end
  end


  /* still here !! Then nothing changed and there's nothing to process*/

  DH0OM002.DAW-INFO-MESSAGE = "NOTHING ADDED OR CHANGED..... ARE YOU JUST LOOKING ?? ";
  DH0OM002.VAGen_EZEMSG = "USE PF3 TO RETURN TO LIST WHEN YOU ARE READY";

  /* **************/
  /* ***************** END DH0OP70 **********************************/
  /* **************/
end // end DH0OP70-PROC-MAP-2

// COMPARE TABLE ROW TO RECORD
Function DH0OP71-COMPARE()
  /* *****************************************************************/
  /* See if anything changed*/
  /* *****************************************************************/

  if (DH0OW01.XSECD == DH0OM002.XSECD
   && DH0OW01.XGPCD == DH0OM002.XGPCD
   && DH0OW01.XSNABRV == DH0OM002.XSNABRV
   && DH0OW01.XSNFOXDT == DH0OM002.XSNFOXDT
   && DH0OW01.XSNYR == DH0OM002.XSNYR
   && DH0OW01.XSN-SESN-BDT == DH0OM002.XSN-SESN-BDT
   && DH0OW01.XSN-SESN-XDT == DH0OM002.XSN-SESN-XDT
   && DH0OW01.XSNOFRDT == DH0OM002.XSNOFRDT
   && DH0OW01.XSNFOBDT == DH0OM002.XSNFOBDT
   && DH0OW01.XDR-PROD-REG-CD == DH0OM002.XDR-PROD-REG-CD
   && DH0OW01.XSNDESC == DH0OM002.XSNDESC
   && DH0OW01.XXXSTAT == DH0OM002.XXXSTAT
   && DH0OW01.DA-PLOG-TXT1 == DH0OM002.DA-PLOG-TXT1
   && DH0OW01.DA-PLOG-TXT2 == DH0OM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
  /* ***************** END DH0OP71 **********************************/
  /* **************/
end // end DH0OP71-COMPARE

// MOVE TEMP STORAGE PAGE TO MAP
Function DH0OP71-TSQ-TO-MAP()
  /* ************************************************************/
  /* Move temp storage page to map*/
  /* ************************************************************/

  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DH0OM001.DAW-SELECT-CD[sysVar.arrayIndex] = DH0OW02.DAW-SELECT-CD[sysVar.arrayIndex];
    DH0OM001.XDR-PROD-REG-CD[sysVar.arrayIndex] = DH0OW02.XDR-PROD-REG-CD[sysVar.arrayIndex];
    DH0OM001.XGPCD[sysVar.arrayIndex] = DH0OW02.XGPCD[sysVar.arrayIndex];
    DH0OM001.XSECD[sysVar.arrayIndex] = DH0OW02.XSECD[sysVar.arrayIndex];
    DH0OM001.XSNYR[sysVar.arrayIndex] = DH0OW02.XSNYR[sysVar.arrayIndex];
    DH0OM001.XSNABRV[sysVar.arrayIndex] = DH0OW02.XSNABRV[sysVar.arrayIndex];
    DH0OM001.XSNDESC[sysVar.arrayIndex] = DH0OW02.XSNDESC[sysVar.arrayIndex];
    DH0OM001.XXXSTAT[sysVar.arrayIndex] = DH0OW02.XXXSTAT[sysVar.arrayIndex];

    DH0OM001.DAW-STATUS-DESC[sysVar.arrayIndex] = DH0OW02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DH0OM001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DH0OW02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DH0OM001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DH0OW02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  /* **************/
  /* ****************** END DH0OP71 *****************************/
  /* **************/
end // end DH0OP71-TSQ-TO-MAP

// BUILD NEW VDATRN01 RECORD
Function DH0OP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DH0OM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DH0OM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DH0OW01.DA-CCAT-KEY-TXT = " ";
  DH0OW01.DH0OW-KEY-XGPCD = DH0OM002.XGPCD;
  DH0OW01.DH0OW-KEY-XDRCD = DH0OM002.XDR-PROD-REG-CD;
  DH0OW01.DH0OW-KEY-XSECD = DH0OM002.XSECD;
  DH0OW01.DH0OW-KEY-XSNYR = DH0OM002.XSNYR;
  VDATRN01.DA-CCAT-KEY-TXT = DH0OW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DH0OW01.XGPCD = DH0OM002.XGPCD;
  DH0OW01.XDR-PROD-REG-CD = DH0OM002.XDR-PROD-REG-CD;
  DH0OW01.XSECD = DH0OM002.XSECD;
  DH0OW01.XSNYR = DH0OM002.XSNYR;
  DH0OW01.XSNABRV = DH0OM002.XSNABRV;
  DH0OW01.XSNDESC = DH0OM002.XSNDESC;
  DH0OW01.XSN-SESN-BDT = DH0OM002.XSN-SESN-BDT;
  DH0OW01.XSN-SESN-XDT = DH0OM002.XSN-SESN-XDT;
  DH0OW01.XSNOFRDT = DH0OM002.XSNOFRDT;
  DH0OW01.XSNFOBDT = DH0OM002.XSNFOBDT;
  DH0OW01.XSNFOXDT = DH0OM002.XSNFOXDT;
  DH0OW01.XXXSTAT = DH0OM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 60;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DH0OW01.YSNGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DH0OP72 **********************************/
  /* **************/
end // end DH0OP72-NEW-TRAN

// BUILD UPDATED VDATRN01 RECORD
Function DH0OP73-OLD-TRAN()
  /* *****************************************************************/
  /* Build an updated transaction row*/
  /* *****************************************************************/


  /* move the prolog information in case it changed*/
  /* The transaction type stays the same*/

  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DH0OM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DH0OM002.DA-PLOG-TXT2;

  /* Build the table row*/

  DH0OW01.XGPCD = DH0OM002.XGPCD;
  DH0OW01.XDR-PROD-REG-CD = DH0OM002.XDR-PROD-REG-CD;
  DH0OW01.XSECD = DH0OM002.XSECD;
  DH0OW01.XSNYR = DH0OM002.XSNYR;
  DH0OW01.XSNDESC = DH0OM002.XSNDESC;
  DH0OW01.XSNABRV = DH0OM002.XSNABRV;
  DH0OW01.XSN-SESN-BDT = DH0OM002.XSN-SESN-BDT;
  DH0OW01.XSN-SESN-XDT = DH0OM002.XSN-SESN-XDT;
  DH0OW01.XSNOFRDT = DH0OM002.XSNOFRDT;
  DH0OW01.XSNFOBDT = DH0OM002.XSNFOBDT;
  DH0OW01.XSNFOXDT = DH0OM002.XSNFOXDT;

  DH0OW01.XXXSTAT = DH0OM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 60;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DH0OW01.YSNGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y"
   || DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DH0OP73 **********************************/
  /* **************/
end // end DH0OP73-OLD-TRAN

// EXIT FROM THE APPLICATION
Function DH0OP99-TERMINATE()
  /* ************************************************************/
  /* Transfer out of the application*/
  /* ************************************************************/

  COMMAREA.CAUSERA = DAWREC.DAWGROUP;

  DH0OS-TS-PURGE-FIL(); /* purge temp storage*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y") /* return to DA01 application*/
    COMMAREA.CATOAP = "DA01";
    XSEXIT();
  end

  if (DAW-EXIT-OPTION == "RETN") /* return to prior application*/
    COMMAREA.CATOAP = COMMAREA.CAFROMAP;
    XSEXIT();
  end

  if (DAW-EXIT-OPTION == "MENU") /* return to main menu*/
    XSPF4();
  end

  if (DAW-EXIT-OPTION == "HELP") /* check for system help*/
    XSPF1(); /* help facility*/
  end

  /* Still here - then XSEXIT didn't allow the transfer so restart*/
  /* the current application and display the error message*/

  if (COMMAREA.CASECRC != 0) /* no message from xsexit*/
    XSDAS-XSEXIT-CAMSG();
  end

  COMMAREA.CAFROMAP = "DH0O";
  sysVar.transferName = "DH0O";
  transfer to program sysVar.transferName passing COMMAREA;

  /* **************/
  /* ****************** END DH0OP99 *****************************/
  /* **************/
end // end DH0OP99-TERMINATE

// ADD TEMP STORAGE PAGE(RECORD)
Function DH0OS-TS-ADD-PAGE()
  /* ************************************************************/
  /* Write temporary storage*/
  /* ************************************************************/

  TA5REC.TA5APPID = "DH0O"; /* appl identification*/
  TA5REC.TA5FNCCD = "A"; /* function code*/
  DH0OW02.DAW-TEMP-STORAGE-LENGTH = 1208;

  call "TA0050" (DH0OW02, TA5REC) {isNoRefresh = yes, isExternal = yes}; /* temp storage subroutine*/

  if (TA5REC.TA5RTNCD == 0)
    DAWREC.DAW-MAX-PAGE = TA5REC.TA5TSQIX;
    set DH0OW02 empty; /* Reset for next write*/
    DAWREC.DAW-ROW-NBR = 0;
  else
    TA1REC.TA1LOCAT[1] = "ADD TSQ";
    TA1REC.TA1LOCAT[2] = "TA0050"; /* io mod called*/
    TA1REC.TA1LOCAT[3] = TA5REC.TA5RTNCD; /* return code*/
    TA1REC.TA1TBLKE = DAWREC.XXX-TBL-PFX; /* first row*/
    TAEREC.TAEDESC1 = TA5REC.TA5DATA; /* subroutine parms*/
    TA1REC.TA1TYPE = "APPL";
    TA1REC.TA1FUNC = "POST";
    TA1PGMNM = COMMAREA.CACURRAP; /* current application*/
    try
      call "TA0020" (TA1REC, TAEREC) {isExternal = yes};
    end
    DH0OM001.DAW-INFO-MESSAGE = "TEMPORARY STORAGE FAILURE - CONTACT HELP DESK";
  end

  /* *************************/
  /* ****************** END DH0OS-ADD-TSQ-PAGE ******************/
  /* *************************/
end // end DH0OS-TS-ADD-PAGE

// PURGE TEMP STORAGE FILE
Function DH0OS-TS-PURGE-FIL()
  /* ************************************************************/
  /* Purge temporary storage*/
  /* ************************************************************/

  TA5REC.TA5APPID = "DH0O"; /* appl identification*/
  TA5REC.TA5FNCCD = "P"; /* function code*/

  call "TA0050" (DH0OW02, TA5REC) {isNoRefresh = yes, isExternal = yes}; /* temp storage subroutine*/

  /* **********************/
  /* ****************** END DH0OS-PURGE-TSQ *********************/
  /* **********************/
end // end DH0OS-TS-PURGE-FIL

// READ TEMP STORAGE PAGE
Function DH0OS-TS-READ-PAGE()
  /* ************************************************************/
  /* Read temporary storage*/
  /* ************************************************************/

  DAWREC.DAW-CURR-PAGE = DH0OM001.DAW-CURR-PAGE;

  set DH0OW02 empty;

  TA5REC.TA5APPID = "DH0O"; /* appl identification*/
  TA5REC.TA5FNCCD = "R"; /* function code*/
  TA5REC.TA5TSQIX = DAWREC.DAW-CURR-PAGE; /* current page*/
  DH0OW02.DAW-TEMP-STORAGE-LENGTH = 1208;

  call "TA0050" (DH0OW02, TA5REC) {isNoRefresh = yes, isExternal = yes}; /* temp storage subroutine*/

  if (TA5REC.TA5RTNCD == 0)
     /* NEXT SENTENCE*/
  else
    TA1REC.TA1LOCAT[1] = "READ TSQ";
    TA1REC.TA1LOCAT[2] = "TA0050"; /* io mod called*/
    TA1REC.TA1LOCAT[3] = TA5REC.TA5RTNCD; /* return code*/
    TA1REC.TA1TBLKE = DAWREC.XXX-TBL-PFX; /* first row*/
    TAEREC.TAEDESC1 = TA5REC.TA5DATA; /* subroutine parms*/
    TA1REC.TA1TYPE = "APPL";
    TA1REC.TA1FUNC = "POST";
    TA1PGMNM = COMMAREA.CACURRAP; /* current application*/
    try
      call "TA0020" (TA1REC, TAEREC) {isExternal = yes};
    end
    DH0OM001.DAW-INFO-MESSAGE = "TEMPORARY STORAGE FAILURE - CONTACT HELP DESK";
  end

  /* *************************/
  /* ****************** END DH0OS-READ-TSQ-PAGE ******************/
  /* *************************/
end // end DH0OS-TS-READ-PAGE

