//*** PROGRAM=DH0O ****
// NIKE TABLE HANDLER - TABLE MAINTENANCE - Season Range by
//                                          Region
// OVERVIEW: This application is used to maintain the season
//           range by region
// 
// DB2 TABLE: DCORP.VYSN0001
// CSP TABLE: YSNTBL
// 
// DATE CREATED: 05/26/97
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   06/18/97  lraine      fix key edit
//   06/26/97  sjohn6      remove xsn edit
//   05/19/99  ECHAN       ADD NOHILITE IN DH0OP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DH0OP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DH0OP70-PROC-MAP-2
//                         DISPLAY STATUS IN DH0OM001
// 06/12/03 CNAMAN Modified    DH0OP69-EDIT-KEY -- Add check
//  for                                      EZETST=0 to avoid
//  subscript error 4038
// *********************
Program DH0O type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DH0OW01 DH0OW01; // record
  DH0OW02 DH0OW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA2REC TA2REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YSNTBL {deleteAfterUse = yes}; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use XDRTBL {deleteAfterUse = yes}; // table
  use XSETBL {deleteAfterUse = yes}; // table
  use XSNTBL {deleteAfterUse = yes}; // table
  use DH0OM.DH0OM001,  // forms
      DH0OM.DH0OM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DH0OP00-DRIVER: DH0OP00-DRIVER();
    exit program;
  end // end main
end // end DH0O

// DRIVER FOR APPLICATION
Function DH0OP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH0OP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH0OP20-BUILD-MAP2(); /* Build the second map*/
  else
    DH0OP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH0OP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH0OP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH0OP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH0OP00 *******************************/
  /* **************/
end // end DH0OP00-DRIVER

// INITIALIZE THE APPLICATION
Function DH0OP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH0OW01 empty; /* Working storage record*/
  set DH0OW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH0OM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0OM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0OM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH0OM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0OM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0OM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH0OM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH0OP10 ******************************/
  /* **************/
end // end DH0OP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH0OP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH0OP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH0OM001.DAW-CURR-PAGE)
    DH0OS-TS-READ-PAGE(); /* read temp storage page*/
    DH0OP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH0OP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0OP20 **********************************/
  /* **************/
end // end DH0OP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DH0OP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DH0OM002.DA-CHNG-USER-ID = sysVar.userID;
  DH0OM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DH0OW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0OM002.DA-PLOG-TXT1 = " ";
    DH0OM002.DA-PLOG-TXT2 = " ";
    DH0OM002.XDR-PROD-REG-CD = " ";
    DH0OM002.XGPCD = " ";
    DH0OM002.XSECD = " ";
    DH0OM002.XSNYR = 0;
    DH0OM002.XSNDESC = " ";
    DH0OM002.XSNABRV = " ";
    DH0OM002.XSN-SESN-BDT = 0;
    DH0OM002.XSN-SESN-XDT = 0;
    DH0OM002.XSNOFRDT = 0;
    DH0OM002.XSNFOBDT = 0;
    DH0OM002.XSNFOXDT = 0;
    DH0OM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DH0OW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DH0OW01.YSNGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DH0OW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DH0OW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DH0OM002.XSECD = DH0OW01.XSECD; /* move to the map*/
      DH0OM002.XGPCD = DH0OW01.XGPCD; /* move to map*/
      DH0OM002.XSNABRV = DH0OW01.XSNABRV;
      DH0OM002.XSNFOXDT = DH0OW01.XSNFOXDT;
      DH0OM002.XSNYR = DH0OW01.XSNYR;
      DH0OM002.XSN-SESN-BDT = DH0OW01.XSN-SESN-BDT;
      DH0OM002.XSN-SESN-XDT = DH0OW01.XSN-SESN-XDT;
      DH0OM002.XSNOFRDT = DH0OW01.XSNOFRDT;
      DH0OM002.XSNFOBDT = DH0OW01.XSNFOBDT;
      DH0OM002.XDR-PROD-REG-CD = DH0OW01.XDR-PROD-REG-CD;
      DH0OM002.XSNDESC = DH0OW01.XSNDESC;
      DH0OM002.XXXSTAT = DH0OW01.XXXSTAT;
      DH0OM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DH0OM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DH0OM002.XDR-PROD-REG-CD = DH0OW02.XDR-PROD-REG-CD[DAW-ROW-NBR];
        DH0OM002.XGPCD = DH0OW02.XGPCD[DAW-ROW-NBR];
        DH0OM002.XSECD = DH0OW02.XSECD[DAW-ROW-NBR];
        DH0OM002.XSNYR = DH0OW02.XSNYR[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DH0OM002.XGPCD in YSNTBL.XGPCD
         && DH0OM002.XSECD == YSNTBL.XSECD[sysVar.arrayIndex]
         && DH0OM002.XDR-PROD-REG-CD == YSNTBL.XDR-PROD-REG-CD[sysVar.arrayIndex]
         && DH0OM002.XSNYR == YSNTBL.XSNYR[sysVar.arrayIndex])
          DH0OW01.DH0OW-FND-FL = "Y";
        else
          while (DH0OW01.DH0OW-FND-FL != "Y"
           && YSNTBL.XGPCD[sysVar.arrayIndex] != "99")
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
            if (DH0OM002.XGPCD == YSNTBL.XGPCD[sysVar.arrayIndex]
             && DH0OM002.XSECD == YSNTBL.XSECD[sysVar.arrayIndex]
             && DH0OM002.XDR-PROD-REG-CD == 
            YSNTBL.XDR-PROD-REG-CD[sysVar.arrayIndex]
             && DH0OM002.XSNYR == YSNTBL.XSNYR[sysVar.arrayIndex])
              DH0OW01.DH0OW-FND-FL = "Y";
            end
          end
        end
        if (DH0OW01.DH0OW-FND-FL == "Y")
          DH0OW01.XDR-PROD-REG-CD = YSNTBL.XDR-PROD-REG-CD[sysVar.arrayIndex];
          DH0OW01.XGPCD = YSNTBL.XGPCD[sysVar.arrayIndex];
          DH0OW01.XSECD = YSNTBL.XSECD[sysVar.arrayIndex];
          DH0OW01.XSNYR = YSNTBL.XSNYR[sysVar.arrayIndex];

          DH0OW01.XSNDESC = YSNTBL.XSNDESC[sysVar.arrayIndex];
          DH0OW01.XSNABRV = YSNTBL.XSNABRV[sysVar.arrayIndex];
          DH0OW01.XSN-SESN-BDT = YSNTBL.XSN-SESN-BDT[sysVar.arrayIndex];
          DH0OW01.XSN-SESN-XDT = YSNTBL.XSN-SESN-XDT[sysVar.arrayIndex];
          DH0OW01.XSNOFRDT = YSNTBL.XSNOFRDT[sysVar.arrayIndex];
          DH0OW01.XSNFOBDT = YSNTBL.XSNFOBDT[sysVar.arrayIndex];
          DH0OW01.XSNFOXDT = YSNTBL.XSNFOXDT[sysVar.arrayIndex];
          DH0OW01.XXXSTAT = YSNTBL.XXXSTAT[sysVar.arrayIndex];
          DH0OW01.ZZZSDT = YSNTBL.ZZZSDT[sysVar.arrayIndex];
          DH0OW01.ZZZCHGTM = YSNTBL.ZZZCHGTM[sysVar.arrayIndex];
          DH0OW01.ZZZCHGDT = YSNTBL.ZZZCHGDT[sysVar.arrayIndex];
          DH0OW01.ZZZCHGCT = YSNTBL.ZZZCHGCT[sysVar.arrayIndex];

          DH0OM002.XSNABRV = YSNTBL.XSNABRV[sysVar.arrayIndex];
          DH0OM002.XSNDESC = YSNTBL.XSNDESC[sysVar.arrayIndex];
          DH0OM002.XSN-SESN-BDT = YSNTBL.XSN-SESN-BDT[sysVar.arrayIndex];
          DH0OM002.XSN-SESN-XDT = YSNTBL.XSN-SESN-XDT[sysVar.arrayIndex];
          DH0OM002.XSNOFRDT = YSNTBL.XSNOFRDT[sysVar.arrayIndex];
          DH0OM002.XSNFOBDT = YSNTBL.XSNFOBDT[sysVar.arrayIndex];
          DH0OM002.XSNFOXDT = YSNTBL.XSNFOXDT[sysVar.arrayIndex];
          DH0OM002.XXXSTAT = YSNTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DH0OW01.YSNGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DH0OW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DH0OW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DH0OM002.XSECD = DH0OW01.XSECD; /* move to the map*/
    DH0OM002.XGPCD = DH0OW01.XGPCD;
                                   /* move to the map*/
    DH0OM002.XSNABRV = DH0OW01.XSNABRV;
    DH0OM002.XSNFOXDT = DH0OW01.XSNFOXDT;
    DH0OM002.XSNYR = DH0OW01.XSNYR;
    DH0OM002.XSN-SESN-BDT = DH0OW01.XSN-SESN-BDT;
    DH0OM002.XSN-SESN-XDT = DH0OW01.XSN-SESN-XDT;
    DH0OM002.XSNOFRDT = DH0OW01.XSNOFRDT;
    DH0OM002.XSNFOBDT = DH0OW01.XSNFOBDT;
    DH0OM002.XDR-PROD-REG-CD = DH0OW01.XDR-PROD-REG-CD;
    DH0OM002.XSNDESC = DH0OW01.XSNDESC;
    DH0OM002.XXXSTAT = DH0OW01.XXXSTAT;
    DH0OM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DH0OM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DH0OM002.XXXSTAT == "A")
    DH0OM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DH0OM002.XXXSTAT == "I")
    DH0OM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DH0OM002.XXXSTAT == " ")
    DH0OM002.DAW-TBL-STATUS-DESC = " ";
  end

  DH0OP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0OP20 **********************************/
  /* **************/
end // end DH0OP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DH0OP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DH0OW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0OP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0OS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YSNTBL.XDR-PROD-REG-CD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH0OP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH0OS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DH0OS-TS-ADD-PAGE();
    end

    DH0OM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DH0OM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DH0OP22 **********************************/
  /* **************/
end // end DH0OP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH0OP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DH0OW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DH0OW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DH0OW01.YSNGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DH0OW02.XDR-PROD-REG-CD[DAW-ROW-NBR] = DH0OW01.XDR-PROD-REG-CD;
  DH0OW02.XGPCD[DAW-ROW-NBR] = DH0OW01.XGPCD;
  DH0OW02.XSECD[DAW-ROW-NBR] = DH0OW01.XSECD;
  DH0OW02.XSNYR[DAW-ROW-NBR] = DH0OW01.XSNYR;
  DH0OW02.XSNABRV[DAW-ROW-NBR] = DH0OW01.XSNABRV;
  DH0OW02.XSNDESC[DAW-ROW-NBR] = DH0OW01.XSNDESC;
  DH0OW02.XXXSTAT[DAW-ROW-NBR] = DH0OW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DH0OW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DH0OW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DH0OW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DH0OW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DH0OW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DH0OW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DH0OW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DH0OW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DH0OW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DH0OW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DH0OP221 ******************************/
  /* ***************/
end // end DH0OP23-DB2-TO-TSQ

