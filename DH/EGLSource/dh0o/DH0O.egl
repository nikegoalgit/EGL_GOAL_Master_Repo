// SET MAP 1 FIELD ATTRIBUTES
Function DH0OP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DH0OM001.DAW-PAGE-LIT = "PAGE";
    set DH0OM001.DAW-CURR-PAGE initialAttributes;
    DH0OM001.DAW-OF-LIT = "OF";
    set DH0OM001.DAW-LAST-PAGE initialAttributes;
    DH0OM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DH0OM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DH0OM001.DAW-PAGE-LIT = " ";
    set DH0OM001.DAW-CURR-PAGE skip, invisible;
    DH0OM001.DAW-OF-LIT = " ";
    set DH0OM001.DAW-LAST-PAGE skip, invisible;
    DH0OM001.DAW-PF7-LIT = " ";
    DH0OM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DH0OM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DH0OM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DH0OM001.XSECD[sysVar.arrayIndex] == " "
       && DH0OM001.XGPCD[sysVar.arrayIndex] == " ")
        set DH0OM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
      else
        set DH0OM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DH0OP25 **************************/
  /* **************/
end // end DH0OP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DH0OP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DH0OM002.XDR-PROD-REG-CD initialAttributes;
    set DH0OM002.XGPCD initialAttributes;
    set DH0OM002.XSECD initialAttributes;
    set DH0OM002.XSNYR initialAttributes;
  else
    set DH0OM002.XDR-PROD-REG-CD skip;
    set DH0OM002.XGPCD skip;
    set DH0OM002.XSECD skip;
    set DH0OM002.XSNYR skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DH0OM002.XSNDESC skip;
    set DH0OM002.XSNABRV skip;
    set DH0OM002.XSN-SESN-BDT skip;
    set DH0OM002.XSN-SESN-XDT skip;
    set DH0OM002.XSNOFRDT skip;
    set DH0OM002.XSNFOBDT skip;
    set DH0OM002.XSNFOXDT skip;
    set DH0OM002.XXXSTAT skip;
  else
    set DH0OM002.XSNDESC initialAttributes;
    set DH0OM002.XSNABRV initialAttributes;
    set DH0OM002.XSN-SESN-BDT initialAttributes;
    set DH0OM002.XSN-SESN-XDT initialAttributes;
    set DH0OM002.XSNOFRDT initialAttributes;
    set DH0OM002.XSNFOBDT initialAttributes;
    set DH0OM002.XSNFOXDT initialAttributes;
    set DH0OM002.XXXSTAT initialAttributes;
  end

  DH0OM002.XGPABRV = " ";

  set DH0OM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DH0OM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH0OM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DH0OM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DH0OM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DH0OM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DH0OM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DH0OM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DH0OP25 **************************/
  /* **************/
end // end DH0OP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DH0OP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DH0OP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DH0OM001.VAGen_EZEMSG = " ";
  DH0OM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DH0OP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DH0OP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DH0OP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DH0OP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DH0OP30 *****************************/
  /* **************/
end // end DH0OP30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DH0OP30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DH0OP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DH0OM002.VAGen_EZEMSG = " ";
