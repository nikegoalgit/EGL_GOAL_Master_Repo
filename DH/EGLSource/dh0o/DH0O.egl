  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL"
   && DH0OM002.XXXSTAT != "I")
    DH0OM002.DAW-INFO-MESSAGE = "CANNOT DELETE TABLE ROW; STATUS IS ACTIVE. PF3 TO RETURN TO LIST";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
  end


  /* **************/
  /* ***************** END DH0OP61 **********************************/
  /* **************/
end // end DH0OP62-EDIT-STAT

