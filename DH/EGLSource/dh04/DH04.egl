  DH04W01.XRL-ACCT-RLAT-DESC = DH04M002.XRL-ACCT-RLAT-DESC;
  DH04W01.XXXSTAT = DH04M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 51;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DH04W01.XRLGROUP;

