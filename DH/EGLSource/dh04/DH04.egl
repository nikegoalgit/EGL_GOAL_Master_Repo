// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   05-10-99  ECHAN       ADD NOHILITE IN DH04P25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DH04P70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DH04P70-PROC-MAP-2
//                         DISPLAY STATUS IN DH04M001
// *********************
Program DH04 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DH04W01 DH04W01; // record
  DH04W02 DH04W02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XRLTBL {deleteAfterUse = yes}; // table
  use DH04M.DH04M001,  // forms
      DH04M.DH04M002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DH04P00-DRIVER: DH04P00-DRIVER();
    exit program;
  end // end main
end // end DH04

// DRIVER FOR APPLICATION
Function DH04P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH04P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH04P20-BUILD-MAP2(); /* Build the second map*/
  else
    DH04P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH04P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH04P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH04P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH04P00 *******************************/
  /* **************/
end // end DH04P00-DRIVER

// INITIALIZE THE APPLICATION
Function DH04P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH04W01 empty; /* Working storage record*/
  set DH04W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH04M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH04M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH04M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH04M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH04M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH04M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH04M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH04P10 ******************************/
  /* **************/
end // end DH04P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH04P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH04P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH04M001.DAW-CURR-PAGE)
    DH04S-TS-READ-PAGE(); /* read temp storage page*/
    DH04P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH04P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH04P20 **********************************/
  /* **************/
end // end DH04P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DH04P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DH04M002.DA-CHNG-USER-ID = sysVar.userID;
  DH04M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DH04W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH04M002.DA-PLOG-TXT1 = " ";
    DH04M002.DA-PLOG-TXT2 = " ";
    DH04M002.XRL-ACCT-RLAT-CD = " ";
    DH04M002.XRL-ACCT-RLAT-ABRV = " ";
    DH04M002.XRL-ACCT-RLAT-DESC = " ";
    DH04M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DH04W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DH04W01.XRLGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DH04W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DH04W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

   /* move to the map*/
      DH04M002.XRL-ACCT-RLAT-CD = DH04W01.XRL-ACCT-RLAT-CD;
      DH04M002.XRL-ACCT-RLAT-ABRV = DH04W01.XRL-ACCT-RLAT-ABRV;
      DH04M002.XRL-ACCT-RLAT-DESC = DH04W01.XRL-ACCT-RLAT-DESC;
      DH04M002.XXXSTAT = DH04W01.XXXSTAT;
      DH04M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DH04M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DH04M002.XRL-ACCT-RLAT-CD = DH04W02.XRL-ACCT-RLAT-CD[DAW-ROW-NBR];
                                        /* get the tbl row*/
        if (DH04M002.XRL-ACCT-RLAT-CD in XRLTBL.XRL-ACCT-RLAT-CD)
   /* save for leter compare*/
          DH04W01.XRL-ACCT-RLAT-CD = XRLTBL.XRL-ACCT-RLAT-CD[sysVar.arrayIndex];
          DH04W01.XRL-ACCT-RLAT-ABRV = XRLTBL.XRL-ACCT-RLAT-ABRV[sysVar.arrayIndex];
          DH04W01.XRL-ACCT-RLAT-DESC = XRLTBL.XRL-ACCT-RLAT-DESC[sysVar.arrayIndex];
          DH04W01.XXXSTAT = XRLTBL.XXXSTAT[sysVar.arrayIndex];
          DH04W01.ZZZSDT = XRLTBL.ZZZSDT[sysVar.arrayIndex];
          DH04W01.ZZZCHGDT = XRLTBL.ZZZCHGDT[sysVar.arrayIndex];
          DH04W01.ZZZCHGTM = XRLTBL.ZZZCHGTM[sysVar.arrayIndex];
          DH04W01.ZZZCHGCT = XRLTBL.ZZZCHGCT[sysVar.arrayIndex];

   /* move to map*/
          DH04M002.XRL-ACCT-RLAT-ABRV = XRLTBL.XRL-ACCT-RLAT-ABRV[sysVar.arrayIndex];
          DH04M002.XRL-ACCT-RLAT-DESC = XRLTBL.XRL-ACCT-RLAT-DESC[sysVar.arrayIndex];
          DH04M002.XXXSTAT = XRLTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DH04W01.XRLGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DH04W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DH04W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

   /* move to the map*/
    DH04M002.XRL-ACCT-RLAT-CD = DH04W01.XRL-ACCT-RLAT-CD;
    DH04M002.XRL-ACCT-RLAT-ABRV = DH04W01.XRL-ACCT-RLAT-ABRV;
    DH04M002.XRL-ACCT-RLAT-DESC = DH04W01.XRL-ACCT-RLAT-DESC;
    DH04M002.XXXSTAT = DH04W01.XXXSTAT;
    DH04M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DH04M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DH04M002.XXXSTAT == "A")
    DH04M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DH04M002.XXXSTAT == "I")
    DH04M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DH04M002.XXXSTAT == " ")
    DH04M002.DAW-TBL-STATUS-DESC = " ";
  end

  DH04P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH04P20 **********************************/
  /* **************/
end // end DH04P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DH04P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DH04W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH04P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH04S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XRLTBL.XRL-ACCT-RLAT-CD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DH04P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DH04S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DH04S-TS-ADD-PAGE();
    end

    DH04M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DH04M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DH04P22 **********************************/
  /* **************/
end // end DH04P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DH04P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DH04W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DH04W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DH04W01.XRLGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DH04W02.XRL-ACCT-RLAT-CD[DAW-ROW-NBR] = DH04W01.XRL-ACCT-RLAT-CD;
  DH04W02.XRL-ACCT-RLAT-ABRV[DAW-ROW-NBR] = DH04W01.XRL-ACCT-RLAT-ABRV;
  DH04W02.XRL-ACCT-RLAT-DESC[DAW-ROW-NBR] = DH04W01.XRL-ACCT-RLAT-DESC;
  DH04W02.XXXSTAT[DAW-ROW-NBR] = DH04W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DH04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DH04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DH04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DH04W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DH04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DH04W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DH04W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DH04W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DH04W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DH04W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DH04P221 ******************************/
  /* ***************/
end // end DH04P23-DB2-TO-TSQ

