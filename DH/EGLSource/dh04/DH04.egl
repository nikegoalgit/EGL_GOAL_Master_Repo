// SET MAP 1 FIELD ATTRIBUTES
Function DH04P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DH04M001.DAW-PAGE-LIT = "PAGE";
    set DH04M001.DAW-CURR-PAGE initialAttributes;
    DH04M001.DAW-OF-LIT = "OF";
    set DH04M001.DAW-LAST-PAGE initialAttributes;
    DH04M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DH04M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DH04M001.DAW-PAGE-LIT = " ";
    set DH04M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DH04M001.DAW-OF-LIT = " ";
    set DH04M001.DAW-LAST-PAGE skip, invisible;
    DH04M001.DAW-PF7-LIT = " ";
    DH04M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DH04M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DH04M001.XRL-ACCT-RLAT-CD[sysVar.arrayIndex] == " ")
      set DH04M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DH04M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DH04P25 **************************/
  /* **************/
end // end DH04P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DH04P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DH04M002.XRL-ACCT-RLAT-CD initialAttributes;
    set DH04M002.XRL-ACCT-RLAT-ABRV initialAttributes;
    set DH04M002.XRL-ACCT-RLAT-DESC initialAttributes;
    set DH04M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DH04M002.XRL-ACCT-RLAT-CD skip;
    set DH04M002.XRL-ACCT-RLAT-ABRV skip;
    set DH04M002.XRL-ACCT-RLAT-DESC skip;
    set DH04M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DH04M002.XRL-ACCT-RLAT-CD skip;
    set DH04M002.XRL-ACCT-RLAT-ABRV initialAttributes;
    set DH04M002.XRL-ACCT-RLAT-DESC initialAttributes;
    set DH04M002.XXXSTAT initialAttributes;
  end

  set DH04M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DH04M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DH04M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DH04M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DH04M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DH04M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DH04M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DH04M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DH04P25 **************************/
  /* **************/
end // end DH04P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DH04P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DH04P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DH04M001.VAGen_EZEMSG = " ";
  DH04M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DH04P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DH04P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DH04P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DH04P70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DH04P30 *****************************/
  /* **************/
end // end DH04P30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DH04P30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DH04P40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DH04M002.VAGen_EZEMSG = " ";
  DH04M002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DH04P25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DH04P50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DH04P60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DH04P70-PROC-MAP-2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
