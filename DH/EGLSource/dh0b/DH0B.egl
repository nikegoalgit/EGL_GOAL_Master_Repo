    end
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL"
   && DH0BM002.XXXSTAT != "I")
    DH0BM002.DAW-INFO-MESSAGE = "CANNOT DELETE TABLE ROW; STATUS IS ACTIVE. PF3 TO RETURN TO LIST";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
  end


  /* **************/
  /* ***************** END DH0BP61 **********************************/
  /* **************/
end // end DH0BP62-EDIT-STAT

