// SET MAP 1 FIELD ATTRIBUTES
Function DH0BP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DH0BM001.DAW-PAGE-LIT = "PAGE";
    set DH0BM001.DAW-CURR-PAGE initialAttributes;
    DH0BM001.DAW-OF-LIT = "OF";
    set DH0BM001.DAW-LAST-PAGE initialAttributes;
    DH0BM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DH0BM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DH0BM001.DAW-PAGE-LIT = " ";
    set DH0BM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DH0BM001.DAW-OF-LIT = " ";
    set DH0BM001.DAW-LAST-PAGE skip, invisible;
    DH0BM001.DAW-PF7-LIT = " ";
    DH0BM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DH0BM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DH0BM001.YNTCD[sysVar.arrayIndex] == " ")
      set DH0BM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DH0BM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
