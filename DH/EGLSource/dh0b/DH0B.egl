// DRIVER FOR APPLICATION
Function DH0BP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DH0BP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DH0BP20-BUILD-MAP2(); /* Build the second map*/
  else
    DH0BP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DH0BP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DH0BP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DH0BP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DH0BP00 *******************************/
  /* **************/
end // end DH0BP00-DRIVER

// INITIALIZE THE APPLICATION
Function DH0BP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DH0BW01 empty; /* Working storage record*/
  set DH0BW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DH0BM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0BM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0BM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DH0BM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DH0BM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DH0BM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DH0BM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DH0BP10 ******************************/
  /* **************/
end // end DH0BP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DH0BP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DH0BP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DH0BM001.DAW-CURR-PAGE)
    DH0BS-TS-READ-FIL(); /* read temp storage page*/
    DH0BP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DH0BP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DH0BP20 **********************************/
  /* **************/
end // end DH0BP20-BUILD-MAP1

