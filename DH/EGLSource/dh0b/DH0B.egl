// BUILD NEW VDATRN01 RECORD
Function DH0BP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DH0BM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DH0BM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DH0BW01.DA-CCAT-KEY-TXT = " ";
  DH0BW01.DH0BW-KEY-YNTCD = DH0BM002.YNTCD;
  VDATRN01.DA-CCAT-KEY-TXT = DH0BW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DH0BW01.YNTCD = DH0BM002.YNTCD;
  DH0BW01.YNTPCKFL = DH0BM002.YNTPCKFL;
  DH0BW01.YNTFCSFL = DH0BM002.YNTFCSFL;
  DH0BW01.YNTLOCFL = DH0BM002.YNTLOCFL;
  DH0BW01.YNTUOSFL = DH0BM002.YNTUOSFL;
  DH0BW01.SY1UNPCS = DH0BM002.SY1UNPCS;
  DH0BW01.YNTCLSLN = DH0BM002.YNTCLSLN;
  DH0BW01.YNTCLSHG = DH0BM002.YNTCLSHG;
  DH0BW01.YNTCLSWD = DH0BM002.YNTCLSWD;
  DH0BW01.YNTCRFT3 = DH0BM002.YNTCRFT3;
  DH0BW01.YNTCRWT = DH0BM002.YNTCRWT;
  DH0BW01.YNTHNGFL = DH0BM002.YNTHNGFL;
  DH0BW01.YNT-CRTN-OVSZ-FL = DH0BM002.YNT-CRTN-OVSZ-FL;
  DH0BW01.XGPCD = DH0BM002.XGPCD;
  DH0BW01.SY1STNBR = DH0BM002.SY1STNBR;
  DH0BW01.XBCCD = DH0BM002.XBCCD;
  DH0BW01.YNTABRV = DH0BM002.YNTABRV;
  DH0BW01.YNTDESC = DH0BM002.YNTDESC;
  DH0BW01.XXXSTAT = DH0BM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 74;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DH0BW01.YNTGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DH0BP72 **********************************/
  /* **************/
end // end DH0BP72-NEW-TRAN

