package pf0g;
import corpy2.YISCD;
import pf.common.PFBISGRP;
import pf.common.PFBREF;
import pf.common.PFCREF;
import pf.common.PFDREF;
import pf.common.PFESEIND;
import pf.common.PFESRTEL;
import pf.common.PFESUMDE;
import pf.common.PFESUMNM;

//*** DATATABLE=PF0GT ****
//             ** predefined summary table **
//   this table contains predefined summaries that can be used
//   as defaults when a user is creating a new summary. the
//   user will first request to add a new summary and will then
//   enter pf13 to view the predefined summaries. the user can
//   then enter an 's' next to the predefined summary that is
//   the most similar to the one being created. the predefined
//   data from that summary is carried over to the data entry
//   screen and the user can then modify the data to create the
//   exact summary desired.
// ************************
DataTable PF0GT type basicTable  {
  shared = yes, resident = no
  }
  10 PFESUMNM PFESUMNM; 
  10 PFESUMDE PFESUMDE; 
  10 PFCREF PFCREF; 
  10 PFBREF PFBREF; 
  10 PFDREF PFDREF; 
  10 PFESRTEL PFESRTEL; 
  10 YISCD YISCD; 
  10 PFBISGRP PFBISGRP; 
  10 PFESEIND PFESEIND; 
  10 PF0GTCOL PF0GTCOL; 
   {contents = [
    ["9999999999", "                              ", 0000, 0000, 0000, "  ", 0000, 0000, " ", 09]
  ]}
end // end PF0GT

