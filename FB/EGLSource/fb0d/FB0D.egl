// Verify the entered data
Function FB0D150()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (COMMAREA.CAITEM > " ") /* Application data was entered*/

    FB0DM010.XXXMODID = "150"; /* Module identification*/
    set FB0DM010.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(40); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* Validate all the select code fields on the map--starting*/
   /* at the bottom of the map.*/
    /* 1). if the select code is non blank and not an 's' it*/
         /* is an error*/
    /* 2). if an 's' has been entered in more than one select*/
        /* code field it's an error*/
   /* ------------------------------------------------------------*/

  FB0DW00.FB0DWCNT = 0;
                                   /* Field to count number of select codes*/
  FB0DW00.FB0DWIXM = +10; /* Start edit at bottom of screen*/

  while (FB0DW00.FB0DWIXM >= 1) /* Check all the select code fields*/

    if (FB0DM010.FB0DMSEL[FB0DWIXM] == "S")
      FB0DW00.FB0DWCNT = FB0DW00.FB0DWCNT + 1;
      FB0DW00.FB0DWXM2 = +1; /* initialize second index*/
      while (FB0DW00.FB0DWXM2 <= 10)
        if (FB0DM010.FB0DMSEL[FB0DWIXM] == FB0DM010.FB0DMSEL[FB0DWXM2]
         && FB0DW00.FB0DWIXM != FB0DW00.FB0DWXM2)
          FB0DM010.XXXMODID = "150"; /* Module identification*/
          set FB0DM010.FB0DMSEL[FB0DWIXM] cursor, bold; /* highlight*/
          converseLib.validationFailed(37); /* 'multiple curves selected*/
        end
        FB0DW00.FB0DWXM2 = FB0DW00.FB0DWXM2 + 1;
      end
    else
      if (FB0DM010.FB0DMSEL[FB0DWIXM] != " ")
        FB0DM010.XXXMODID = "150"; /* Module identification*/
        set FB0DM010.FB0DMSEL[FB0DWIXM] cursor, bold; /* Highlight*/
        converseLib.validationFailed(41); /* 'Invalid value entered'*/

      end
    end

    FB0DWIXM = FB0DWIXM - 1;
  end

   /* ------------------------------------------------------------*/
   /* Indicate an error if any maintenance/change application*/
   /* has been requested and no curve has been selected.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf12 /* Application FB0p*/
   || converseVar.eventKey is pf13 /* Application FB0n*/
   || converseVar.eventKey is pf14) /* Application FB00*/

    if (FB0DW00.FB0DWCNT == +0) /* No 's' entered in any field*/
      FB0DM010.XXXMODID = "150"; /* Module identification*/
      converseLib.validationFailed(58); /* 'No curve selected for inquiry*/
    end
  end

  if (converseVar.validationMsgNum != 0)
    exit stack;
  end

   /* ------------------------------------------------------------*/
   /* Check the style number field to see if it has changed.*/
   /* If a new style number has been entered set the obtain*/
   /* data flag to 'y'.*/
   /* ------------------------------------------------------------*/

  if (FB0DM010.SY2CLRID == "   ")
    FB0DM010.SY2CLRID = "00 "; /* Init color and style to zeros*/
  end
  if (FB0DM010.XDMCD == "  ")
    FB0DM010.XDMCD = "00";
  end

  if (converseVar.eventKey is enter) /* Enter was pressed*/
    if (FB0DM010.SY1STNBR != FB0DW01.SY1STNBR /* New style number entered*/
     || FB0DM010.SY2CLRID != FB0DW01.SY2CLRID /* New color*/
     || FB0DM010.XDMCD != FB0DW01.XDMCD)      /* New dimension*/
      FB0DW00.FB0DWSWR = "Y"; /* New data request*/
    end
  end
  if (converseVar.eventKey is enter /* Enter was pressed*/
   && FB0DM010.SY1STNBR == "      ") /* No style number entered*/
    FB0DW00.FB0DWSWR = "Y"; /* New data request, to force an edit*/
  end

end // end FB0D150

// Process valid input request
Function FB0D160()
   /* ------------------------------------------------------------*/
   /* Determine what application to transfer to based upon*/
   /* pfkeys.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf12
   || converseVar.eventKey is pf13
   || converseVar.eventKey is pf14)
     /* Initialize passed data fields*/
    FBWREC.FBWKEY = " ";
    FBWREC.FB1CRVST = "0";

    FB0DW00.FB0DWIXM = +1; /* Initialize index*/

    /* ------------------------------------------------------------*/
     /* Determine index value by finding field with select code*/
    /* ------------------------------------------------------------*/
    while (FB0DW00.FB0DWIXM <= 10
     && FB0DM010.FB0DMSEL[FB0DWIXM] != "S")
      FB0DW00.FB0DWIXM = FB0DW00.FB0DWIXM + 1;
    end
    /* ------------------------------------------------------------*/
     /* Load the fbwrec prior to transfer*/
       /* (Use the value of the index to move the correct sequence*/
        /* number and curve standard for the curve selected.*/
    /* ------------------------------------------------------------*/
    FBWREC.FB1INSEQ = FB0DW00.FB1INSEQ[FB0DWIXM];
    FBWREC.FB1CRVST = FB0DW00.FB1CRVST[FB0DWIXM];
    FBWREC.FB4CRVNB = FB0DW00.FB4CRVNB[FB0DWIXM];

    /* ------------------------------------------------------------*/
       /* Load fbwrec with product curve key data*/
    /* ------------------------------------------------------------*/
     /* key fields for vcurve01 record*/
    FBWREC.XGPCD = FB0DW00.XGPCD;
    FBWREC.SY1STNBR = FB4REC.SY1STNBR;
    FBWREC.SY2CLRID = FB4REC.SY2CLRID;
    FBWREC.XDMCD = FB4REC.XDMCD;
    FBWREC.XQACD = FB4REC.XQACD;
    FBWREC.PC2INSEG = FB4REC.PC2INSEG;
    FBWREC.XSACD = FB4REC.XSACD;
    FBWREC.XPGCD = FB4REC.XPGCD;
    FBWREC.XUMCD = FB4REC.XUMCD;

    COMMAREA.CAUSERA = FBWREC.FBWGROUP; /* fbwrec to commarea*/

    if (converseVar.eventKey is pf12)
      COMMAREA.CATOAP = "FB0P"; /* to application*/
    end

    if (converseVar.eventKey is pf13)
      COMMAREA.CATOAP = "FB0N"; /* to application*/
    end

    if (converseVar.eventKey is pf14)
      COMMAREA.CATOAP = "FB0O"; /* to application*/
    end

    XSFBS01();
    FB0DM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* if bad transfer*/
    exit stack; /* return to show error msg*/
  end

   /* ------------------------------------------------------------*/
   /* If there is no new data request and the application did*/
   /* not do a pfkey transfer, the current data will be*/
   /* redisplayed. so storage areas need not be updated and*/
   /* the maps should not be cleared.*/
   /* ------------------------------------------------------------*/
  if (FB0DW00.FB0DWSWR == "0")

    return;
  end

  set FB0DW01 empty; /* M010  display storage*/
  set FB0DW02 empty; /* M020  display storage*/
  move 0 to FB0DW00.FB1INSEQ[1] for all; /* Initialize seq nbr array in storage*/

   /* ------------------------------------------------------------*/
   /* Save the key map data in storage*/
   /* ------------------------------------------------------------*/

  FB0DW01.XGPCD = FB0DW00.XGPCD; /* GPC code- map 1*/
  FB0DW01.SY1STNBR = FB0DM010.SY1STNBR; /* Style code- map 1*/
  FB0DW02.SY1STNBR = FB0DM010.SY1STNBR; /* Style code- map 2*/
  FB0DW01.SY2CLRID = FB0DM010.SY2CLRID; /* Color code- map 1*/
  FB0DW02.SY2CLRID = FB0DM010.SY2CLRID; /* Color code- map 2*/
  FB0DW01.XDMCD = FB0DM010.XDMCD; /* Dimension code- map 1*/
  FB0DW02.XDMCD = FB0DM010.XDMCD; /* Dimension code- map 2*/


   /* ------------------------------------------------------------*/
   /* Clear map input/output areas.*/
   /* ------------------------------------------------------------*/
  set FB0DM010 initial; /* M010 input/output*/
  set FB0DM020 initial; /* M020 input/output*/


end // end FB0D160

// map 2 processing
Function FB0D200()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display map 2.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Format the map output area and display the map.*/
   /* 2) Check the attention identifier used to respond.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/
   /* map 2 processing*/
   /* ------------------------------------------------------------*/

  FB0D210(); /* Format and display the map*/

  FB0D220(); /* Check attention identifier*/


end // end FB0D200

