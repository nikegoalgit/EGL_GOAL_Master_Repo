//*** PROGRAM=FB0D ****
// This application displays a list of curves ordered by date
// last maintained. The user selects the curve to be inquired
// on and goes to one of the inquiry applications via pfkeys.
// 
// A blank screen with the data fields protected and ready for
// key inputs is initially displayed.  If a product is entered
// up to ten curves will be listed for that product.
// 
// A shift right pf key will display more data about the
// curves.
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
//          02/12/89  WDISHM> New application.
//          10/01/93  pmainw  Fix commarea problem - cspv4.
//          09/27/94  JALLE1  Default GPC to '02' if not
//                            passed in from COMMAREA.
// *********************
Program FB0D type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "FF00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  FB0DW00 FB0DW00; // record
  FB0DW01 FB0DW01; // record
  FB0DW02 FB0DW02; // record
  FB1REC FB1REC; // record
  FB3REC FB3REC; // record
  FB4REC FB4REC; // record
  FB5REC FB5REC; // record
  FB6REC FB6REC; // record
  FBWREC FBWREC; // record
  PG2REC PG2REC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TAEREC TAEREC; // record
  XX0XW01 XX0XW01; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use FB0DM.FB0DM010,  // forms
      FB0DM.FB0DM020
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPFB001: XPFB001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/

    set FB0DM010 initial; /* M010 input/output*/
    set FB0DM020 initial; /* M020 input/output*/
    set FB0DW00 empty; /* Working storage*/
    set FB0DW01 empty; /* M010 display storage*/
    set FB0DW02 empty; /* M020 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set XX0XW01 empty; /* Date/time subroutine parameters*/


    if (COMMAREA.XGPCD == " ") /* gpc not entered*/
      COMMAREA.XGPCD = "02"; /* Default to '02'*/
    end

    FB0DW00.XGPCD = COMMAREA.XGPCD; /* gpc code to working storage*/


     /* ------------------------------------------------------------*/
     /* If the fbwrec contains product key data the data flag is*/
     /* set to 'y' (data requested) otherwise the flag is set to*/
     /* 'n'.*/
     /* ------------------------------------------------------------*/

    if (FBWREC.SY1STNBR > "  "
     || FBWREC.SY2CLRID > "  "
     || FBWREC.XDMCD > "  ")
      FB0DW00.FB0DWSWR = "Y"; /* data requested*/
      FB0DW01.SY1STNBR = FBWREC.SY1STNBR; /* style number to storage*/
      FB0DW02.SY1STNBR = FBWREC.SY1STNBR; /* style number to storage*/
      FB0DW01.SY2CLRID = FBWREC.SY2CLRID; /* color to storage*/
      FB0DW02.SY2CLRID = FBWREC.SY2CLRID; /* color to storage*/
      FB0DW01.XDMCD = FBWREC.XDMCD; /* dimension to storage*/
      FB0DW02.XDMCD = FBWREC.XDMCD; /* dimension to storage*/
      set FB0DM010.SY1STNBR cursor; /* set cursor to style*/
    else
      FB0DW00.FB0DWSWR = "N"; /* no data requested*/
    end


    FB0DW00.FB0DWSWM = "1"; /* Map to be displayed switch*/


     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display the maps.*/
     /* ------------------------------------------------------------*/

    goto FB0D001; /* Converse the main process*/


    FB0D001: FB0D001();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto FB0D001; /* Converse the main process*/


  end // end main
end // end FB0D

// size/pct inquiry
Function FB0D001()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) If specified, display the first map.*/
   /* 2) Otherwise, if specified, display the second map.*/
   /* 3) Otherwise, indicate error and exit to Master Menu.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  if (FB0DW00.FB0DWSWM == "1") /* Map 1 was requested*/

    FB0D100(); /* Display map 1*/

  else
    if (FB0DW00.FB0DWSWM == "2") /* Map 1 was requested*/

      FB0D200(); /* Display map 2*/

    else

      TA1REC.TA1FUNC = "RETN"; /* Termination function*/
      TA1REC.TA1TYPE = "APPL"; /* Type of error*/
      TA1REC.TA1LOC = "INVALID MAP NUMBER WAS REQUESTED";
      TA1REC.TA1MAPNO = FB0DW00.FB0DWSWM; /* Map number*/
      TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
      TA1REC.TA1PGMNO = "001"; /* Program number*/

      call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

      COMMAREA.CATOAP = "FV0M"; /* TO application*/

      XSFBS01(); /* Exit to another application*/
      FB0DM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* If bad transfer*/

      exit program; /* Severe error exit to CICS*/

    end

  end


end // end FB0D001

// map 1 processing
Function FB0D100()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/
   /* map 1 processing*/
   /* ------------------------------------------------------------*/

  FB0D110(); /* Obtain the data to display*/

  FB0D120(); /* Format and display map 1*/

  FB0D130(); /* Check attention identifier*/

  FB0D140(); /* Reset fields to normal intensity*/

  FB0D150(); /* Verify the entered data*/

  FB0D160(); /* Process valid input request*/


end // end FB0D100

// Obtain the data to display
Function FB0D110()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (FB0DW00.FB0DWSWR == "N" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* Or an error was found*/
    return; /* Return to previous process*/
  end

   /* ------------------------------------------------------------*/
   /* obtain the product curve data*/
   /* ------------------------------------------------------------*/

  FB0D111(); /* Obtain product curve data*/

  if (converseVar.validationMsgNum == 0) /* No errors found*/
    FB0DW00.FB0DWSWR = "0"; /* Set obtain data flag to no*/
  end

end // end FB0D110

// Product curve processing
Function FB0D111()
   /* ------------------------------------------------------------*/
   /* Obtain the style description and the colors description*/
   /* for the map heading.*/
   /* ------------------------------------------------------------*/

  FB0D112(); /* Obtain style and color desc*/


   /* ------------------------------------------------------------*/
   /* Clear row storage area*/
   /* ------------------------------------------------------------*/

  set FB4REC empty; /* clear row storage area*/

   /* ------------------------------------------------------------*/
   /* Format the product curve key*/
   /* ------------------------------------------------------------*/

  FB4REC.FB1INSEQ = -1; /* wild card*/
  FB4REC.XGPCD = FB0DW00.XGPCD; /* product group code*/
  FB4REC.SY1STNBR = FB0DW01.SY1STNBR; /* style number*/
  FB4REC.SY2CLRID = FB0DW01.SY2CLRID; /* color*/
  FB4REC.XDMCD = FB0DW01.XDMCD; /* dimension*/
  FB4REC.XQACD = "01";
  FB4REC.PC2INSEG = "000";
  FB4REC.XSACD = "000";
  FB4REC.XPGCD = "00";
  FB4REC.XUMCD = PG2REC.XUMCD;

   /* ------------------------------------------------------------*/
   /* IO to select and open vcurve01*/
   /* ------------------------------------------------------------*/

  FB0D113(); /* Select and open vcurve01 cursor*/

   /* ------------------------------------------------------------*/
   /* fetch the first vcurve01 row*/
   /* ------------------------------------------------------------*/

  FB0DS01(); /* Fetch vcurve01 row*/

  FB0DW00.FB0DWIXM = +1; /* Initialize map index*/

   /* ------------------------------------------------------------*/
   /* If no data to display, return a message to the user.*/
   /* Otherwise get all the vcurve01 data.*/
   /* ------------------------------------------------------------*/

  if (FB0DW00.FB0DWCCF == "Y") /* No vcurve01 data available*/

    FB0DM010.XXXMODID = "111"; /* module identification*/
    converseLib.validationFailed(18); /* 'No data to display'*/
    exit stack; /* Return to flow*/
  end

  while (FB0DW00.FB0DWCCF != "Y") /* Until end vcurve01 cursor*/
    FB0D114(); /* Fetch next vcurve01 rows*/
  end

   /* ------------------------------------------------------------*/
   /* IO to close vcurve01 set*/
   /* ------------------------------------------------------------*/
  FB0D115();

end // end FB0D111

// obtain style/color description
Function FB0D112()
   /* ------------------------------------------------------------*/
   /* Clear row storage areas, format the key*/
   /* ------------------------------------------------------------*/


  set PG2REC empty; /* Clear row storage area*/

  PG2REC.XGPCD = FB0DW00.XGPCD; /* GPC*/
  PG2REC.SY1STNBR = FB0DW01.SY1STNBR; /* Style*/
  PG2REC.SY2CLRID = FB0DW01.SY2CLRID; /* Color*/
  PG2REC.XDMCD = FB0DW01.XDMCD; /* Dimension*/

   /* ------------------------------------------------------------*/
   /* Select the row to display from the table.*/
   /* ------------------------------------------------------------*/

  try
    call "IO3120" ("S ", SQLCA, PG2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      FB0DW01.SY1STYNM = PG2REC.SY1STYNM; /* Style description*/
      FB0DW02.SY1STYNM = PG2REC.SY1STYNM; /* Style description*/
      FB0DW01.SY2CLRDS = PG2REC.SY2CLRAB; /* Color description*/
      FB0DW02.SY2CLRDS = PG2REC.SY2CLRAB; /* Color description*/

      return; /* Return to previous process*/
    else
      if (SQLCA.VAGen_SQLCODE == 100) /* or row not found*/
        /* ------------------------------------------------------------*/
         /* Indicate that no data was found to display.*/
        /* ------------------------------------------------------------*/
        FB0DM010.XXXMODID = "11A"; /* Module identification*/
        set FB0DM010.SY1STNBR bold; /* Highlight*/
        set FB0DM010.SY2CLRID bold; /* Highlight*/
        set FB0DM010.XDMCD bold; /* Highlight*/
        converseLib.validationFailed(22); /* 'No data to display'*/
        exit stack;

      end
    end

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL Commarea*/

  FB0DM010.XXXMODID = "11A"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(19); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(01); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT THE TABLE ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FB0DM010.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DPRODG  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VSYCLR01"; /* Table view name*/
  TA1REC.TA1TBLKE = PG2REC.PG2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/

end // end FB0D112

// IO to select & open vcurve01
Function FB0D113()
  FB0DW00.FB0DWCCF = "N"; /* End of vcurve01 flag*/


   /* ------------------------------------------------------------*/
   /* Select the requested set of rows from the table.*/
   /* ------------------------------------------------------------*/

  try
    call "IO3020" ("S1", SQLCA, FB4REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select table set*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FB0DM010.XXXMODID = "11B"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(19); /* 'Resources unavailable'*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(01); /* 'Error - Please nofity programming'*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "IO TO OPEN DFBUY.VCURVE01";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FB0DM010.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFBUY   "; /* Data base name*/
  TA1REC.TA1TBLVU = "VCURVE01"; /* Table view name*/
  TA1REC.TA1TBLKE = FB4REC.FB4KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/



end // end FB0D113

// fetch all vcurve01 rows
Function FB0D114()
   /* ------------------------------------------------------------*/
   /* The vcurve01 set is ordered descending by change date so*/
   /* the most current date will be the first row for an*/
   /* internal sequence number.  However each sequence number*/
   /* can have up to 3 row.  Consequently the sequence numbers*/
   /* must be saved in the working storage record and if a*/
   /* duplicate is obtained it is bypassed.*/
   /* ------------------------------------------------------------*/

  FB0DW00.FB0DWXM2 = +1;
                                   /* set index to 1(this is not map index)*/

  while (FB0DW00.FB0DWXM2 <= +10)
    if (FB4REC.FB1INSEQ == FB0DW00.FB1INSEQ[FB0DWXM2]
     && FB4REC.FB4CRVNB == FB0DW00.FB4CRVNB[FB0DWXM2])
       /* Sequence number has already been selected for display*/
      FB0DS01(); /* Fetch the next vcurve01 row*/
      return; /* Return to previous process*/

    end
    FB0DW00.FB0DWXM2 = FB0DW00.FB0DWXM2 + 1;

  end

  FB0DW00.FB1INSEQ[FB0DWIXM] = FB4REC.FB1INSEQ;
  FB0DW00.FB4CRVNB[FB0DWIXM] = FB4REC.FB4CRVNB; /* Curve number*/

   /* ------------------------------------------------------------*/
   /* Move data from vcurve01 row to storage*/
   /* ------------------------------------------------------------*/

  FB0DW01.FB4CRVNB[FB0DWIXM] = FB4REC.FB4CRVNB; /* Curve number*/

  if (FB4REC.FB4STDFL == "Y")
    FB0DW01.FB4STDFL[FB0DWIXM] = "*"; /* Standard*/
    FB0DW02.FB4STDFL[FB0DWIXM] = "*"; /* Standard*/
  else
    FB0DW01.FB4STDFL[FB0DWIXM] = " "; /* Standard*/
    FB0DW02.FB4STDFL[FB0DWIXM] = " "; /* Standard*/
  end

   /* ------------------------------------------------------------*/
   /* Move updated date to working storage to reformat for map*/
   /* display*/
   /* ------------------------------------------------------------*/

  FB0DW00.FB0DWFDT = FB4REC.ZZZCHGDT; /* Updated date*/
  FB0DW02.FB0DWUPM[FB0DWIXM] = FB0DW00.FB0DWFMM; /* Change month*/
  FB0DW02.FB0DWUPD[FB0DWIXM] = FB0DW00.FB0DWFDD; /* Change day*/
  FB0DW02.FB0DWUPY[FB0DWIXM] = FB0DW00.FB0DWFYY; /* Change year*/

   /* ------------------------------------------------------------*/
   /* Obtain remaining tables*/
   /* ------------------------------------------------------------*/

  FB0DS02(); /* Select vselhd01 row*/
  FB0DS04(); /* Get order type group(s)*/

   /* ------------------------------------------------------------*/
   /* Fetch the next vcurve01 row*/
   /* ------------------------------------------------------------*/

  FB0DS01(); /* Fetch the next vcurve01*/

  FB0DW00.FB0DWIXM = FB0DW00.FB0DWIXM + 1; /* increment map index*/

   /* ------------------------------------------------------------*/
   /* The application can display only 10 lines of data on the*/
   /* screen.  If there are more than 10 curves on the database*/
   /* set flag to yes and bypass the remaining curve data.*/
   /* ------------------------------------------------------------*/

  if (FB0DW00.FB0DWCCF == "N"
   && FB0DW00.FB0DWIXM > 10)
    FB0DW00.FB0DWCCF = "Y";
  end

end // end FB0D114

// IO to close vcurve01 set
Function FB0D115()
   /* ------------------------------------------------------------*/
   /* Close the selected set of table rows.*/
   /* ------------------------------------------------------------*/

  try
    call "IO3020" ("C1", SQLCA, FB4REC) {isNoRefresh = yes, isExternal = yes};
  end /* Close table set*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  FB0DM010.XXXMODID = "11D"; /* Module identification*/
  converseLib.validationFailed(01); /* 'Error - Please nofity programming'*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "CLOSE VCURVE01 TABLE ROWS";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FB0DM010.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFBUY    "; /* Data base name*/
  TA1REC.TA1TBLVU = "VCURVE01"; /* Table view name*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FB0D115

// Format and converse map 1
Function FB0D120()
   /* ------------------------------------------------------------*/
   /* If an error was found, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error message to display*/

    set FB0DM010 alarm; /* Indicate sound alarm*/
    FBWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If map not formated, format data from storage for display.*/
   /* ------------------------------------------------------------*/

  if (FB0DM010.XXXCURDT == 0) /* Map not formatted*/
    FB0D121(); /* Format the data on the map*/
  end

   /* ------------------------------------------------------------*/
   /* If there are no errors set attributes for select fields to*/
   /* allow entry if curves have been displayed.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum == 0)
    FB0DW00.FB0DWIXM = 1; /* Initialize line index*/

    while (FB0DW00.FB0DWIXM <= 10 /* All possible curves on screen*/
     && FB0DW01.XXXUSRID[FB0DWIXM] > " ") /* Curves displayed*/

      if (FB0DW00.FB0DWIXM == 1) /* First select field*/

        set FB0DM010.FB0DMSEL[FB0DWIXM] cursor, normal; /* Position cursor*/
      else
        set FB0DM010.FB0DMSEL[FB0DWIXM] normal; /* Unprotect*/

      end

      FB0DW00.FB0DWIXM = FB0DW00.FB0DWIXM + 1; /* Increment line index*/

    end
  end


   /* ------------------------------------------------------------*/
   /* Format any heading or other information in the map.*/
   /* ------------------------------------------------------------*/

  FB0DM010.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/
  if (FBWREC.XXXMSGNO == 9999)
    converseLib.validationFailed();
  else
    converseLib.validationFailed(FBWREC.XXXMSGNO);
  end /* Message number*/
  FB0DM010.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FB0DM010.CAITEM = COMMAREA.CAITEM; /* Application data*/

  converse FB0DM010 ;
   /* ------------------------------------------------------------*/
   /* Initialize message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FBWREC.XXXMSGNO = 0; /* Message number*/
  FB0DM010.XXXMODID = " "; /* Module identification*/
  COMMAREA.CATOAP = FB0DM010.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FB0DM010.CAITEM; /* Application data*/


end // end FB0D120

// Format the map 1 display data
Function FB0D121()

   /* ------------------------------------------------------------*/
   /* Format the heading data from the display storage.*/
   /* storage.*/
   /* ------------------------------------------------------------*/

  FB0DM010.SY1STNBR = FB0DW01.SY1STNBR; /* Style number*/
  FB0DM010.SY2CLRID = FB0DW01.SY2CLRID; /* Color identification*/
  FB0DM010.XDMCD = FB0DW01.XDMCD; /* Dimension code*/
  FB0DM010.SY1STYNM = FB0DW01.SY1STYNM; /* Style description*/
  FB0DM010.SY2CLRDS = FB0DW01.SY2CLRDS; /* Color description*/

   /* ------------------------------------------------------------*/
   /* Format the detail data from the display storage.*/
   /* ------------------------------------------------------------*/
  FB0DW00.FB0DWIXM = 1; /* Initialize line index*/

  while (FB0DW00.FB0DWIXM <= 10 /* Until screen is full*/
   && FB0DW01.XXXUSRID[FB0DWIXM] > " ") /* or no more curves*/

    /* ------------------------------------------------------------*/
     /* Set atributes for select field to allow entry if any*/
     /* curves have been displayed.*/
    /* ------------------------------------------------------------*/
    if (FB0DW00.FB0DWIXM == 1) /* First select field*/

      set FB0DM010.FB0DMSEL[FB0DWIXM] cursor, normal; /* Position cursor*/
    else
      set FB0DM010.FB0DMSEL[FB0DWIXM] normal; /* Unprotect*/

    end
    FB0DM010.FB4CRVNB[FB0DWIXM] = FB0DW01.FB4CRVNB[FB0DWIXM];
    FB0DM010.FB1DUMNB[FB0DWIXM] = FB0DW01.FB1DUMNB[FB0DWIXM];
    FB0DM010.FB4STDFL[FB0DWIXM] = FB0DW01.FB4STDFL[FB0DWIXM];
