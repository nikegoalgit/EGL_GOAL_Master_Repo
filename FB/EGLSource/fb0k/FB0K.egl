// Check attention identifier
Function FB0K300()
   /* ------------------------------------------------------------*/
   /* reset no user action taken switch*/
   /* ------------------------------------------------------------*/

  FB0KW00.FB0KWNA = "N"; /* reset switch*/

   /* ------------------------------------------------------------*/
   /* reset request for data switch*/
   /* ------------------------------------------------------------*/

  FB0KW00.FB0KWSDR = "Y"; /* reset switch*/

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (COMMAREA.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      XSFBS01();

    else

      converseLib.validationFailed(44); /* 'exit to application unsuccessful'*/
      FB0KM010.XXXMODID = "300"; /* module identification*/

    end

    /* ------------------------------------------------------------*/
     /* If transfer failed, display returned error message.*/
    /* ------------------------------------------------------------*/

    FB0KM010.XXXMODID = "300"; /* Module identification*/
    set FB0KM010.CATOAP cursor, bold; /* Highlight*/
    converseLib.validationFailed(44); /* 'exit to application unsuccessful'*/

    exit stack; /* Exit to display map*/

  end

  if (FB0KW00.FB0KWSNP == "Y") /* no product selected*/

    if (converseVar.eventKey is pf1 /* help menu*/
     || converseVar.eventKey is pf3 /* exit*/
     || converseVar.eventKey is pf5) /* prod menu*/
       /* next sentence*/

    else
      set FB0KM010.SY1STNBR bold; /* highlight*/
      converseLib.validationFailed(42); /* no product data*/
      FB0KM010.XXXMODID = "300"; /* module identification*/
      exit stack;

    end
  end

   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    COMMAREA.CATOAP = "FB0K"; /* selection criteria*/
    XSFBS01();
    FB0KM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* if bad transfer*/
    exit stack; /* return to show error msg*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level, or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1) /* PF1 was pressed*/
    COMMAREA.CAITEM = "FB0KM010"; /* Application find code*/
    COMMAREA.CATOAP = "GD01"; /* size/maint sel*/
    XSFBS01(); /* Exit to another application*/
    FB0KM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* if bad transfer*/
    exit stack; /* return to show error msg*/
  else

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FB0C"; /* size/maint sel*/
      XSFBS01();
      FB0KM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* if bad transfer*/
      exit stack; /* return to show error msg*/
    else

      if (converseVar.eventKey is pf5) /* pf5 was pressed*/
        COMMAREA.CATOAP = "FY0M"; /* prod menu*/
        XSFBS01();
        FB0KM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* if bad transfer*/
        exit stack; /* return to show error msg*/
      end
    end
  end

  if (converseVar.eventKey is pf7 /* scrolling requested*/
   || converseVar.eventKey is pf8) /* scrolling requested*/
    FB0KW00.FB0KWPGS = "Y"; /* set paging switch*/
    while (FB0KW00.FB0KWPGS != "N")
      FB0K310(); /* scroll data*/
    end
  end

  if (converseVar.eventKey is pf13) /* PF13 was pressed*/
    if (FB0KW00.FB0KWSPR != "I") /* product data not invalid*/
      COMMAREA.CATOAP = "FB0J"; /* size percent maintenance*/
      XSFBS01();
      FB0KM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* if bad transfer*/
      exit stack; /* return to show error msg*/
    end
  end

  if (converseVar.eventKey is pf15) /* PF15 was pressed*/
    if (FB0KW00.FB0KWSPR != "I") /* product data not invalid*/
      COMMAREA.CATOAP = "FB0L"; /* region percent maintenance*/
      XSFBS01();
      FB0KM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* if bad transfer*/
      exit stack; /* return to show error msg*/
    end
  end

  if (converseVar.eventKey is pf16) /* PF16 was pressed*/
    if (FB0KW00.FB0KWSPR != "I") /* product data not invalid*/
      COMMAREA.CATOAP = "FB0I"; /* copy a region*/
      XSFBS01();
      FB0KM010.VAGen_EZEMSG = COMMAREA.CAMSG; /* if bad transfer*/
      exit stack; /* return to show error msg*/
    end
  end

  if (converseVar.eventKey is enter) /* enter was pressed*/
    if (FB0KW00.FB0KWRNA == "Y") /* resources previously unavailable*/
      /* next sentence*/
    else
      FB0KW00.FB0KWNA = "Y"; /* indicate no user action taken*/
    end
    return;
  end

  FB0KM010.XXXMODID = "300"; /* Module identification*/
  converseLib.validationFailed(3); /* 'pf key not supported'*/
  exit stack; /* return to show error msg*/


end // end FB0K300

// Check for scrolling request
Function FB0K310()

  if (converseVar.eventKey is pf7)
    FB0KW00.FB0KWSDR = "N";
    if (FB0KW00.FB0KWSPR != "I") /* product data not invalid*/
      FB0KW00.FB0KWSTR = FB0KW00.FB0KWSTR - 11;
      FB0KW00.FB0KWEND = FB0KW00.FB0KWEND - 11;

      if (FB0KW00.FB0KWSTR <= 0)
        converseLib.validationFailed(6);
        FB0KW00.FB0KWSTR = FB0KW00.FB0KWSTR + 11; /* reset start*/
        FB0KW00.FB0KWEND = FB0KW00.FB0KWEND + 11; /* reset end*/
        exit stack;

      else
        FB0KW00.FB0KWMIX = 1;
        FB0KW00.FB0KWIX2 = FB0KW00.FB0KWSTR;
        while (FB0KW00.FB0KWIX2 <= FB0KW00.FB0KWEND)
          FB0K311(); /* move stored fields to map*/
          FB0KW00.FB0KWIX2 = FB0KW00.FB0KWIX2 + 1;
          FB0KW00.FB0KWMIX = FB0KW00.FB0KWMIX + 1;
        end
        exit stack;
      end
    end
  end

  if (converseVar.eventKey is pf8)
    FB0KW00.FB0KWSDR = "N";
    if (FB0KW00.FB0KWSPR != "I") /* product data not invalid*/
      FB0KW00.FB0KWSTR = FB0KW00.FB0KWSTR + 11;
      FB0KW00.FB0KWEND = FB0KW00.FB0KWEND + 11;

      if (FB0KW00.FB0KWSTR > FB0KW00.FB0KWTOT)
        converseLib.validationFailed(7);
        FB0KW00.FB0KWSTR = FB0KW00.FB0KWSTR - 11; /* reset start*/
        FB0KW00.FB0KWEND = FB0KW00.FB0KWEND - 11; /* reset end*/
        exit stack;

      else
        FB0KW00.FB0KWMIX = 1;
        FB0KW00.FB0KWIX2 = FB0KW00.FB0KWSTR;
        while (FB0KW00.FB0KWIX2 <= FB0KW00.FB0KWEND)
          FB0K311(); /* move stored fields to map*/
          FB0KW00.FB0KWIX2 = FB0KW00.FB0KWIX2 + 1;
          FB0KW00.FB0KWMIX = FB0KW00.FB0KWMIX + 1;
        end
        exit stack;
      end
    end
  end

  if (FB0KW00.FB0KWSPR == "I") /* product data invalid*/
    FB0KW00.FB0KWPGS = "N"; /* indicate paging requests completed*/
  end

  if (converseVar.eventKey not pf7 /* no scrolling requested*/
   && converseVar.eventKey not pf8) /* no scrolling requested*/
    FB0KW00.FB0KWPGS = "N"; /* indicate paging requests completed*/
  end


end // end FB0K310

