// Format and converse the map
Function FH0A-20000()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0)
    FHWREC.XXX-MSG-NBR = converseVar.validationMsgNum;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If the map must be formatted, format the data in the map.*/
   /* ------------------------------------------------------------*/
  ;
  if (FH0AW00.XXX-MAP-FMAT-FL == "Y")
    FH0A-21000();
  end
  ;
   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/
  ;
  FH0AM001.XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/
  ;
  FH0AM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FH0AM001.CAITEM = COMMAREA.CAITEM; /* Application data*/
  ;
   /* ------------------------------------------------------------*/
   /* If a message is to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/
  ;
  if (FHWREC.XXX-MSG-NBR != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ")  /* or message text*/
    ;
    if (FHWREC.XXX-MSG-NBR != 0) /* Message to display*/
      ;
      if (FHWREC.XXX-MSG-NBR == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(FHWREC.XXX-MSG-NBR);
      end /* Message number*/
    else
      FH0AM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/
      ;
    end
    ;
    FH0AM001.XXX-MSG-LOC-ID = FHWREC.XXX-MSG-LOC-ID;
                                   /* Message location*/
    ;
    set FH0AM001 alarm; /* Indicate sound alarm*/
    ;
  end
  ;
  converse FH0AM001 ;
  ;
   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/
  ;
  FHWREC.XXX-MSG-LOC-ID = " "; /* Message location*/
  FHWREC.XXX-MSG-NBR = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/
  ;
  COMMAREA.CATOAP = FH0AM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FH0AM001.CAITEM; /* Application data*/
  ;
   /* ------------------------------------------------------------*/
   /* Reset modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/
  ;
  set FH0AM001.FH0AM-NEED-YYMM initialAttributes; /* Need month*/
  set FH0AM001.XDVCD initialAttributes; /* Division code*/
  set FH0AM001.YSMXOT initialAttributes; /* Sum order type*/
  set FH0AM001.XCTCD initialAttributes; /* Country code*/
  set FH0AM001.XLCCD initialAttributes; /* Life cycle code*/
  set FH0AM001.XQACD initialAttributes; /* Quality code*/
  set FH0AM001.SY1STNBR initialAttributes; /* Style number*/
  set FH0AM001.SY2CLRID initialAttributes; /* Color identification*/
  set FH0AM001.XDMCD initialAttributes; /* Dimension code*/
  set FH0AM001.XWHCD initialAttributes; /* Warehouse code*/
  set FH0AM001.XDSCD initialAttributes; /* Direct ship code*/
  set FH0AM001.SP1ID initialAttributes; /* Sales program ID*/
  set FH0AM001.FH1PNAC initialAttributes; /* National account*/
  set FH0AM001.FH2CUCXL initialAttributes; /* Original cancel*/
  set FH0AM001.FH2MO initialAttributes; /* Original maint out*/
  set FH0AM001.FH0AM-AUTH-FUT initialAttributes; /* Original auth fut*/
  set FH0AM001.FH8ADQTY initialAttributes; /* Change quantity*/
  set FH0AM001.FH0AM-ADJ-COMNT-1 initialAttributes; /* Comments #1*/
  set FH0AM001.FH0AM-ADJ-COMNT-2 initialAttributes; /* Comments #2*/
  ;
  ;
end // end FH0A-20000

