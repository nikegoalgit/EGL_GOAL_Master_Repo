  ;
end // end FH0A-30000

// proc for pf3, pf4, or pa2
Function FH0A-32000()
   /* ------------------------------------------------------------*/
   /* If pf1, help, was pressed*/
   /* or pf3, exit, was pressed*/
   /* or pf5, main menu was pressed*/
   /* If pf1, help, was pressed*/
   /* exit to the help menu*/
   /* If pf3, exit, was pressed*/
   /* exit to the footwear history menu, fh0a*/
   /* If pf5, main menu was pressed*/
   /* exit to the main menu*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf1
   || converseVar.eventKey is pf3
   || converseVar.eventKey is pf5)
    ;
    if (converseVar.eventKey is pf1)
      COMMAREA.CAITEM = "FH0MM001"; /* appl find code*/
    end
    ;
    if (converseVar.eventKey is pf3)
      COMMAREA.CATOAP = "FH0M"; /* System Menu*/
    end
    ;
    XSFHS-EXIT(); /* Exit to another application*/
    FHWREC.XXX-MSG-LOC-ID = "32000";
    COMMAREA.CAITEM = " ";
    ;
    exit stack;
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If pa2, cancel, was pressed*/
   /* clear the map fields, set the defaults, and exit*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pa2)
    ;
    FH0AW00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/
    FH0AW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/
    ;
    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/
    ;
    set FH0AW01 empty;
    FH0AW01.XDVCD = "01";
    FH0AW01.XCTCD = "USA ";
    FH0AW01.XLCCD = "10";
    FH0AW01.XQACD = "01";
    FH0AW01.SP1ID = "*";
    FH0AW00.FH0AW-ENTER-FLG = "N";
    FH0AW00.FH0AW-PF15-FLG = "N";
    FH0AW00.FH0AW-PF16-FLG = "N";
    ;
    set FH0AM001.FH0AM-NEED-YYMM cursor;
    ;
    exit stack;
    ;
  end
  ;
  ;
end // end FH0A-32000

// Verify data that was entered
Function FH0A-40000()
   /* ------------------------------------------------------------*/
   /* If any lists were requested, display the pop up window.*/
   /* ------------------------------------------------------------*/
  ;
  if (FH0AM001.XDVCD == "?" /* division code*/
   || FH0AM001.XCTCD == "?" /* country code*/
   || FH0AM001.XLCCD == "?" /* life cycle code*/
   || FH0AM001.XQACD == "?" /* quality code*/
   || FH0AM001.XDMCD == "?" /* dimension code*/
   || FH0AM001.XWHCD == "?" /* warehouse code*/
   || FH0AM001.XDSCD == "?") /* direct ship code*/
    ;
    FH0A-41000(); /* Display corporate table lists*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/
  ;
  if (COMMAREA.CAITEM > " ")
    ;
    FHWREC.XXX-MSG-LOC-ID = "40000";
    converseLib.validationFailed(7);
    set FH0AM001.CAITEM cursor, bold;
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* Determine if any of the required fields were changed.*/
   /* ------------------------------------------------------------*/
  ;
  if (FH0AM001.FH0AM-NEED-YYMM == 0
   && FH0AM001.YSMXOT == " "
   && FH0AM001.SY1STNBR == " "
   && FH0AM001.SY2CLRID == " "
   && FH0AM001.XDMCD == " "
   && FH0AM001.XWHCD == " "
   && FH0AM001.XDSCD == " "
   && FH0AM001.FH1PNAC == " ")
    ;
    FH0A-42000(); /* Check for required key fields*/
    ;
  else
    if (FH0AM001.FH0AM-NEED-YYMM != FH0AW01.FH0AM-NEED-YYMM
     || FH0AM001.XDVCD != FH0AW01.XDVCD
     || FH0AM001.YSMXOT != FH0AW01.YSMXOT
     || FH0AM001.XLCCD != FH0AW01.XLCCD
     || FH0AM001.XQACD != FH0AW01.XQACD
     || FH0AM001.XCTCD != FH0AW01.XCTCD
     || FH0AM001.SY1STNBR != FH0AW01.SY1STNBR
     || FH0AM001.SY2CLRID != FH0AW01.SY2CLRID
     || FH0AM001.XDMCD != FH0AW01.XDMCD
     || FH0AM001.XWHCD != FH0AW01.XWHCD
     || FH0AM001.XDSCD != FH0AW01.XDSCD
     || FH0AM001.SP1ID != FH0AW01.SP1ID
     || FH0AM001.FH1PNAC != FH0AW01.FH1PNAC)
      ;
      FH0A-42000(); /* Check for required key fields*/
      FH0AW00.XXX-RQT-DATA-FL = "Y";
      ;
      if (FH0AW00.FH0AW-PF15-FLG == "Y" /* pf15 pressed*/
       || FH0AW00.FH0AW-PF16-FLG == "Y") /* pf16 pressed*/
        ;
        converseLib.validationFailed(27); /* can't modify keys*/
        FHWREC.XXX-MSG-LOC-ID = "40000";
        FH0AW00.FH0AW-PF15-FLG = "N";
        FH0AW00.FH0AW-PF16-FLG = "N";
        FH0AW00.FH0AW-ENTER-FLG = "N";
        FH0A-43000(); /* determine which key was changed*/
        ;
      end
      ;
    else
      ;
     /* ------------------------------------------------------------*/
     /* keys are the same, verify adjustment quantity and comments*/
     /* ------------------------------------------------------------*/
      ;
      if (FH0AM001.FH0AM-ADJ-COMNT-1 != FH0AW01.FH0AM-ADJ-COMNT-1
       || FH0AM001.FH0AM-ADJ-COMNT-2 != FH0AW01.FH0AM-ADJ-COMNT-2
       || FH0AM001.FH8ADQTY != FH0AW01.FH8ADQTY)
        ;
        if (converseVar.eventKey is pf13) /* pf13 was pressed*/
          ;
          converseLib.validationFailed(27); /* can't modify keys*/
          FHWREC.XXX-MSG-LOC-ID = "40000";
          FH0AW00.FH0AW-PF15-FLG = "N";
          FH0AW00.FH0AW-PF16-FLG = "N";
          FH0AW00.FH0AW-ENTER-FLG = "N";
          ;
          if (FH0AM001.FH0AM-ADJ-COMNT-1 != FH0AW01.FH0AM-ADJ-COMNT-1
           || FH0AM001.FH0AM-ADJ-COMNT-2 != FH0AW01.FH0AM-ADJ-COMNT-2)
            set FH0AM001.FH0AM-ADJ-COMNT-1 cursor, bold;
            set FH0AM001.FH0AM-ADJ-COMNT-2 bold;
          end
          if (FH0AM001.FH8ADQTY != FH0AW01.FH8ADQTY)
            set FH0AM001.FH8ADQTY cursor, bold;
          end
          ;
        else
          FH0A-44000();
          ;
        end
        ;
     /* ------------------------------------------------------------*/
     /* keys and adjustment quantity and comments are the same,*/
     /* a change may have occurred with pf15 or pf16 request.*/
     /* ------------------------------------------------------------*/
        ;
      else
        ;
        FH0A-44000();
        ;
      end
    end
  end
  ;
   /* ------------------------------------------------------------*/
   /* If any error was found*/
   /* Reset the enter flag*/
   /* Exit to display the map*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0)
    ;
    FH0AW00.FH0AW-ENTER-FLG = "N";
    ;
    exit stack;
    ;
  end
  ;
  ;
end // end FH0A-40000

