// Verify key data
Function FH0A-42000()
   /* ------------------------------------------------------------------*/
   /* Verify quality code*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.XQACD > " "           
   && FH0AM001.XQACD in XQATBL.XQACD /* code in table*/
   && XQATBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active*/
    ;
    FH0AM001.XQAABRV = XQATBL.XQAABRV[sysVar.arrayIndex];
    ;
  else
    FH0AM001.XQAABRV = " ";
    set FH0AM001.XQACD cursor, bold;
    converseLib.validationFailed(8);
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify summary order type group code.*/
   /* Only "FT", futures or "PR", prop are valid codes.*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.YSMXOT == "FT")
    FH0AM001.FH0AM-SUM-XOT-DESC = "FUTURES";
  else
    if (FH0AM001.YSMXOT == "PR")
      FH0AM001.FH0AM-SUM-XOT-DESC = "PROP   ";
    else
      FH0AM001.FH0AM-SUM-XOT-DESC = " ";
      set FH0AM001.YSMXOT cursor, bold;
      converseLib.validationFailed(21);
    end
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify production national account flag equal "Y"es or "N"o.*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.FH1PNAC == "Y"
   || FH0AM001.FH1PNAC == "N")
    ;
     /* next sentence*/
    ;
  else
    set FH0AM001.FH1PNAC cursor, bold;
    converseLib.validationFailed(24);
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify direct ship code*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.XDSCD >= " "          
   && FH0AM001.XDSCD in XDSTBL.XDSCD /* code in table*/
   && XDSTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active status*/
    ;
    FH0AM001.XDSABRV = XDSTBL.XDSABRV[sysVar.arrayIndex];
  else
    ;
    FH0AM001.XDSABRV = " ";
    set FH0AM001.XDSCD cursor, bold;
    converseLib.validationFailed(30);
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify country code*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.XCTCD > " "           
   && FH0AM001.XCTCD in XCTTBL.XCTCD /* code in table*/
   && XCTTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active*/
    ;
    FH0AM001.XCTABRV = XCTTBL.XCTABRV[sysVar.arrayIndex];
    ;
  else
    FH0AM001.XCTABRV = " ";
    set FH0AM001.XCTCD cursor, bold;
    converseLib.validationFailed(15);
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify division code.*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.XDVCD > " "           
   && FH0AM001.XDVCD in XDVTBL.XDVCD /* code in table*/
   && XDVTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active*/
    ;
    FH0AM001.XDVABRV = XDVTBL.XDVABRV[sysVar.arrayIndex];
    ;
  else
    FH0AM001.XDVABRV = " ";
    set FH0AM001.XDVCD cursor, bold;
    converseLib.validationFailed(22);
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify sales program id*/
   /* An "*" indicates all (any) sales program id*/
   /* A " " indicates no sales program id*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.SP1ID != "*"
   && FH0AM001.SP1ID != " ")
    FH0AS-IO0070-S(); /* fetch sales prgm abrv*/
  else
    FH0AM001.SP1ABRV = " ";
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify warehouse code*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.XWHCD == " ")
    ;
     /* next sentence*/
    ;
  else
    if (FH0AM001.XWHCD in XWHTBL.XWHCD /* code in table*/
     && XWHTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active status*/
      ;
      FH0AM001.XWHABRV = XWHTBL.XWHABRV[sysVar.arrayIndex];
    else
      set FH0AM001.XWHCD cursor, bold;
      FH0AM001.XWHABRV = " ";
      converseLib.validationFailed(26);
    end
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify relationship between warehouse and direct ship codes*/
   /* If a direct ship code = 1 then no warehouses code s/b entered*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.XWHCD == " ")
    if (FH0AM001.XDSCD == "1")
      ;
       /* next sentence*/
      ;
    else
      set FH0AM001.XWHCD cursor, bold;
      set FH0AM001.XDSCD bold;
      FH0AM001.XWHABRV = " ";
      FH0AM001.XDVABRV = " ";
      converseLib.validationFailed(18); /* invalid combo*/
    end
  else
    if (FH0AM001.XDSCD == "1")
      set FH0AM001.XWHCD cursor, bold;
      set FH0AM001.XDSCD bold;
      FH0AM001.XWHABRV = " ";
      FH0AM001.XDVABRV = " ";
      converseLib.validationFailed(18); /* invalid combo*/
    end
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify life cycle code*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.XLCCD > " "           
   && FH0AM001.XLCCD in XLCTBL.XLCCD /* code in table*/
   && XLCTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active*/
    ;
    FH0AM001.XLCABRV = XLCTBL.XLCABRV[sysVar.arrayIndex];
    ;
  else
    FH0AM001.XLCABRV = " ";
    set FH0AM001.XLCCD cursor, bold;
    converseLib.validationFailed(31);
  end
  ;
   /* ------------------------------------------------------------------*/
   /* Verify style/color/dim codes*/
   /* ------------------------------------------------------------------*/
  ;
  if (FH0AM001.SY1STNBR > " "
   && FH0AM001.SY2CLRID > " "
   && FH0AM001.XDMCD > " ")
    ;
    FH0AS-IO3120-S();
    ;
  else
    set FH0AM001.SY1STNBR cursor, bold;
    set FH0AM001.SY2CLRID bold;
    set FH0AM001.XDMCD bold;
    converseLib.validationFailed(14);
    FH0AM001.SY1SSTNM = " ";
    FH0AM001.SY2CLRAB = " ";
  end
  ;
   /* ------------------------------------------------------------*/
   /* Verify need month*/
   /* Verify month is between January and December*/
   /* ------------------------------------------------------------*/
  ;
  FH0AW01.FH0AM-NEED-YYMM = FH0AM001.FH0AM-NEED-YYMM;
  if (FH0AW01.FH0AW-NEED-MM >= 01
   && FH0AW01.FH0AW-NEED-MM <= 12)
    ;
     /* next sentence*/
    ;
  else
    set FH0AM001.FH0AM-NEED-YYMM cursor, bold;
    converseLib.validationFailed(12);
  end
  ;
   /* ------------------------------------------------------------*/
   /* If an error occurred*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0)
    FHWREC.XXX-MSG-LOC-ID = "42000";
  end
  ;
  ;
end // end FH0A-42000

