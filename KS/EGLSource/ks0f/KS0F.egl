  try
    call "IO6110" ("S ", SQLCA, VHSSHD01) {isNoRefresh = yes, isExternal = yes};
  end /* Select unique*/
  ;
  /* ------------------------------------------------------------*/
  /* If successful, initialize the line index and cursor switch.*/
  /* ------------------------------------------------------------*/
