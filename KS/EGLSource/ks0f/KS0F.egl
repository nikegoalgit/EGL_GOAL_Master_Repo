// Check attention identifier
Function KS0F-13000()
  /* ------------------------------------------------------------*/
  /* If CANCEL was requested, clear the map fields and exit.*/
  /* ------------------------------------------------------------*/
  ;
  KSWREC.KSW-HTS-NBR = " ";
  ;
  if (converseVar.eventKey is pakey) /* PA was pressed*/
    ;
    if (KS0FW00.KS0FW-SWR == "Y") /* Request for data pending*/
      set KS0FW01 empty; /* M001 display storage*/
    end
    ;
    KS0FW00.KS0FW-SWF = "N"; /* Format the map switch*/
    KS0FW00.KS0FW-SWR = "Y"; /* Request for data switch*/
    ;
    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If FAST PATH was requested, transfer to the application.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FM001.CATOAP > " ") /* FAST PATH was requested*/
    ;
    if (converseVar.eventKey is enter) /* ENTER was pressed*/
      ;
      KS0F-11100(); /* purge temp storage*/
      ;
      XSKS-S0001(); /* Exit to another application*/
      ;
      set KS0FM001.CATOAP cursor, bold; /* Highlight*/
      ;
    else
      ;
      KSWREC.XXXPRCNM = "13000"; /* Module ideKSification*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/
      ;
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  ;
  /* ------------------------------------------------------------*/
  /* If HELP, exit to the previous level or exit to the Master*/
  /* Menu was requested, exit to another application.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4 /* or PF4 was pressed*/
   || converseVar.eventKey is pf21) /* or PF21 was pressed*/
    ;
    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "KS0FM001";
    end
    ;
    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      KSWREC.KS-HTS-CPTR-CD = KS0FM001.KS0FM-HTS-CPTR-CD;
      KSWREC.KS-HTS-HDG-CD = KS0FM001.KS0FM-HTS-HDG-CD;
      COMMAREA.CATOAP = "KS0E"; /* To application*/
    end
    ;
    if (converseVar.eventKey is pf21) /* PF21 was pressed*/
      COMMAREA.CATOAP = "KS0A"; /* TO application*/
    end
    ;
    XSKS-S0001(); /* Exit to another application*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Determine whether a valid attention identifier was used to*/
  /* invoke the transaction.  Display an error message to the*/
  /* user if the PF key used is not supported.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* PF7  was not pressed*/
   && converseVar.eventKey not pf8   /* PF8  was not pressed*/
   && converseVar.eventKey not pf13  /* PF13 was not pressed*/
   && converseVar.eventKey not pf14  /* PF14 was not pressed*/
   && converseVar.eventKey not pf15) /* PF15 was not pressed*/
    ;
    KSWREC.XXXPRCNM = "13000"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    ;
    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  if (converseVar.eventKey is pf13) /* PF13 was pressed*/
    if (COMMAREA.CAADDFL == "N") /* chg flag = no*/
      KSWREC.XXXPRCNM = "13000"; /* Module identification*/
      converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    end
  end
  ;
  if (converseVar.eventKey is pf14) /* PF14 was pressed*/
    if (COMMAREA.CACHGFL == "N") /* chg flag = no*/
      KSWREC.XXXPRCNM = "13000"; /* Module identification*/
      converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    end
  end
  ;
end // end KS0F-13000

// Reset highlighted fields
Function KS0F-14000()
  /* ------------------------------------------------------------*/
  /* Reset all modifiable field attributes to normal intensity.*/
  /* ------------------------------------------------------------*/
  ;
  set KS0FM001.KS0FM-HTS-NBR initialAttributes; /* HTS number*/
  set KS0FM001.KS0FM-HTS-CPTR-CD initialAttributes; /* Chapter cd*/
  set KS0FM001.KS0FM-HTS-CPTR-SDESC initialAttributes; /* Chapter descr*/
  set KS0FM001.KS0FM-HTS-HDG-CD initialAttributes; /* heading cd*/
  set KS0FM001.KS0FM-HTS-HDG-SDESC initialAttributes; /* heading description*/
  set KS0FM001.KS0FM-HTS-SHDG-CD initialAttributes; /* sub-hdg cd*/
  set KS0FM001.KS0FM-HTS-SHDG-SDESC initialAttributes; /* sub-hdg description*/
  set KS0FM001.KS0FM-STAT initialAttributes; /* Status*/
  ;
   /* the attributes for the detail lines are also set in KS0F-12100*/
  ;
  KS0FW00.KS0FW-IXM = 1; /* set index to 1*/
  while (KS0FW00.KS0FW-IXM <= 13) /* for all detail lines*/
    if (KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM] > " ")
      set KS0FM001.KS0FMSEL[KS0FW-IXM] normal;
    else
      set KS0FM001.KS0FMSEL[KS0FW-IXM] initialAttributes;
    end
    set KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM] initialAttributes;
    set KS0FM001.KS0FM-DTL-HTS-SHDG-DESC[KS0FW-IXM] initialAttributes;
    KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1; /* set index up by 1*/
  end
  set KS0FM001.CATOAP initialAttributes; /* TO application*/
  set KS0FM001.CAITEM initialAttributes; /* Application data*/
  ;
end // end KS0F-14000

// Verify data that was entered
Function KS0F-15000()
  /* ------------------------------------------------------------*/
  /* Highlight any application data that was entered.*/
  /* ------------------------------------------------------------*/
  ;
  if (COMMAREA.CAITEM > " ") /* Application data was entered*/
    ;
    KSWREC.XXXPRCNM = "15000"; /* Module identification*/
    set KS0FM001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Indicate only with FAST PATH*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Determine whether a valid request to scroll was entered.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7 /* scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/
    ;
    KS0F-15100(); /* verify scroll request*/
    ;
  else
    ;
    KS0F-15200(); /* verify data in line*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If any error was found, exit to display the map.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* Error was found*/
    exit stack; /* Exit to display map*/
  end
  ;
end // end KS0F-15000

// verify scroll request
Function KS0F-15100()
  /* ------------------------------------------------------------*/
  /* If scroll back was requested, ensure that pages exist.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && KS0FW00.XXXTSPAG < 2)       /* and no previous pages*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    converseLib.validationFailed(2); /* No previous page to display*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If scroll forward was requested, ensure that pages exist.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && KS0FW00.XXXTSPAG >= KS0FW00.TA5TSQIX) /* and no more pages*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    converseLib.validationFailed(3); /* No more pages to display*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Indicate an error for any data that was modified.*/
  /* ------------------------------------------------------------*/
  ;
  KS0FW00.KS0FW-IXM = 13; /* Initialize line index*/
  ;
  while (KS0FW00.KS0FW-IXM > 0) /* Until end of lines*/
    ;
    if (KS0FM001.KS0FMSEL[KS0FW-IXM] != KS0FW01.KS0FW-SEL[KS0FW-IXM])
      ;
      KSWREC.XXXPRCNM = "15100"; /* Module identification*/
      set KS0FM001.KS0FMSEL[KS0FW-IXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(14); /* Field may not change*/
      ;
    end
    ;
    KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM - 1; /* Decrement line number*/
    ;
  end
  ;
  if (KS0FM001.KS0FM-STAT != KS0FW01.KS0FW-STAT) /* Status changed*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    set KS0FM001.KS0FM-STAT cursor, bold; /* Highlight*/
    converseLib.validationFailed(14); /* Field may not change*/
    ;
  end
  ;
  if (KS0FM001.KS0FM-HTS-SHDG-CD != KS0FW01.KS0FW-HTS-SHDG-CD) /* sub-hdg chg*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    set KS0FM001.KS0FM-HTS-SHDG-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(14); /* Field may not change*/
    ;
  end
  ;
  if (KS0FM001.KS0FM-HTS-HDG-CD != KS0FW01.KS0FW-HTS-HDG-CD) /* hdg chgd*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    set KS0FM001.KS0FM-HTS-HDG-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(14); /* Field may not change*/
    ;
  end
  ;
  if (KS0FM001.KS0FM-HTS-CPTR-CD != KS0FW01.KS0FW-HTS-CPTR-CD) /* cptr chgd*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    set KS0FM001.KS0FM-HTS-CPTR-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(14); /* Field may not change*/
    ;
  end
  ;
  if (KS0FM001.KS0FM-HTS-NBR != KS0FW01.KS0FW-HTS-NBR) /* hts # changed*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    set KS0FM001.KS0FM-HTS-NBR cursor, bold; /* Highlight*/
    converseLib.validationFailed(14); /* Field may not change*/
    ;
  end
  ;
end // end KS0F-15100

// verify data in the line
Function KS0F-15200()
  /* ------------------------------------------------------------*/
  /* If enter was pressed and an HS Number was entered, transfer*/
  /* to the application depending upon the number entered.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FM001.KS0FM-HTS-NBR > " " /* if hts nbr entered and*/
   && converseVar.eventKey is enter) /* enter pressed*/
    ;
    KS0FW00.KS0FW-HTS-NUMBER = KS0FM001.KS0FM-HTS-NBR;
    ;
    KS0F-15230(); /* verify hts number*/
    ;
    if (converseVar.validationMsgNum != 0) /* if there were any errors*/
      return; /* return to previous process*/
    end
    ;
    KS0FW00.KS0FW-HTS-NBR = KS0FM001.KS0FM-HTS-NBR;
    ;
    if (KS0FW00.KS0FW-HTS-2ND-SHDG-CD != " "
     || KS0FW00.KS0FW-HTS-1ST-SHDG-CD != " ")
      COMMAREA.CATOAP = "KS0G"; /* set destination application*/
    else
      ;
      if (KS0FW00.KS0FW-HTS-HDG-CD != " ")
        COMMAREA.CATOAP = "KS0F"; /* set destination application*/
      else
        ;
        if (KS0FW00.KS0FW-HTS-CPTR-CD != " ")
          COMMAREA.CATOAP = "KS0E"; /* set destination application*/
        end
      end
    end
    ;
    KSWREC.KSW-HTS-NBR = KS0FM001.KS0FM-HTS-NBR;
    ;
    KS0F-11100(); /* clear temp storage*/
    ;
    XSKS-S0001(); /* exit to another application*/
    ;
    KS0FW00.KS0FW-SWD = "1"; /* set map to be displayed switch*/
    ;
    exit stack;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If status was entered and is not an 'a', 'i' or ' ', then it*/
  /* is in error.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FM001.KS0FM-STAT != " ") /* if status not equal space*/
    if (KS0FM001.KS0FM-STAT != "A") /* and not equal 'a'*/
      if (KS0FM001.KS0FM-STAT != "I") /* and not equal 'i'*/
                                  /* then it's invalid*/
        KSWREC.XXXPRCNM = "15200"; /* Module identification*/
        set KS0FM001.KS0FM-STAT cursor, bold; /* Highlight*/
        converseLib.validationFailed(19); /* invalid status code*/
      end
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* Verify the heading number entered and move in short desc.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FM001.KS0FM-HTS-HDG-CD > " ")
    KS0FW00.KS0FW-HTS-HDG-CD = KS0FM001.KS0FM-HTS-HDG-CD;
    KS0F-15220(); /* select heading row*/
    if (VAGen_SQLCODE == 0) /* if there were no errors*/
      KS0FM001.KS0FM-HTS-HDG-CD = VHSHDG01.KS-HTS-HDG-CD; /* cptr cd to map*/
      KS0FM001.KS0FM-HTS-HDG-SDESC = VHSHDG01.KS-HTS-HDG-SDESC; /* short desc to map*/
      KS0FW00.KS0FW-HTS-HDG-SDESC = VHSHDG01.KS-HTS-HDG-SDESC; /* short desc to save*/
    else
      KS0FM001.KS0FM-HTS-HDG-SDESC = " ";
      converseLib.validationFailed(44); /* set error nbr*/
      set KS0FM001.KS0FM-HTS-HDG-CD cursor, bold;
    end
  else
    KS0FM001.KS0FM-HTS-HDG-SDESC = " ";
    converseLib.validationFailed(44); /* set error nbr*/
    set KS0FM001.KS0FM-HTS-HDG-CD cursor, bold;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Verify the chapter number entered and move in short desc.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FM001.KS0FM-HTS-CPTR-CD > " ")
    KS0FW00.KS0FW-HTS-CPTR-CD = KS0FM001.KS0FM-HTS-CPTR-CD;
    KS0F-15210(); /* select chapter row*/
    if (VAGen_SQLCODE == 0) /* if there were no errors*/
      KS0FM001.KS0FM-HTS-CPTR-CD = VHSCHP01.KS-HTS-CPTR-CD; /* cptr cd to map*/
      KS0FM001.KS0FM-HTS-CPTR-SDESC = VHSCHP01.KS-HTS-CPTR-SDESC; /* short desc to map*/
      KS0FW00.KS0FW-HTS-CPTR-SDESC = VHSCHP01.KS-HTS-CPTR-SDESC; /* short desc to save*/
    else
      KS0FM001.KS0FM-HTS-CPTR-SDESC = " ";
      converseLib.validationFailed(34); /* set error nbr*/
      set KS0FM001.KS0FM-HTS-CPTR-CD cursor, bold;
    end
  else
    KS0FM001.KS0FM-HTS-CPTR-SDESC = " ";
    converseLib.validationFailed(34); /* set error nbr*/
    set KS0FM001.KS0FM-HTS-CPTR-CD cursor, bold;
  end
  ;
  if (converseVar.validationMsgNum != 0) /* if there were any errors*/
    KS0FW00.KS0FW-IXM = 1; /* set index*/
    while (KS0FW00.KS0FW-IXM <= 13) /* clear detail lines*/
      KS0FM001.KS0FMSEL[KS0FW-IXM] = " ";
      set KS0FM001.KS0FMSEL[KS0FW-IXM] invisible, skip;
      KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM] = " ";
      KS0FM001.KS0FM-DTL-HTS-SHDG-DESC[KS0FW-IXM] = " ";
      KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1; /* set index up by 1*/
    end
    return; /* return to previous process*/
  end
  ;
  ;
  if (converseVar.validationMsgNum != 0) /* if there were any errors*/
    return; /* return to previous process*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* If selection was entered and is not a "S", indicate that*/
  /* only "S" may be entered to select a line.*/
  /* ------------------------------------------------------------*/
  ;
  KS0FW00.KS0FW-IXM = 13; /* set index*/
  ;
  while (KS0FW00.KS0FW-IXM >= 1)
    if (KS0FM001.KS0FMSEL[KS0FW-IXM] != KS0FW01.KS0FW-SEL[KS0FW-IXM]
     && KS0FM001.KS0FMSEL[KS0FW-IXM] != "S") /* and is not a "S"*/
      ;
      KSWREC.XXXPRCNM = "15200"; /* Module identification*/
      set KS0FM001.KS0FMSEL[KS0FW-IXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(13); /* Only "S" may be entered*/
      ;
    end
    KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM - 1; /* decrement index*/
  end
  ;
end // end KS0F-15200

// select a chapter row
Function KS0F-15210()
  /* ------------------------------------------------------------*/
  /* select table row*/
  /* ------------------------------------------------------------*/
  ;
  set VHSCHP01 empty; /* clear work record*/
  ;
  VHSCHP01.KS-HTS-CPTR-CD = KS0FM001.KS0FM-HTS-CPTR-CD; /* cptr cd*/
  ;
  try
    call "IO6080" ("S ", SQLCA, VHSCHP01) {isNoRefresh = yes, isExternal = yes};
  end /* select row*/
  ;
  /* ------------------------------------------------------------*/
  /* If row exists, return*/
  /* If data not found, return an error message to the user.*/
  /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      return; /* Return to previous process*/
    else
      if (SQLCA.VAGen_SQLCODE == 100) /* row not found*/
        converseLib.validationFailed(34); /* set error msg*/
        KS0FW00.KS0FW-SWR = "N"; /* do not format the map*/
        KS0FM001.KS0FM-HTS-CPTR-SDESC = " "; /* clear short desc*/
        set KS0FM001.KS0FM-HTS-CPTR-CD cursor, bold; /* set cursor*/
        KS0FW00.KS0FW-IXM = 1; /* set index to 1*/
        while (KS0FW00.KS0FW-IXM <= 13) /* for all detail lines*/
          set KS0FM001.KS0FMSEL[KS0FW-IXM] invisible, skip;
          KS0FM001.KS0FMSEL[KS0FW-IXM] = " ";
          KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM] = " ";
          KS0FM001.KS0FM-DTL-HTS-SHDG-DESC[KS0FW-IXM] = " ";
          KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1; /* set index up by 1*/
        end
        return; /* Return to previous process*/
      end
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/
  ;
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
  ;
  KSWREC.XXXPRCNM = "15210"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(11); /* Data in use by another*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    VGVar.handleSysLibraryErrors = 0;
    sysLib.rollback(); /* Roll back all changes*/
    ;
    converseLib.validationFailed(10); /* Unexpected error occurred*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT CHAPTER ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = KSWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1DBASE = "DCUSTM  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VHSCHP01"; /* Table view name*/
  TA1REC.TA1TBLKE = VHSCHP01.KS-HTS-CPTR-CD; /* Table key*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
end // end KS0F-15210

// select a heading row
Function KS0F-15220()
  /* ------------------------------------------------------------*/
  /* select table row*/
  /* ------------------------------------------------------------*/
  ;
  set VHSHDG01 empty; /* clear work record*/
  ;
  VHSHDG01.KS-HTS-CPTR-CD = KS0FM001.KS0FM-HTS-CPTR-CD; /* cptr cd*/
  VHSHDG01.KS-HTS-HDG-CD = KS0FM001.KS0FM-HTS-HDG-CD; /* hdg cd*/
  ;
  try
    call "IO6100" ("S ", SQLCA, VHSHDG01) {isNoRefresh = yes, isExternal = yes};
  end /* select row*/
  ;
  /* ------------------------------------------------------------*/
  /* If row exists, return*/
  /* If data not found, return an error message to the user.*/
  /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      return; /* Return to previous process*/
    else
      if (SQLCA.VAGen_SQLCODE == 100) /* row not found*/
        converseLib.validationFailed(44); /* set error msg*/
        KS0FW00.KS0FW-SWR = "N"; /* do not format the map*/
        KS0FM001.KS0FM-HTS-HDG-SDESC = " "; /* clear short desc*/
        set KS0FM001.KS0FM-HTS-HDG-CD cursor, bold; /* set cursor*/
        KS0FW00.KS0FW-IXM = 1; /* set index to 1*/
        while (KS0FW00.KS0FW-IXM <= 13) /* for all detail lines*/
          set KS0FM001.KS0FMSEL[KS0FW-IXM] invisible, skip;
          KS0FM001.KS0FMSEL[KS0FW-IXM] = " ";
          KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM] = " ";
          KS0FM001.KS0FM-DTL-HTS-SHDG-DESC[KS0FW-IXM] = " ";
          KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1; /* set index up by 1*/
        end
        return; /* Return to previous process*/
      end
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/
  ;
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
  ;
  KSWREC.XXXPRCNM = "15220"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(11); /* Data in use by another*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    VGVar.handleSysLibraryErrors = 0;
    sysLib.rollback(); /* Roll back all changes*/
    ;
    converseLib.validationFailed(10); /* Unexpected error occurred*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  KS0FW01.KS0FW-STAT = KS0FW00.KS0FW-STAT; /* default status code*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT HEADING ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = KSWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1DBASE = "DCUSTM  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VHSHDG01"; /* Table view name*/
  TA1REC.TA1TBLKE = VHSHDG01.KS-HTS-HDG-CD; /* Table key*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
end // end KS0F-15220

// verify format of hts number
Function KS0F-15230()
  /* ------------------------------------------------------------*/
  /* Verify the format of the HTS nbr entered.*/
  /* ------------------------------------------------------------*/
  /*  */
  /* ------------------------------------------------------------*/
  /* correct format is ===> 9999.99.9999*/
  /* this routine verifies that there is a ' ' or '.' in positions*/
  /* 5 and 8. If any of the other positions contain a '.', it*/
  /* is in error.*/
  /* ------------------------------------------------------------*/
  ;
  KS0FW00.KS0FW-IXM = 1; /* initialize index*/
  ;
  while (KS0FW00.KS0FW-IXM <= 14)
    if (KS0FW00.KS0FW-IXM == 5 /* these 3 positions must be blank or*/
     || KS0FW00.KS0FW-IXM == 8 /* equal '.'. if not, it's in error*/
     || KS0FW00.KS0FW-IXM == 14)
      if (KS0FW00.KS0FW-IXM == 5 /* these 3 positions must be blank or*/
       || KS0FW00.KS0FW-IXM == 8) /* equal '.'. if not, it's in error*/
        if (KS0FW00.KS0FW-HTS-BYTE[KS0FW-IXM] != " ")
          if (KS0FW00.KS0FW-HTS-BYTE[KS0FW-IXM] != ".")
            KSWREC.XXXPRCNM = "15230"; /* Module id*/
            set KS0FM001.KS0FM-HTS-NBR cursor, bold;
            converseLib.validationFailed(35); /* Invalid HTS nbr*/
          end
        end
      else
        if (KS0FW00.KS0FW-IXM == 14) /* equal ' '. if not, it's in error*/
          if (KS0FW00.KS0FW-HTS-BYTE[KS0FW-IXM] != " ")
            KSWREC.XXXPRCNM = "15230"; /* Module id*/
            set KS0FM001.KS0FM-HTS-NBR cursor, bold;
            converseLib.validationFailed(35); /* Invalid HTS nbr*/
          end
        end
      end
    else
      if (KS0FW00.KS0FW-HTS-BYTE[KS0FW-IXM] == ".")
        KSWREC.XXXPRCNM = "15230"; /* Module id*/
        set KS0FM001.KS0FM-HTS-NBR cursor, bold;
        converseLib.validationFailed(35); /* Invalid HTS nbr*/
      end
    end
    KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1; /* set index up by 1*/
  end
  ;
end // end KS0F-15230

// Process valid input request
Function KS0F-16000()
  /* ------------------------------------------------------------*/
  /* Indicate that the map must be formatted before display.*/
  /* ------------------------------------------------------------*/
  ;
  KS0FW00.KS0FW-SWF = "Y"; /* Format the map switch*/
  ;
  /* ------------------------------------------------------------*/
  /* If a scroll backward was requested, decrement the page*/
  /* number and return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7) /* Scroll back requested*/
    ;
    KS0FW00.XXXTSPAG = KS0FW00.XXXTSPAG - 1; /* Decrement page number*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If a scroll forward was requested, increment the page*/
  /* number and return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf8) /* Scroll forward requested*/
    ;
    KS0FW00.XXXTSPAG = KS0FW00.XXXTSPAG + 1; /* Increment page number*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* if enter was pressed and a sub-hdg was entered, find that sub-hdg*/
  /* in the table and position the cursor on that sub-hdg.*/
  /*  */
  /* if enter was pressed and a selection was made, move selected*/
  /* chapter, heading and sub-heading numbers to kswrec and xfer to KS0g.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is enter) /* enter pressed?*/
    ;
     /* If any key field was entered, we need to reformat the map*/
     /* with new data. If a sub-hdg was entered, we need to position*/
     /* the cursor on that sub-hdg.*/
    ;
    if (KS0FM001.KS0FM-HTS-CPTR-CD != KS0FW01.KS0FW-HTS-CPTR-CD /* cptr*/
     || KS0FM001.KS0FM-HTS-HDG-CD != KS0FW01.KS0FW-HTS-HDG-CD   /* cptr*/
     || KS0FM001.KS0FM-STAT != KS0FW01.KS0FW-STAT)              /* or status changed*/
      KS0FW00.KS0FW-SWF = "Y"; /* format the map*/
      KS0FW00.KS0FW-SWR = "Y"; /* yes, request new data*/
      KS0FW00.KS0FW-HTS-CPTR-CD = KS0FM001.KS0FM-HTS-CPTR-CD;
      KS0FW00.KS0FW-HTS-HDG-CD = KS0FM001.KS0FM-HTS-HDG-CD;
      if (KS0FM001.KS0FM-HTS-SHDG-CD > " ") /* sub-hdg cd entered?*/
        KS0FW00.KS0FW-CPTR-BKDN = KS0FM001.KS0FM-HTS-CPTR-CD; /* save cptr cd*/
        KS0FW00.KS0FW-HDG-BKDN = KS0FM001.KS0FM-HTS-HDG-CD; /* save hdg cd*/
        KS0FW00.KS0FW-SHDG-BKDN = KS0FM001.KS0FM-HTS-SHDG-CD; /* save sub-hdg cd*/
        KS0FW00.KS0FW-LAST-CHGD-SHDG = KS0FW00.KS0FW-CPTR-HDG-SHDG-BKDN; /* save cptr/hdg/sub-hdg cds*/
      else
        KS0FW00.KS0FW-LAST-CHGD-SHDG = " ";
      end
      KS0FW00.KS0FW-STAT = KS0FM001.KS0FM-STAT; /* save new status*/
      set KS0FW01 empty; /* clear tsq rec area*/
      return; /* return*/
    end
     /* if a sub-hdg was entered, we need to position the cursor*/
     /* on that sub-hdg.*/
    ;
    if (KS0FM001.KS0FM-HTS-SHDG-CD > " ") /* sub-hdg cd entered?*/
      KS0FW00.KS0FW-CPTR-BKDN = KS0FM001.KS0FM-HTS-CPTR-CD; /* save cptr cd*/
      KS0FW00.KS0FW-HDG-BKDN = KS0FM001.KS0FM-HTS-HDG-CD; /* save hdg cd*/
      KS0FW00.KS0FW-SHDG-BKDN = KS0FM001.KS0FM-HTS-SHDG-CD; /* save sub-hdg cd*/
      KS0FW00.KS0FW-LAST-CHGD-SHDG = KS0FW00.KS0FW-CPTR-HDG-SHDG-BKDN; /* save cptr/hdg/sub-hdg cds*/
      KS0FW00.KS0FW-SWR = "Y"; /* yes, request new data*/
      KS0FW00.KS0FW-SWF = "Y"; /* format the map*/
      return; /* return*/
    end
    ;
    KS0FW00.KS0FW-IXM = 1; /* reset index*/
    while (KS0FW00.KS0FW-IXM <= 13) /* Until end of lines*/
      ;
      if (KS0FM001.KS0FMSEL[KS0FW-IXM] == "S") /* this line selected?*/
        ;
        KS0FW00.KS0FW-CPTR-HDG-SHDG-BKDN = KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM]; /* work area*/
        KSWREC.KS-HTS-CPTR-CD = KS0FW00.KS0FW-CPTR-BKDN; /* chapter cd*/
        KSWREC.KS-HTS-HDG-CD = KS0FW00.KS0FW-HDG-BKDN; /* heading cd*/
        KSWREC.KSW-HTS-SHDG-CD = KS0FW00.KS0FW-SHDG-BKDN; /* sub-hdg cd*/
        KS0F-11100(); /* clear temp storage*/
        ;
        COMMAREA.CATOAP = "KS0G"; /* set destination application*/
        ;
        XSKS-S0001(); /* exit to another application*/
        ;
        KS0FW00.KS0FW-SWD = "1"; /* set map to be displayed switch*/
        ;
        exit stack;
        ;
      end
      KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1; /* Increment line index*/
    end
    ;
  end
  ;
  if (converseVar.eventKey is pf13) /* F13 pressed?*/
    KS0FW00.KS0FW-SWD = "2"; /* Map to be displayed switch*/
    KS0FW00.KS0FW-FNC = "A"; /* set function switch*/
    KS0FW00.KS0FW-SAVE-ADD-CNT = 0; /* set add cntr to control*/
                                         /* msg on map 2*/
    KS0FW00.KS0FW-HTS-CPTR-CD = KS0FM001.KS0FM-HTS-CPTR-CD;
    KS0FW00.KS0FW-HTS-HDG-CD = KS0FM001.KS0FM-HTS-HDG-CD;
  end
  ;
  KS0FW00.KS0FW-NEXT-SEL-SHDG = " "; /* clear sel field save*/
  ;
  if (converseVar.eventKey is pf14) /* F14 pressed?*/
    KS0FW00.KS0FW-SWR = "Y"; /* data is requested*/
    KS0FW00.KS0FW-SWD = "2"; /* Map to be displayed switch*/
    KS0FW00.KS0FW-FNC = "C"; /* set function switch*/
    KS0FW00.KS0FW-IXM = 1; /* reset index*/
    while (KS0FW00.KS0FW-IXM <= 13) /* Until end of lines*/
      ;
      if (KS0FM001.KS0FMSEL[KS0FW-IXM] == "S") /* this line selected?*/
        if (KS0FW00.KS0FW-NEXT-SEL-SHDG == " ") /* first one blank*/
          KS0FW00.KS0FW-SELECTED-SHDGS[KS0FW-IXM] = KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM]; /* save selected shdgs*/
          KS0FW00.KS0FW-NEXT-SEL-SHDG = KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM]; /* save next sub-hdg to change*/
          KS0FW00.KS0FW-SEL-INDEX = KS0FW00.KS0FW-IXM; /* selected idx*/
        else
          KS0FW00.KS0FW-SELECTED-SHDGS[KS0FW-IXM] = KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM]; /* save selected hdgs*/
        end
      end
      KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1; /* Increment line index*/
      ;
    end
    if (KS0FW00.KS0FW-NEXT-SEL-SHDG == " ") /* if none were selected*/
      converseLib.validationFailed(43); /* set error msg nbr*/
      KS0FW00.KS0FW-SWR = "N"; /* do not request data*/
      KS0FW00.KS0FW-SWF = "N"; /* do not format map*/
      KS0FW00.KS0FW-SWD = "1"; /* Map to be displayed switch*/
      return; /* return*/
    end
  end
  ;
  if (converseVar.eventKey is pf15) /* F15 pressed?*/
    set KSWREC empty; /* clear pass record*/
    KSWREC.KS-HTS-CPTR-CD = KS0FM001.KS0FM-HTS-CPTR-CD;
    KSWREC.KS-HTS-HDG-CD = KS0FM001.KS0FM-HTS-HDG-CD;
    ;
    KS0FW00.KS0FW-IXM = 1; /* reset index*/
    while (KS0FW00.KS0FW-IXM <= 13) /* Until end of lines*/
      ;
      if (KS0FM001.KS0FMSEL[KS0FW-IXM] == "S") /* this line selected?*/
        ;
        KSWREC.KSW-HTS-NBR = KS0FM001.KS0FM-DTL-HTS-SHDG-CD[KS0FW-IXM];
        KS0FW00.KS0FW-IXM = 14; /* end the search*/
        ;
      end
      KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1; /* Increment line index*/
    end
    KS0F-11100(); /* clear temp storage*/
    ;
    COMMAREA.CATOAP = "KS0G"; /* set destination application*/
    ;
    XSKS-S0001(); /* exit to another application*/
    ;
    KS0FW00.KS0FW-SWD = "1"; /* set map to be displayed switch*/
    ;
    exit stack;
    ;
  end
  ;
  if (converseVar.eventKey is pf21) /* PF21 pressed?*/
    ;
    COMMAREA.CATOAP = "KS0A"; /* set destination application*/
    ;
    KS0F-11100(); /* clear temp storage*/
    ;
    XSKS-S0001(); /* exit to another application*/
    ;
    KS0FW00.KS0FW-SWD = "1"; /* set map to be displayed switch*/
    ;
    exit stack;
  end
end // end KS0F-16000

// display the selected table row
Function KS0F-20000()
  /* ------------------------------------------------------------*/
  /* This is the main process used to display a map.  Processing*/
  /* always returns to the top of this process and falls through*/
  /* subordinate processes until an error is found or a valid*/
  /* request is processed.  Processing sequence is as follows:*/
  /*  */
  /* 1) Obtain the data to display.*/
  /* 2) Format the map output area and display the map.*/
  /* 3) Check the attention identifier used to respond.*/
  /* 4) Reset highlighted fields to normal intensity.*/
  /* 5) Verify the data that was entered.*/
  /* 6) Process a valid input request.*/
  /*  */
  /* In all subordinate processes, "EZEFLO" is used to exit to*/
  /* redisplay the map when an error is found.  This is done to*/
  /* minimize the coding effort required to skip subsequent*/
  /* processing under error conditions.*/
  /* ------------------------------------------------------------*/
  ;
  KS0F-21000(); /* Obtain the data to display*/
  ;
  KS0F-22000(); /* Format and display the map*/
  ;
  KS0F-23000(); /* Check attention identifier*/
  ;
  KS0F-24000(); /* Reset fields to normal intensity*/
  ;
  KS0F-25000(); /* Verify the data that was entered*/
  ;
  KS0F-26000(); /* Process valid input request*/
  ;
  ;
end // end KS0F-20000

// obtain the data to display
Function KS0F-21000()
  /* ------------------------------------------------------------*/
  /* If data was not requested or an error was found, return to*/
  /* the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FW00.KS0FW-SWR != "Y") /* Data was not requested*/
    if (converseVar.validationMsgNum != 0) /* or an error was found*/
      return; /* Return to previous process*/
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* if an add function was requested, there will be no data to*/
  /* display, so return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FW00.KS0FW-FNC == "A") /* if add function ?*/
    return;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Clear the map display storage area and save the key fields.*/
  /* ------------------------------------------------------------*/
  ;
  set KS0FW02 empty; /* M002 display storage*/
  ;
  /* ------------------------------------------------------------*/
  /* Obtain the table row to be displayed.*/
  /* ------------------------------------------------------------*/
  ;
  KS0F-21100(); /* Obtain the table row*/
  ;
  /* ------------------------------------------------------------*/
  /* Save the data from the table row in display storage.*/
  /* ------------------------------------------------------------*/
  ;
  if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
    ;
    KS0F-21200(); /* Save in display storage*/
    ;
  else
    ;
    KSWREC.XXXPRCNM = "21000"; /* Module identification*/
    KSWREC.XXXMSGNO = 18; /* No data was found to display*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Indicate that the request for data is complete.*/
  /* ------------------------------------------------------------*/
  ;
  KS0FW00.KS0FW-SWR = "N"; /* Request for data switch*/
  ;
end // end KS0F-21000

// select requested table row
Function KS0F-21100()
