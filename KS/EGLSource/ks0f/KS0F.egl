// verify data that was entered
Function KS0F-25000()
  /* ------------------------------------------------------------*/
  /* Highlight any application data that was entered.*/
  /* ------------------------------------------------------------*/
  ;
  if (COMMAREA.CAITEM > " ") /* Application data was entered*/
    ;
    KSWREC.XXXPRCNM = "25000"; /* Module identification*/
    set KS0FM002.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/
    ;
  end
  ;
  KS0FW00.KS0FW-DATA-CHGD-SW = "Y"; /* initialize change switch*/
  ;
  /* ------------------------------------------------------------*/
  /* If this is a change function and*/
  /* If data on the screen was not changed, set switch to 'n'.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FW00.KS0FW-FNC == "C") /* if change function*/
    if (KS0FM002.KS0FM-HTS-SHDG-SDESC == 
    KS0FW02.KS0FW-HTS-SHDG-SDESC   /* if all data*/
     && KS0FM002.KS0FM-HTS-SHDG-DESC[1] == 
    KS0FW02.KS0FW-HTS-SHDG-DESC[1] /* the same ?*/
     && KS0FM002.KS0FM-HTS-SHDG-DESC[2] == 
    KS0FW02.KS0FW-HTS-SHDG-DESC[2] 
     && KS0FM002.KS0FM-HTS-SHDG-DESC[3] == 
    KS0FW02.KS0FW-HTS-SHDG-DESC[3] 
     && KS0FM002.KS0FM-HTS-SHDG-DESC[4] == 
    KS0FW02.KS0FW-HTS-SHDG-DESC[4] 
     && KS0FM002.KS0FM-HTS-SHDG-DESC[5] == 
    KS0FW02.KS0FW-HTS-SHDG-DESC[5] 
     && KS0FM002.KS0FM-HTS-SHDG-DESC[6] == 
    KS0FW02.KS0FW-HTS-SHDG-DESC[6] 
     && KS0FM002.KS0FM-HTS-SHDG-DESC[7] == 
    KS0FW02.KS0FW-HTS-SHDG-DESC[7] 
     && KS0FM002.KS0FM-STAT ==     
    KS0FW02.KS0FW-STAT)           
      KS0FW00.KS0FW-DATA-CHGD-SW = "N"; /* indicate data unchgd*/
      return;
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* If status was changed, verify the entered code.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FM002.KS0FM-STAT == " ") /* if status not entered*/
    KS0FM002.KS0FM-STAT = "A"; /* default to 'a'*/
  end
  ;
  if (KS0FM002.KS0FM-STAT != "A") /* if status not equal to 'a' and*/
    if (KS0FM002.KS0FM-STAT != "I") /* if status not equal to 'i'*/
      KS0FM002.KS0FM-STAT-DESC = " "; /* clear descr field*/
      set KS0FM002.KS0FM-STAT cursor, bold; /* highlight*/
      converseLib.validationFailed(23); /* set msg nbr*/
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* If description lines were entered, format them so that there*/
  /* are no blank lines. (ie. if user enters only the last line,*/
  /* then move that line to line 1).*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FM002.KS0FM-HTS-SHDG-DESC[1] == " ") /* if all 5 lines*/
    if (KS0FM002.KS0FM-HTS-SHDG-DESC[2] == " ") /* blank*/
      if (KS0FM002.KS0FM-HTS-SHDG-DESC[3] == " ")
        if (KS0FM002.KS0FM-HTS-SHDG-DESC[4] == " ")
          if (KS0FM002.KS0FM-HTS-SHDG-DESC[5] == " ")
            if (KS0FM002.KS0FM-HTS-SHDG-DESC[6] == " ")
              if (KS0FM002.KS0FM-HTS-SHDG-DESC[7] == " ")
                set KS0FM002.KS0FM-HTS-SHDG-DESC[1] cursor, bold;
                converseLib.validationFailed(52); /* set msg nbr*/
              end
            end
          end
        end
      end
    end
  end
  ;
  KS0FW00.KS0FW-LINE[1] = KS0FM002.KS0FM-HTS-SHDG-DESC[1];
  KS0FW00.KS0FW-LINE[2] = KS0FM002.KS0FM-HTS-SHDG-DESC[2];
  KS0FW00.KS0FW-LINE[3] = KS0FM002.KS0FM-HTS-SHDG-DESC[3];
  KS0FW00.KS0FW-LINE[4] = KS0FM002.KS0FM-HTS-SHDG-DESC[4];
  KS0FW00.KS0FW-LINE[5] = KS0FM002.KS0FM-HTS-SHDG-DESC[5];
  KS0FW00.KS0FW-LINE[6] = KS0FM002.KS0FM-HTS-SHDG-DESC[6];
  KS0FW00.KS0FW-LINE[7] = KS0FM002.KS0FM-HTS-SHDG-DESC[7];
  ;
  KS0FM002.KS0FM-HTS-SHDG-DESC[1] = " "; /* clear screen lines*/
  KS0FM002.KS0FM-HTS-SHDG-DESC[2] = " ";
  KS0FM002.KS0FM-HTS-SHDG-DESC[3] = " ";
  KS0FM002.KS0FM-HTS-SHDG-DESC[4] = " ";
  KS0FM002.KS0FM-HTS-SHDG-DESC[5] = " ";
  KS0FM002.KS0FM-HTS-SHDG-DESC[6] = " ";
  KS0FM002.KS0FM-HTS-SHDG-DESC[7] = " ";
  ;
  KS0FW00.KS0FW-IX1 = 1; /* set index for w-s*/
  KS0FW00.KS0FW-IX2 = 1; /* set index for screen*/
  ;
  /* ------------------------------------------------------------*/
  /* the following code will compress the description lines from*/
  /* working-storage to the screen.*/
  /* ------------------------------------------------------------*/
  ;
  while (KS0FW00.KS0FW-IX1 <= 7)
    if (KS0FW00.KS0FW-LINE[KS0FW-IX1] == " ") /* if this line blank*/
      KS0FW00.KS0FW-IX1 = KS0FW00.KS0FW-IX1 + 1; /* set index up by 1*/
    else
      KS0FM002.KS0FM-HTS-SHDG-DESC[KS0FW-IX2] = KS0FW00.KS0FW-LINE[KS0FW-IX1];
      KS0FW00.KS0FW-IX1 = KS0FW00.KS0FW-IX1 + 1; /* set index up by 1*/
      KS0FW00.KS0FW-IX2 = KS0FW00.KS0FW-IX2 + 1; /* set index up by 1*/
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* if the sub-hdg number short description not entered,*/
  /* indicate error.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FM002.KS0FM-HTS-SHDG-SDESC == " ") /* if equal spaces*/
    set KS0FM002.KS0FM-HTS-SHDG-SDESC cursor, bold; /* highlight*/
    converseLib.validationFailed(51); /* set msg nbr*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* if add requested and the sub-hdg number is not entered,*/
  /* indicate error.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FW00.KS0FW-FNC == "A") /* if add requested*/
    if (KS0FM002.KS0FM-HTS-SHDG-CD == " ") /* if equal spaces*/
      set KS0FM002.KS0FM-HTS-SHDG-CD cursor, bold; /* highlight*/
      converseLib.validationFailed(50); /* set msg nbr*/
    else
      KS0FW00.KS0FW-HTS-NUMBER = KS0FM002.KS0FM-HTS-SHDG-CD;
      if (KS0FW00.KS0FW-HTS-BYTE[1] == " "
       || KS0FW00.KS0FW-HTS-BYTE[2] == " "
       || KS0FW00.KS0FW-HTS-BYTE[4] == " "
       || KS0FW00.KS0FW-HTS-BYTE[5] == " ")
        set KS0FM002.KS0FM-HTS-SHDG-CD cursor, bold; /* highlight*/
        converseLib.validationFailed(50); /* set msg nbr*/
      else
        if (KS0FW00.KS0FW-HTS-BYTE[3] != " "
         && KS0FW00.KS0FW-HTS-BYTE[3] != ".")
          set KS0FM002.KS0FM-HTS-SHDG-CD cursor, bold; /* highlight*/
          converseLib.validationFailed(50); /* set msg nbr*/
        end
      end
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* If any errors were found, exit to display the map.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end
  ;
end // end KS0F-25000

// process valid input request
Function KS0F-26000()
  /* ------------------------------------------------------------*/
  /* Indicate that the map must be formatted before display.*/
  /* ------------------------------------------------------------*/
  ;
  KS0FW00.KS0FW-SWF = "Y"; /* Format the map switch*/
  ;
  /* ------------------------------------------------------------*/
  /* Obtain the current date and time.*/
  /* ------------------------------------------------------------*/
  ;
  XX0XS01(); /* Obtain date and time*/
  ;
  /* ------------------------------------------------------------*/
  /* If this is an 'update' function, update the table row.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FW00.KS0FW-FNC == "C") /* is this an change request?*/
    ;
    if (KS0FW00.KS0FW-DATA-CHGD-SW == "Y") /* if any data changed*/
      ;
      KS0FW00.KS0FW-DATA-CHGD-SW = "N"; /* turn off data chgd sw*/
      ;
      /* ------------------------------------------------------------*/
      /* Update the changed data in the table row.*/
      /* ------------------------------------------------------------*/
      ;
      KS0F-26100(); /* Update table row*/
      ;
      KS0FW00.KS0FW-SWR = "Y"; /* Request for data switch*/
      ;
      if (converseVar.validationMsgNum != 0) /* if not successful*/
        KS0FW00.KS0FW-SWD = "2"; /* request map 2*/
        KS0FW00.KS0FW-SWF = "Y"; /* request format map 2*/
        return;
      end
      /* ------------------------------------------------------------*/
      /* Indicate that the table update was successfully completed.*/
      /* ------------------------------------------------------------*/
      ;
      KS0FW00.KS0FW-CPTR-BKDN = VHSSHD01.KS-HTS-CPTR-CD;
      KS0FW00.KS0FW-HDG-BKDN = VHSSHD01.KS-HTS-HDG-CD;
      KS0FW00.KS0FW-PERIOD1 = ".";
      KS0FW00.KS0FW-1ST-SHDG-CD = VHSSHD01.KS-HTS-1ST-SHDG-CD;
      KS0FW00.KS0FW-2ND-SHDG-CD = VHSSHD01.KS-HTS-2ND-SHDG-CD;
      KS0FW00.KS0FW-PERIOD = ".";
      KS0FW00.KS0FW-SHDG-BKDN = KS0FW00.KS0FW-SHDG-FORMAT-FLD;
      KS0FW00.KS0FW-LAST-CHGD-SHDG = KS0FW00.KS0FW-CPTR-HDG-SHDG-BKDN;
      ;
      KSWREC.XXXPRCNM = "26000"; /* Module identification*/
      KSWREC.XXXMSGNO = 1; /* Table update was completed*/
    else
      ;
      KS0FW00.KS0FW-SWR = "N"; /* do not request data*/
      KS0FW00.KS0FW-SWF = "N"; /* do not format map 2*/
      KS0FW00.KS0FW-CPTR-HDG-SHDG-BKDN = KS0FM002.KS0FM-HTS-HDG-CD;
      KS0FW00.KS0FW-PERIOD1 = ".";
      KS0FW00.KS0FW-SHDG-BKDN = KS0FM002.KS0FM-HTS-SHDG-CD;
      KS0FW00.KS0FW-LAST-CHGD-SHDG = KS0FW00.KS0FW-CPTR-HDG-SHDG-BKDN;
      ;
    end
    ;
    KS0F-26200(); /* Search for next selection*/
    ;
    /* ------------------------------------------------------------*/
    /* Commit all changes made to protected resources.*/
    /* ------------------------------------------------------------*/
    ;
    VGVar.handleSysLibraryErrors = 0;
    sysLib.commit(); /* Commit all changes*/
    ;
    set KS0FW01 empty; /* clear working storage*/
    ;
  else
    ;
    /* ------------------------------------------------------------*/
    /* code following is for adding chapters to the table.*/
    /* ------------------------------------------------------------*/
    ;
    KS0F-26300(); /* add row to table*/
    ;
    if (converseVar.validationMsgNum != 0) /* if not successful*/
      KS0FW00.KS0FW-SWD = "2"; /* request map 2*/
      KS0FW00.KS0FW-SWF = "N"; /* do not format map 2*/
      KS0FW00.KS0FW-SWR = "N"; /* do not request data*/
      KSWREC.XXXMSGNO = 1; /* successful update*/
      return;
    end
    ;
    KS0FW00.KS0FW-CPTR-BKDN = VHSSHD01.KS-HTS-CPTR-CD;
    KS0FW00.KS0FW-HDG-BKDN = VHSSHD01.KS-HTS-HDG-CD;
