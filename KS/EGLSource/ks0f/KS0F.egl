  KS0FW00.KS0FW-SAVE-CHG-CNT = VHSSHD01.ZZZCHGCT; /* save for later*/
  ;
end // end KS0F-21200

// format and converse the map
Function KS0F-22000()
  /* ------------------------------------------------------------*/
  /* If an error was found, save the error message number.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* Error was found*/
    KSWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* If requested, format data to display.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FW00.KS0FW-SWF == "Y") /* map must be formatted*/
    KS0F-22100(); /* format the data on the map*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* Format the current date and any FAST PATH information.*/
  /* ------------------------------------------------------------*/
  ;
  KS0FM002.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/
  ;
  KS0FM002.CATOAP = COMMAREA.CATOAP; /* TO application*/
  KS0FM002.CAITEM = COMMAREA.CAITEM; /* Application data*/
  ;
  /* ------------------------------------------------------------*/
  /* If a message to display, indicate sound audible alarm.*/
  /* ------------------------------------------------------------*/
  ;
  if (KSWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/
    ;
    if (KSWREC.XXXMSGNO != 0) /* Message to display*/
      ;
      if (KSWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(KSWREC.XXXMSGNO);
      end /* Message number*/
    else
      KS0FM002.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/
      ;
    end
    ;
    KS0FM002.XXXPRCNM = KSWREC.XXXPRCNM; /* Module identification*/
    ;
    set KS0FM002 alarm; /* Indicate sound alarm*/
    ;
  end
  ;
  converse KS0FM002 ;
  ;
  /* ------------------------------------------------------------*/
  /* Clear the message fields and save FAST PATH information.*/
  /* ------------------------------------------------------------*/
  ;
  KSWREC.XXXPRCNM = " "; /* Module identification*/
  KSWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/
  ;
  COMMAREA.CATOAP = KS0FM002.CATOAP; /* TO application*/
  COMMAREA.CAITEM = KS0FM002.CAITEM; /* Application data*/
  ;
end // end KS0F-22000

// format the display area
Function KS0F-22100()
  /* ------------------------------------------------------------*/
  /* Clear the fields in the map storage area to be formatted.*/
  /* ------------------------------------------------------------*/
  ;
  set KS0FM002 initial; /* M002 input/output area*/
  ;
  KS0FW00.KS0FW-SWF = "N"; /* Format the map switch*/
  ;
  /* ------------------------------------------------------------*/
  /* Format the data in the map from the display storage.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0FW00.KS0FW-FNC == "A") /* add requested?*/
    KS0FM002.KS0FM-MAP-DESC = "ADD   "; /* title*/
    KS0FW00.KS0FW-CPTR-CD = KS0FW00.KS0FW-HTS-CPTR-CD;
    KS0FW00.KS0FW-HDG-CD = KS0FW00.KS0FW-HTS-HDG-CD;
    KS0FM002.KS0FM-HTS-HDG-CD = KS0FW00.KS0FW-CPTR-HDG-CDS;
    KS0FM002.KS0FM-HTS-CPTR-SDESC = KS0FW00.KS0FW-HTS-CPTR-SDESC;
    KS0FM002.KS0FM-HTS-HDG-SDESC = KS0FW00.KS0FW-HTS-HDG-SDESC;
    KS0FM002.KS0FM-CREATE-DT = VGVar.currentShortGregorianDate;
    KS0FM002.KS0FM-MOD-DT = VGVar.currentShortGregorianDate;
    KS0FM002.KS0FM-USER = COMMAREA.CAUSERID;
    if (KS0FW00.KS0FW-SAVE-ADD-CNT > 0) /* if we have added one before ?*/
      converseLib.validationFailed(1); /* update successful*/
    else
      converseLib.validationFailed(54); /* please enter new hs chapter sub-hdg*/
    end
    set KS0FM002.KS0FM-HTS-SHDG-CD cursor, initialAttributes; /* allow entry*/
  else
    KS0FM002.KS0FM-MAP-DESC = "CHANGE"; /* title*/
    KS0FM002.KS0FM-CREATE-DT = KS0FW02.KS0FW-CREATE-DT;
    KS0FM002.KS0FM-MOD-DT = KS0FW02.KS0FW-MOD-DT;
    KS0FM002.KS0FM-USER = KS0FW02.KS0FW-USER;
    KS0FW00.KS0FW-CPTR-CD = KS0FW00.KS0FW-HTS-CPTR-CD;
    KS0FW00.KS0FW-HDG-CD = KS0FW00.KS0FW-HTS-HDG-CD;
    KS0FM002.KS0FM-HTS-HDG-CD = KS0FW00.KS0FW-CPTR-HDG-CDS;
    KS0FM002.KS0FM-HTS-CPTR-SDESC = KS0FW00.KS0FW-HTS-CPTR-SDESC;
    KS0FM002.KS0FM-HTS-HDG-SDESC = KS0FW00.KS0FW-HTS-HDG-SDESC;
    KS0FM002.KS0FM-HTS-SHDG-CD = KS0FW02.KS0FW-HTS-SHDG-CD;
    set KS0FM002.KS0FM-HTS-HDG-CD skip; /* protect hgd cd*/
    set KS0FM002.KS0FM-HTS-SHDG-CD skip; /* protect sub-hdg cd*/
    set KS0FM002.KS0FM-HTS-SHDG-SDESC cursor; /* cursor position*/
    KS0FM002.KS0FM-HTS-SHDG-SDESC = KS0FW02.KS0FW-HTS-SHDG-SDESC;
    KS0FM002.KS0FM-HTS-SHDG-DESC[1] = KS0FW02.KS0FW-HTS-SHDG-DESC[1];
    KS0FM002.KS0FM-HTS-SHDG-DESC[2] = KS0FW02.KS0FW-HTS-SHDG-DESC[2];
    KS0FM002.KS0FM-HTS-SHDG-DESC[3] = KS0FW02.KS0FW-HTS-SHDG-DESC[3];
    KS0FM002.KS0FM-HTS-SHDG-DESC[4] = KS0FW02.KS0FW-HTS-SHDG-DESC[4];
    KS0FM002.KS0FM-HTS-SHDG-DESC[5] = KS0FW02.KS0FW-HTS-SHDG-DESC[5];
    KS0FM002.KS0FM-HTS-SHDG-DESC[6] = KS0FW02.KS0FW-HTS-SHDG-DESC[6];
    KS0FM002.KS0FM-HTS-SHDG-DESC[7] = KS0FW02.KS0FW-HTS-SHDG-DESC[7];
    KS0FM002.KS0FM-STAT = KS0FW02.KS0FW-STAT;
  end
  ;
  KS0FM002.XXXCURDT = VGVar.currentShortGregorianDate;
  ;
  if (KS0FM002.KS0FM-STAT == " ")
    KS0FM002.KS0FM-STAT = "A"; /* default status code*/
  end
  ;
  if (KS0FM002.KS0FM-STAT == "A")
    KS0FM002.KS0FM-STAT-DESC = "ACTIVE  ";
  else
    KS0FM002.KS0FM-STAT-DESC = "INACTIVE";
  end
  ;
end // end KS0F-22100

// check attention identifier
Function KS0F-23000()
  /* ------------------------------------------------------------*/
  /* If CANCEL was requested, clear the map fields and exit.*/
  /* If a request for data is pending, reset that request.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    ;
    if (KS0FW00.KS0FW-SWR == "Y") /* Request for data*/
      set KS0FW02 empty; /* M002 display storage*/
    end
    ;
    KS0FW00.KS0FW-SWF = "Y"; /* Format the map switch*/
    KS0FW00.KS0FW-SWR = "N"; /* Request for data switch*/
    ;
    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If FAST PATH was requested, transfer to the application.*/
  /* ------------------------------------------------------------*/
  ;
  if (COMMAREA.CATOAP > " ") /* FAST PATH was requested*/
    ;
    if (converseVar.eventKey is enter) /* ENTER was pressed*/
      ;
      KS0F-11100(); /* Purge any temporary storage*/
      ;
      XSKS-S0001(); /* Exit to another application*/
      ;
      set KS0FM002.CATOAP cursor, bold; /* Highlight*/
      ;
      KS0FW00.KS0FW-SWD = "2"; /* Map to be displayed switch*/
      KS0FW00.KS0FW-SWF = "N"; /* do not format the map*/
      KS0FW00.KS0FW-SWF = "N"; /* do not request data*/
      ;
    else
      ;
      KSWREC.XXXPRCNM = "23000"; /* Module identification*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/
      ;
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If HELP, exit to the previous level or exit to the Master*/
  /* Menu was requested, exit to another application.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4 /* or PF4 was pressed*/
   || converseVar.eventKey is pf21) /* or PF21 was pressed*/
    ;
    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "KS0FM002";
    end
    ;
    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      ;
      KS0FW00.KS0FW-SWD = "1"; /* Map to be displayed switch*/
      KS0FW00.KS0FW-SWF = "Y"; /* Format the map switch*/
      KS0FW00.KS0FW-SWR = "Y"; /* request for data switch*/
      ;
      KS0FW00.KS0FW-CPTR-BKDN = VHSSHD01.KS-HTS-CPTR-CD;
      KS0FW00.KS0FW-HDG-BKDN = VHSSHD01.KS-HTS-HDG-CD;
      KS0FW00.KS0FW-PERIOD1 = ".";
      KS0FW00.KS0FW-1ST-SHDG-CD = VHSSHD01.KS-HTS-1ST-SHDG-CD;
      KS0FW00.KS0FW-PERIOD = ".";
      KS0FW00.KS0FW-2ND-SHDG-CD = VHSSHD01.KS-HTS-2ND-SHDG-CD;
      KS0FW00.KS0FW-SHDG-BKDN = KS0FW00.KS0FW-SHDG-FORMAT-FLD;
      KS0FW00.KS0FW-LAST-CHGD-SHDG = KS0FW00.KS0FW-CPTR-HDG-SHDG-BKDN;
      ;
       /* clear selected sub-headings*/
      ;
      KS0FW00.KS0FW-IXM = 1;
      ;
      while (KS0FW00.KS0FW-IXM <= 13)
        KS0FW00.KS0FW-SELECTED-SHDGS[KS0FW-IXM] = " ";
        KS0FW00.KS0FW-IXM = KS0FW00.KS0FW-IXM + 1;
      end
      ;
      exit stack; /* Exit to display map*/
      ;
    end
    ;
    if (converseVar.eventKey is pf21) /* PF21 was pressed*/
      COMMAREA.CATOAP = "KS0A";
    end
    ;
    KS0F-11100(); /* Purge any temporary storage*/
    ;
    XSKS-S0001(); /* Exit to another application*/
    ;
    KS0FW00.KS0FW-SWD = "1"; /* Map to be displayed switch*/
    KS0FW00.KS0FW-SWF = "Y"; /* Format the map switch*/
    KS0FW00.KS0FW-SWR = "Y"; /* request for data switch*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Determine whether a valid attention identifier was used to*/
  /* invoke the transaction.  Display an error message to the*/
  /* user if the PF key used is not supported.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey not enter) /* ENTER was not pressed*/
    ;
    KSWREC.XXXPRCNM = "23000"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    ;
    if (converseVar.eventKey is pa2) /* PA2 key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
end // end KS0F-23000

