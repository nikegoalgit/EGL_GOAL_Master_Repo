// Format and converse the map
Function KS0D-12000()
  /* ------------------------------------------------------------*/
  /* If an error was found, save the error message number.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* Error was found*/
    KSWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* If obtain data failed or no error, format data to display.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-SWR == "Y" /* Obtain data failed*/
   || converseVar.validationMsgNum == 0) /* or no error found*/
    ;
    if (KS0DW00.KS0DW-SWF == "Y") /* Map must be formatted*/
      KS0D-12100(); /* Format the data on the map*/
    end
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Format the current date and any FAST PATH information.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DM001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/
  ;
  KS0DM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  KS0DM001.CAITEM = COMMAREA.CAITEM; /* Application data*/
  ;
  /* ------------------------------------------------------------*/
  /* If a message to display, indicate sound audible alarm.*/
  /* ------------------------------------------------------------*/
  ;
  if (KSWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/
    ;
    if (KSWREC.XXXMSGNO != 0) /* Message to display*/
      ;
      if (KSWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(KSWREC.XXXMSGNO);
      end /* Message number*/
    else
      KS0DM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/
      ;
    end
    ;
    KS0DM001.XXXPRCNM = KSWREC.XXXPRCNM; /* Module identification*/
    ;
    set KS0DM001 alarm; /* Indicate sound alarm*/
    ;
  end
  ;
  converse KS0DM001 ;
  ;
  /* ------------------------------------------------------------*/
  /* Clear the message fields and save FAST PATH information.*/
  /* ------------------------------------------------------------*/
  ;
  KSWREC.XXXPRCNM = " "; /* Module identification*/
  KSWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/
  ;
  COMMAREA.CATOAP = KS0DM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = KS0DM001.CAITEM; /* Application data*/
  ;
end // end KS0D-12000

// Format the display data
Function KS0D-12100()
  /* ------------------------------------------------------------*/
  /* Clear the fields in the map storage area to be formatted.*/
  /* ------------------------------------------------------------*/
  ;
  set KS0DM001 initial; /* M001 input/output area*/
  ;
  KS0DW00.KS0DW-SWF = "N"; /* Format the map switch*/
  ;
  /* ------------------------------------------------------------*/
  /* if we have just come back from a change and there is no data*/
  /* to display, set the last changed chapter field to blanks in*/
  /* order by bypass the setting of the cursor routine which reads*/
  /* the TSQ.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.TA5TSQIX == 0) /* no data read ?*/
    KS0DW00.KS0DW-LAST-CHGD-CPTR = " "; /* bypass reading tsq*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* If we have just returned from KS0E, put the cursor on the*/
  /* chapter number in kswrec.ks-hts-cptr-cd.*/
  /* ------------------------------------------------------------*/
  ;
  if (KSWREC.KS-HTS-CPTR-CD > " ") /* chapter nbr present*/
    KS0DW00.KS0DW-LAST-CHGD-CPTR = KSWREC.KS-HTS-CPTR-CD;
  end
  /* ------------------------------------------------------------*/
  /* If we have just returned from changing a chapter(s), we need*/
  /* to position the cursor on the last chapter changed.*/
  /* The following code finds the tsq page that the chapter is on.*/
  /* This is also applicable for adds.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-LAST-CHGD-CPTR > " ") /* just back from change?*/
    KS0DW00.KS0DW-IX3 = 1; /* initialize index*/
    while (KS0DW-IX3 <= 100)
      if (KS0DW00.KS0DW-LAST-CHGD-CPTR < 
      KS0DW00.KS0DW-CPTR-CD[KS0DW-IX3])
        KS0DW00.KS0DW-IX3 = KS0DW00.KS0DW-IX3 - 1; /* take prev one*/
        if (KS0DW00.KS0DW-IX3 < 1) /* if index < 1*/
          KS0DW00.KS0DW-IX3 = 1; /* set to 1*/
        end
        KS0DW00.XXXTSPAG = KS0DW00.KS0DW-TS-PAGE-NBR[KS0DW-IX3]; /* set ts page nbr*/
        KS0DW00.KS0DW-LAST-CHGD-CPTR = KS0DW00.KS0DW-CPTR-CD[KS0DW-IX3]; /* for the cursor*/
                                          /* positioning routine*/
        KS0DW00.KS0DW-IX3 = 100; /* quit the search*/
      else
        if (KS0DW00.KS0DW-CPTR-CD[KS0DW-IX3] == " ") /* cptr nbr blank?*/
          KS0DW00.KS0DW-IX3 = KS0DW00.KS0DW-IX3 - 1; /* take prev one*/
          if (KS0DW00.KS0DW-IX3 < 1) /* if index < 1*/
            KS0DW00.KS0DW-IX3 = 1; /* set to 1*/
          end
          KS0DW00.KS0DW-LAST-CHGD-CPTR = KS0DW00.KS0DW-CPTR-CD[KS0DW-IX3]; /* for the cursor*/
                                            /* positioning routine*/
          KS0DW00.XXXTSPAG = KS0DW00.KS0DW-TS-PAGE-NBR[KS0DW-IX3]; /* set ts page nbr*/
          KS0DW00.KS0DW-IX3 = 100; /* quit the search*/
        end
      end
      KS0DW00.KS0DW-IX3 = KS0DW00.KS0DW-IX3 + 1; /* set index up by 1*/
    end
    KS0D-12110(); /* read tsq page*/
    set KS0DM001.XXXLITPG skip; /* "PAGE" literal*/
    KS0DM001.XXXTSPAG = KS0DW00.XXXTSPAG; /* Current page*/
    set KS0DM001.XXXLITOF skip; /* "OF" literal*/
    KS0DM001.TA5TSQIX = KS0DW00.TA5TSQIX; /* Highest page*/
  else
    /* ------------------------------------------------------------*/
    /* If the page in storage is not the page to be displayed,*/
    /* obtain the desired page from temporary storage.*/
    /* ------------------------------------------------------------*/
    ;
    if (KS0DW00.TA5TSQIX > 0) /* Pages were formatted*/
      ;
      if (KS0DW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/
        ;
        /* ------------------------------------------------------------*/
        /* If the current page has been changed, update the page in*/
        /* storage before obtaining the next one to display.*/
        /* ------------------------------------------------------------*/
        ;
        if (KS0DW00.KS0DW-SWU == "Y") /* Current page was changed*/
          ;
          KS0D-12120(); /* Update temporary storage page*/
          ;
          KS0DW00.KS0DW-SWU = "N"; /* Storage update switch*/
          ;
        end
        ;
        /* ------------------------------------------------------------*/
        /* If the desired page number is greater than the highest page*/
        /* in storage, default to the highest page number.*/
        /* ------------------------------------------------------------*/
        ;
        if (KS0DW00.XXXTSPAG > KS0DW00.TA5TSQIX) /* Desired greater highest*/
          KS0DW00.XXXTSPAG = KS0DW00.TA5TSQIX; /* Default to highest*/
        end
        ;
        KS0D-12110(); /* Obtain temporary storage page*/
        ;
      end
      ;
      /* ------------------------------------------------------------*/
      /* Display the page information on the screen.*/
      /* ------------------------------------------------------------*/
      ;
      set KS0DM001.XXXLITPG skip; /* "PAGE" literal*/
      KS0DM001.XXXTSPAG = KS0DW00.XXXTSPAG; /* Current page*/
      set KS0DM001.XXXLITOF skip; /* "OF" literal*/
      KS0DM001.TA5TSQIX = KS0DW00.TA5TSQIX; /* Highest page*/
      ;
    else
      ;
      set KS0DM001.XXXLITPG invisible, skip; /* "PAGE" literal*/
      KS0DM001.XXXTSPAG = " "; /* Current page*/
      set KS0DM001.XXXLITOF invisible, skip; /* "OF" literal*/
      KS0DM001.TA5TSQIX = " "; /* Highest page*/
      ;
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* Format the data in the map from the display storage.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DM001.KS0DM-HTS-NBR = KS0DW01.KS0DW-HTS-NBR; /* HTS number*/
  KS0DM001.KS0DM-HTS-CPTR-CD = KS0DW01.KS0DW-HTS-CPTR-CD; /* cptr*/
  KS0DM001.KS0DM-STAT = KS0DW01.KS0DW-STAT; /* status code*/
  ;
  KS0DW00.KS0DW-IXM = 1; /* Initialize line index*/
  ;
  while (KS0DW00.KS0DW-IXM <= 15) /* Until screen is full*/
    ;
    KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM] = KS0DW01.KS0DW-DTL-CPTR-CD[KS0DW-IXM];
    KS0DM001.KS0DM-DTL-HTS-CPTR-DESC[KS0DW-IXM] = KS0DW01.KS0DW-DTL-CPTR-DESC[KS0DW-IXM];
    ;
    KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* Increment line index*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* the default cursor position is hts nbr.*/
  /* the routine below may change the cursor position.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DM001.XXXTSPAG > 1 /* other than first page*/
   && KS0DM001.KS0DM-DTL-HTS-CPTR-CD[1] > " ") /* cd present?*/
    set KS0DM001.KS0DMSEL[1] cursor;
  else
    set KS0DM001.KS0DM-HTS-NBR cursor, normal;
  end
  ;
  /* ------------------------------------------------------------*/
  /* if we're returning from an add or change, position the cursor*/
  /* on the chapter we just finished with.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-IXM = 1; /* initialize index*/
  ;
  while (KS0DW-IXM <= 15) /* until all lines are processed*/
    if (KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM] > " ") /* cd present?*/
      if (KS0DW00.KS0DW-LAST-CHGD-CPTR > " ")
        if (KS0DW00.KS0DW-LAST-CHGD-CPTR == 
        KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM])
          set KS0DM001.KS0DMSEL[KS0DW-IXM] cursor, normal;
        else
          set KS0DM001.KS0DMSEL[KS0DW-IXM] normal;
        end
      else
        set KS0DM001.KS0DMSEL[KS0DW-IXM] normal;
      end
    else
      set KS0DM001.KS0DMSEL[KS0DW-IXM] invisible, skip;
    end
    KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* set index up by 1*/
  end
  ;
  KS0DW00.KS0DW-LAST-CHGD-CPTR = " "; /* clear last cptr*/
  ;
  if (COMMAREA.CACHGFL == "Y")
    set KS0DM001.KS0DM-PF14 bold, skip;
  end
  ;
  if (COMMAREA.CAADDFL == "Y")
    set KS0DM001.KS0DM-PF13 bold, skip;
  end
  ;
end // end KS0D-12100

// read tsq page
Function KS0D-12110()
  /* ------------------------------------------------------------*/
  /* Obtain the next page of data to display.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW01.XXXTSLEN = KS0DW00.XXXTSLEN; /* Temporary storage length*/
  ;
  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5TSQIX = KS0DW00.XXXTSPAG; /* Page number*/
  TA5REC.TA5FNCCD = "R"; /* Function code*/
  ;
  try
    call "TA0050" (KS0DW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/
  ;
  /* ------------------------------------------------------------*/
  /* If successful, return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/
  ;
  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/
  ;
  KSWREC.XXXPRCNM = "12110"; /* Module identification*/
  converseLib.validationFailed(10); /* Unexpected error occurred*/
  ;
  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "READ PAGE FROM TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = KSWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
end // end KS0D-12110

