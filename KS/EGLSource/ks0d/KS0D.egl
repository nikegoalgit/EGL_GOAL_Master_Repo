// Format and converse the map
Function KS0D-22000()
  /* ------------------------------------------------------------*/
  /* If an error was found, save the error message number.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* Error was found*/
    KSWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* If requested, format data to display.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-SWF == "Y") /* Map must be formatted*/
    KS0D-22100(); /* Format the data on the map*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* Format the current date and any FAST PATH information.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DM002.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/
  ;
  KS0DM002.CATOAP = COMMAREA.CATOAP; /* TO application*/
  KS0DM002.CAITEM = COMMAREA.CAITEM; /* Application data*/
  ;
  /* ------------------------------------------------------------*/
  /* If a message to display, indicate sound audible alarm.*/
  /* ------------------------------------------------------------*/
  ;
  if (KSWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/
    ;
    if (KSWREC.XXXMSGNO != 0) /* Message to display*/
      ;
      if (KSWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(KSWREC.XXXMSGNO);
      end /* Message number*/
    else
      KS0DM002.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/
      ;
    end
    ;
    KS0DM002.XXXPRCNM = KSWREC.XXXPRCNM; /* Module identification*/
    ;
    set KS0DM002 alarm; /* Indicate sound alarm*/
    ;
  end
  ;
  converse KS0DM002 ;
  ;
  /* ------------------------------------------------------------*/
  /* Clear the message fields and save FAST PATH information.*/
  /* ------------------------------------------------------------*/
  ;
  KSWREC.XXXPRCNM = " "; /* Module identification*/
  KSWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/
  ;
  COMMAREA.CATOAP = KS0DM002.CATOAP; /* TO application*/
  COMMAREA.CAITEM = KS0DM002.CAITEM; /* Application data*/
  ;
end // end KS0D-22000

// Format the display data
Function KS0D-22100()
  /* ------------------------------------------------------------*/
  /* Clear the fields in the map storage area to be formatted.*/
  /* ------------------------------------------------------------*/
  ;
  set KS0DM002 initial; /* M002 input/output area*/
  ;
  KS0DW00.KS0DW-SWF = "N"; /* Format the map switch*/
  ;
  /* ------------------------------------------------------------*/
  /* Format the data in the map from the display storage.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-FNC == "A") /* add requested?*/
    KS0DM002.KS0DM-MAP-DESC = "ADD   "; /* title*/
    KS0DM002.KS0DM-CREATE-DT = VGVar.currentShortGregorianDate;
    KS0DM002.KS0DM-MOD-DT = VGVar.currentShortGregorianDate;
    KS0DM002.KS0DM-USER = COMMAREA.CAUSERID;
    if (KS0DW00.KS0DW-SAVE-ADD-CNT > 0) /* if we have added one before ?*/
      converseLib.validationFailed(1); /* update successful*/
    else
      converseLib.validationFailed(39); /* please enter new hs chapter number*/
    end
    set KS0DM002.KS0DM-HTS-CPTR-CD cursor, initialAttributes; /* allow entry*/
  else
    KS0DM002.KS0DM-MAP-DESC = "CHANGE"; /* title*/
    KS0DM002.KS0DM-CREATE-DT = KS0DW02.KS0DW-CREATE-DT;
    KS0DM002.KS0DM-MOD-DT = KS0DW02.KS0DW-MOD-DT;
    KS0DM002.KS0DM-USER = KS0DW02.KS0DW-USER;
    KS0DM002.KS0DM-HTS-CPTR-CD = KS0DW02.KS0DW-HTS-CPTR-CD;
    set KS0DM002.KS0DM-HTS-CPTR-CD skip; /* protect cptr cd*/
    set KS0DM002.KS0DM-HTS-CPTR-SDESC cursor; /* cursor position*/
    KS0DM002.KS0DM-HTS-CPTR-SDESC = KS0DW02.KS0DW-HTS-CPTR-SDESC;
    KS0DM002.KS0DM-HTS-CPTR-DESC[1] = KS0DW02.KS0DW-HTS-CPTR-DESC[1];
    KS0DM002.KS0DM-HTS-CPTR-DESC[2] = KS0DW02.KS0DW-HTS-CPTR-DESC[2];
    KS0DM002.KS0DM-HTS-CPTR-DESC[3] = KS0DW02.KS0DW-HTS-CPTR-DESC[3];
    KS0DM002.KS0DM-HTS-CPTR-DESC[4] = KS0DW02.KS0DW-HTS-CPTR-DESC[4];
    KS0DM002.KS0DM-HTS-CPTR-DESC[5] = KS0DW02.KS0DW-HTS-CPTR-DESC[5];
    KS0DM002.KS0DM-HTS-CPTR-DESC[6] = KS0DW02.KS0DW-HTS-CPTR-DESC[6];
    KS0DM002.KS0DM-HTS-CPTR-DESC[7] = KS0DW02.KS0DW-HTS-CPTR-DESC[7];
    KS0DM002.KS0DM-STAT = KS0DW02.KS0DW-STAT;
  end
  ;
  KS0DM002.XXXCURDT = VGVar.currentShortGregorianDate;
  ;
  if (KS0DM002.KS0DM-STAT == " ")
    KS0DM002.KS0DM-STAT = "A"; /* default status code*/
  end
  ;
  if (KS0DM002.KS0DM-STAT == "A")
    KS0DM002.KS0DM-STAT-DESC = "ACTIVE  ";
  else
    KS0DM002.KS0DM-STAT-DESC = "INACTIVE";
  end
  ;
end // end KS0D-22100

// Check attention identifier
Function KS0D-23000()
  /* ------------------------------------------------------------*/
  /* If CANCEL was requested, clear the map fields and exit.*/
  /* If a request for data is pending, reset that request.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pa2) /* PA2 was pressed*/
    ;
    if (KS0DW00.KS0DW-SWR == "Y") /* Request for data*/
      set KS0DW02 empty; /* M002 display storage*/
    end
    ;
    KS0DW00.KS0DW-SWF = "Y"; /* Format the map switch*/
    KS0DW00.KS0DW-SWR = "N"; /* Request for data switch*/
    ;
    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If FAST PATH was requested, transfer to the application.*/
  /* ------------------------------------------------------------*/
  ;
  if (COMMAREA.CATOAP > " ") /* FAST PATH was requested*/
    ;
    if (converseVar.eventKey is enter) /* ENTER was pressed*/
      ;
      KS0D-11100(); /* Purge any temporary storage*/
      ;
      XSKS-S0001(); /* Exit to another application*/
      ;
      set KS0DM002.CATOAP cursor, bold; /* Highlight*/
      ;
      KS0DW00.KS0DW-SWD = "2"; /* Map to be displayed switch*/
      KS0DW00.KS0DW-SWF = "N"; /* do not format the map*/
      KS0DW00.KS0DW-SWF = "N"; /* do not request data*/
      ;
    else
      ;
      KSWREC.XXXPRCNM = "23000"; /* Module identification*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/
      ;
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If HELP, exit to the previous level or exit to the Master*/
  /* Menu was requested, exit to another application.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4 /* or PF4 was pressed*/
   || converseVar.eventKey is pf21) /* or PF21 was pressed*/
    ;
    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "KS0DM002"; /* Application find code*/
    end
    ;
    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      KS0DW00.KS0DW-SWD = "1"; /* Map to be displayed switch*/
      KS0DW00.KS0DW-SWF = "Y"; /* Format the map switch*/
      KS0DW00.KS0DW-SWR = "Y"; /* request for data switch*/
      KS0DW00.KS0DW-LAST-CHGD-CPTR = VHSCHP01.KS-HTS-CPTR-CD;
      ;
       /* clear table of selected chapters*/
      ;
      KS0DW00.KS0DW-IXM = 1;
      ;
      while (KS0DW00.KS0DW-IXM <= 15)
        KS0DW00.KS0DW-SELECTED-CPTRS[KS0DW-IXM] = " ";
        KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1;
      end
      ;
      exit stack; /* Exit to display map*/
    end
    ;
    KS0D-11100(); /* Purge any temporary storage*/
    ;
    XSKS-S0001(); /* Exit to another application*/
    ;
    KS0DW00.KS0DW-SWD = "1"; /* Map to be displayed switch*/
    KS0DW00.KS0DW-SWF = "Y"; /* Format the map switch*/
    KS0DW00.KS0DW-SWR = "Y"; /* request for data switch*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Determine whether a valid attention identifier was used to*/
  /* invoke the transaction.  Display an error message to the*/
  /* user if the PF key used is not supported.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey not enter) /* ENTER was not pressed*/
    ;
    KSWREC.XXXPRCNM = "23000"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    ;
    if (converseVar.eventKey is pa2) /* PA2 key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
end // end KS0D-23000

// Reset highlighted fields
Function KS0D-24000()
  /* ------------------------------------------------------------*/
  /* Reset all modifiable field attributes to normal intensity.*/
  /* ------------------------------------------------------------*/
  ;
  set KS0DM002.KS0DM-MAP-DESC initialAttributes; /* map description*/
  set KS0DM002.KS0DM-HTS-CPTR-CD initialAttributes; /* chapter code*/
  set KS0DM002.KS0DM-HTS-CPTR-SDESC initialAttributes; /* chapter short desc*/
  set KS0DM002.KS0DM-HTS-CPTR-DESC[1] initialAttributes; /* chapter decription line 1*/
  set KS0DM002.KS0DM-HTS-CPTR-DESC[2] initialAttributes; /* chapter decription line 2*/
  set KS0DM002.KS0DM-HTS-CPTR-DESC[3] initialAttributes; /* chapter decription line 3*/
  set KS0DM002.KS0DM-HTS-CPTR-DESC[4] initialAttributes; /* chapter decription line 4*/
  set KS0DM002.KS0DM-HTS-CPTR-DESC[5] initialAttributes; /* chapter decription line 5*/
  set KS0DM002.KS0DM-STAT initialAttributes; /* status*/
  set KS0DM002.KS0DM-STAT-DESC initialAttributes; /* status description*/
  set KS0DM002.KS0DM-CREATE-DT initialAttributes; /* creation date*/
  set KS0DM002.KS0DM-MOD-DT initialAttributes; /* modification date*/
  set KS0DM002.KS0DM-USER initialAttributes; /* user*/
  ;
  set KS0DM002.CATOAP initialAttributes; /* Commarea TO application*/
  set KS0DM002.CAITEM initialAttributes; /* Commarea application data*/
  ;
end // end KS0D-24000

// Verify data that was entered
Function KS0D-25000()
  /* ------------------------------------------------------------*/
  /* Highlight any application data that was entered.*/
  /* ------------------------------------------------------------*/
  ;
  if (COMMAREA.CAITEM > " ") /* Application data was entered*/
    ;
    KSWREC.XXXPRCNM = "25000"; /* Module identification*/
    set KS0DM002.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/
    ;
  end
  ;
  KS0DW00.KS0DW-DATA-CHGD-SW = "Y"; /* initialize change switch*/
  ;
  /* ------------------------------------------------------------*/
  /* If this is a change function and*/
  /* If data on the screen was not changed, set switch to 'n'.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-FNC == "C") /* if change function*/
    if (KS0DM002.KS0DM-HTS-CPTR-SDESC == 
    KS0DW02.KS0DW-HTS-CPTR-SDESC   /* if all data*/
     && KS0DM002.KS0DM-HTS-CPTR-DESC[1] == 
    KS0DW02.KS0DW-HTS-CPTR-DESC[1] /* the same ?*/
     && KS0DM002.KS0DM-HTS-CPTR-DESC[2] == 
    KS0DW02.KS0DW-HTS-CPTR-DESC[2] 
     && KS0DM002.KS0DM-HTS-CPTR-DESC[3] == 
    KS0DW02.KS0DW-HTS-CPTR-DESC[3] 
     && KS0DM002.KS0DM-HTS-CPTR-DESC[4] == 
    KS0DW02.KS0DW-HTS-CPTR-DESC[4] 
     && KS0DM002.KS0DM-HTS-CPTR-DESC[5] == 
    KS0DW02.KS0DW-HTS-CPTR-DESC[5] 
     && KS0DM002.KS0DM-HTS-CPTR-DESC[6] == 
    KS0DW02.KS0DW-HTS-CPTR-DESC[6] 
     && KS0DM002.KS0DM-HTS-CPTR-DESC[7] == 
    KS0DW02.KS0DW-HTS-CPTR-DESC[7] 
     && KS0DM002.KS0DM-STAT ==     
    KS0DW02.KS0DW-STAT)           
      KS0DW00.KS0DW-DATA-CHGD-SW = "N"; /* indicate data unchgd*/
      return;
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* If status was changed, verify the entered code.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DM002.KS0DM-STAT == " ") /* if status not entered*/
    KS0DM002.KS0DM-STAT = "A"; /* default to 'a'*/
  end
  ;
  if (KS0DM002.KS0DM-STAT != "A") /* if status not equal to 'a' and*/
    if (KS0DM002.KS0DM-STAT != "I") /* if status not equal to 'i'*/
      KS0DM002.KS0DM-STAT-DESC = " "; /* clear description field*/
      set KS0DM002.KS0DM-STAT cursor, bold; /* highlight*/
      converseLib.validationFailed(23); /* set msg nbr*/
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* If description lines were entered, format them so that there*/
  /* are no blank lines. (ie. if user enters only the last line,*/
  /* then move that line to line 1).*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DM002.KS0DM-HTS-CPTR-DESC[1] == " ") /* if all 5 lines are*/
    if (KS0DM002.KS0DM-HTS-CPTR-DESC[2] == " ") /* blank*/
      if (KS0DM002.KS0DM-HTS-CPTR-DESC[3] == " ")
        if (KS0DM002.KS0DM-HTS-CPTR-DESC[4] == " ")
          if (KS0DM002.KS0DM-HTS-CPTR-DESC[5] == " ")
            if (KS0DM002.KS0DM-HTS-CPTR-DESC[6] == " ")
              if (KS0DM002.KS0DM-HTS-CPTR-DESC[7] == " ")
                set KS0DM002.KS0DM-HTS-CPTR-DESC[1] cursor, bold;
                converseLib.validationFailed(37); /* set msg nbr*/
              end
            end
          end
        end
      end
    end
  end
  ;
  KS0DW00.KS0DW-LINE[1] = KS0DM002.KS0DM-HTS-CPTR-DESC[1];
  KS0DW00.KS0DW-LINE[2] = KS0DM002.KS0DM-HTS-CPTR-DESC[2];
  KS0DW00.KS0DW-LINE[3] = KS0DM002.KS0DM-HTS-CPTR-DESC[3];
  KS0DW00.KS0DW-LINE[4] = KS0DM002.KS0DM-HTS-CPTR-DESC[4];
  KS0DW00.KS0DW-LINE[5] = KS0DM002.KS0DM-HTS-CPTR-DESC[5];
  KS0DW00.KS0DW-LINE[6] = KS0DM002.KS0DM-HTS-CPTR-DESC[6];
  KS0DW00.KS0DW-LINE[7] = KS0DM002.KS0DM-HTS-CPTR-DESC[7];
  ;
  KS0DM002.KS0DM-HTS-CPTR-DESC[1] = " "; /* clear screen lines*/
  KS0DM002.KS0DM-HTS-CPTR-DESC[2] = " ";
  KS0DM002.KS0DM-HTS-CPTR-DESC[3] = " ";
  KS0DM002.KS0DM-HTS-CPTR-DESC[4] = " ";
  KS0DM002.KS0DM-HTS-CPTR-DESC[5] = " ";
  KS0DM002.KS0DM-HTS-CPTR-DESC[6] = " ";
  KS0DM002.KS0DM-HTS-CPTR-DESC[7] = " ";
  ;
  KS0DW00.KS0DW-IX1 = 1; /* set index for w-s*/
  KS0DW00.KS0DW-IX2 = 1; /* set index for screen*/
  ;
  /* ------------------------------------------------------------*/
  /* the following code will compress the description lines from*/
  /* working-storage to the screen.*/
  /* ------------------------------------------------------------*/
  ;
  while (KS0DW00.KS0DW-IX1 <= 7)
    if (KS0DW00.KS0DW-LINE[KS0DW-IX1] == " ") /* if this line blank*/
      KS0DW00.KS0DW-IX1 = KS0DW00.KS0DW-IX1 + 1; /* set index up by 1*/
    else
      KS0DM002.KS0DM-HTS-CPTR-DESC[KS0DW-IX2] = KS0DW00.KS0DW-LINE[KS0DW-IX1];
      KS0DW00.KS0DW-IX1 = KS0DW00.KS0DW-IX1 + 1; /* set index up by 1*/
      KS0DW00.KS0DW-IX2 = KS0DW00.KS0DW-IX2 + 1; /* set index up by 1*/
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* if the chapter number short description not entered,*/
  /* indicate error.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DM002.KS0DM-HTS-CPTR-SDESC == " ") /* if equal spaces*/
    set KS0DM002.KS0DM-HTS-CPTR-SDESC cursor, bold; /* highlight*/
    converseLib.validationFailed(36); /* set msg nbr*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* if add requested and the chapter number is not entered,*/
  /* indicate error.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-FNC == "A") /* if add requested*/
    if (KS0DM002.KS0DM-HTS-CPTR-CD == " ") /* if equal spaces*/
      set KS0DM002.KS0DM-HTS-CPTR-CD cursor, bold; /* highlight*/
      converseLib.validationFailed(34); /* set msg nbr*/
    else
      KS0DW00.KS0DW-HTS-NUMBER = KS0DM002.KS0DM-HTS-CPTR-CD;
      if (KS0DW00.KS0DW-HTS-BYTE[1] == " " /* if either position is blank*/
       || KS0DW00.KS0DW-HTS-BYTE[2] == " ") /* it's in error*/
        set KS0DM002.KS0DM-HTS-CPTR-CD cursor, bold; /* highlight*/
        converseLib.validationFailed(34); /* set msg nbr*/
      end
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* If any errors were found, exit to display the map.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end
  ;
end // end KS0D-25000

// Process valid input request
Function KS0D-26000()
  /* ------------------------------------------------------------*/
  /* Indicate that the map must be formatted before display.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-SWF = "Y"; /* Format the map switch*/
  ;
  /* ------------------------------------------------------------*/
  /* Obtain the current date and time.*/
  /* ------------------------------------------------------------*/
  ;
  XX0XS01(); /* Obtain date and time*/
  ;
  /* ------------------------------------------------------------*/
  /* If this is an 'update' function, update the table row.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-FNC == "C") /* is this an change request?*/
    ;
    if (KS0DW00.KS0DW-DATA-CHGD-SW == "Y") /* if any data changed*/
      ;
      KS0DW00.KS0DW-DATA-CHGD-SW = "N"; /* turn off data chgd sw*/
      ;
      /* ------------------------------------------------------------*/
      /* Update the changed data in the table row.*/
      /* ------------------------------------------------------------*/
      ;
      KS0D-26100(); /* Update table row*/
      ;
      KS0DW00.KS0DW-SWR = "Y"; /* Request for data switch*/
      ;
      if (converseVar.validationMsgNum != 0) /* if not successful*/
        KS0DW00.KS0DW-SWD = "2"; /* request map 2*/
        KS0DW00.KS0DW-SWF = "Y"; /* request format map 2*/
        return;
      end
      /* ------------------------------------------------------------*/
      /* Indicate that the table update was successfully completed.*/
      /* ------------------------------------------------------------*/
      ;
      KS0DW00.KS0DW-LAST-CHGD-CPTR = VHSCHP01.KS-HTS-CPTR-CD;
      ;
      KSWREC.XXXPRCNM = "26000"; /* Module identification*/
      KSWREC.XXXMSGNO = 1; /* Table update was completed*/
    else
      ;
      KS0DW00.KS0DW-SWR = "N"; /* do not request data*/
      KS0DW00.KS0DW-SWF = "N"; /* do not format map 2*/
      KS0DW00.KS0DW-LAST-CHGD-CPTR = KS0DM002.KS0DM-HTS-CPTR-CD;
      ;
    end
    ;
    KS0D-26300(); /* Search for next selection*/
    ;
    /* ------------------------------------------------------------*/
    /* Commit all changes made to protected resources.*/
    /* ------------------------------------------------------------*/
    ;
    VGVar.handleSysLibraryErrors = 0;
    sysLib.commit(); /* Commit all changes*/
    ;
    set KS0DW01 empty; /* clear working storage*/
    ;
  else
    ;
    /* ------------------------------------------------------------*/
    /* code following is for adding chapters to the table.*/
    /* ------------------------------------------------------------*/
    ;
    KS0D-26200(); /* add row to table*/
    ;
    if (converseVar.validationMsgNum != 0) /* if not successful*/
      KS0DW00.KS0DW-SWD = "2"; /* request map 2*/
      KS0DW00.KS0DW-SWF = "N"; /* do not format map 2*/
      KS0DW00.KS0DW-SWR = "N"; /* do not request data*/
      KSWREC.XXXMSGNO = 1; /* successful update*/
      return;
    end
    ;
    KS0DW00.KS0DW-LAST-CHGD-CPTR = VHSCHP01.KS-HTS-CPTR-CD;
    ;
  end
end // end KS0D-26000

// Update the changed table row
Function KS0D-26100()
  /* ------------------------------------------------------------*/
  /* select table row first to see if it exists*/
  /* ------------------------------------------------------------*/
  ;
  set VHSCHP01 empty; /* clear work record*/
  ;
  VHSCHP01.KS-HTS-CPTR-CD = KS0DM002.KS0DM-HTS-CPTR-CD; /* cptr cd*/
  ;
  try
    call "IO6080" ("S ", SQLCA, VHSCHP01) {isNoRefresh = yes, isExternal = yes};
  end /* select row*/
  ;
  /* ------------------------------------------------------------*/
  /* If row exists, check change count*/
  /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      if (VHSCHP01.ZZZCHGCT != KS0DW00.KS0DW-SAVE-CHG-CNT) /**/
        converseLib.validationFailed(12); /* set error msg*/
        return; /* Return to previous process*/
      else
         /* next sentence*/
      end
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* Format the data from the map in the table row.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-LAST-CHGD-CPTR = KS0DM002.KS0DM-HTS-CPTR-CD;
  VHSCHP01.KS-HTS-CPTR-CD = KS0DM002.KS0DM-HTS-CPTR-CD; /* cptr cd*/
  VHSCHP01.KS-HTS-CPTR-SDESC = KS0DM002.KS0DM-HTS-CPTR-SDESC;
  VHSCHP01.KS-HTS-CPTR-DESC[1] = KS0DM002.KS0DM-HTS-CPTR-DESC[1];
  VHSCHP01.KS-HTS-CPTR-DESC[2] = KS0DM002.KS0DM-HTS-CPTR-DESC[2];
  VHSCHP01.KS-HTS-CPTR-DESC[3] = KS0DM002.KS0DM-HTS-CPTR-DESC[3];
  VHSCHP01.KS-HTS-CPTR-DESC[4] = KS0DM002.KS0DM-HTS-CPTR-DESC[4];
  VHSCHP01.KS-HTS-CPTR-DESC[5] = KS0DM002.KS0DM-HTS-CPTR-DESC[5];
  VHSCHP01.KS-HTS-CPTR-DESC[6] = KS0DM002.KS0DM-HTS-CPTR-DESC[6];
  VHSCHP01.KS-HTS-CPTR-DESC[7] = KS0DM002.KS0DM-HTS-CPTR-DESC[7];
  VHSCHP01.XXXSTAT = KS0DM002.KS0DM-STAT;
  VHSCHP01.XXXUSRID = COMMAREA.CAUSERID;
  ;
  VHSCHP01.ZZZCHGDT = XX0XW01.XX0XWDAT; /* Change date*/
  VHSCHP01.ZZZCHGTM = XX0XW01.XX0XWTIM; /* Change time*/
  ;
  /* ------------------------------------------------------------*/
  /* Update the changed row in the table.*/
  /* ------------------------------------------------------------*/
  ;
  try
    call "IO6080" ("U ", SQLCA, VHSCHP01) {isNoRefresh = yes, isExternal = yes};
  end /* Update row*/
  ;
  /* ------------------------------------------------------------*/
  /* If successful, return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      return; /* Return to previous process*/
    end
    ;
    /* ------------------------------------------------------------*/
    /* If row not found, indicate the data was updated by another.*/
    /* ------------------------------------------------------------*/
    ;
    if (SQLCA.VAGen_SQLCODE == 100) /* Row not found*/
      ;
      VGVar.handleSysLibraryErrors = 0;
      sysLib.rollback(); /* Roll back all changes*/
      ;
      KS0DW00.KS0DW-SWR = "Y"; /* Request for data switch*/
      KS0DW00.KS0DW-SWF = "Y"; /* format the map switch*/
      KS0DW00.KS0DW-SWD = "2"; /* display map 2*/
      ;
      KSWREC.XXXPRCNM = "26100"; /* Module identification*/
      KSWREC.XXXMSGNO = 12; /* Data was updated by another*/
      ;
      exit stack; /* Exit to display map*/
      ;
    end
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If data not available, return an error message to the user.*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/
  ;
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
  ;
  KSWREC.XXXPRCNM = "26100"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(11); /* Data in use by another*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    VGVar.handleSysLibraryErrors = 0;
    sysLib.rollback(); /* Roll back all changes*/
    ;
    converseLib.validationFailed(10); /* Unexpected error occurred*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "UPDATE THE TABLE ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = KSWREC.XXXPRCNM; /* Program number*/
