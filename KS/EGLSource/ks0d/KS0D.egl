// add a table row
Function KS0D-26200()
  /* ------------------------------------------------------------*/
  /* select table row first to see if it exists*/
  /* ------------------------------------------------------------*/
  ;
  set VHSCHP01 empty; /* clear work record*/
  ;
  VHSCHP01.KS-HTS-CPTR-CD = KS0DM002.KS0DM-HTS-CPTR-CD; /* cptr cd*/
  ;
  try
    call "IO6080" ("S ", SQLCA, VHSCHP01) {isNoRefresh = yes, isExternal = yes};
  end /* select row*/
  ;
  /* ------------------------------------------------------------*/
  /* If row exists, return with error msg*/
  /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      converseLib.validationFailed(38); /* set error msg*/
      return; /* Return to previous process*/
    else
       /* next sentence*/
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* Format the data from the map in the table row.*/
  /* ------------------------------------------------------------*/
  ;
  set VHSCHP01 empty; /* clear work record*/
  ;
  VHSCHP01.KS-HTS-CPTR-CD = KS0DM002.KS0DM-HTS-CPTR-CD; /* cptr cd*/
  VHSCHP01.KS-HTS-CPTR-SDESC = KS0DM002.KS0DM-HTS-CPTR-SDESC;
  VHSCHP01.KS-HTS-CPTR-DESC[1] = KS0DM002.KS0DM-HTS-CPTR-DESC[1];
  VHSCHP01.KS-HTS-CPTR-DESC[2] = KS0DM002.KS0DM-HTS-CPTR-DESC[2];
  VHSCHP01.KS-HTS-CPTR-DESC[3] = KS0DM002.KS0DM-HTS-CPTR-DESC[3];
  VHSCHP01.KS-HTS-CPTR-DESC[4] = KS0DM002.KS0DM-HTS-CPTR-DESC[4];
  VHSCHP01.KS-HTS-CPTR-DESC[5] = KS0DM002.KS0DM-HTS-CPTR-DESC[5];
  VHSCHP01.KS-HTS-CPTR-DESC[6] = KS0DM002.KS0DM-HTS-CPTR-DESC[6];
  VHSCHP01.KS-HTS-CPTR-DESC[7] = KS0DM002.KS0DM-HTS-CPTR-DESC[7];
  VHSCHP01.XXXSTAT = KS0DM002.KS0DM-STAT;
  VHSCHP01.XXXUSRID = COMMAREA.CAUSERID;
  ;
  VHSCHP01.ZZZSDT = XX0XW01.XX0XWDAT; /* Set-up date*/
  VHSCHP01.ZZZCHGDT = XX0XW01.XX0XWDAT; /* change date*/
  VHSCHP01.ZZZCHGTM = XX0XW01.XX0XWTIM; /* change time*/
  ;
  /* ------------------------------------------------------------*/
  /* add the row in the table.*/
  /* ------------------------------------------------------------*/
  ;
  try
    call "IO6080" ("A ", SQLCA, VHSCHP01) {isNoRefresh = yes, isExternal = yes};
  end /* Update row*/
  ;
  /* ------------------------------------------------------------*/
  /* If successful, return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000") /* Call successful*/
    ;
    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
      KS0DW00.KS0DW-SAVE-ADD-CNT = KS0DW00.KS0DW-SAVE-ADD-CNT + 1;
      return; /* Return to previous process*/
    end
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If data not added, return an error message to the user.*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/
  ;
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
  ;
  KSWREC.XXXPRCNM = "26200"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(11); /* Data in use by another*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    VGVar.handleSysLibraryErrors = 0;
    sysLib.rollback(); /* Roll back all changes*/
    ;
    converseLib.validationFailed(10); /* Unexpected error occurred*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "ADD THE TABLE ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = KSWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1DBASE = "DCUSTM  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VHSCHP01"; /* Table view name*/
  TA1REC.TA1TBLKE = VHSCHP01.KS-HTS-CPTR-CD; /* Table key*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
end // end KS0D-26200

// search for next selection
Function KS0D-26300()
  /* ------------------------------------------------------------*/
  /* restore index value of last selected chapter, which was*/
  /* just changed.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-SEL-INDEX; /* restore index value*/
  KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* set up by 1*/
  ;
  /* ------------------------------------------------------------*/
  /* Determine if another line was selected for viewing.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-NEXT-SEL-CPTR = " "; /* clear next selected cptr*/
  ;
  while (KS0DW00.KS0DW-NEXT-SEL-CPTR == " " /* until a cptr is found*/
   && KS0DW00.KS0DW-IXM <= 15) /* and index <= 15*/
    ;
    if (KS0DW00.KS0DW-SELECTED-CPTRS[KS0DW-IXM] > " ") /* Line selected?*/
      KS0DW00.KS0DW-NEXT-SEL-CPTR = KS0DW00.KS0DW-SELECTED-CPTRS[KS0DW-IXM]; /* next cptr to chg*/
      KS0DW00.KS0DW-LAST-CHGD-CPTR = KS0DW00.KS0DW-SELECTED-CPTRS[KS0DW-IXM]; /* last chgd cptr*/
      KS0DW00.KS0DW-SEL-INDEX = KS0DW00.KS0DW-IXM; /* save index value*/
      KS0DW00.KS0DW-SWR = "Y"; /* Request for data switch*/
      KS0DW00.KS0DW-SWF = "Y"; /* Request format map 2*/
      return; /* process next chapter*/
    else
      KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* set up by 1*/
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* clear table of selected chapters*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-IXM = 1; /* initialize index*/
  ;
  while (KS0DW00.KS0DW-IXM <= 15) /* while index <= 15*/
    KS0DW00.KS0DW-SELECTED-CPTRS[KS0DW-IXM] = " "; /* clear field*/
    KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* set up by 1*/
  end
  ;
  KS0DW00.KS0DW-SWR = "Y"; /* Request for data switch*/
  KS0DW00.KS0DW-SWF = "Y"; /* format the map*/
  KS0DW00.KS0DW-SWD = "1"; /* Map to be displayed switch*/
  exit stack; /* go display screen #1*/
  ;
end // end KS0D-26300

