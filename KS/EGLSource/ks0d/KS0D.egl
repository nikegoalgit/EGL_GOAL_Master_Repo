// Check attention identifier
Function KS0D-13000()
  /* ------------------------------------------------------------*/
  /* If CANCEL was requested, clear the map fields and exit.*/
  /* ------------------------------------------------------------*/
  KSWREC.KSW-HTS-NBR = " ";
  ;
  if (converseVar.eventKey is pakey) /* PA2 was pressed*/
    ;
    if (KS0DW00.KS0DW-SWR == "Y") /* Request for data*/
      set KS0DW01 empty; /* M001 display storage*/
    end
    ;
    KS0DW00.KS0DW-SWF = "Y"; /* Format the map switch*/
    KS0DW00.KS0DW-SWR = "N"; /* Request for data switch*/
    ;
    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If FAST PATH was requested, transfer to the application.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DM001.CATOAP > " ") /* FAST PATH was requested*/
    ;
    if (converseVar.eventKey is enter) /* ENTER was pressed*/
      ;
      KS0D-11100(); /* Purge temporary storage*/
      ;
      XSKS-S0001(); /* Exit to another application*/
      ;
      set KS0DM001.CATOAP cursor, bold; /* Highlight*/
      ;
    else
      ;
      KSWREC.XXXPRCNM = "13000"; /* Module identification*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/
      ;
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4) /* or PF4 was pressed*/
    ;
    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "KS0DM001"; /* application find code*/
    end
    ;
    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      set KSWREC empty; /* clear work record*/
      COMMAREA.CATOAP = "KS0A"; /* TO application*/
    end
    ;
    KS0D-11100(); /* Purge temporary storage*/
    ;
    XSKS-S0001(); /* Exit to another application*/
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf13  /* and PF13 was not pressed*/
   && converseVar.eventKey not pf14  /* and PF14 was not pressed*/
   && converseVar.eventKey not pf15) /* and PF15 was not pressed*/
    ;
    KSWREC.XXXPRCNM = "13000"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    ;
    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end
    ;
    exit stack; /* Exit to display map*/
    ;
  end
  ;
  if (converseVar.eventKey is pf13) /* PF13 was pressed*/
    if (COMMAREA.CAADDFL == "N") /* chg flag = no*/
      KSWREC.XXXPRCNM = "13000"; /* Module identification*/
      converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    end
  end
  ;
  if (converseVar.eventKey is pf14) /* PF14 was pressed*/
    if (COMMAREA.CACHGFL == "N") /* chg flag = no*/
      KSWREC.XXXPRCNM = "13000"; /* Module identification*/
      converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/
    end
  end
  ;
end // end KS0D-13000

// Reset highlighted fields
Function KS0D-14000()
  /* ------------------------------------------------------------*/
  /* Reset all modifiable field attributes to normal intensity.*/
  /* ------------------------------------------------------------*/
  ;
  set KS0DM001.KS0DM-HTS-NBR initialAttributes; /* HTS number*/
  set KS0DM001.KS0DM-HTS-CPTR-CD initialAttributes; /* Chapter cd*/
  set KS0DM001.KS0DM-STAT initialAttributes; /* Status*/
  ;
   /* the attributes for the detail lines are also set in ks0d-12100*/
  ;
  KS0DW00.KS0DW-IXM = 1; /* set index to 1*/
  while (KS0DW00.KS0DW-IXM <= 15) /* for all detail lines*/
    if (KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM] > " ")
      set KS0DM001.KS0DMSEL[KS0DW-IXM] normal;
    else
      set KS0DM001.KS0DMSEL[KS0DW-IXM] invisible, skip;
    end
    set KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM] initialAttributes;
    set KS0DM001.KS0DM-DTL-HTS-CPTR-DESC[KS0DW-IXM] initialAttributes;
    KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* set index up by 1*/
  end
  ;
  set KS0DM001.CATOAP initialAttributes; /* TO application*/
  set KS0DM001.CAITEM initialAttributes; /* Application data*/
  ;
end // end KS0D-14000

// Verify data that was entered
Function KS0D-15000()
  /* ------------------------------------------------------------*/
  /* Highlight any application data that was entered.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DM001.CAITEM > " ") /* Application data was entered*/
    ;
    KSWREC.XXXPRCNM = "15000"; /* Module identification*/
    set KS0DM001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Determine whether a valid request to scroll was entered.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/
    ;
    KS0D-15100(); /* Verify scroll request*/
    set KSWREC empty; /* clear kswrec*/
  else
    ;
    KS0D-15200(); /* Verify data in the line*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* If any errors were found, exit to display the map.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end
  ;
end // end KS0D-15000

// Verify scroll request
Function KS0D-15100()
  /* ------------------------------------------------------------*/
  /* If scroll back was requested, ensure that pages exist.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && KS0DW00.XXXTSPAG < 2)       /* and no previous pages*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    converseLib.validationFailed(2); /* No previous page to display*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If scroll forward was requested, ensure that pages exist.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && KS0DW00.XXXTSPAG >= KS0DW00.TA5TSQIX) /* and no more pages*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    converseLib.validationFailed(3); /* No more pages to display*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Indicate an error for any data that was modified.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-IXM = 15; /* Initialize line index*/
  ;
  while (KS0DW00.KS0DW-IXM > 0) /* Until end of lines*/
    ;
    if (KS0DM001.KS0DMSEL[KS0DW-IXM] != KS0DW01.KS0DW-SEL[KS0DW-IXM])
      ;
      KSWREC.XXXPRCNM = "15100"; /* Module identification*/
      set KS0DM001.KS0DMSEL[KS0DW-IXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(14); /* Field may not change*/
      ;
    end
    ;
    KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM - 1; /* Decrement line number*/
    ;
  end
  ;
  if (KS0DM001.KS0DM-STAT != KS0DW01.KS0DW-STAT) /* Status changed*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    set KS0DM001.KS0DM-STAT cursor, bold; /* Highlight*/
    converseLib.validationFailed(14); /* Field may not change*/
    ;
  end
  ;
  if (KS0DM001.KS0DM-HTS-CPTR-CD != KS0DW01.KS0DW-HTS-CPTR-CD) /* cptr chgd*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    set KS0DM001.KS0DM-HTS-CPTR-CD cursor, bold; /* Highlight*/
    converseLib.validationFailed(14); /* Field may not change*/
    ;
  end
  ;
  if (KS0DM001.KS0DM-HTS-NBR != KS0DW01.KS0DW-HTS-NBR) /* hts # changed*/
    ;
    KSWREC.XXXPRCNM = "15100"; /* Module identification*/
    set KS0DM001.KS0DM-HTS-NBR cursor, bold; /* Highlight*/
    converseLib.validationFailed(14); /* Field may not change*/
    ;
  end
  ;
end // end KS0D-15100

// Verify data in the line
Function KS0D-15200()
  /* ------------------------------------------------------------*/
  /* If status was entered and is not an 'a', 'i' or ' ', then it*/
  /* is in error.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DM001.KS0DM-STAT != " ") /* if status not equal space*/
    if (KS0DM001.KS0DM-STAT != "A") /* and not equal 'a'*/
      if (KS0DM001.KS0DM-STAT != "I") /* and not equal 'i'*/
                                  /* then it's invalid*/
        KSWREC.XXXPRCNM = "15200"; /* Module identification*/
        set KS0DM001.KS0DM-STAT cursor, bold; /* Highlight*/
        converseLib.validationFailed(19); /* invalid status code*/
      end
    end
  end
  ;
  ;
  /* ------------------------------------------------------------*/
  /* Verify the format of the HTS nbr entered.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DM001.KS0DM-HTS-NBR > " ") /* if hts # entered*/
    KS0DW00.KS0DW-HTS-NUMBER = KS0DM001.KS0DM-HTS-NBR;
    KS0D-15210();
  end
  ;
  if (converseVar.validationMsgNum != 0) /* if there were any errors*/
    return; /* return to previous process*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* If selection was entered and is not a "S", indicate that*/
  /* only "S" may be entered to select a line.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-IXM = 15; /* set index*/
  ;
  while (KS0DW00.KS0DW-IXM >= 1)
    if (KS0DM001.KS0DMSEL[KS0DW-IXM] != KS0DW01.KS0DW-SEL[KS0DW-IXM]
     && KS0DM001.KS0DMSEL[KS0DW-IXM] != "S") /* and is not a "S"*/
      ;
      KSWREC.XXXPRCNM = "15200"; /* Module identification*/
      set KS0DM001.KS0DMSEL[KS0DW-IXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(13); /* Only "S" may be entered*/
      ;
    end
    KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM - 1; /* decrement index*/
  end
  ;
end // end KS0D-15200

// verify format of HTS number
Function KS0D-15210()
  /* ------------------------------------------------------------*/
  /* Verify the format of the HTS nbr entered.*/
  /* ------------------------------------------------------------*/
  /*  */
  /* ------------------------------------------------------------*/
  /* correct format is ===> 9999.99.9999*/
  /* this routine verifies that there is a ' ' or '.' in positions*/
  /* 5 and 8. If any of the other positions contain a '.', it*/
  /* is in error.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-IXM = 1; /* initialize index*/
  ;
  while (KS0DW00.KS0DW-IXM <= 14)
    if (KS0DW00.KS0DW-IXM == 5 /* these 3 positions must be blank or*/
     || KS0DW00.KS0DW-IXM == 8 /* equal '.'. if not, it's in error*/
     || KS0DW00.KS0DW-IXM == 14)
      if (KS0DW00.KS0DW-IXM == 5 /* these 3 positions must be blank or*/
       || KS0DW00.KS0DW-IXM == 8) /* equal '.'. if not, it's in error*/
        if (KS0DW00.KS0DW-HTS-BYTE[KS0DW-IXM] != " ")
          if (KS0DW00.KS0DW-HTS-BYTE[KS0DW-IXM] != ".")
            KSWREC.XXXPRCNM = "15210"; /* Module id*/
            set KS0DM001.KS0DM-HTS-NBR cursor, bold;
            converseLib.validationFailed(35); /* Invalid HTS nbr*/
          end
        end
      else
        if (KS0DW00.KS0DW-IXM == 14) /* equal ' '. if not, it's in error*/
          if (KS0DW00.KS0DW-HTS-BYTE[KS0DW-IXM] != " ")
            KSWREC.XXXPRCNM = "15210"; /* Module id*/
            set KS0DM001.KS0DM-HTS-NBR cursor, bold;
            converseLib.validationFailed(35); /* Invalid HTS nbr*/
          end
        end
      end
    else
      if (KS0DW00.KS0DW-HTS-BYTE[KS0DW-IXM] == ".")
        KSWREC.XXXPRCNM = "15210"; /* Module id*/
        set KS0DM001.KS0DM-HTS-NBR cursor, bold;
        converseLib.validationFailed(35); /* Invalid HTS nbr*/
      end
    end
    KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* set index up by 1*/
  end
  ;
end // end KS0D-15210

// Process valid input request
Function KS0D-16000()
  /* ------------------------------------------------------------*/
  /* Indicate that the map must be formatted before display.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-SWF = "Y"; /* Format the map switch*/
  ;
  /* ------------------------------------------------------------*/
  /* If a scroll backward was requested, decrement the page*/
  /* number and return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf7) /* Scroll back requested*/
    ;
    KS0DW00.XXXTSPAG = KS0DW00.XXXTSPAG - 1; /* Decrement page number*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If a scroll forward was requested, increment the page*/
  /* number and return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is pf8) /* Scroll forward requested*/
    ;
    KS0DW00.XXXTSPAG = KS0DW00.XXXTSPAG + 1; /* Increment page number*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* if enter was pressed and a chapter was entered, find that chapter*/
  /* in the table and position the cursor on that chapter.*/
  /*  */
  /* if enter was pressed and a selection was made, move selected*/
  /* chapter number to kswrec and xfer to KS0E.*/
  /* ------------------------------------------------------------*/
  ;
  if (converseVar.eventKey is enter) /* enter pressed?*/
    ;
    if (KS0DM001.KS0DM-HTS-NBR > " ") /* if hts nbr entered*/
      KS0DW00.KS0DW-HTS-NBR = KS0DM001.KS0DM-HTS-NBR;
      ;
      if (KS0DW00.KS0DW-HTS-2ND-SHDG-CD != " "
       || KS0DW00.KS0DW-HTS-1ST-SHDG-CD != " ")
        COMMAREA.CATOAP = "KS0G"; /* set destination application*/
      else
        ;
        if (KS0DW00.KS0DW-HTS-HDG-CD != " ")
          COMMAREA.CATOAP = "KS0F"; /* set destination application*/
        else
          ;
          if (KS0DW00.KS0DW-HTS-CPTR-CD != " ")
            COMMAREA.CATOAP = "KS0E"; /* set destination application*/
          end
        end
      end
      ;
      KSWREC.KSW-HTS-NBR = KS0DM001.KS0DM-HTS-NBR;
      ;
      KS0D-11100(); /* clear temp storage*/
      ;
      XSKS-S0001(); /* exit to another application*/
      ;
      KS0DW00.KS0DW-SWD = "1"; /* set map to be displayed switch*/
      ;
      exit stack;
    end
    ;
    if (KS0DM001.KS0DM-HTS-CPTR-CD > " ") /* chapter nbr entered?*/
      KS0DW00.KS0DW-LAST-CHGD-CPTR = KS0DM001.KS0DM-HTS-CPTR-CD; /* save cptr nbr*/
      KS0DW00.KS0DW-SWF = "Y"; /* format the map*/
      set KSWREC empty; /* clear pass area*/
      return; /* return*/
    end
    ;
    if (KS0DM001.KS0DM-STAT != KS0DW00.KS0DW-STAT) /* if status changed*/
      KS0DW00.KS0DW-SWR = "Y"; /* set request for data switch*/
      set KS0DW01 empty; /* clear tsq rec area*/
      KS0DW00.KS0DW-STAT = KS0DM001.KS0DM-STAT; /* save new status*/
      return; /* return*/
    end
    ;
    KS0DW00.KS0DW-IXM = 1; /* reset index*/
    while (KS0DW00.KS0DW-IXM <= 15) /* Until end of lines*/
      ;
      if (KS0DM001.KS0DMSEL[KS0DW-IXM] == "S") /* this line selected?*/
        ;
        KSWREC.KS-HTS-CPTR-CD = KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM];
        KS0D-11100(); /* clear temp storage*/
        ;
        COMMAREA.CATOAP = "KS0E"; /* set destination application*/
        ;
        XSKS-S0001(); /* exit to another application*/
        ;
        KS0DW00.KS0DW-SWD = "1"; /* set map to be displayed switch*/
        ;
        exit stack;
        ;
      end
      KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* Increment line index*/
    end
    ;
  end
  ;
  if (converseVar.eventKey is pf13) /* F13 pressed?*/
    KS0DW00.KS0DW-SWD = "2"; /* Map to be displayed switch*/
    KS0DW00.KS0DW-FNC = "A"; /* set function switch*/
    KS0DW00.KS0DW-SAVE-ADD-CNT = 0; /* set add cntr to control*/
                                         /* msg on map 2*/
  end
  ;
  KS0DW00.KS0DW-NEXT-SEL-CPTR = " "; /* clear sel field save*/
  ;
  if (converseVar.eventKey is pf14) /* F14 pressed?*/
    KS0DW00.KS0DW-SWR = "Y"; /* data is requested*/
    KS0DW00.KS0DW-SWD = "2"; /* Map to be displayed switch*/
    KS0DW00.KS0DW-FNC = "C"; /* set function switch*/
    KS0DW00.KS0DW-IXM = 1; /* reset index*/
    while (KS0DW00.KS0DW-IXM <= 15) /* Until end of lines*/
      ;
      if (KS0DM001.KS0DMSEL[KS0DW-IXM] == "S") /* this line selected?*/
        if (KS0DW00.KS0DW-NEXT-SEL-CPTR == " ") /* first one blank*/
          KS0DW00.KS0DW-SELECTED-CPTRS[KS0DW-IXM] = KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM]; /* save selected cptrs*/
          KS0DW00.KS0DW-NEXT-SEL-CPTR = KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM]; /* save next chapter to change*/
          KS0DW00.KS0DW-SEL-INDEX = KS0DW00.KS0DW-IXM; /* selected idx*/
        else
          KS0DW00.KS0DW-SELECTED-CPTRS[KS0DW-IXM] = KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM]; /* save selected cptrs*/
        end
      end
      KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* Increment line index*/
      ;
    end
    if (KS0DW00.KS0DW-NEXT-SEL-CPTR == " ") /* if none were selected*/
      converseLib.validationFailed(43); /* set error msg nbr*/
      KS0DW00.KS0DW-SWR = "N"; /* do not request data*/
      KS0DW00.KS0DW-SWF = "N"; /* do not format map*/
      KS0DW00.KS0DW-SWD = "1"; /* Map to be displayed switch*/
      return; /* return*/
    end
  end
  ;
  if (converseVar.eventKey is pf15) /* F15 pressed?*/
    set KSWREC empty; /* clear pass record*/
    KS0DW00.KS0DW-IXM = 1; /* reset index*/
    while (KS0DW00.KS0DW-IXM <= 15) /* Until end of lines*/
      ;
      if (KS0DM001.KS0DMSEL[KS0DW-IXM] == "S") /* this line selected?*/
        ;
        KSWREC.KSW-HTS-NBR = KS0DM001.KS0DM-DTL-HTS-CPTR-CD[KS0DW-IXM]; /* take the selected nbr*/
        KS0DW00.KS0DW-IXM = 16; /* end the search*/
        ;
      end
      KS0DW00.KS0DW-IXM = KS0DW00.KS0DW-IXM + 1; /* Increment line index*/
    end
    KS0D-11100(); /* clear temp storage*/
    ;
    COMMAREA.CATOAP = "KS0E"; /* set destination application*/
    ;
    XSKS-S0001(); /* exit to another application*/
    ;
    KS0DW00.KS0DW-SWD = "1"; /* set map to be displayed switch*/
    ;
    exit stack;
    ;
  end
  ;
end // end KS0D-16000

// Display the selected table row
Function KS0D-20000()
  /* ------------------------------------------------------------*/
  /* This is the main process used to display a map.  Processing*/
  /* always returns to the top of this process and falls through*/
  /* subordinate processes until an error is found or a valid*/
  /* request is processed.  Processing sequence is as follows:*/
  /*  */
  /* 1) Obtain the data to display.*/
  /* 2) Format the map output area and display the map.*/
  /* 3) Check the attention identifier used to respond.*/
  /* 4) Reset highlighted fields to normal intensity.*/
  /* 5) Verify the data that was entered.*/
  /* 6) Process a valid input request.*/
  /*  */
  /* In all subordinate processes, "EZEFLO" is used to exit to*/
  /* redisplay the map when an error is found.  This is done to*/
  /* minimize the coding effort required to skip subsequent*/
  /* processing under error conditions.*/
  /* ------------------------------------------------------------*/
  ;
  KS0D-21000(); /* Obtain the data to display*/
  ;
  KS0D-22000(); /* Format and display the map*/
  ;
  KS0D-23000(); /* Check attention identifier*/
  ;
  KS0D-24000(); /* Reset fields to normal intensity*/
  ;
  KS0D-25000(); /* Verify the entered data*/
  ;
  KS0D-26000(); /* Process valid input request*/
  ;
  ;
end // end KS0D-20000

// Obtain the data to display
Function KS0D-21000()
  /* ------------------------------------------------------------*/
  /* If data was not requested or an error was found, return to*/
  /* the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-SWR == "N") /* Data was not requested*/
    if (converseVar.validationMsgNum != 0) /* any errors found ?*/
      return; /* Return to previous process*/
    end
  end
  ;
  /* ------------------------------------------------------------*/
  /* If an add function was requested, there will be no data to*/
  /* display, so return to the previous process.*/
  /* ------------------------------------------------------------*/
  ;
  if (KS0DW00.KS0DW-FNC == "A") /* if add function?*/
    return; /* return to previous process*/
  end
  ;
  /* ------------------------------------------------------------*/
  /* Clear the map display storage area and save the key fields.*/
  /* ------------------------------------------------------------*/
  ;
  set KS0DW02 empty; /* M002 display storage*/
  ;
  KS0DW02.KS0DW-HTS-CPTR-CD = KS0DW00.KS0DW-NEXT-SEL-CPTR;
  ;
  /* ------------------------------------------------------------*/
  /* Obtain the table row to be displayed.*/
  /* ------------------------------------------------------------*/
  ;
  KS0D-21100(); /* Obtain the table row*/
  ;
  /* ------------------------------------------------------------*/
  /* Save the data from the table row in display storage.*/
  /* ------------------------------------------------------------*/
  ;
  if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/
    ;
    KS0D-21200(); /* Save in display storage*/
    ;
  else
    ;
    KSWREC.XXXPRCNM = "21000"; /* Module identification*/
    KSWREC.XXXMSGNO = 18; /* No data was found to display*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* Indicate that the request for data is complete.*/
  /* ------------------------------------------------------------*/
  ;
  KS0DW00.KS0DW-SWR = "N"; /* Request for data switch*/
  ;
end // end KS0D-21000

// Select the requested table row
Function KS0D-21100()
  /* ------------------------------------------------------------*/
  /* Select the requested row from the table.*/
  /* ------------------------------------------------------------*/
  ;
  set VHSCHP01 empty; /* Clear row storage area*/
  ;
  VHSCHP01.KS-HTS-CPTR-CD = KS0DW00.KS0DW-NEXT-SEL-CPTR;
  ;
  try
    call "IO6080" ("S ", SQLCA, VHSCHP01) {isNoRefresh = yes, isExternal = yes};
  end /* Select unique*/
  ;
  /* ------------------------------------------------------------*/
  /* If successful, initialize the line index and cursor switch.*/
  /* ------------------------------------------------------------*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/
    ;
    return; /* Return to previous process*/
    ;
  end
  ;
  /* ------------------------------------------------------------*/
  /* If data not available, return an error message to the user.*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/
  ;
  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/
  ;
  KSWREC.XXXPRCNM = "21100"; /* Module identification*/
  ;
  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/
    ;
    converseLib.validationFailed(11); /* Data in use by another*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/
    ;
  else
    ;
    converseLib.validationFailed(10); /* Unexpected error occurred*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/
    ;
    converseLib.clearScreen();
     /* Clear screen before display*/
    ;
  end
  ;
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT ROW FROM TABLE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = KSWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1DBASE = "DCUSTM  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VHSCHP01"; /* Table view name*/
  ;
  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/
  ;
  exit stack; /* Exit to display map*/
  ;
end // end KS0D-21100

