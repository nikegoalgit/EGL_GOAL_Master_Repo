  FF00W01.XXXTSLEN = FF00W00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5TSQIX = FF00W00.XXXTSPAG; /* Page number*/
