// Save & accm TARGETS
Function FF00192()
   /* ------------------------------------------------------------*/
   /* Store and accumulate targets.*/
   /* ------------------------------------------------------------*/

  if (FFWREC.YPGCD == "* ") /* if type equals all*/
    FFXREC.FFXNUMB = FF4REC.FF4RTRGT - FF4REC.FF4NLTGT; /* Dom trgt*/
  end

  if (FFWREC.YPGCD == "01") /* if type equals 'inline'*/
    /* ------------------------------------------------------------*/
     /* Calculate inline target: prdcnt trgt - nil trgt - mkup trgt*/
    /* ------------------------------------------------------------*/

    FFXREC.FFXNUMB = FF4REC.FF4RTRGT; /* load work field*/
    FFXREC.FFXNUMB = FFXREC.FFXNUMB - FF4REC.FF4NLTGT;
    FFXREC.FFXNUMB = FFXREC.FFXNUMB - FF4REC.FF4MKTGT;
  end

  if (FFWREC.YPGCD == "02") /* if type equals 'makeup'*/
    FFXREC.FFXNUMB = FF4REC.FF4MKTGT; /* Load translation fld*/
  end


  FF00W03.FF00WHSB = FFXREC.FFXNUMB; /* Hold trgt for var calc*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF00W03.FF00WTGT[FF00WCNT] = FFXREC.FFXCHAR; /* Load trgt amt*/

  FF00W03.FF00WTTB = FF00W03.FF00WTTB + FFXREC.FFXNUMB;
                                   /* accm trgt grd tot*/


end // end FF00192

// Translate TOTALS
Function FF00193()
   /* ------------------------------------------------------------*/
   /* Translate TOTALS.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF00W03.FF00WCLT[FF00WCNT];
                                   /* Load translation fld*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF00W03.FF00WTOT[FF00WCNT] = FFXREC.FFXCHAR; /* Load tot amt*/

  FF00W03.FF00WTTA = FF00W03.FF00WTTA + FFXREC.FFXNUMB; /* accm TOT grd tot*/


end // end FF00193

// Calculate VARIANCE
Function FF00194()
   /* ------------------------------------------------------------*/
   /* Calculate VARIANCE.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF00W03.FF00WCLT[FF00WCNT]; /* load total fld*/
  FFXREC.FFXNUMB = FFXREC.FFXNUMB - FF00W03.FF00WHSB; /* subtract hold trgt*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF00W03.FF00WVAR[FF00WCNT] = FFXREC.FFXCHAR; /* Load variance*/

  FF00W03.FF00WTTC = FF00W03.FF00WTTC + FFXREC.FFXNUMB; /* accm grand var*/


end // end FF00194

// Translate and load grand tots
Function FF00195()
   /* ------------------------------------------------------------*/
   /* Translate and load Grand TOTAL.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF00W03.FF00WTTA; /* Load translation fld*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF00W03.FF00WTOT[FF00WCNT] = FFXREC.FFXCHAR; /* Load amt*/

   /* ------------------------------------------------------------*/
   /* Translate and load Grand Total for TARGET.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF00W03.FF00WTTB; /* Load translation fld*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF00W03.FF00WTGT[FF00WCNT] = FFXREC.FFXCHAR; /* Load amt*/

   /* ------------------------------------------------------------*/
   /* Translate and load Grand Total for Variance.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF00W03.FF00WTTC; /* Load translation fld*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF00W03.FF00WVAR[FF00WCNT] = FFXREC.FFXCHAR; /* Load amt*/


end // end FF00195

// Format and converse the map
Function FF00200()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/

    FFWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/

  if (FF00W00.FF00WSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (FF00W00.FF00WSWF == "Y") /* Map must be formatted*/
      FF00210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FF00M001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FF00M001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FF00M001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (FFWREC.XXXMSGNO != 0) /* Message to display*/

    FF00M001.XXXMODID = FFWREC.XXXMODID; /* Module identification*/
    if (FFWREC.XXXMSGNO == 9999)
      converseLib.validationFailed();
    else
      converseLib.validationFailed(FFWREC.XXXMSGNO);
    end /* Message number*/

    set FF00M001 alarm; /* Indicate sound alarm*/

  end

  converse FF00M001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FFWREC.XXXMODID = " "; /* Module identification*/
  FFWREC.XXXMSGNO = 0; /* Message number*/

  COMMAREA.CATOAP = FF00M001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FF00M001.CAITEM; /* Application data*/


end // end FF00200

// Format the display data
Function FF00210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set FF00M001 initial; /* M001 input/output area*/

  FF00W00.FF00WSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (FF00W00.TA5TSQIX > 0) /* Pages were formatted*/

    if (FF00W00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (FF00W00.XXXTSPAG > FF00W00.TA5TSQIX) /* Desired greater highest*/
        FF00W00.XXXTSPAG = FF00W00.TA5TSQIX; /* Default to highest*/
      end

      FF00211(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set FF00M001.XXXLITPG skip; /* "PAGE" literal*/
    FF00M001.XXXTSPAG = FF00W00.XXXTSPAG; /* Current page*/
    set FF00M001.XXXLITOF skip; /* "OF" literal*/
    FF00M001.TA5TSQIX = FF00W00.TA5TSQIX; /* Highest page*/

    set FF00M001.FF00MNMA skip; /* Make "TOTAL" visable*/
    set FF00M001.FF00MNMB skip; /* Make "TARGET" visable*/
    set FF00M001.FF00MNMC skip; /* Make "VARIANCE" visable*/

  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  FF00M001.FFZBYYMM = FF00W01.FFZBYYMM; /* Beginning month*/
  FF00M001.FFZEYYMM = FF00W01.FFZEYYMM; /* Ending month*/

  FF00M001.YPGCD = FFWREC.YPGCD; /* Type*/
  FF00M001.YPGABRV = FFWREC.YPGABRV; /* Abbreviation*/

  FF00M001.YSMXDV = FFWREC.YSMXDV; /* Division group*/
  FF00M001.YSMSMDSC = FFWREC.YSMSMDSC; /* Abbreviation*/

  if (FF00W01.XPLABRV[1] > " ") /* If line exists*/
    set FF00M001.FF00MSEL[1] cursor; /* place cursor*/
  end

  FF00W00.FF00WIXM = 1; /* Initialize line index*/
  while (FF00W00.FF00WIXM <= 13 /* Until screen is full*/
   && FF00W01.XPLABRV[FF00WIXM] > " ") /* and line exists*/

    set FF00M001.FF00MSEL[FF00WIXM] normal; /* Make "SEL" visable*/
    FF00M001.XPLABRV[FF00WIXM] = FF00W01.XPLABRV[FF00WIXM];
    FF00M001.FF2CRVFL[FF00WIXM] = FF00W01.FF2CRVFL[FF00WIXM];
    FF00M001.FF00WLIN[FF00WIXM] = FF00W01.FF00WLIN[FF00WIXM];

    FF00W00.FF00WIXM = FF00W00.FF00WIXM + 1; /* Increment line index*/

  end

  FF00W00.FF00WCNT = 1; /* Initialize counter*/
  while (FF00W00.FF00WCNT <= FF00W03.FF00WNBM) /* while cnt LE nbr mo*/

    FF00M001.FF00WRMO[FF00WCNT] = FF00W01.FFZIYYMM[FF00WCNT];

    FF00M001.FF00WTOT[FF00WCNT] = FF00W03.FF00WTOT[FF00WCNT];
    FF00M001.FF00WTGT[FF00WCNT] = FF00W03.FF00WTGT[FF00WCNT];
    FF00M001.FF00WVAR[FF00WCNT] = FF00W03.FF00WVAR[FF00WCNT];

    FF00W00.FF00WCNT = FF00W00.FF00WCNT + 1; /* Increment line index*/

  end

   /* ------------------------------------------------------------*/
   /* Move one more set, past nbr of months, to pick up totals.*/
   /* ------------------------------------------------------------*/

  if (FF00W03.FF00WNBM > 0) /* If any months exist*/

    FF00M001.FF00WRMO[FF00WCNT] = "TOTL"; /* Load "TOTL" to header*/

    FF00M001.FF00WTOT[FF00WCNT] = FF00W03.FF00WTOT[FF00WCNT];
    FF00M001.FF00WTGT[FF00WCNT] = FF00W03.FF00WTGT[FF00WCNT];
    FF00M001.FF00WVAR[FF00WCNT] = FF00W03.FF00WVAR[FF00WCNT];

  end

end // end FF00210

// Obtain TSQ Record
Function FF00211()
   /* ------------------------------------------------------------*/
