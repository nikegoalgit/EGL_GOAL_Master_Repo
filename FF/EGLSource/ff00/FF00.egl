// Translate SBUY & add to tots
Function FF00155()
   /* ------------------------------------------------------------*/
   /* If there is a sug buy, add it to the accumulating sug buy.*/
   /* If at the end of the cursor, translate the accumulated*/
   /* amount, and add that to the totals.*/
   /* ------------------------------------------------------------*/

  if (FF00W00.FF00WEOC != "Y") /* If there is a sug buy*/

    FF00W00.FF00WACC = FF00W00.FF00WACC + FF6REC.FF6SUGBY;

  else

    FFXREC.FFXNUMB = FF00W00.FF00WACC; /* load for translate*/

    if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
     || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

      XSFFS12(); /* Translation number*/

    else /* Use thousands*/

      XSFFS14(); /* Translation number*/

    end

    FF00W02.FF00WAMT[FF00WCNT] = FFXREC.FFXCHAR; /* Load mask*/

    /* ------------------------------------------------------------*/
     /* Add to total for this line.*/
    /* ------------------------------------------------------------*/

    FF00W02.FF00WLNT = FF00W02.FF00WLNT + FFXREC.FFXNUMB; /* Add to tot*/

    /* ------------------------------------------------------------*/
     /* Add to total for this month (column).*/
    /* ------------------------------------------------------------*/

    FF00W00.FF00WFDA = FF00W03.FF00WCLT[FF00WCNT]; /* Load cl tot*/
    FF00W00.FF00WFDA = FF00W00.FF00WFDA + FFXREC.FFXROUND; /* Add to tot*/
    FF00W03.FF00WCLT[FF00WCNT] = FF00W00.FF00WFDA; /* Restore tot*/

  end

   /* ------------------------------------------------------------*/
   /* If no sug buy was found for a field, the field will be for-*/
   /* matted wrong.  Move a 'character' zero to display field.*/
   /* ------------------------------------------------------------*/

  if (FF00W02.FF00WAMT[FF00WCNT] == "0    ") /* If no sug buy at all*/

    FF00W02.FF00WAMT[FF00WCNT] = "   0"; /* Move to display field*/

  end
end // end FF00155

