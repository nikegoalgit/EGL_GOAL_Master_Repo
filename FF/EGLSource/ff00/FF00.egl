// Obtain & process MONTH data
Function FF00190()
   /* ------------------------------------------------------------*/
   /* Use MONTH for trgt fields, move totals, & calc variance.*/
   /* ------------------------------------------------------------*/

  FF00W00.FF00WCNT = 1; /* set counter*/

  while (FF00W00.FF00WCNT <= FFZREC.FFZNUMMO) /* count LE nbr mos*/

    /* ------------------------------------------------------------*/
     /* Clear fields.*/
    /* ------------------------------------------------------------*/

    set FF4REC empty;

    FF00W00.FF00WFDA = 0; /* Clear work field*/

    /* ------------------------------------------------------------*/
     /* Format keys.*/
    /* ------------------------------------------------------------*/

    FF4REC.FF4FCTMO = FFZREC.FFZINDMO[FF00WCNT]; /* month*/

    /* ------------------------------------------------------------*/
     /* Select MONTH.*/
    /* ------------------------------------------------------------*/

    FF00191(); /* Select MONTH*/

    /* ------------------------------------------------------------*/
     /* Save targets.  (If not found, you are moving zeroes.)*/
    /* ------------------------------------------------------------*/

    FF00192(); /* Save Target fields*/

    /* ------------------------------------------------------------*/
     /* Translate TOTALS.*/
    /* ------------------------------------------------------------*/

    FF00193(); /* Translate totals*/

    /* ------------------------------------------------------------*/
     /* Calculate VARIANCE.*/
    /* ------------------------------------------------------------*/

    FF00194(); /* calculate variance*/

    /* ------------------------------------------------------------*/
     /* Add one to counter.*/
    /* ------------------------------------------------------------*/

    FF00W00.FF00WCNT = FF00W00.FF00WCNT + 1; /* add 1 to counter*/

  end

  FF00195(); /* Translate and load totals*/


end // end FF00190

