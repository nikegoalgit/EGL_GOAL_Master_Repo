// Verify selection
Function FF0V540()
   /* ------------------------------------------------------------*/
   /* If selection was made, ensure an "S" & no prior selections.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.FF0VMSEL[FF0VWIXM] > " ") /* Selection was entered*/

    if (FF0VM001.FF0VMSEL[FF0VWIXM] != "S") /* Valid selection character*/

      FFWREC.XXXMODID = "540"; /* Module identification*/
      set FF0VM001.FF0VMSEL[FF0VWIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(41); /* Indicate only "S" may be used*/

    else

      FF0VW00.FF0VWIX2 = FF0VW00.FF0VWIXM - 1; /* Set counter*/

      while (FF0VW00.FF0VWIX2 > 0) /* While counter greater than 0*/
        if (FF0VM001.FF0VMSEL[FF0VWIX2] > " ") /* Valid selection character*/

          FFWREC.XXXMODID = "540"; /* Module identification*/
          set FF0VM001.FF0VMSEL[FF0VWIXM] cursor, bold; /* Highlight*/
          converseLib.validationFailed(73); /* More than one selection*/

        end

        FF0VW00.FF0VWIX2 = FF0VW00.FF0VWIX2 - 1; /* Decrement counter*/

      end
    end
  end


end // end FF0V540

// Process valid input request
Function FF0V600()
   /* ------------------------------------------------------------*/
   /* Indicate that the map must be formatted before display.*/
   /* ------------------------------------------------------------*/

  FF0VW00.FF0VWSWF = "Y"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the key was changed, update FFWREC, and request data.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.FFZBYYMM != FF0VW01.FFZBYYMM /* Beginning month changed*/
   || FF0VM001.FFZEYYMM != FF0VW01.FFZEYYMM /* or ending month changed*/
   || FF0VM001.XPLCD != FFWREC.XPLCD        /* or type changed*/
   || FF0VM001.YPGCD != FFWREC.YPGCD        /* or type changed*/
   || FF0VM001.YSMXDV != FFWREC.YSMXDV)     /* or division group changed*/

    /* ------------------------------------------------------------*/
     /* Update the modifiable data in the FF system commarea.*/
    /* ------------------------------------------------------------*/

    FFWREC.FFABEGMO = FFZREC.FFZBEGMO; /* Beginning month*/
    FFWREC.FFAENDMO = FFZREC.FFZENDMO; /* Ending month*/
    FFWREC.XPLCD = FF0VM001.XPLCD; /* Type code*/
    FFWREC.XPLABRV = FF0VM001.XPLABRV; /* Type name*/
    FFWREC.YPGCD = FF0VM001.YPGCD; /* Type code*/
    FFWREC.YPGABRV = FF0VM001.YPGABRV; /* Type name*/
    FFWREC.YSMXDV = FF0VM001.YSMXDV; /* Division group code*/
    FFWREC.YSMSMDSC = FF0VM001.YSMSMDSC; /* Division group name*/

    FF0VW00.FF0VWSWR = "Y"; /* Request for data switch*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    FF0VW00.XXXTSPAG = FF0VW00.XXXTSPAG - 1; /* Decrement page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    FF0VW00.XXXTSPAG = FF0VW00.XXXTSPAG + 1; /* Increment page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If line was selected, transfer to display the forecast.*/
   /* ------------------------------------------------------------*/

  if ("S" in FF0VM001.FF0VMSEL) /* Line selected*/

    FFWREC.XPCCD = FF0VW01.XPCCD[sysVar.arrayIndex]; /* Move line code*/
    FFWREC.XPCABRV = FF0VW01.XPCABRV[sysVar.arrayIndex]; /* Move line name*/

    COMMAREA.CATOAP = "FF05"; /* TO application*/

    FF0V610(); /* Transfer to display forecast*/

  end


end // end FF0V600

// Transfer to display forecast
Function FF0V610()
   /* ------------------------------------------------------------*/
   /* Purge any temporary storage and transfer control to the*/
   /* forecast application.*/
   /* ------------------------------------------------------------*/

  FF0V110(); /* Purge any temporary storage*/

  XSFFS01(); /* Exit to another application*/

   /* ------------------------------------------------------------*/
   /* If the request failed, display the returned error message.*/
   /* ------------------------------------------------------------*/

  COMMAREA.CATOAP = " "; /* TO application*/

  exit stack; /* Exit to display map*/


end // end FF0V610

