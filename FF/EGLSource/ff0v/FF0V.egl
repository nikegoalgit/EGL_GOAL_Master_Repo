// Obtain & process line trgts
Function FF0V190()
   /* ------------------------------------------------------------*/
   /* Obtain line targets, move totals, & calc variance.*/
   /* ------------------------------------------------------------*/

  FF0VW00.FF0VWCNT = 1; /* set counter*/

  while (FF0VW00.FF0VWCNT <= FFZREC.FFZNUMMO) /* count LE nbr mos*/

    /* ------------------------------------------------------------*/
     /* Clear fields.*/
    /* ------------------------------------------------------------*/

    FF0VW00.FF0VWFDA = 0; /* Clear work field*/

    /* ------------------------------------------------------------*/
     /* Obtain targets.*/
    /* ------------------------------------------------------------*/

    FF0V191(); /* Obtain LINE targets*/

    /* ------------------------------------------------------------*/
     /* Translate TOTALS.*/
    /* ------------------------------------------------------------*/

    FF0V192(); /* Translate totals*/

    /* ------------------------------------------------------------*/
     /* Calculate VARIANCE.*/
    /* ------------------------------------------------------------*/

    FF0V193(); /* calculate variance*/

    /* ------------------------------------------------------------*/
     /* Add one to counter.*/
    /* ------------------------------------------------------------*/

    FF0VW00.FF0VWCNT = FF0VW00.FF0VWCNT + 1; /* add 1 to counter*/

  end

  FF0V194(); /* Translate and load totals*/


end // end FF0V190

// Obtain targets
Function FF0V191()
   /* ------------------------------------------------------------*/
   /* Obtain target.*/
   /* ------------------------------------------------------------*/

  if (FFWREC.YPGCD != "*") /* If type, not equal 'all'*/

    /* ------------------------------------------------------------*/
     /* Obtain data from DFCAST.VMLTYP01.*/
    /* ------------------------------------------------------------*/

    FF0VS02(); /* Select MLTYP*/

    FFXREC.FFXNUMB = FF6REC.FF4RTRGT; /* Load wk fld*/

    FF0VW03.FF0VWHTG = FF6REC.FF4RTRGT; /* Hold trgt for calc*/

    FF0VS03(); /* Translate target*/

  else

    /* ------------------------------------------------------------*/
     /* Obtain data from DFCAST.VMLINE01.*/
    /* ------------------------------------------------------------*/

    FF0VS04(); /* Select MLINE*/

    FFXREC.FFXNUMB = FF5REC.FF4RTRGT; /* Load wk fld*/

    FF0VW03.FF0VWHTG = FF5REC.FF4RTRGT; /* Hold trgt for calc*/

    FF0VS03(); /* Translate target*/

  end


end // end FF0V191

// Translate TOTALS & add tot
Function FF0V192()
   /* ------------------------------------------------------------*/
   /* Translate TOTALS.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF0VW03.FF0VWCLT[FF0VWCNT];
                                   /* Load translation fld*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF0VW03.FF0VWTOT[FF0VWCNT] = FFXREC.FFXCHAR; /* Load tot amt*/

  FF0VW03.FF0VWTTA = FF0VW03.FF0VWTTA + FFXREC.FFXNUMB; /* accm TOT grd tot*/


end // end FF0V192

// Calculate VARIANCE & add tot
Function FF0V193()
   /* ------------------------------------------------------------*/
   /* Calculate VARIANCE.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF0VW03.FF0VWCLT[FF0VWCNT]; /* load total fld*/
  FFXREC.FFXNUMB = FFXREC.FFXNUMB - FF0VW03.FF0VWHTG; /* subtract hold trgt*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF0VW03.FF0VWVAR[FF0VWCNT] = FFXREC.FFXCHAR; /* Load variance*/

  FF0VW03.FF0VWTTC = FF0VW03.FF0VWTTC + FFXREC.FFXNUMB; /* accm grand var*/


end // end FF0V193

// Translate and load grand tots
Function FF0V194()
   /* ------------------------------------------------------------*/
   /* Translate and load Grand TOTAL.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF0VW03.FF0VWTTA; /* Load translation fld*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF0VW03.FF0VWTOT[FF0VWCNT] = FFXREC.FFXCHAR; /* Load amt*/

   /* ------------------------------------------------------------*/
   /* Translate and load Grand Total for TARGET.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF0VW03.FF0VWTTB; /* Load translation fld*/

   /* GET C*/
  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF0VW03.FF0VWTGT[FF0VWCNT] = FFXREC.FFXCHAR; /* Load amt*/

   /* ------------------------------------------------------------*/
   /* Translate and load Grand Total for Variance.*/
   /* ------------------------------------------------------------*/

  FFXREC.FFXNUMB = FF0VW03.FF0VWTTC; /* Load translation fld*/

  if (FFXREC.FFXNUMB >= +10000000 /* Use millions*/
   || FFXREC.FFXNUMB <= -1000000) /* Use millions*/

    XSFFS12(); /* Translation number*/

  else /* Use thousands*/

    XSFFS14(); /* Translation number*/

  end

  FF0VW03.FF0VWVAR[FF0VWCNT] = FFXREC.FFXCHAR; /* Load amt*/


end // end FF0V194

// Format and converse the map
Function FF0V200()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/

    FFWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/

  if (FF0VW00.FF0VWSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (FF0VW00.FF0VWSWF == "Y") /* Map must be formatted*/
      FF0V210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FF0VM001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FF0VM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FF0VM001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (FFWREC.XXXMSGNO != 0) /* Message to display*/

    FF0VM001.XXXMODID = FFWREC.XXXMODID; /* Module identification*/
    if (FFWREC.XXXMSGNO == 9999)
      converseLib.validationFailed();
    else
      converseLib.validationFailed(FFWREC.XXXMSGNO);
    end /* Message number*/

    set FF0VM001 alarm; /* Indicate sound alarm*/

  end

  converse FF0VM001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  FFWREC.XXXMODID = " "; /* Module identification*/
  FFWREC.XXXMSGNO = 0; /* Message number*/

  COMMAREA.CATOAP = FF0VM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = FF0VM001.CAITEM; /* Application data*/


end // end FF0V200

// Format the display data
Function FF0V210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set FF0VM001 initial; /* M001 input/output area*/

  FF0VW00.FF0VWSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (FF0VW00.TA5TSQIX > 0) /* Pages were formatted*/

    if (FF0VW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the desired page number is greater than the highest page*/
       /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (FF0VW00.XXXTSPAG > FF0VW00.TA5TSQIX) /* Desired greater highest*/
        FF0VW00.XXXTSPAG = FF0VW00.TA5TSQIX; /* Default to highest*/
      end

      FF0V211(); /* Obtain temporary storage page*/

    end

    /* ------------------------------------------------------------*/
     /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set FF0VM001.XXXLITPG skip; /* "PAGE" literal*/
    FF0VM001.XXXTSPAG = FF0VW00.XXXTSPAG; /* Current page*/
    set FF0VM001.XXXLITOF skip; /* "OF" literal*/
    FF0VM001.TA5TSQIX = FF0VW00.TA5TSQIX; /* Highest page*/

    set FF0VM001.FF0VMNMA skip; /* Make "TOTAL" visable*/
    set FF0VM001.FF0VMNMB skip; /* Make "TARGET" visable*/
    set FF0VM001.FF0VMNMC skip; /* Make "VARIANCE" visable*/

  end

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  FF0VM001.FFZBYYMM = FFWREC.FFABEGMO; /* Beginning month*/
  FF0VM001.FFZEYYMM = FFWREC.FFAENDMO; /* Ending month*/

  FF0VM001.XPLCD = FFWREC.XPLCD; /* Line*/
  FF0VM001.XPLABRV = FFWREC.XPLABRV; /* Abbreviation*/

  FF0VM001.YPGCD = FFWREC.YPGCD; /* Type*/
  FF0VM001.YPGABRV = FFWREC.YPGABRV; /* Abbreviation*/

  FF0VM001.YSMXDV = FFWREC.YSMXDV; /* Division group*/
  FF0VM001.YSMSMDSC = FFWREC.YSMSMDSC; /* Abbreviation*/

  if (FF0VW01.XPCABRV[1] > " ") /* If line exists*/
    set FF0VM001.FF0VMSEL[1] cursor; /* Place cursor*/
  end

  FF0VW00.FF0VWIXM = 1; /* Initialize line index*/
  while (FF0VW00.FF0VWIXM <= 13 /* Until screen is full*/
   && FF0VW01.XPCABRV[FF0VWIXM] > " ") /* and line exists*/

    set FF0VM001.FF0VMSEL[FF0VWIXM] normal; /* Make "SEL" visable*/
    FF0VM001.XPCABRV[FF0VWIXM] = FF0VW01.XPCABRV[FF0VWIXM];
    FF0VM001.FF2CRVFL[FF0VWIXM] = FF0VW01.FF2CRVFL[FF0VWIXM];
    FF0VM001.FF0VWLIN[FF0VWIXM] = FF0VW01.FF0VWLIN[FF0VWIXM];

    FF0VW00.FF0VWIXM = FF0VW00.FF0VWIXM + 1; /* Increment line index*/

  end

  FF0VW00.FF0VWCNT = 1; /* Initialize counter*/
  while (FF0VW00.FF0VWCNT <= FF0VW03.FF0VWNBM) /* while cnt LE nbr mo*/

    FF0VM001.FF0VWRMO[FF0VWCNT] = FF0VW01.FFZIYYMM[FF0VWCNT];

    FF0VM001.FF0VWTOT[FF0VWCNT] = FF0VW03.FF0VWTOT[FF0VWCNT];
    FF0VM001.FF0VWTGT[FF0VWCNT] = FF0VW03.FF0VWTGT[FF0VWCNT];
    FF0VM001.FF0VWVAR[FF0VWCNT] = FF0VW03.FF0VWVAR[FF0VWCNT];

    FF0VW00.FF0VWCNT = FF0VW00.FF0VWCNT + 1; /* Increment line index*/

  end

   /* ------------------------------------------------------------*/
   /* Move one more set, past nbr of months, to pick up totals.*/
   /* ------------------------------------------------------------*/

  if (FF0VW03.FF0VWNBM > 0) /* If any months exist*/

    FF0VM001.FF0VWRMO[FF0VWCNT] = "TOTL"; /* Load "TOTL" to header*/

    FF0VM001.FF0VWTOT[FF0VWCNT] = FF0VW03.FF0VWTOT[FF0VWCNT];
    FF0VM001.FF0VWTGT[FF0VWCNT] = FF0VW03.FF0VWTGT[FF0VWCNT];
    FF0VM001.FF0VWVAR[FF0VWCNT] = FF0VW03.FF0VWVAR[FF0VWCNT];

  end

end // end FF0V210

// Obtain TSQ Record
Function FF0V211()
   /* ------------------------------------------------------------*/
   /* Obtain the next page of data to display.*/
   /* ------------------------------------------------------------*/

  FF0VW01.XXXTSLEN = FF0VW00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5TSQIX = FF0VW00.XXXTSPAG; /* Page number*/
  TA5REC.TA5FNCCD = "R"; /* Function code*/

  try
    call "TA0050" (FF0VW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  FFWREC.XXXMODID = "211"; /* Module identification*/
  converseLib.validationFailed(01); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "READ PAGE FROM TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FFWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end FF0V211

// Check attention identifier
Function FF0V300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (FF0VW00.FF0VWSWR == "Y") /* Request for data*/
      set FF0VW01 empty; /* M001 display storage*/
    end

    FF0VW00.FF0VWSWF = "Y"; /* Format the map switch*/
    FF0VW00.FF0VWSWR = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      FF0V110(); /* Purge temporary storage*/

      XSFFS01(); /* Exit to another application*/

      set FF0VM001.CATOAP cursor, bold; /* Highlight*/

    else

      FFWREC.XXXMODID = "300"; /* Module identification*/
      converseLib.validationFailed(36); /* Indicate PF key not valid*/

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1  /* PF1 was pressed*/
   || converseVar.eventKey is pf3  /* or PF3 was pressed*/
   || converseVar.eventKey is pf5  /* or PF5 was pressed*/
   || converseVar.eventKey is pf13 /* or PF13 was pressed*/
   || converseVar.eventKey is pf14 /* or PF14 was pressed*/
   || converseVar.eventKey is pf16) /* or PF16 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FF0VM001"; /* Application find code*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FF03"; /* TO application*/
    end

    if (converseVar.eventKey is pf13) /* PF13 was pressed*/
      COMMAREA.CATOAP = "FF0U"; /* TO application*/
    end

    if (converseVar.eventKey is pf14) /* PF14 was pressed*/
      COMMAREA.CATOAP = "FF00"; /* TO application*/
    end

    if (converseVar.eventKey is pf16) /* PF16 was pressed*/
      COMMAREA.CATOAP = "FF0I"; /* TO application*/
    end

    FF0V110(); /* Purge temporary storage*/

    XSFFS01(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8)  /* and PF8 was not pressed*/

    FFWREC.XXXMODID = "300"; /* Module identification*/
    converseLib.validationFailed(3); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end FF0V300

// Reset highlighted fields
Function FF0V400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set FF0VM001.FFZBYYMM initialAttributes; /* Beginning month*/
  set FF0VM001.FFZEYYMM initialAttributes; /* Ending month*/
  set FF0VM001.XPLCD initialAttributes; /* Line*/
  set FF0VM001.YPGCD initialAttributes; /* Type*/
  set FF0VM001.YSMXDV initialAttributes; /* Division group*/

  FF0VW00.FF0VWIXM = 1; /* Initialize line index*/

  while (FF0VW00.FF0VWIXM <= 13 /* Until end of lines*/
   && FF0VM001.XPCABRV[FF0VWIXM] > " ") /* or end of display*/

    if (FF0VM001.XPCABRV[FF0VWIXM] > " ") /* Line exists*/
      set FF0VM001.FF0VMSEL[FF0VWIXM] normal; /* Reset select flag*/
    end

    FF0VW00.FF0VWIXM = FF0VW00.FF0VWIXM + 1; /* Increment line index*/

  end

  set FF0VM001.CATOAP initialAttributes; /* TO application*/
  set FF0VM001.CAITEM initialAttributes; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If no lists were requested, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.XPLCD == "?" /* Line list requested*/
   || FF0VM001.YPGCD == "?") /* Type list requested*/

    if (converseVar.eventKey not enter) /* ENTER was not pressed*/

      FFWREC.XXXMODID = "400"; /* Module identification*/
      converseLib.validationFailed(116); /* Indicate PF key not valid*/

      exit stack; /* Exit to display map*/

    end

  else

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of line codes.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.XPLCD == "?") /* Line list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "  LINE"; /* Heading*/
    XX0XW02.XGPCD = COMMAREA.XGPCD; /* GPC code*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXPL01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FF0VM001.XPLCD = FFWREC.XPLCD; /* Restore prior code*/
      FF0VM001.XPLABRV = FFWREC.XPLABRV; /* Abbreviation*/
    else
      FF0VM001.XPLCD = XPLTBL.XPLCD[XX0XWPIX]; /* Selected code*/
      FF0VM001.XPLABRV = XPLTBL.XPLABRV[XX0XWPIX]; /* Abbreviation*/

    end

    set FF0VM001.XPLCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of type codes.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.YPGCD == "?") /* Type list requested*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "  TYPE"; /* Heading*/
    XX0XW02.YPGFFFL = "Y"; /* Prdctn fcst flag*/

    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPYPG01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FF0VM001.YPGCD = FFWREC.YPGCD; /* Restore prior code*/
      FF0VM001.YPGABRV = FFWREC.YPGABRV; /* Abbreviation*/
    else
      FF0VM001.YPGCD = YPGTBL.YPGCD[XX0XWPIX]; /* Selected code*/
      FF0VM001.YPGABRV = YPGTBL.YPGABRV[XX0XWPIX]; /* Abbreviation*/

    end

    set FF0VM001.YPGCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the requested lists have been displayed and*/
   /* that ENTER must be pressed to process any changes.*/
   /* ------------------------------------------------------------*/

  converseLib.clearScreen();
   /* Clear screen before display*/

  FFWREC.XXXMODID = "400"; /* Module identification*/
  converseLib.validationFailed(97); /* Indicate press ENTER*/

  exit stack; /* Exit to display map*/


end // end FF0V400

// Verify the entered data
Function FF0V500()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.CAITEM > " ") /* Application data was entered*/

    FFWREC.XXXMODID = "500"; /* Module identification*/
    set FF0VM001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(40); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid request to scroll was entered.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    FF0V510(); /* Verify scroll request*/

  else

    /* ------------------------------------------------------------*/
     /* If the search key was changed, verify the key fields.*/
    /* ------------------------------------------------------------*/

    if (FF0VM001.FFZBYYMM != FF0VW01.FFZBYYMM /* Beginning date changed*/
     || FF0VM001.FFZEYYMM != FF0VW01.FFZEYYMM /* or ending date changed*/
     || FF0VM001.XPLCD != FFWREC.XPLCD        /* or line changed*/
     || FF0VM001.YPGCD != FFWREC.YPGCD        /* or type changed*/
     || FF0VM001.YSMXDV != FFWREC.YSMXDV      /* or division group changed*/
                                              
     || FF0VM001.FFZBYYMM == 0                /* Beginning date missing*/
     || FF0VM001.FFZEYYMM == 0                /* Endning date missing*/
     || FF0VM001.XPLCD == " "                 /* or line missing*/
     || FF0VM001.YPGCD == " "                 /* or type missing*/
     || FF0VM001.YSMXDV == " ")               /* or division group missing*/

      FF0V520(); /* Verify the key fields*/
      FF0V530(); /* Verify relationship*/

    else

      /* ------------------------------------------------------------*/
       /* Otherwise, if page was formatted, verify selection.*/
      /* ------------------------------------------------------------*/

      if (FF0VW00.TA5TSQIX > 0) /* Page was formatted*/

        FF0VW00.FF0VWIXM = 13; /* Initialize line index*/

        while (FF0VW00.FF0VWIXM > 0) /* Until end of lines*/

          FF0V540(); /* Verify selection*/

          FF0VW00.FF0VWIXM = FF0VW00.FF0VWIXM - 1; /* Decrement line number*/

        end

      end

    end

  end

   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/
  end


end // end FF0V500

// Verify scroll request
Function FF0V510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && FF0VW00.XXXTSPAG < 2)       /* and no previous pages*/

    FFWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(6); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && FF0VW00.XXXTSPAG >= FF0VW00.TA5TSQIX) /* and no more pages*/

    FFWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(7); /* No more pages to display*/

  end

   /* ------------------------------------------------------------*/
   /* Highlight any selection that was entered.*/
   /* ------------------------------------------------------------*/

  FF0V511(); /* Highlight any selection*/

   /* ------------------------------------------------------------*/
   /* Highlight any key field that was changed.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.YSMXDV != FFWREC.YSMXDV) /* Division group changed*/

    FFWREC.XXXMODID = "510"; /* Module identification*/
    set FF0VM001.YSMXDV cursor, bold; /* Highlight*/
    converseLib.validationFailed(5); /* Modifications not allowed*/

  end

  if (FF0VM001.YPGCD != FFWREC.YPGCD) /* Type changed*/

    FFWREC.XXXMODID = "510"; /* Module identification*/
    set FF0VM001.YPGCD cursor, bold; /* Highlight*/
    converseLib.validationFailed(5); /* Modifications not allowed*/

  end

  if (FF0VM001.XPLCD != FFWREC.XPLCD) /* Line changed*/

    FFWREC.XXXMODID = "510"; /* Module identification*/
    set FF0VM001.XPLCD cursor, bold; /* Highlight*/
    converseLib.validationFailed(5); /* Modifications not allowed*/

  end

  if (FF0VM001.FFZEYYMM != FF0VW01.FFZEYYMM) /* Ending date changed*/

    FFWREC.XXXMODID = "510"; /* Module identification*/
    set FF0VM001.FFZEYYMM cursor, bold; /* Highlight*/
    converseLib.validationFailed(5); /* Modifications not allowed*/

  end

  if (FF0VM001.FFZBYYMM != FF0VW01.FFZBYYMM) /* Beginning date changed*/

    FFWREC.XXXMODID = "510"; /* Module identification*/
    set FF0VM001.FFZBYYMM cursor, bold; /* Highlight*/
    converseLib.validationFailed(5); /* Modifications not allowed*/

  end


end // end FF0V510

// Highlight any selection
Function FF0V511()
   /* ------------------------------------------------------------*/
   /* Indicate an error for any selection that was entered.*/
   /* ------------------------------------------------------------*/

  FF0VW00.FF0VWIXM = 13; /* Initialize line index*/

  while (FF0VW00.FF0VWIXM > 0) /* Until end of lines*/

    if (FF0VM001.FF0VMSEL[FF0VWIXM] > " ") /* Selection entered*/

      FFWREC.XXXMODID = "511"; /* Module identification*/
      set FF0VM001.FF0VMSEL[FF0VWIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(219); /* Modifications not allowed*/

    end


    FF0VW00.FF0VWIXM = FF0VW00.FF0VWIXM - 1; /* Decrement line number*/

  end


end // end FF0V511

// Verify the key fields
Function FF0V520()
   /* ------------------------------------------------------------*/
   /* Highlight any selection that was entered.*/
   /* ------------------------------------------------------------*/

  FF0V511(); /* Highlight any selection*/

   /* ------------------------------------------------------------*/
   /* If division group was entered, verify division group code.*/
   /* ------------------------------------------------------------*/

  set YSMREC empty; /* Row storage area*/

  YSMREC.YSMSUMCD = FF0VM001.YSMXDV; /* Division group code*/
  TA1REC.TA1PGMNM = "FF0V520 "; /* Calling program*/

  XSFFS20(); /* Verify division group*/

   /* ------------------------------------------------------------*/
   /* If not found, indicate missing or invalid.*/
   /* ------------------------------------------------------------*/

  if (SQLCA.VAGen_SQLCODE == 0) /* Table row found*/

    FF0VM001.YSMSMDSC = YSMREC.YSMSMDSC; /* Abbreviation*/

  else

    FF0VM001.YSMSMDSC = " "; /* Abbreviation*/

    FFWREC.XXXMODID = "520"; /* Module identification*/
    set FF0VM001.YSMXDV cursor, bold; /* Highlight*/
    converseLib.validationFailed(118); /* Indicate missing or invalid*/

  end

   /* ------------------------------------------------------------*/
   /* Verify type code.*/
   /* ------------------------------------------------------------*/

  if (FF0VM001.YPGCD == "*") /* type equal 'all'*/
    FF0VM001.YPGABRV = "*ALL*"; /* Abbreviation*/
  else

    if (FF0VM001.YPGCD in YPGTBL.YPGCD /* Code in table*/
     && YPGTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active status*/

      FF0VM001.YPGABRV = YPGTBL.YPGABRV[sysVar.arrayIndex]; /* Abbreviation*/

    else

      /* ------------------------------------------------------------*/
       /* If not found or inactive, indicate missing or invalid.*/
      /* ------------------------------------------------------------*/

      FF0VM001.YPGABRV = " "; /* Abbreviation*/

      FFWREC.XXXMODID = "520"; /* Module identification*/
      set FF0VM001.YPGCD cursor, bold; /* Highlight*/
      converseLib.validationFailed(118); /* Indicate missing or invalid*/

    end

  end

   /* ------------------------------------------------------------*/
   /* Verify line code.*/
   /* ------------------------------------------------------------*/

  XPLREC.XPLCD = FF0VM001.XPLCD; /* Format key*/
  XPLREC.XGPCD = COMMAREA.XGPCD; /* Format key*/

  if (XPLREC.XPLKEY in XPLTBL.XPLKEY /* Code in table*/
   && XPLTBL.XXXSTAT[sysVar.arrayIndex] == "A") /* and active status*/

    FF0VM001.XPLABRV = XPLTBL.XPLABRV[sysVar.arrayIndex]; /* Abbreviation*/

  else

    /* ------------------------------------------------------------*/
     /* If not found or inactive, indicate missing or invalid.*/
    /* ------------------------------------------------------------*/

    FF0VM001.XPLABRV = " "; /* Abbreviation*/

    FFWREC.XXXMODID = "520"; /* Module identification*/
    set FF0VM001.XPLCD cursor, bold; /* Highlight*/
    converseLib.validationFailed(118); /* Indicate missing or invalid*/

  end

   /* ------------------------------------------------------------*/
   /* Verify the beginning and ending dates in the entered range.*/
   /* ------------------------------------------------------------*/

  set FFZREC empty; /* Subroutine parameters*/

  FFZREC.FFZSRANG = "Y";
  FFZREC.FFZBYYMM = FF0VM001.FFZBYYMM; /* Beginning date*/
  FFZREC.FFZEYYMM = FF0VM001.FFZEYYMM; /* Ending date*/

  XSFFS30(); /* Verify the range*/

   /* ------------------------------------------------------------*/
   /* If not zero return code, indicate missing or invalid.*/
   /* ------------------------------------------------------------*/

  if (FFZREC.FFZENDRC != "0") /* Ending date error*/

    FFWREC.XXXMODID = "520"; /* Module identification*/
    set FF0VM001.FFZEYYMM cursor, bold; /* Highlight*/

    if (FFZREC.FFZENDRC == "1") /* No date entered*/
      converseLib.validationFailed(8); /* Required field missing*/
    else

      if (FFZREC.FFZENDRC == "2") /* Date is invalid*/
        converseLib.validationFailed(17); /* Invalid date entered*/
      else

        converseLib.validationFailed(151); /* Date is out of range*/

      end

    end

  end

  if (FFZREC.FFZBEGRC != "0") /* Beginning date error*/

    FFWREC.XXXMODID = "520"; /* Module identification*/
    set FF0VM001.FFZBYYMM cursor, bold; /* Highlight*/

    if (FFZREC.FFZBEGRC == "1") /* No date entered*/
      converseLib.validationFailed(8); /* Required field missing*/
    else

      if (FFZREC.FFZBEGRC == "2") /* Date is invalid*/
        converseLib.validationFailed(17); /* Invalid date entered*/
      else

        converseLib.validationFailed(151); /* Date is out of range*/

      end

    end

  end


end // end FF0V520

// Verify FFXRF
Function FF0V530()
   /* ------------------------------------------------------------*/
   /* Verify that key is in FFXRF.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum == 0) /* If there are no errors*/

    /* ------------------------------------------------------------*/
     /* Clear the row storage area.*/
    /* ------------------------------------------------------------*/

    set FF2REC empty; /* clear DFCAST.VFFXRF01 record.*/

    /* ------------------------------------------------------------*/
     /* Format keys for call to DFCAST.VFFXRF01.*/
    /* ------------------------------------------------------------*/

    FF2REC.XPCCD = "*"; /* category*/
    FF2REC.XPLCD = FF0VM001.XPLCD; /* line*/
    FF2REC.YPGCD = FF0VM001.YPGCD; /* type*/
    FF2REC.YSMXDV = FF0VM001.YSMXDV; /* div grp*/

    /* ------------------------------------------------------------*/
     /* Select the row to display from the table.*/
    /* ------------------------------------------------------------*/

    FF0V531(); /* Open cursor for DFCAST.VFFXRF01*/

    FF0V532(); /* fetch FFXRF*/

    /* ------------------------------------------------------------*/
     /* If successful, close FFXRF.*/
    /* ------------------------------------------------------------*/

    if (SQLCA.VAGen_SQLCODE != 0) /* Row not found code*/

      /* ------------------------------------------------------------*/
       /* If not found, indicate invalid.*/
      /* ------------------------------------------------------------*/

      FFWREC.XXXMODID = "530"; /* Module identification*/
      set FF0VM001.XPLCD bold; /* Highlight*/
      set FF0VM001.YSMXDV bold; /* Highlight*/
      set FF0VM001.YPGCD cursor, bold; /* Highlight*/
      converseLib.validationFailed(203); /* Indicate invalid relationship*/

    end

    FF0V533(); /* close cursor*/

  end


end // end FF0V530

// Open  DFCAST.VFFXRF01
Function FF0V531()
   /* ------------------------------------------------------------*/
   /* Open FFXRF.*/
   /* ------------------------------------------------------------*/

  try
    call "IO4000" ("S1", SQLCA, FF2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select row*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0) /* Normal return code*/

      return; /* Return to previous process*/

    end

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL Commarea*/

  FFWREC.XXXMODID = "531"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(47); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(01); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "OPEN CURSOR";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = FFWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DFCAST  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VFFXRF01"; /* Table view name*/
  TA1REC.TA1TBLKE = FF2REC.FF2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


