// Save data in display storage
Function FF0V160()
   /* ------------------------------------------------------------*/
   /* If first line on page, save beginning and ending months.*/
   /* ------------------------------------------------------------*/

  if (FF0VW00.FF0VWIXM == 1) /* First line on page*/

    FF0VW01.FFZBYYMM = FFWREC.FFABEGMO; /* Beginning month*/
    FF0VW01.FFZEYYMM = FFWREC.FFAENDMO; /* Ending month*/

    FF0VW00.FF0VWCNT = 1; /* Set counter to 1*/
    while (FF0VW00.FF0VWCNT <= FFZREC.FFZNUMMO) /* While counter LE nbr mos*/

      FF0VW01.FFZIYYMM[FF0VWCNT] = FFZREC.FFZIYYMM[FF0VWCNT];
      FF0VW00.FF0VWCNT = FF0VW00.FF0VWCNT + 1; /* Add 1 to counter*/

    end

  end

   /* ------------------------------------------------------------*/
   /* If end of cursor, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (FF0VW00.FF0VWSWC == "Y") /* End of cursor*/
    return; /* Return to previous process*/
  end

   /* ------------------------------------------------------------*/
   /* Save the data from the table row in display storage.*/
   /* ------------------------------------------------------------*/

  FF0VW01.XPCCD[FF0VWIXM] = FF2REC.XPCCD; /* Save CAT cd for xfer*/

  XPCREC.XPCCD = FF2REC.XPCCD; /* Format xpc key*/
  XPCREC.XGPCD = COMMAREA.XGPCD; /* Format xpc key*/
  if (XPCREC.XPCKEY in XPCTBL.XPCKEY)
    FF0VW01.XPCABRV[FF0VWIXM] = XPCTBL.XPCABRV[sysVar.arrayIndex];
  end

  FF0VW01.FF2CRVFL[FF0VWIXM] = FF0VW00.FF0VWHFL; /* Reviewed flag*/

  FF0VW01.FF0VWLIN[FF0VWIXM] = FF0VW02.FF0VWMSK; /* Line mask to map*/

  FF0VW00.FF0VWIXM = FF0VW00.FF0VWIXM + 1; /* Increment line index*/


end // end FF0V160

