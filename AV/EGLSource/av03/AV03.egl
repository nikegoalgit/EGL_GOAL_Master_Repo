      AVWREC.XXXMODID = "550"; /* Module identification*/
      set AV03M001.AV03MSEL[AV03WIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(278); /* Indicate no security clearance*/

      return;

    else
      AVWREC.AVWMODSW = "Y"; /* Modified data switch*/

    end

    /* ------------------------------------------------------------*/
     /* Verify if 'a' entered, that style number and color entered.*/
    /* ------------------------------------------------------------*/
    if (AV03M001.AV03MSEL[AV03WIXM] == "A") /* If 'a' entered*/

      if (AV03M001.XDMCD[AV03WIXM] == " " /* If dimension missing*/
       && AV03M001.AV03WPSW[AV03WIXM] != "Y") /* If styl not on D/B*/
        AV03M001.XDMCD[AV03WIXM] = "00"; /* Default to '00'*/
      end

      if (AV03M001.SY2CLRID[AV03WIXM] == " ") /* If color missing*/
        AVWREC.XXXMODID = "550"; /* Module identification*/
        set AV03M001.SY2CLRID[AV03WIXM] cursor, bold; /* Highlight*/
        converseLib.validationFailed(111); /* Indicate missing*/
      end

      if (AV03M001.SY1STNBR[AV03WIXM] == " ") /* If style missing*/
        AVWREC.XXXMODID = "550"; /* Module identification*/
        set AV03M001.SY1STNBR[AV03WIXM] cursor, bold; /* Highlight*/
        converseLib.validationFailed(63); /* Indicate missing*/
      end

      /* ------------------------------------------------------------*/
       /* Verify that this line is not protected.  (If protected,*/
       /* the style exists on the D/B.*/
      /* ------------------------------------------------------------*/
      if (AV03M001.AV03WPSW[AV03WIXM] == "Y") /* If style already existing*/
        AVWREC.XXXMODID = "550"; /* Module identification*/
        set AV03M001.AV03MSEL[AV03WIXM] cursor, bold; /* Highlight*/
        converseLib.validationFailed(287); /* Indicate product already exists*/
      else

        /* ------------------------------------------------------------*/
         /* Verify that style not on D/B.  (Listed elsewhere on page.)*/
        /* ------------------------------------------------------------*/
        AV03551(); /* Select style*/

        if (SQLCA.VAGen_SQLCODE == 0) /* If style missing*/
          AVWREC.XXXMODID = "550"; /* Module identification*/
          set AV03M001.AV03MSEL[AV03WIXM] cursor, bold; /* Highlight*/
          converseLib.validationFailed(287); /* Indicate product already exists*/
        end

      end
    end

    /* ------------------------------------------------------------*/
     /* Verify if 'd' entered, that product exists.*/
    /* ------------------------------------------------------------*/
    if (AV03M001.AV03MSEL[AV03WIXM] == "D") /* If 'D' entered*/

      if (AV03M001.AV03WPSW[AV03WIXM] != "Y") /* If product not from D/B*/
        AVWREC.XXXMODID = "550"; /* Module identification*/
        set AV03M001.AV03MSEL[AV03WIXM] cursor, bold; /* Highlight*/
        converseLib.validationFailed(288); /* Indicate product not there*/
      end

    end
  end


end // end AV03550

