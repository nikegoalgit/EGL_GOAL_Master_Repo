// Process valid input request
Function AV03600()
   /* ------------------------------------------------------------*/
   /* Indicate that the map must be formatted before display.*/
   /* ------------------------------------------------------------*/
  AV03W00.AV03WSWF = "Y"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If a scroll backward was requested, decrement the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    AV03W00.XXXTSPAG = AV03W00.XXXTSPAG - 1; /* Decrement page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a scroll forward was requested, increment the page*/
   /* number and return to the previous process.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    AV03W00.XXXTSPAG = AV03W00.XXXTSPAG + 1; /* Increment page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If PF9 comfire pressed, process adds and deletes.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf9) /* If PF9 was presses*/

    /* ------------------------------------------------------------*/
     /* Clear out modified data switch and message.*/
    /* ------------------------------------------------------------*/
    AVWREC.XXXMSGNO = 0; /* Clear message out*/
    AVWREC.AVWMODSW = "N"; /* Clear modified data switch*/

    /* ------------------------------------------------------------*/
     /* If products added on screen, insert in BLNKS.*/
    /* ------------------------------------------------------------*/
    if ("A" in AV03M001.AV03MSEL) /* Style(s) to be inserted*/

      /* ------------------------------------------------------------*/
       /* Delete 'blank' BLNKS row, if it exists.*/
      /* ------------------------------------------------------------*/
      AV03610(); /* Delete 'blank' BLNKS*/

      AV03W00.AV03WIXM = 1; /* Initialize line index*/

      while (AV03W00.AV03WIXM <= 15) /* Until screen is full*/
        if (AV03M001.AV03MSEL[AV03WIXM] == "A") /* and line added*/

          AV03620(); /* Select FINISHED STYLE*/

          if (SQLCA.VAGen_SQLCODE == +100) /* row not found*/
            AV03630(); /* Insert BLNKS*/
          end
        end

        AV03W00.AV03WIXM = AV03W00.AV03WIXM + 1; /* Increment line index*/

      end
      AV03W00.AV03WSWR = "Y"; /* Request for data switch*/
    end

    /* ------------------------------------------------------------*/
     /* If products marked deleted on screen, delete from BLNKS.*/
    /* ------------------------------------------------------------*/
    if ("D" in AV03M001.AV03MSEL) /* Style(s) to be deleted*/

      AV03W00.AV03WIXM = 1; /* Initialize line index*/

      while (AV03W00.AV03WIXM <= 15) /* Until screen is full*/
        if (AV03M001.AV03MSEL[AV03WIXM] == "D") /* and line deleted*/

          AV03640(); /* Delete BLNKS*/
          AV03650(); /* Check insert 'blank' BLNKS*/

        end

        AV03W00.AV03WIXM = AV03W00.AV03WIXM + 1; /* Increment line index*/

      end
      AV03W00.AV03WSWR = "Y"; /* Request for data switch*/
    end

    if ("C" in AV03M001.AV03MSEL) /* Style(s) to be deleted*/

      AV03W00.AV03WIXM = 1; /* Initialize line index*/

      while (AV03W00.AV03WIXM <= 15) /* Until screen is full*/
        if (AV03M001.AV03MSEL[AV03WIXM] == "C") /* and line deleted*/

          AV03640(); /* Delete BLNKS*/
          AV03630(); /* Insert BLNKS*/

        end

        AV03W00.AV03WIXM = AV03W00.AV03WIXM + 1; /* Increment line index*/

      end
      AV03W00.AV03WSWR = "Y"; /* Request for data switch*/
    end

  else
    /* ------------------------------------------------------------*/
     /* Load the map into storage.*/
    /* ------------------------------------------------------------*/
    AV03W00.AV03WIXM = 1; /* Initialize line index*/

    while (AV03W00.AV03WIXM <= 15) /* Until screen is full*/

      AV03660(); /* Load map to W01*/

      AV03W00.AV03WIXM = AV03W00.AV03WIXM + 1; /* Increment line index*/

    end
  end


end // end AV03600

