//*** PROGRAM=AV03 ****
// This application displays all the products associated with a
// particular program (owner group) code.  This code must be
// entered by the user.  With security clearance, the user may
// add or delete products.  If no data was found, he will
// receive blank lines so that he can add products.  Without
// security clearance, the user will see any existing products
// or if there is no data, he will receive a message that 'no
// products were found'.  With or without security clearance,
// the user may select any existing product and transfer to
// AV03 to see more information concerning the product.
// 
// The user must press PF9 to allow add or delete of products.
// He may scroll forward and backward to view all products by
// pressing PF7 and PF8.  He may exit to any allowed applica-
// tion, via FAST PATH or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE     NAME   DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
//          12/10/90  RHETRI  New application.
//          02/18/91  RHETRI  Look at SYCLR: SY_SCN_STY_IND
//                            rather than family code = "21".
//          03/07/91  RHETRI  Remove edit that prevents adding
//                            products not found in SYCLR.
//          04/13/92  SCOCHR  REMOVE BLANK DIMENSION
//          10/31/97  MBURLE  Y2KFIX: regenerated for latest
//                            version of XX0XS01.
// *********************
Program AV03 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "AV00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  AV03W00 AV03W00; // record
  AV03W01 AV03W01; // record
  AV03W02 AV03W02; // record
  AVHREC AVHREC; // record
  AVIREC AVIREC; // record
  AVWREC AVWREC; // record
  COMMAREA COMMAREA ; // record
  PG2REC PG2REC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  XDMREC XDMREC; // record
  XOWREC XOWREC; // record
  XX0XW01 XX0XW01; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XDMTBL; // table
  use XOWTBL; // table
  use AV03M.AV03M001 // form
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPAV001: XPAV001();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/

    set AV03M001 initial; /* M001 input/output*/
    set AV03W00 empty; /* Working storage*/
    set AV03W01 empty; /* M001 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set TA5REC empty; /* Temporary storage parameters*/
    set XX0XW01 empty; /* Date/time subroutine parameters*/

    AV03W00.AV03WSWF = "Y"; /* Format the map switch*/

     /* ------------------------------------------------------------*/
     /* Set up defaults.*/
     /* ------------------------------------------------------------*/

    AV03W00.XGPCD = "03"; /* Default GPC code*/
    AV03W00.XWHCD = "51"; /* Default warehouse code*/

    AV03W00.AV03WSWR = "Y"; /* Request for data switch*/


     /* ------------------------------------------------------------*/
     /* If user has security clearance, set security-switch.*/
     /* ------------------------------------------------------------*/

    if (COMMAREA.CAADDFL == "Y" /* If add clearance exists*/
     && COMMAREA.CADELFL == "Y") /* If delete clearance exists*/

      AV03W00.AV03WSSW = "Y"; /* Security switch*/
    else

      AV03W00.AV03WSSW = "N"; /* Security switch*/
    end

     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto AV03000; /* Converse to check from application*/


    AV03000: AV03000();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto AV03001; /* Converse the main process*/


    AV03001: AV03001();
     /* ------------------------------------------------------------*/
      /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto AV03001; /* Converse to main process*/


  end // end main
end // end AV03

// Check the FROM application
Function AV03000()
   /* ------------------------------------------------------------*/
   /* Determine whether control came from an allowed application.*/
   /* ------------------------------------------------------------*/

  if (COMMAREA.CAFROMAP == "AV02") /* From allowed application*/

     /* NEXT SENTENCE*/

  else

    /* ------------------------------------------------------------*/
     /* Indicate invalid transfer and return to FROM application.*/
    /* ------------------------------------------------------------*/

    XSNOWAY(); /* Invalid transfer was attempted*/

  end


end // end AV03000

// Main process to display a map
Function AV03001()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Reset highlighted fields to normal intensity.*/
   /* 5) Verify the data that was entered.*/
   /* 6) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  AV03100(); /* Obtain the data to display*/

  AV03200(); /* Format and display the map*/

  AV03300(); /* Check attention identifier*/

  AV03400(); /* Reset fields to normal intensity*/

  AV03500(); /* Verify the entered data*/

  AV03600(); /* Process valid input request*/


end // end AV03001

// Obtain the data to display
Function AV03100()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (AV03W00.AV03WSWR != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Establish the temporary storage queue page length.*/
   /* ------------------------------------------------------------*/

  AV03W00.XXXTSLEN = 825; /* Temporary storage page length*/

   /* ------------------------------------------------------------*/
   /* Purge any existing pages from the temporary storage queue.*/
   /* ------------------------------------------------------------*/

  AV03S01(); /* Purge any temporary storage*/

   /* ------------------------------------------------------------*/
   /* Obtain date and time.*/
   /* ------------------------------------------------------------*/

  XX0XS01(); /* Obtain date and time*/

   /* ------------------------------------------------------------*/
   /* Obtain data for lines.*/
   /* ------------------------------------------------------------*/

  AV03120(); /* Open BLNKS*/

   /* ------------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ------------------------------------------------------------*/

  AV03W00.AV03WBSW = "Y"; /* Blank page switch*/
  while (AV03W00.AV03WSWC == "N") /* Until end of cursor*/

    AV03130(); /* Fetch BLNKS*/

    if (AV03W00.AV03WSWC == "N") /* If row was found*/

      AV03140(); /* Obtain and process products*/

      /* ------------------------------------------------------------*/
       /* Save data in display storage.*/
      /* ------------------------------------------------------------*/

      AV03150(); /* Save data*/

      /* ------------------------------------------------------------*/
       /* If the page is full, add the page to the storage queue.*/
      /* ------------------------------------------------------------*/

      if (AV03W00.AV03WIXM > 15) /* Page is full*/
        AV03160(); /* Add page to temporary storage*/
      end

    end

  end

   /* ------------------------------------------------------------*/
   /* Close the cursor for DAVAIL.VBLNKS01.*/
   /* ------------------------------------------------------------*/

  AV03170(); /* Close BLNKS*/

   /* ------------------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ------------------------------------------------------------*/

  if (AV03W00.AV03WIXM > 1) /* Page was formatted*/
    AV03160(); /* Add page to temporary storage*/
    AV03W00.AV03WBSW = "N"; /* Blank page switch*/
  end

   /* ------------------------------------------------------------*/
   /* If no pages were formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/
  if (AV03W00.TA5TSQIX == 0) /* No pages formatted*/

    if (AV03W00.AV03WSSW != "Y") /* No security clearance*/
      AVWREC.XXXMODID = "100"; /* Module identification*/
      AVWREC.XXXMSGNO = 194; /* Indicate no data to display*/

    else
      AV03W00.XXXTSPAG = 0; /* Desired page number*/
      AV03W00.TA5TSQIX = 0; /* Highest page number*/

    end

  else
    /* ------------------------------------------------------------*/
     /* Data was found, check conditions for adding blank page.*/
    /* ------------------------------------------------------------*/
    AV03W00.XXXTSPAG = 1; /* Desired page number*/

    if (AV03W00.AV03WBSW == "Y" /* If blank page required*/
     && AV03W00.AV03WSSW == "Y") /* Security clearance*/
      AV03160(); /* Add page to temporary storage*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  AV03W00.AV03WSWR = "N"; /* Request for data switch*/


end // end AV03100

// Open BLNKS
Function AV03120()
   /* ------------------------------------------------------------*/
   /* Clear copybook.*/
   /* ------------------------------------------------------------*/

  set AVHREC empty; /* Clear row storage area*/

   /* ------------------------------------------------------------*/
   /* Format key.*/
   /* ------------------------------------------------------------*/

  AVHREC.XOWCD = AVWREC.XOWCD; /* Owner group*/
  AVHREC.SY1BSTNB = AVWREC.SY1BSTNB; /* Blank style nbr*/
  AVHREC.SY2BCLCD = AVWREC.SY2BCLCD; /* Blank color cd*/

   /* ------------------------------------------------------------*/
   /* Open cursor for BLNKS.*/
   /* ------------------------------------------------------------*/

  try
    call "IO5470" ("S2", SQLCA, AVHREC) {isNoRefresh = yes, isExternal = yes};
  end /* Select table set*/

   /* ------------------------------------------------------------*/
   /* If successful, initialize the line index and cursor switch.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    AV03W00.AV03WIXM = 1; /* Display line index*/
    AV03W00.AV03WCNT = 0; /* Counter for nbr of styles*/
    AV03W00.AV03WSWC = "N"; /* End of cursor switch*/
    AV03W00.AV03WDSW = "N"; /* Data found switch*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  AVWREC.XXXMODID = "120"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(75); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(264); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT SET OF TABLE ROWS";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = AVWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DAVAIL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VBLNKS01"; /* Table view name*/
  TA1REC.TA1TBLKE = AVHREC.AVHKEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end AV03120

// Fetch BLNKS
Function AV03130()
   /* ------------------------------------------------------------*/
   /* Fetch BLNKS.*/
   /* ------------------------------------------------------------*/

  try
    call "IO5470" ("N2", SQLCA, AVHREC) {isNoRefresh = yes, isExternal = yes};
  end /* Fetch next row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/
    if (SQLCA.VAGen_SQLCODE == +0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == +100) /* Row not found*/

      AV03131(); /* Check switches*/

      return; /* Return to previous process*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  AVWREC.XXXMODID = "130"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(75); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(264); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "FETCH NEXT TABLE ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = AVWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DAVAIL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VBLNKS01"; /* Table view name*/
  TA1REC.TA1TBLKE = AVHREC.AVHKEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end AV03130

// Check switches
Function AV03131()
   /* ------------------------------------------------------------*/
   /* If end of selected set, indicate end of cursor.*/
   /* ------------------------------------------------------------*/
  if (SQLCA.VAGen_SQLCODE == 100 /* End of selected set*/
   || AVHREC.SY1STNBR == AVHREC.SY1BSTNB) /* No finished style*/

    AV03W00.AV03WSWC = "Y"; /* Indicate end of cursor*/

  else
    /* ------------------------------------------------------------*/
     /* Row found, set switch.*/
    /* ------------------------------------------------------------*/

    AV03W00.AV03WDSW = "Y"; /* Data found switch*/

  end


end // end AV03131

// Select SYCLR
Function AV03140()
   /* ------------------------------------------------------------*/
   /* Clear copybook.*/
   /* ------------------------------------------------------------*/

  set PG2REC empty; /* Clear copybook*/
  AV03W00.AV03WVSW = "N"; /* Valid product switch*/

   /* ------------------------------------------------------------*/
   /* Format keys.*/
   /* ------------------------------------------------------------*/

  PG2REC.SY1STNBR = AVHREC.SY1STNBR; /* style nbr*/
  PG2REC.SY2CLRID = AVHREC.SY2CLRID; /* color cd*/
  PG2REC.XDMCD = AVHREC.XDMCD; /* dim cd*/
  PG2REC.XGPCD = AV03W00.XGPCD; /* GPC cd*/

   /* ------------------------------------------------------------*/
   /* Select MLTYP.*/
   /* ------------------------------------------------------------*/

  try
    call "IO3120" ("S ", SQLCA, PG2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == 0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == 100) /* Row not found*/

      if (SQLCA.VAGen_SQLCODE == 0) /* If row found*/
        AV03W00.AV03WVSW = "Y"; /* Valid product switch*/
      end

      return; /* Return to previous process*/

    end

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL Commarea*/

  AVWREC.XXXMODID = "140"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(75); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(264); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = AVWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DPRODG  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VSYCLR01"; /* Table view name*/
  TA1REC.TA1TBLKE = PG2REC.PG2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end AV03140

// Save data in display storage
Function AV03150()
   /* ------------------------------------------------------------*/
   /* If end of cursor, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (AV03W00.AV03WSWC == "Y") /* End of cursor*/
    return; /* Return to previous process*/
  end

   /* ------------------------------------------------------------*/
   /* Save the data from the table row in display storage.*/
   /* ------------------------------------------------------------*/

  AV03W01.AV03WPSW[AV03WIXM] = "Y"; /* Prod in BLNKS*/

  AV03W01.SY1STNBR[AV03WIXM] = AVHREC.SY1STNBR; /* Style*/
  AV03W01.SY2CLRID[AV03WIXM] = AVHREC.SY2CLRID; /* Color*/
  AV03W01.XDMCD[AV03WIXM] = AVHREC.XDMCD; /* Dim*/
  AV03W01.AVHBLKFL[AV03WIXM] = AVHREC.AVHBLKFL; /* Bulk flag*/
  AV03W01.XXXSTAT[AV03WIXM] = AVHREC.XXXSTAT; /* STATUS CODE*/

   /* ------------------------------------------------------------*/
   /* If style found in product file, use data - else default it.*/
   /* ------------------------------------------------------------*/
  if (AV03W00.AV03WVSW == "Y") /* If valid product*/
    AV03W01.SY1SSTNM[AV03WIXM] = PG2REC.SY1SSTNM; /* Style name*/
    AV03W01.SY2CLRAB[AV03WIXM] = PG2REC.SY2CLRAB; /* Color abrv*/

    XDMREC.XDMCD = PG2REC.XDMCD; /* Dim desc*/
    if (XDMREC.XDMCD in XDMTBL.XDMCD)
      AV03W01.XDMDESC[AV03WIXM] = XDMTBL.XDMDESC[sysVar.arrayIndex];
    end

  else
    AV03W01.SY1SSTNM[AV03WIXM] = "STYLE NOT IN ";
    AV03W01.SY2CLRAB[AV03WIXM] = "PRODUCT FILE";
    AV03W01.XDMDESC[AV03WIXM] = "************";

  end

   /* ------------------------------------------------------------*/
   /* Add to counters.*/
   /* ------------------------------------------------------------*/
  AV03W00.AV03WIXM = AV03W00.AV03WIXM + 1; /* Increment line index*/
  AV03W00.AV03WCNT = AV03W00.AV03WCNT + 1; /* Increment style counter*/


end // end AV03150

// Add page to temporary storage
Function AV03160()
   /* ------------------------------------------------------------*/
   /* Add the next page to temporary storage.*/
   /* ------------------------------------------------------------*/

  AV03W01.XXXTSLEN = AV03W00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "A"; /* Function code*/

  try
    call "TA0050" (AV03W01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, initialize the storage area and indicators*/
   /* to be used.  Return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    set AV03W01 empty; /* M001 display storage*/

    AV03W00.AV03WIXM = 1; /* Display line index*/

    AV03W00.TA5TSQIX = TA5REC.TA5TSQIX; /* Highest page number*/
    TA5REC.TA5TSQIX = 0; /* Current page number*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  AVWREC.XXXMODID = "160"; /* Module identification*/
  converseLib.validationFailed(264); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "ADD PAGE TO TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = AVWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end AV03160

// Close BLNKS
Function AV03170()
   /* ------------------------------------------------------------*/
   /* Close BLNKS.*/
   /* ------------------------------------------------------------*/

  try
    call "IO5470" ("C2", SQLCA, AVHREC) {isNoRefresh = yes, isExternal = yes};
  end /* Close table set*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL commarea*/

  AVWREC.XXXMODID = "170"; /* Module identification*/
  converseLib.validationFailed(264); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "CLOSE CURSOR";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = AVWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DAVAIL  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VBLNKS01"; /* Table view name*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end AV03170

// Format and converse the map
Function AV03200()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/

    AVWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/

  end

   /* ------------------------------------------------------------*/
   /* If data requested or no error, format the data to display.*/
   /* ------------------------------------------------------------*/

  if (AV03W00.AV03WSWR == "Y" /* If data was requested*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (AV03W00.AV03WSWF == "Y") /* Map must be formatted*/
      AV03210(); /* Format the data on the map*/
    end

  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  AV03M001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  AV03M001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  AV03M001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (AVWREC.XXXMSGNO != 0) /* Message to display*/

    AV03M001.XXXMODID = AVWREC.XXXMODID; /* Module identification*/
    if (AVWREC.XXXMSGNO == 9999)
      converseLib.validationFailed();
    else
      converseLib.validationFailed(AVWREC.XXXMSGNO);
    end /* Message number*/

    set AV03M001 alarm; /* Indicate sound alarm*/

  end

  converse AV03M001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  AVWREC.XXXMODID = " "; /* Module identification*/
  AVWREC.XXXMSGNO = 0; /* Message number*/

  COMMAREA.CATOAP = AV03M001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = AV03M001.CAITEM; /* Application data*/


end // end AV03200

// Format the display data
Function AV03210()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set AV03M001 initial; /* M001 input/output area*/

  AV03W00.AV03WSWF = "N"; /* Format the map switch*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (AV03W00.TA5TSQIX > 0 /* Pages were formatted*/
   && AV03W00.AV03WDSW == "Y") /* Data found switch*/

    if (AV03W00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

      /* ------------------------------------------------------------*/
       /* If the desired page number is less or equal the highest*/
       /* page in storage, obtain temporary storage.*/
      /* ------------------------------------------------------------*/
      if (AV03W00.XXXTSPAG > AV03W00.TA5TSQIX) /* Desired greater highest*/
        AV03W00.XXXTSPAG = AV03W00.TA5TSQIX;
      end

      AV03211(); /* Obtain temporary storage page*/

    end
    /* ------------------------------------------------------------*/
     /* Format the page information on the screen.*/
    /* ------------------------------------------------------------*/
    AV03M001.XXXTSPAG = AV03W00.XXXTSPAG; /* Current page*/
    AV03M001.TA5TSQIX = AV03W00.TA5TSQIX; /* Highest page*/

  end

   /* ------------------------------------------------------------*/
   /* Format the key to the map from the display storage.*/
   /* ------------------------------------------------------------*/

  AV03M001.XOWCD = AVWREC.XOWCD; /* Owner group code*/
  AV03M001.XOWDESC = AVWREC.XOWDESC; /* Owner group desc*/
  AV03M001.SY1BSTNB = AVWREC.SY1BSTNB; /* Blank style number*/
  AV03M001.AV03MSNM = AVWREC.SY1SSTNM; /* Blank style name*/
  AV03M001.SY2BCLCD = AVWREC.SY2BCLCD; /* Blank color number*/
  AV03M001.AV03MSCL = AVWREC.SY2CLRAB; /* Blank color name*/

   /* ------------------------------------------------------------*/
   /* Format the data in the map from the display storage.*/
   /* ------------------------------------------------------------*/

  if (AV03M001.XOWCD > " ") /* If owner grp entered*/
    set AV03M001.AV03MSEL[1] cursor; /* Place cursor*/
  end

   /* ------------------------------------------------------------*/
   /* Load the styles from BLNKS (stored in W01) to screen.*/
   /* ------------------------------------------------------------*/
  AV03W00.AV03WIXM = 1; /* Initialize line index*/

  while (AV03W00.AV03WIXM <= 15) /* Until screen is full*/

    AV03M001.AV03WPSW[AV03WIXM] = AV03W01.AV03WPSW[AV03WIXM];
    AV03M001.AV03MSEL[AV03WIXM] = AV03W01.AV03MSEL[AV03WIXM];

    AV03M001.SY1STNBR[AV03WIXM] = AV03W01.SY1STNBR[AV03WIXM];
    AV03M001.SY2CLRID[AV03WIXM] = AV03W01.SY2CLRID[AV03WIXM];
    AV03M001.XDMCD[AV03WIXM] = AV03W01.XDMCD[AV03WIXM];

    AV03M001.SY1SSTNM[AV03WIXM] = AV03W01.SY1SSTNM[AV03WIXM];
    AV03M001.SY2CLRAB[AV03WIXM] = AV03W01.SY2CLRAB[AV03WIXM];
    AV03M001.XDMDESC[AV03WIXM] = AV03W01.XDMDESC[AV03WIXM];

    AV03M001.AVHBLKFL[AV03WIXM] = AV03W01.AVHBLKFL[AV03WIXM];

    AV03M001.XXXSTAT[AV03WIXM] = AV03W01.XXXSTAT[AV03WIXM];

    if (AV03W01.AV03WPSW[AV03WIXM] != "Y" /* if from D/B*/
     && AV03W00.AV03WSSW == "Y")          /* and security exists*/
      set AV03M001.SY1STNBR[AV03WIXM] normal; /* make field enterable*/
      set AV03M001.SY2CLRID[AV03WIXM] normal; /* make field enterable*/
      set AV03M001.XDMCD[AV03WIXM] normal; /* Make field enterable*/
      set AV03M001.AVHBLKFL[AV03WIXM] normal; /* Make field enterable*/
      set AV03M001.XXXSTAT[AV03WIXM] normal; /* Make field enterable*/
    end

    AV03W00.AV03WIXM = AV03W00.AV03WIXM + 1; /* Increment line index*/

  end


end // end AV03210

// Obtain TSQ Record
Function AV03211()
   /* ------------------------------------------------------------*/
   /* Obtain the next page of data to display.*/
   /* ------------------------------------------------------------*/

  AV03W01.XXXTSLEN = AV03W00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5TSQIX = AV03W00.XXXTSPAG; /* Page number*/
  TA5REC.TA5FNCCD = "R"; /* Function code*/

  try
    call "TA0050" (AV03W01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

   /* ------------------------------------------------------------*/
   /* If successful, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  AVWREC.XXXMODID = "211"; /* Module identification*/
  converseLib.validationFailed(264); /* Indicate system error*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "READ PAGE FROM TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = AVWREC.XXXMODID; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end AV03211

// Check attention identifier
Function AV03300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (AV03W00.AV03WSWR == "Y") /* Request for data*/
      set AV03W01 empty; /* M001 display storage*/
    end

    AV03W00.AV03WSWF = "Y"; /* Format the map switch*/
    AV03W00.AV03WSWR = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (AV03M001.CATOAP > " ") /* FAST PATH was requested*/
    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      if (AVWREC.AVWMODSW != "Y") /* if data mod sw not = 'y'*/
        AV03S01(); /* Purge temporary storage*/
      end

      if (AVWREC.AVWMODSW == "Y"    /* if data mod sw 'y'*/
       || COMMAREA.CATOAP == "AV01" /* If returning to AV01*/
       || COMMAREA.CATOAP == "AV02") /* or returning to AV02*/
         /* next sentence*/
      else
        AV03S02(); /* Purge temp storage for AV01*/
        AV03S03(); /* Purge temp storage for AV02*/
      end

      XPAV003(); /* Exit to another application*/

      set AV03M001.CATOAP cursor, bold; /* Highlight*/

    else

      AVWREC.XXXMODID = "300"; /* Module identification*/
      converseLib.validationFailed(23); /* Indicate PF key not valid*/

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4 /* or PF4 was pressed*/
   || converseVar.eventKey is pf5 /* or PF5 was pressed*/
   || converseVar.eventKey is pf6) /* or PF6 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "AV03M001"; /* Application find code*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "AV02"; /* TO application*/
    end

    if (converseVar.eventKey is pf5) /* PF5 was pressed*/
      COMMAREA.CATOAP = "AV0M"; /* TO application*/
    end

    if (converseVar.eventKey is pf6) /* PF6 was pressed*/
      COMMAREA.CATOAP = "AV0S"; /* TO application*/
    end

    if (AVWREC.AVWMODSW != "Y") /* if data mod sw not = 'y'*/
      AV03S01(); /* Purge temporary storage*/
    end

    if (AVWREC.AVWMODSW == "Y"    /* if data mod sw 'y'*/
     || COMMAREA.CATOAP == "AV01" /* or returning to AV01*/
     || COMMAREA.CATOAP == "AV02") /* or returning to AV02*/
       /* next sentence*/
    else
      AV03S02(); /* Purge temp storage for AV01*/
      AV03S03(); /* Purge temp storage for AV02*/
    end

    XPAV003(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf9)  /* and PF9 was not pressed*/

    AVWREC.XXXMODID = "300"; /* Module identification*/
    converseLib.validationFailed(279); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end AV03300

// Reset highlighted fields
Function AV03400()
   /* ------------------------------------------------------------*/
   /* Reset all modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set AV03M001.XOWCD initialAttributes; /* program code*/

  AV03W00.AV03WIXM = 1; /* Initialize line index*/

  while (AV03W00.AV03WIXM <= 15) /* Until end of lines*/

    set AV03M001.AV03MSEL[AV03WIXM] normal; /* Reset select flag*/

    if (AV03W01.AV03WPSW[AV03WIXM] != "Y" /* If not from D/B*/
     && AV03W00.AV03WSSW == "Y")          /* and security exists*/
      set AV03M001.SY1STNBR[AV03WIXM] normal; /* Reset field*/
      set AV03M001.SY2CLRID[AV03WIXM] normal; /* Reset field*/
      set AV03M001.XDMCD[AV03WIXM] normal; /* Reset Field*/
      set AV03M001.AVHBLKFL[AV03WIXM] normal; /* Reset field*/
    end

    AV03W00.AV03WIXM = AV03W00.AV03WIXM + 1; /* Increment line index*/

  end

  set AV03M001.CATOAP initialAttributes; /* TO application*/
  set AV03M001.CAITEM initialAttributes; /* Application data*/


end // end AV03400

// Verify the entered data
Function AV03500()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (AV03M001.CAITEM > " ") /* Application data was entered*/

    AVWREC.XXXMODID = "500"; /* Module identification*/
    set AV03M001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(274); /* Enter only with FAST PATH*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid request to scroll was entered.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    AV03510(); /* Verify scroll request*/

  else
    /* ------------------------------------------------------------*/
     /* Verify any selections.*/
    /* ------------------------------------------------------------*/

    AV03W00.AV03WIXM = 15; /* Initialize line index*/
    while (AV03W00.AV03WIXM > 0) /* Until end of lines*/

      /* ------------------------------------------------------------*/
       /* If any style/color/dim was added:*/
      /* ------------------------------------------------------------*/
      if (AV03M001.XDMCD[AV03WIXM] != AV03W01.XDMCD[AV03WIXM]
       || AV03M001.SY2CLRID[AV03WIXM] != AV03W01.SY2CLRID[AV03WIXM]
       || AV03M001.SY1STNBR[AV03WIXM] != AV03W01.SY1STNBR[AV03WIXM])

        AV03530(); /* Verify style/color/dim*/
      end

      /* ------------------------------------------------------------*/
       /* If Bulk only flag was added:*/
      /* ------------------------------------------------------------*/
      if (AV03M001.AVHBLKFL[AV03WIXM] != AV03W01.AVHBLKFL[AV03WIXM])

        AV03540(); /* Verify bulk flag*/
      end

      /* ------------------------------------------------------------*/
       /* If any selection made:*/
      /* ------------------------------------------------------------*/
      if (AV03M001.AV03MSEL[AV03WIXM] > " ") /* Selection was entered*/
        AV03550(); /* Verify selections*/
      end

      /* ------------------------------------------------------------*/
       /* Subtract from loop counter.*/
      /* ------------------------------------------------------------*/
      AV03W00.AV03WIXM = AV03W00.AV03WIXM - 1; /* Decrement line number*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If any errors were found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Errors were found*/
    exit stack; /* Exit to display map*/

  else
    /* ------------------------------------------------------------*/
     /* If 'a' or 'd' entered, ask user to comfirm selection.*/
    /* ------------------------------------------------------------*/
    if ("D" in AV03M001.AV03MSEL /* If any 'd' entered*/
     || "A" in AV03M001.AV03MSEL) /* If any 'a' entered*/

      if (AV03W00.AV03WSSW == "Y") /* If security clearance*/

        AVWREC.XXXMODID = "500"; /* Module identification*/
        AVWREC.XXXMSGNO = 289; /* Indicate please confirm*/

      end

    end
  end


end // end AV03500

// Verify scroll request
Function AV03510()
   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && AV03W00.XXXTSPAG < 2)       /* And no previous pages*/

    AVWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(10); /* No previous page to display*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && AV03W00.XXXTSPAG >= AV03W00.TA5TSQIX) /* and no more pages*/

    AVWREC.XXXMODID = "510"; /* Module identification*/
    converseLib.validationFailed(11); /* No more pages to display*/

  end

   /* ------------------------------------------------------------*/
   /* Highlight any selection that was entered.*/
   /* ------------------------------------------------------------*/

  AV03511(); /* Highlight any selection*/

   /* ------------------------------------------------------------*/
   /* Highlight key field if it was changed.*/
   /* ------------------------------------------------------------*/

  if (AV03M001.XOWCD != AVWREC.XOWCD) /* Owner group code*/

    AVWREC.XXXMODID = "510"; /* Module identification*/
    set AV03M001.XOWCD cursor, bold; /* Highlight*/
    converseLib.validationFailed(275); /* Modifications not allowed*/

  end


end // end AV03510

// Highlight any selection
Function AV03511()
   /* ------------------------------------------------------------*/
   /* Indicate an error for any selection that was entered.*/
   /* ------------------------------------------------------------*/

  AV03W00.AV03WIXM = 15; /* Initialize line index*/

  while (AV03W00.AV03WIXM > 0) /* Until end of lines*/

    if (AV03M001.AV03MSEL[AV03WIXM] > " ") /* Selection entered*/

      AVWREC.XXXMODID = "511"; /* Module identification*/
      set AV03M001.AV03MSEL[AV03WIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(275); /* Modifications not allowed*/

    end

    AV03W00.AV03WIXM = AV03W00.AV03WIXM - 1; /* Decrement line number*/

  end


end // end AV03511

// Verify style/color/dim
Function AV03530()
   /* ------------------------------------------------------------*/
   /* Verify if style/color/dim entered, that 'a' is selected.*/
   /* field and that style/color/dim is a valid product.*/
   /* ------------------------------------------------------------*/

  if (AV03M001.XDMCD[AV03WIXM] == " ") /* If dimension missing*/
    AV03M001.XDMCD[AV03WIXM] = "00"; /* Default to '00'*/
  end

  if (AV03M001.SY2CLRID[AV03WIXM] != " ") /* If color entered*/
    if (AV03M001.AV03MSEL[AV03WIXM] != "A" /* If not 'a' entered*/
     && AV03M001.AV03MSEL[AV03WIXM] != " ") /* and not blank*/

      AVWREC.XXXMODID = "530"; /* Module identification*/
      set AV03M001.SY2CLRID[AV03WIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(111); /* Indicate missing*/

    end
  end

  if (AV03M001.SY1STNBR[AV03WIXM] != " ") /* If style entered*/
    if (AV03M001.AV03MSEL[AV03WIXM] != "A" /* If not 'a' entered*/
     && AV03M001.AV03MSEL[AV03WIXM] != " ") /* and not blank*/

      AVWREC.XXXMODID = "530"; /* Module identification*/
      set AV03M001.SY1STNBR[AV03WIXM] cursor, bold; /* Highlight*/
      converseLib.validationFailed(63); /* Indicate missing*/

    end
  end

   /* ------------------------------------------------------------*/
   /* Verify style/color/dim is a valid product.*/
   /* ------------------------------------------------------------*/
  if (AV03M001.SY2CLRID[AV03WIXM] != " " /* If color entered*/
   || AV03M001.SY1STNBR[AV03WIXM] != " ") /* or style entered*/

    AV03531(); /* Select SYCLR*/

    if (SQLCA.VAGen_SQLCODE == 0) /* If row found*/

      /* ------------------------------------------------------------*/
       /* Verify indicator equal 's' and blnk sty/colr the same.*/
      /* ------------------------------------------------------------*/
      if (PG2REC.PG2SCRFL == "S" /* If style ind  = 's'*/
       && AV03M001.SY1BSTNB == PG2REC.SY1BSTNB
       && AV03M001.SY2BCLCD == PG2REC.SY2BCLCD)

        AV03M001.SY1SSTNM[AV03WIXM] = PG2REC.SY1SSTNM;
        AV03M001.SY2CLRAB[AV03WIXM] = PG2REC.SY2CLRAB;

        XDMREC.XDMCD = PG2REC.XDMCD;
        if (XDMREC.XDMCD in XDMTBL.XDMCD)
          AV03M001.XDMDESC[AV03WIXM] = XDMTBL.XDMDESC[sysVar.arrayIndex];
        end

      else

        AVWREC.XXXMODID = "530"; /* Module identification*/
        set AV03M001.SY1STNBR[AV03WIXM] cursor, bold; /* Highlight*/
        set AV03M001.SY2CLRID[AV03WIXM] bold; /* Highlight*/
        set AV03M001.XDMCD[AV03WIXM] bold; /* Highlight*/
        converseLib.validationFailed(296); /* Indicate product invalid for blank*/

      end

    else
      AV03M001.SY1SSTNM[AV03WIXM] = "STYLE NOT IN ";
      AV03M001.SY2CLRAB[AV03WIXM] = "PRODUCT FILE";
      AV03M001.XDMDESC[AV03WIXM] = "************";

    end
  end

   /* ------------------------------------------------------------*/
   /* Verify bulk only flag.*/
   /* ------------------------------------------------------------*/
  if (AV03M001.AVHBLKFL[AV03WIXM] == " ") /* If bulk flag missing*/
    AV03M001.AVHBLKFL[AV03WIXM] = "N"; /* Default to 'n'*/
  end

  if (AV03M001.AVHBLKFL[AV03WIXM] != "Y" /* If bulk flag not 'y'*/
   && AV03M001.AVHBLKFL[AV03WIXM] != "N") /* and bulk flag not 'n'*/
    AVWREC.XXXMODID = "530"; /* Module identification*/
    set AV03M001.AVHBLKFL[AV03WIXM] cursor, bold; /* Highlight*/
    converseLib.validationFailed(286); /* Indicate missing*/

  end

end // end AV03530

// Select SYCLR
Function AV03531()
   /* ------------------------------------------------------------*/
   /* Clear copybook.*/
   /* ------------------------------------------------------------*/

  set PG2REC empty; /* Clear copybook*/

   /* ------------------------------------------------------------*/
   /* Format keys.*/
   /* ------------------------------------------------------------*/

  PG2REC.SY1STNBR = AV03M001.SY1STNBR[AV03WIXM]; /* style nbr*/
  PG2REC.SY2CLRID = AV03M001.SY2CLRID[AV03WIXM]; /* color cd*/
  PG2REC.XDMCD = AV03M001.XDMCD[AV03WIXM]; /* dim cd*/
  PG2REC.XGPCD = AV03W00.XGPCD; /* GPC cd*/

   /* ------------------------------------------------------------*/
   /* Select MLTYP.*/
   /* ------------------------------------------------------------*/

  try
    call "IO3120" ("S ", SQLCA, PG2REC) {isNoRefresh = yes, isExternal = yes};
  end /* Select row*/

   /* ------------------------------------------------------------*/
   /* If successful or not found, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000") /* Call successful*/

    if (SQLCA.VAGen_SQLCODE == +0 /* Normal return code*/
     || SQLCA.VAGen_SQLCODE == +100) /* row not found*/

      return; /* Return to previous process*/

    end

  end

   /* ------------------------------------------------------------*/
   /* If data not available, return an error message to the user.*/
   /* Otherwise, indicate that a system error has occurred.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  TAEREC.SQLCAREC = SQLCA.SQLCAREC; /* SQL Commarea*/

  AVWREC.XXXMODID = "541"; /* Module identification*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == -911)   /* and resource not available*/

    converseLib.validationFailed(75); /* Indicate not available*/
    TA1REC.TA1FUNC = "POST"; /* Termination function*/

  else

    converseLib.validationFailed(264); /* Indicate system error*/
    TA1REC.TA1FUNC = "RETN"; /* Termination function*/

    converseLib.clearScreen();
     /* Clear screen before display*/

  end

  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "SELECT ROW";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = AVWREC.XXXMODID; /* Program number*/
  TA1REC.TA1DBASE = "DPRODG  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VSYCLR01"; /* Table view name*/
  TA1REC.TA1TBLKE = PG2REC.PG2KEY; /* Table key*/

  call "TA0020" (TA1REC, TAEREC) {isNoRefresh = yes, isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end AV03531

