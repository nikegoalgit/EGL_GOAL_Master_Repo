end // end TA0F-14100

// Verify scroll request
Function TA0F-14200()
   /* ------------------------------------------------------------*/
   /* Indicate key fields may not change when scroll is requested.*/
   /* ------------------------------------------------------------*/

  if (TA0FM001.XXX-ERR-TBL-PFX-ID != TA0FW01.XXX-ERR-TBL-PFX-ID)
                                   /* Style changed*/

    TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
    converseLib.validationFailed(124); /* Field may not change*/
    set TA0FM001.XXX-ERR-TBL-PFX-ID cursor, bold; /* Highlight*/

  end

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* Scroll forward request*/
    if (TA0FM001.XXX-CSP-ERRMSG-ID != TA0FW01.XXX-CSP-ERRMSG-ID)

      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(124); /* Field may not change*/
      set TA0FM001.XXX-CSP-ERRMSG-ID cursor, bold; /* Highlight*/

    end
  end

  if (converseVar.eventKey is pf9 /* if UPDATE/ADD record request*/
   || converseVar.eventKey is pf10) /* if DELETE record request*/
    /* next sentence*/

  else

    if (TA0FM001.ZFL-LANG-CD != TA0FW01.ZFL-LANG-CD)

      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(124); /* Field may not change*/
      set TA0FM001.ZFL-LANG-CD cursor, bold; /* Highlight*/

    end
  end

   /* ------------------------------------------------------------*/
   /* If scrolls were requested, set taofw-ss-index value*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* Scroll back request*/
    if (TA0FW00.TA0FW-SS-PAGE-CNT <= 1)
      TA0FW00.TA0FW-SS-IDX = 0; /* reset to 0*/
    end

    if (TA0FW00.TA0FW-SS-PAGE-CNT >= 11)
      TA0FW00.TA0FW-SS-IDX = 28; /* reset to 28*/
    end

    if (TA0FW00.TA0FW-SS-PAGE-CNT >= 0 /* if page-cnt 1 thru 10*/
     && TA0FW00.TA0FW-SS-PAGE-CNT <= 11)
      TA0FW00.TA0FW-SS-IDX = (TA0FW00.TA0FW-SS-PAGE-CNT * 3) - 2;
    end

  end

   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that records exists*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back request*/
    if (TA0FW00.TA0FW-SS-IDX < 1) /* no more recs*/

      TA0FW00.TA0FW-SS-IDX = 1; /* reset to 1*/
      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(112); /* No previous records to display*/

    end
  end


   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward request*/
    if (TA0FW00.TA0FW-SS-IDX > TA0FW00.TA0FW-ARRAY-SIZE) /* no more recs*/

      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(113); /* No more records to display*/

    end
  end


   /* ------------------------------------------------------------*/
   /* If request prev error message number*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf5) /* Scroll forward request*/
    TA0FM001.XXX-CSP-ERRMSG-ID = TA0FM001.XXX-CSP-ERRMSG-ID - 1; /* down by 1*/
    if (TA0FM001.XXX-CSP-ERRMSG-ID <= 1) /* err msg 1 is at first of table*/

      TA0FM001.XXX-CSP-ERRMSG-ID = 1; /* re-set errmsg to 1*/
      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      TAZREC.XXX-MSG-NBR = 109; /* No more records to display*/
    end
  end


   /* ------------------------------------------------------------*/
   /* If request next error message number*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf6) /* Scroll forward request*/
    TA0FM001.XXX-CSP-ERRMSG-ID = TA0FM001.XXX-CSP-ERRMSG-ID + 1; /* down by 1*/
    if (TA0FM001.XXX-CSP-ERRMSG-ID > 4096) /* csp errmsg nbr gt 4096*/

      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(108); /* No more records to display*/
    end
  end

   /* ------------------------------------------------------------*/
   /* If Error found, bypass PF9 & PF10 requests*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* error was found*/
    return; /* exit to display map*/
  end


   /* ------------------------------------------------------------*/
   /* If UPDATE or ADD Record requested ... PF9*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf9) /* UPDATE a record (DB2 row)*/
    if (TA0FM001.ZFL-LANG-CD == "EN") /* can't update English Err Messages*/
      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(110); /* can't update English Err Messages*/
      return; /* exit routine*/
    end

    if (TA0FM001.TA0FM-REQ-LANG-MSG-TXT-P2 == " ") /* if req msg txt blank*/
      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(136); /* can't update English Err Messages*/
      return; /* exit routine*/
    end

    TA0F-14210(); /* read GOAL SECURITY File*/

    if (TA0FW00.TA0FW-SCTY-REC-FND-SW == "Y") /* record found*/
      if (COMMAREA.CAADDFL == "Y" /* if add flag = y*/
       || COMMAREA.CACHGFL == "Y") /* if add flag = y*/
                  /* next sentence*/
      else
        TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
        converseLib.validationFailed(130); /* not authorized for UPDATE Process*/
        return; /* exit routine*/
      end
    else
      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(130); /* not authorized for UPDATE Process*/
      return; /* exit routine*/
    end

    TA0FS-IO7140-INIT(); /* init work area for IOMOD select*/

    TA0FS-IO7140-S(); /* select unique: read DERMSG.VCSP4001*/


    /* ------------------------------------------------------------*/
    /* If UPDATE  Record requested ... PF9*/
    /* ------------------------------------------------------------*/

    if (SQLCA.VAGen_SQLCODE == 0) /* if ROW found after SELECT UNIQUE*/
      TA0FS-UPDATE-ROW(); /* UPDATE DB2 Row*/
      TA0FS-IO7140-U(); /* update unique: DERMSG.VCSP4001*/

      if (SQLCA.VAGen_SQLCODE == 0) /* UPDATE succesful*/
        TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
        TAZREC.XXX-MSG-NBR = 134; /* UPDATE successful*/
        TA0FW00.XXX-RQT-DATA-FL = "Y"; /* req data flag*/
        return; /* exit routine*/
      else
        if (SQLCA.VAGen_SQLCODE == -911) /* record not avilable*/
          TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
          converseLib.validationFailed(121); /* record not available*/
          return; /* exit routine*/
        else
          TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
          converseLib.validationFailed(120); /* unexpected error*/
          return; /* exit routine*/
        end
      end

    else

      /* ------------------------------------------------------------*/
      /* If ADD Record requested ... PF9*/
      /* ------------------------------------------------------------*/

      if (SQLCA.VAGen_SQLCODE == 100) /* if ROW not found after SELECT UNIQUE*/
        TA0FS-ADD-ROW(); /* ADD DB2 Row*/
        TA0FS-IO7140-A(); /* add unique: DERMSG.VCSP4001*/

        if (SQLCA.VAGen_SQLCODE == 0) /* ADD succesful*/
          TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
          TAZREC.XXX-MSG-NBR = 134; /* ADD successful*/
          TA0FW00.XXX-RQT-DATA-FL = "Y"; /* req data flag*/
          return; /* exit routine*/
        else
          if (SQLCA.VAGen_SQLCODE == -911) /* record not avilable*/
            TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
            converseLib.validationFailed(121); /* record not available*/
            return; /* exit routine*/
          else
            TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
            converseLib.validationFailed(120); /* unexpected error*/
            return; /* exit routine*/
          end
        end

      end

    end
  end


   /* ------------------------------------------------------------*/
   /* If DELETE Record requested ... PF10*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf10) /* DELETE a record (DB2 row)*/
    if (TA0FM001.ZFL-LANG-CD == "EN") /* can't update English Err Messages*/
      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(111); /* can't update English Err Messages*/
      return; /* exit routine*/
    end

    TA0F-14210(); /* read GOAL SECURITY File*/

    if (TA0FW00.TA0FW-SCTY-REC-FND-SW == "Y") /* record found*/
      if (COMMAREA.CADELFL == "Y") /* if add flag = y*/
                  /* next sentence*/
      else
        TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
        converseLib.validationFailed(131); /* not authorized for DELETE Process*/
        return; /* exit routine*/
      end
    else
      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(131); /* not authorized for DELETE Process*/
      return; /* exit routine*/
    end

    TA0FS-IO7140-INIT(); /* init work area for IOMOD select*/

    TA0FS-IO7140-S(); /* select unique: read DERMSG.VCSP4001*/

    if (SQLCA.VAGen_SQLCODE == 0) /* if ROW found*/
      TA0FS-IO7140-D(); /* DELETE DB2 Row*/

      if (SQLCA.VAGen_SQLCODE == 0) /* UPDATE succesful*/
        TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
        TAZREC.XXX-MSG-NBR = 133; /* record DELETE successful*/
        TA0FW00.XXX-RQT-DATA-FL = "Y"; /* req data flag*/
        return; /* exit routine*/
      else
        if (SQLCA.VAGen_SQLCODE == -911) /* record not avilable*/
          TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
          converseLib.validationFailed(135); /* record not available*/
          return; /* exit routine*/
        else
          TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
          converseLib.validationFailed(120); /* unexpected error*/
          return; /* exit routine*/
        end
      end

    else
      TAZREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
      converseLib.validationFailed(132); /* no record to DELETE*/
      return; /* exit routine*/

    end

  end


end // end TA0F-14200

// read grp/appl record
Function TA0F-14210()
  set MN1REC empty; /* set record empty*/

   /* ------------------------------------------------------------------*/
   /* this process is used to read the user/appl record on goal security*/
   /* set up file key for application read*/
   /* ------------------------------------------------------------------*/

  MN3REC.MN1KEY = " "; /* clear key area*/
