  TA0FW00.TA0FW-SYS-ERRMSG-FND-SW = "Y";
                          /* assume systems's errmsg found in table*/

  if (TA0FW01.XXX-ERR-TBL-PFX-ID == "AL") /* AL00ENU message file*/
