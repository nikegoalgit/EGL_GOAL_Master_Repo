Function TA01S-CHECK-ERR()
   /* ------------------------------------------------------------*/
   /* Convert and find out if any addresses were changed.*/
   /* ------------------------------------------------------------*/

  TA01W00.TA01W-SET-PAGE-FL = "N";

   /* ------------------------------------------------------------*/
   /* Check error address*/
   /* ------------------------------------------------------------*/

  if (TA01M002.IM-ERRTNT-NETWK-ADDR-1 == " ")
    TA01W02.IM-ERR-TNTNM-ID = "          ";
  else

    TA01W00.XXX-1PART-TNTNM-NM = TA01M002.IM-ERRTNT-NETWK-ADDR-1;
    TA01W00.XXX-2PART-TNTNM-NM = TA01M002.IM-ERRTNT-NETWK-ADDR-2;

    TA01W00.TA01W-TNTNM-FOUND-FLAG = "N";

    TA01W00.TAW-ADDRESS-NAME = "ERROR"; /* May be used in Popup window*/

    TA01S-CHECK-TNTNM(); /* Check for pop-up, *'s, & convert.*/

    if (TA01W00.TA01W-TNTNM-FOUND-FLAG == "Y") /* Flag set in ta01-24110*/
   /* Save IM-EMP-DEVICE-ID if good,*/
      TA01W02.IM-ERR-TNTNM-ID = TA01W00.IM-EMP-DEVICE-ID;

      TA01M002.IM-ERRTNT-NETWK-ADDR-1 = TA01W00.XXX-1PART-TNTNM-NM;
      TA01M002.IM-ERRTNT-NETWK-ADDR-2 = TA01W00.XXX-2PART-TNTNM-NM;

    else /* Otherwise indicate error.*/
