      set TA01M002.IM-ERRTNT-NETWK-ADDR-2 bold;
    end

  end

  if (TA01W00.TA01W-SET-PAGE-FL == "Y")

    converseLib.clearScreen();
     /* Clear out the pop-up map.*/

  end


end // end TA01S-CHECK-ERR

// Check file name
Function TA01S-CHECK-FNAME()
   /* ------------------------------------------------------------*/
   /* Force filename if action type is 'FILE'.*/
   /* All files must be a-z, A-Z, one "." allowed*/
   /* ------------------------------------------------------------*/

  if (TA01M002.IM-IMDS-FIL-NM-1 <= " ")

    if (TA01M002.IM-IMDS-ACT-TYP == "FILE")

      TAWREC.XXX-MSG-LOC-ID = "24100";
      converseLib.validationFailed(47); /* File name missing.*/
      TA01W00.TAW-ERROR-FLAG = "Y";

      set TA01M002.IM-IMDS-FIL-NM-1 bold, cursor;
      set TA01M002.IM-IMDS-FIL-NM-2 bold;

    end

    return; /* No file, no edit.*/

  end

  TA01W02.IM-IMDS-FIL-NM-1 = TA01M002.IM-IMDS-FIL-NM-1;
  TA01W02.IM-IMDS-FIL-NM-2 = TA01M002.IM-IMDS-FIL-NM-2;

  TA01W00.TA01W-FILE-ARRAY = TA01W02.IM-IMDS-FIL-NM;

  TA01W00.TA01W-INDEX = 1;
  TA01W-EOLN = "N";

  /* check for aphanumeric chars for file name.  only letters and*/
  /* numbers allowed.*/
  while (TA01W00.TA01W-INDEX <= 114) /* 'Till the end of the file.*/

    if (TA01W-EOLN == "N")
      if ((TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] >= "A"
       && TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] <= "I")
       || (TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] >= "J"
       && TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] <= "R")
       || (TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] >= "S"
       && TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] <= "Z")
       || (TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] >= "0"
       && TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] <= "9")
       || TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] == ".")
                                  /* NEXT SENTENCE*/
      else

        if (TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] == " ")
          TA01W-EOLN = "Y";
        else
          TA01S-NAME-ERR();
          return;
        end

      end
    else
      if (TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] != " ")
        TA01S-NAME-ERR();
        return;
      end
    end
  /* check for valid first char*/
    if (TA01W-INDEX == 1
     && ((TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] >= "0"
     && TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] <= "9")
     || (TA01W00.TA01W-FILE-CHAR[TA01W-INDEX] == ".")))
      TA01S-NAME-ERR();
      return;
    end
    TA01W00.TA01W-INDEX = TA01W00.TA01W-INDEX + 1;

  end


end // end TA01S-CHECK-FNAME

