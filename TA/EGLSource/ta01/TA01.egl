//*** PROGRAM=TA01 ****
// This application may only be entered by users whose names
// are found on the DIMDS.VSECUR01 table with a report id of
// "Security".  ( XSTAS-00000 )
// 
// This application allows a user to inquire and maintain rows
// in the DIMDS.VDIST001 table.
// 
// Users may only see and update rows that have a corrosonding
// row in the DIMDS.VSECUR01 table with that report id.
// 
// New reports will have the user id and the report id added to
// the VSECUR01 table in the backround without prompting.
// 
// There are two different screens that may be displayed.
// 
// The first screen displayed to the user allows specification
// of the search criteria to be used to list the rows in the
// table.  The list is displayed and the user may page backward
// and forward to view the information.
// 
// When a line is selected on the screen, the second screen is
// displayed, showing all information for that table row.
// 
// If any data is modified, it is verified and, if complete and
// correct, the table row is updated with the changed data.
// The screen is redisplayed to the user, indicating that the
// update is completed.
// 
// If more than one line was selected on the first screen, the
// next selection is displayed when the user presses the ENTER
// key without making any change to the displayed data.
// 
// When the user exits the second screen (either via PF3 key or
// after all selections have been viewed) the first screen is
// redisplayed, indicating the selections that were viewed.
// 
// The user may exit to any allowed application, via FAST PATH
// or PF key, at any time.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE    USERID  DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// tsup     08/10/98  sbusho  original development for new IMDS
// tsup     03/25/99  jbuch1  Enhanced editing for new IMDS
// *********************
Program TA01 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "TA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  SQLCA SQLCA; // record
  TA01REC TA01REC; // record
  TA01W00 TA01W00; // record
  TA01W01 TA01W01; // record
  TA01W02 TA01W02; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  TAMREC02 TAMREC02; // record
  TAWREC TAWREC; // record
  VDIST001 VDIST001; // record
  VSECUR01 VSECUR01; // record
  VTNTNM01 VTNTNM01; // record
  XX0XW01 XX0XW01; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use TA01M.TA01M001,  // forms
      TA01M.TA01M002, 
      TA01M.XMTAM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPTA-00000: XPTA-00000();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/

    set TA01M001 initial; /* M001 input/output*/
    set TA01M002 initial; /* M002 input/output*/
    set TA01W00 empty; /* Working storage*/
    set TA01W01 empty; /* M001 display storage*/
    set TA01W02 empty; /* M002 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set TA5REC empty; /* Temporary storage parameters*/
    set XX0XW01 empty; /* Date/time subroutine parameters*/

    TA01W00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/
    TA01W00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/
    TA01W00.TAW-ERROR-FLAG = "N";

    TA01W00.XXX-MAP-DSP-CD = "1"; /* Map to be displayed*/

    TA01W00.XXX-TEMP-STG-LNTH = 2888; /* Temporary storage length*/
    TA01W00.XXX-INX-LMT-NBR = 13; /* Number of lines on a page*/

     /* ------------------------------------------------------------*/
     /* Flow to check security table for authorized user.*/
     /* ------------------------------------------------------------*/

    goto XSTAS-SECURITY;


    XSTAS-SECURITY: XSTAS-SECURITY();

     /* -------------------------------------------*/
     /* Flow to converse main process.*/
     /* -------------------------------------------*/

    goto TA01-00000; /* Flow to main process.*/


    TA01-00000: TA01-00000();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto TA01-00000; /* Converse the main process*/


  end // end main
end // end TA01

// Determine which map to display
Function TA01-00000()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) If specified, display the map that was requested.*/
   /* 2) Otherwise, indicate error and exit to System Menu.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.XXX-MAP-DSP-CD == "1") /* Map 1 was requested*/

    TA01-10000(); /* Display map 1*/

    exit stack; /* Exit to display map*/

  end

  if (TA01W00.XXX-MAP-DSP-CD == "2") /* Map 2 was requested*/

    TA01-20000(); /* Display map 2*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Write the error to the Error Diagnosis Reporting Facility.*/
   /* ------------------------------------------------------------*/

  TAWREC.XXX-MSG-LOC-ID = "00000"; /* Message location*/
  TAWREC.XXX-MSG-NBR = 10; /* Unexpected error occurred*/
  TA1REC.TA1FUNC = "RETN"; /* Termination function*/

  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1PGMNM = TAWREC.XXX-MSG-LOC-ID; /* Message location*/
  TA1REC.TA1LOC = "INVALID MAP NUMBER REQUESTED"; /* Description*/
  TA1REC.TA1MAPNO = TA01W00.XXX-MAP-DSP-CD; /* Map requested*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

   /* ------------------------------------------------------------*/
   /* Exit to display the application system menu.*/
   /* ------------------------------------------------------------*/

  COMMAREA.CATOAP = "MN0M"; /* Main Menu*/

  XSTAS-EXIT(); /* Exit to another application*/

   /* ------------------------------------------------------------*/
   /* Abnormally terminate the transaction if the exit failed.*/
   /* ------------------------------------------------------------*/

  TA1REC.TA1FUNC = "ABND"; /* Termination function*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/


end // end TA01-00000

// Main process to display a map
Function TA01-10000()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Verify the data that was entered.*/
   /* 5) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  TA01-11000(); /* Obtain the data to display*/

  TA01-12000(); /* Format and display the map*/

  TA01-13000(); /* Check attention identifier*/

  TA01-14000(); /* Verify the entered data*/

  TA01-15000(); /* Process valid input request*/


end // end TA01-10000

// Obtain the data to display
Function TA01-11000()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.XXX-RQT-DATA-FL != "Y" /* Data was not requested*/
   || TA01W00.TAW-ERROR-FLAG == "Y") /* or an error was found*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* ------------------------------------------------------------*/

  if (TAWREC.XXX-MSG-NBR == 0) /* No message to display*/
    TA01W00.XXX-CURR-PAGE-NBR = 1; /* Page number to display*/
  end

   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/

  TA01S-TA0050-P(); /* Purge any temporary storage*/

  TA01W00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

  TA01W01.TAW-RPT-FIL-KEY = TA01M001.TAW-RPT-FIL-KEY; /* Save key fields*/
  TA01W01.TAW-SCRTY-USER-KEY = TA01M001.TAW-SCRTY-USER-KEY;

   /* ------------------------------------------------------------*/
   /* Select the information to display in the map heading.*/
   /* ------------------------------------------------------------*/

  set TA01REC empty; /* Clear row storage area*/

  if (TA01W01.TAW-RPT-FIL-KEY <= " ")

    TA01REC.XXX-RPT-FIL-ID = "*";

  else

    TA01REC.XXX-RPT-FIL-ID = TA01W01.TAW-RPT-FIL-KEY;

  end

   /* ------------------------------------------------------------*/
   /* Always default to current user's id for report selection.*/
   /* This option now disabled, ASKIP,DARK on map.*/
   /* Other commented code in ta01-12000.*/
   /* ------------------------------------------------------------*/

  TA01REC.XXX-SCRTY-USER-ID = COMMAREA.CAUSERID;
  TA01M001.TAW-SCRTY-USER-KEY = COMMAREA.CAUSERID;
  TA01W01.TAW-SCRTY-USER-KEY = COMMAREA.CAUSERID;
                     /* Display default to user.*/

   /* IF ta01W01.SCRTY-USER-KEY GT ' ';*/
    /* ;*/
    /* MOVE ta01W01.SCRTY-USER-KEY TO ta01REC.XXX-SCRTY-USER-ID;*/
    /* ;*/
   /* ELSE;                            /* Use user's id as a default*/
    /* ;*/
    /* IF ta01W01.RPT-FIL-KEY LE ' ';*/
      /* ;*/
      /* MOVE COMMAREA.CAUSERID TO ta01REC.XXX-SCRTY-USER-ID;*/
      /* MOVE COMMAREA.CAUSERID TO ta01M001.SCRTY-USER-KEY;*/
      /* MOVE COMMAREA.CAUSERID TO ta01W01.SCRTY-USER-KEY;*/
      /* ;               /* Display default to user.*/
    /* ELSE;*/
      /* ;*/
      /* MOVE '*' TO ta01REC.XXX-SCRTY-USER-ID;*/
      /* ;*/
    /* END;*/
   /* ;*/
   /* END;*/

  TA01S-TA0580-S1(); /* Select the table join*/

   /* ------------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ------------------------------------------------------------*/

  TA01W00.XXX-INX-FLD-NBR = 1; /* Display line index*/

  TA01W00.TA01W-REPORT-EOF-FL = "N"; /* End of cursor flag*/

  while (TA01W00.TA01W-REPORT-EOF-FL == "N") /* Until end of cursor*/

    TA01S-TA0580-N1(); /* Fetch next row in join*/

    /* ------------------------------------------------------------*/
    /* If not end of cursor, save the data in display storage.*/
    /* ------------------------------------------------------------*/

    if (TA01W00.TA01W-REPORT-EOF-FL == "N") /* Not end of cursor*/

      /* ------------------------------------------------------------*/
      /* Save the data from the table row in display storage.*/
      /* ------------------------------------------------------------*/

      TA01W01.XXX-RPT-FIL-ID[XXX-INX-FLD-NBR] = TA01REC.XXX-RPT-FIL-ID;
          /* Report ID*/
      TA01W01.XXX-SCRTY-USER-ID[XXX-INX-FLD-NBR] = TA01REC.XXX-SCRTY-USER-ID;
          /* Security user ID*/
      TA01W01.IM-TRGT-TNTNM-ID[XXX-INX-FLD-NBR] = TA01REC.IM-TRGT-TNTNM-ID;
          /* Target ID*/
      TA01W01.IM-AGNT-TNTNM-ID[XXX-INX-FLD-NBR] = TA01REC.IM-AGNT-TNTNM-ID;
          /* Agent ID*/
      TA01W01.IM-NTFY-TNTNM-ID[XXX-INX-FLD-NBR] = TA01REC.IM-NTFY-TNTNM-ID;
          /* Notify ID*/
      TA01W01.IM-ERR-TNTNM-ID[XXX-INX-FLD-NBR] = TA01REC.IM-ERR-TNTNM-ID;
          /* Error ID*/
      TA01W01.IM-IMDS-SBJT[XXX-INX-FLD-NBR] = TA01REC.IM-IMDS-SBJT;
          /* Subject*/
      TA01W01.IM-IMDS-ACT-TYP[XXX-INX-FLD-NBR] = TA01REC.IM-IMDS-ACT-TYP;
          /* Action type*/
      TA01W01.IM-IMDS-FIL-NM[XXX-INX-FLD-NBR] = TA01REC.IM-IMDS-FIL-NM;
          /* File name*/
      /* ------------------------------------------------------------*/
      /* Increment the line index to the next row in display storage.*/
      /* ------------------------------------------------------------*/

      TA01W00.XXX-INX-FLD-NBR = TA01W00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/

    end

    /* ------------------------------------------------------------*/
    /* If the page is full, add the page to the storage queue.*/
    /* ------------------------------------------------------------*/

    if (TA01W00.XXX-INX-FLD-NBR > TA01W00.XXX-INX-LMT-NBR) /* Page is full*/

      TA01S-TA0050-A(); /* Add page to temporary storage*/

      TA01W00.XXX-INX-FLD-NBR = 1; /* Display line index*/

    end

  end

   /* ------------------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.XXX-INX-FLD-NBR > 1) /* Page was formatted*/
    TA01S-TA0050-A(); /* Add page to temporary storage*/
  end

   /* ------------------------------------------------------------*/
   /* Close the cursor for the selected set.*/
   /* ------------------------------------------------------------*/

  TA01S-TA0580-C1(); /* Close join cursor*/

   /* ------------------------------------------------------------*/
   /* If no pages were formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.XXX-HI-PAGE-NBR == 0) /* No pages formatted*/

    TAWREC.XXX-MSG-LOC-ID = "11000"; /* Message location*/
    converseLib.validationFailed(20); /* No data was found to display*/
    TA01W00.TAW-ERROR-FLAG = "Y";

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  TA01W00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/


end // end TA01-11000

// Format and converse the map
Function TA01-12000()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.TAW-ERROR-FLAG == "Y") /* Error was found*/
    TAWREC.XXX-MSG-NBR = converseVar.validationMsgNum; /* Message number*/
  end

   /* ------------------------------------------------------------*/
   /* If the map must be formatted, format the data in the map.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.XXX-MAP-FMAT-FL == "Y") /* Map must be formatted*/
    TA01-12100(); /* Format the data in the map*/
  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  TA01M001.XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/

  TA01M001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  TA01M001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (TAWREC.XXX-MSG-NBR != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ")  /* or message text*/

    if (TAWREC.XXX-MSG-NBR != 0) /* Message to display*/

      if (TAWREC.XXX-MSG-NBR == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(TAWREC.XXX-MSG-NBR);
      end /* Message number*/
    else
      TA01M001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    TA01M001.XXX-MSG-LOC-ID = TAWREC.XXX-MSG-LOC-ID;
                                   /* Message location*/

    set TA01M001 alarm; /* Indicate sound alarm*/

  end

  converse TA01M001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  TAWREC.XXX-MSG-LOC-ID = " "; /* Message location*/
  TAWREC.XXX-MSG-NBR = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/
  TA01W00.TAW-ERROR-FLAG = "N"; /* Set error flag off.*/

  COMMAREA.CATOAP = TA01M001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = TA01M001.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* Reset modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set TA01M001.TAW-RPT-FIL-KEY normal; /* Report ID*/

   /* SET ta01M001.SCRTY-USER-KEY NORMAL;*/
   /* This option not in use.  Corrosponding comments in ta01-11000*/

  TA01W00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (TA01W00.XXX-INX-FLD-NBR <= TA01W00.XXX-INX-LMT-NBR
   /* Until end of screen*/
   && TA01M001.XXX-RPT-FIL-ID[XXX-INX-FLD-NBR] > " ")
                                   /* or end of formatted lines*/

    set TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] normal;
                                   /* Select indicator*/

    TA01W00.XXX-INX-FLD-NBR = TA01W00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/

  end

  set TA01M001.CATOAP normal; /* TO application*/
  set TA01M001.CAITEM normal; /* Application data*/


end // end TA01-12000

// Format the data in the map
Function TA01-12100()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set TA01M001 initial; /* M001 input/output area*/

  TA01W00.XXX-MAP-FMAT-FL = "N"; /* Format the map flag*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.XXX-HI-PAGE-NBR > 0) /* Pages were formatted*/

    if (TA01W00.XXX-CURR-PAGE-NBR != TA5REC.TA5TSQIX)
                                   /* Desired page not current*/

      /* ------------------------------------------------------------*/
      /* If the current page has been changed, update the page in*/
      /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/

      if (TA01W00.XXX-PAGE-UPD-FL == "Y") /* Current page was changed*/

        TA01S-TA0050-U(); /* Update temporary storage page*/

        TA01W00.XXX-PAGE-UPD-FL = "N"; /* Page update flag*/

      end

      /* ------------------------------------------------------------*/
      /* If the desired page number is greater than the highest page*/
      /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/

      if (TA01W00.XXX-CURR-PAGE-NBR > TA01W00.XXX-HI-PAGE-NBR)
                                   /* Desired greater highest*/
        TA01W00.XXX-CURR-PAGE-NBR = TA01W00.XXX-HI-PAGE-NBR;
                                   /* Default to highest*/
      end

      TA01S-TA0050-R(); /* Read temporary storage page*/

    end

    /* ------------------------------------------------------------*/
    /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set TA01M001.XXX-CURR-PAGE-DESC skip; /* "PAGE" literal*/
    TA01M001.XXX-CURR-PAGE-NBR = TA01W00.XXX-CURR-PAGE-NBR;
                                   /* Current page*/
    set TA01M001.XXX-HI-PAGE-DESC skip; /* "OF" literal*/
    TA01M001.XXX-HI-PAGE-NBR = TA01W00.XXX-HI-PAGE-NBR;
                                   /* Highest page*/

  end

   /* ------------------------------------------------------------*/
   /* If Report ID missing, position the cursor on that field.*/
   /* ------------------------------------------------------------*/

  if (TA01W01.TAW-RPT-FIL-KEY <= " ")
    set TA01M001.TAW-RPT-FIL-KEY cursor; /* Position cursor*/
  end

   /* ------------------------------------------------------------*/
   /* Format the map fields from the display storage area.*/
   /* ------------------------------------------------------------*/

  TA01M001.TAW-RPT-FIL-KEY = TA01W01.TAW-RPT-FIL-KEY;
  TA01M001.TAW-SCRTY-USER-KEY = TA01W01.TAW-SCRTY-USER-KEY;

  TA01W00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (TA01W00.XXX-INX-FLD-NBR <= TA01W00.XXX-INX-LMT-NBR
   /* Until screen is full*/
  )
    if (TA01W00.XXX-INX-FLD-NBR == 1) /* First select field*/

      set TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, normal;
    else
      set TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] normal;

    end

    TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] = TA01W01.XXX-LN-SEL-IND[XXX-INX-FLD-NBR];

    TA01M001.XXX-RPT-FIL-ID[XXX-INX-FLD-NBR] = TA01W01.XXX-RPT-FIL-ID[XXX-INX-FLD-NBR];

    TA01M001.IM-IMDS-ACT-TYP[XXX-INX-FLD-NBR] = TA01W01.IM-IMDS-ACT-TYP[XXX-INX-FLD-NBR];

    if (TA01W01.IM-TRGT-TNTNM-ID[XXX-INX-FLD-NBR] != " ")

      set VTNTNM01 empty;
      VTNTNM01.XXX-NTWK-ADDR = "*";
      VTNTNM01.IM-EMP-DEVICE-ID = TA01W01.IM-TRGT-TNTNM-ID[XXX-INX-FLD-NBR];
      TA01S-TANTNM02-S();
      TA01M001.IM-NETWK-ADDR[XXX-INX-FLD-NBR] = VTNTNM01.XXX-NTWK-ADDR;

    end

    TA01W00.XXX-INX-FLD-NBR = TA01W00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/

  end


end // end TA01-12100

// Check attention identifier
Function TA01-13000()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (TA01W00.XXX-RQT-DATA-FL == "Y") /* Request for data pending*/
      set TA01W01 empty; /* M001 display storage*/
    end

    TA01W00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/
    TA01W00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (TA01M001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      TA01S-TA0050-P(); /* Purge any temporary storage*/

      XSTAS-EXIT(); /* Exit to another application*/

      set TA01M001.CATOAP cursor, bold; /* Highlight*/

    else

      TAWREC.XXX-MSG-LOC-ID = "13000"; /* Message location*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/
      TA01W00.TAW-ERROR-FLAG = "Y";

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If a PF key transfer requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1  /* PF1  was pressed*/
   || converseVar.eventKey is pf3  /* or PF3  was pressed*/
   || converseVar.eventKey is pf4  /* or PF4  was pressed*/
   || converseVar.eventKey is pf15 /* or PF15 was pressed*/
   || converseVar.eventKey is pf16) /* or PF16 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "TA01M001"; /* Application find code*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "MN0M"; /* Main Menu*/
    end

    if (converseVar.eventKey is pf15) /* PF3 was pressed*/
      COMMAREA.CATOAP = "TA0S"; /* Main Menu*/
    end

    if (converseVar.eventKey is pf16) /* PF3 was pressed*/
      COMMAREA.CATOAP = "TA02"; /* Main Menu*/
    end

    TA01S-TA0050-P(); /* Purge any temporary storage*/

    XSTAS-EXIT(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf9   /* and PF9  was not pressed*/
  )                                 
    TAWREC.XXX-MSG-LOC-ID = "13000"; /* Message location*/
    converseLib.validationFailed(2); /* PA/PF key pressed is not valid*/
    TA01W00.TAW-ERROR-FLAG = "Y";

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end TA01-13000

// Verify data that was entered
Function TA01-14000()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (TA01M001.CAITEM > " ") /* Application data was entered*/

    TAWREC.XXX-MSG-LOC-ID = "14000"; /* Message location*/
    converseLib.validationFailed(21); /* Enter only with FAST PATH*/
    set TA01M001.CAITEM cursor, bold; /* Highlight*/
    TA01W00.TAW-ERROR-FLAG = "Y";

  end

   /* ------------------------------------------------------------*/
   /* If scroll requested, determine if any other field changed.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    TA01-14100(); /* Highlight other changed fields*/

    /* ------------------------------------------------------------*/
    /* Determine whether a valid request to scroll was entered.*/
    /* ------------------------------------------------------------*/

    TA01-14200(); /* Verify scroll request*/

  else

    /* ------------------------------------------------------------*/
    /* If key field changed, determine if any other field changed.*/
    /* ------------------------------------------------------------*/

    if (TA01M001.TAW-RPT-FIL-KEY != TA01W01.TAW-RPT-FIL-KEY
     || TA01M001.TAW-SCRTY-USER-KEY != TA01W01.TAW-SCRTY-USER-KEY)
                                   /* If report id or scrty user changed*/

      TA01-14100(); /* Highlight other changed fields*/

    else

      /* ------------------------------------------------------------*/
      /* Otherwise, verify any modifiable data that was changed.*/
      /* ------------------------------------------------------------*/

      if (TA01W00.XXX-HI-PAGE-NBR > 0) /* Page was formatted*/

        TA01W00.XXX-INX-FLD-NBR = TA01W00.XXX-INX-LMT-NBR;
                                   /* Line index*/

        while (TA01W00.XXX-INX-FLD-NBR > 0) /* Until end of lines*/

          TA01-14400(); /* Verify data in the line*/

          TA01W00.XXX-INX-FLD-NBR = TA01W00.XXX-INX-FLD-NBR - 1;
                                   /* Decrement line number*/
        end
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* If any error was found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/
    exit stack; /* Exit to display map*/
  end


end // end TA01-14000

// Highlight other changed fields
Function TA01-14100()
   /* ------------------------------------------------------------*/
   /* If a modifiable field was changed, indicate that field may*/
   /* not be changed when key is changed or a pf key is pressed.*/
   /* ------------------------------------------------------------*/

  TA01W00.XXX-INX-FLD-NBR = TA01W00.XXX-INX-LMT-NBR; /* Line index*/

  while (TA01W00.XXX-INX-FLD-NBR > 0) /* Until end of lines*/

    if (TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != " "
     && TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != "V")

      TAWREC.XXX-MSG-LOC-ID = "14100"; /* Message location*/
      converseLib.validationFailed(22); /* Field may not change*/
      set TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
      TA01W00.TAW-ERROR-FLAG = "Y";

    end

    TA01W00.XXX-INX-FLD-NBR = TA01W00.XXX-INX-FLD-NBR - 1;
                                   /* Decrement line number*/

  end


end // end TA01-14100

// Verify scroll request
Function TA01-14200()
   /* ------------------------------------------------------------*/
   /* Indicate key fields may not change when scroll is requested.*/
   /* ------------------------------------------------------------*/

  if (TA01M001.TAW-SCRTY-USER-KEY != TA01W01.TAW-SCRTY-USER-KEY)

    TAWREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
    converseLib.validationFailed(23); /* Field may not change*/
    set TA01M001.TAW-SCRTY-USER-KEY cursor, bold; /* Highlight*/
    TA01W00.TAW-ERROR-FLAG = "Y";

  end

  if (TA01M001.TAW-RPT-FIL-KEY != TA01W01.TAW-RPT-FIL-KEY)

    TAWREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
    converseLib.validationFailed(23); /* Field may not change*/
    set TA01M001.TAW-RPT-FIL-KEY cursor, bold; /* Highlight*/
    TA01W00.TAW-ERROR-FLAG = "Y";

  end

   /* ------------------------------------------------------------*/
   /* If scroll back was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   && TA01W00.XXX-CURR-PAGE-NBR < 2) /* and no previous pages*/

    TAWREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
    converseLib.validationFailed(3); /* No previous page to display*/
    TA01W00.TAW-ERROR-FLAG = "Y";

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, ensure that pages exist.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8 /* Scroll forward request*/
   && TA01W00.XXX-CURR-PAGE-NBR >= TA01W00.XXX-HI-PAGE-NBR)
                                   /* and no more pages*/

    TAWREC.XXX-MSG-LOC-ID = "14200"; /* Message location*/
    converseLib.validationFailed(4); /* No more pages to display*/
    TA01W00.TAW-ERROR-FLAG = "Y";

  end


end // end TA01-14200

// Verify data in the line
Function TA01-14400()
   /* ------------------------------------------------------------*/
   /* If selection was entered and is not a "S", indicate that*/
   /* only "S" may be entered to select a line.*/
   /* 'V' is valid for lines that have been viewed.*/
   /* ------------------------------------------------------------*/

  if (TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != " "
   && TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != "S"
   && TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] != "V")
                                   /* is not a space or a "S"*/

    TAWREC.XXX-MSG-LOC-ID = "14400"; /* Message location*/
    converseLib.validationFailed(5); /* Only "S" may be entered*/
    set TA01M001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, bold;
    TA01W00.TAW-ERROR-FLAG = "Y";

  end


end // end TA01-14400

// Process valid input request
Function TA01-15000()
   /* ------------------------------------------------------------*/
   /* If the key fields changed, request the data to display.*/
   /* ------------------------------------------------------------*/

  if (TA01M001.TAW-RPT-FIL-KEY != TA01W01.TAW-RPT-FIL-KEY
   || TA01M001.TAW-SCRTY-USER-KEY != TA01W01.TAW-SCRTY-USER-KEY)

    TA01W00.XXX-RQT-DATA-FL = "Y"; /* Request for data flag*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll backward was requested, decrement the page number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    TA01W00.XXX-CURR-PAGE-NBR = TA01W00.XXX-CURR-PAGE-NBR - 1;
                                   /* Decrement page number*/

    TA01W00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, increment the page number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    TA01W00.XXX-CURR-PAGE-NBR = TA01W00.XXX-CURR-PAGE-NBR + 1;
                                   /* Increment page number*/

    TA01W00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If add was requested, show other map.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf9) /* Add record was requested*/

    set TA01M002 initial; /* Reset map2.*/

    TA01W00.TA01W-ADD-FLAG = "Y"; /* Request for add flag*/
    TA01W00.XXX-MAP-DSP-CD = "2"; /* Map to be displayed*/

    TA01W00.XXX-INX-FLD-NBR = 1; /* For returning update.*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If a line was selected, format the key fields and indicate*/
   /* that map 002 is to be displayed.*/
   /* ------------------------------------------------------------*/

  if ("S" in TA01M001.XXX-LN-SEL-IND) /* Line was selected*/

    set TA01M002 initial; /* Reset map2.*/

    TA01W00.XXX-INX-FLD-NBR = sysVar.arrayIndex; /* Map line index*/

    TA01S-MOVE-TO-MAP2(); /* Move selected line data to map2.*/

    TA01W00.XXX-MAP-DSP-CD = "2"; /* Map to be displayed*/

  end


end // end TA01-15000

// Main process to display a map
Function TA01-20000()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Verify the data that was entered.*/
   /* 5) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  TA01-21000(); /* Obtain the data to display*/

  TA01-22000(); /* Format and display the map*/

  TA01-23000(); /* Check attention identifier*/

  TA01-24000(); /* Verify the entered data*/

  TA01-25000(); /* Process valid input request*/


end // end TA01-20000

// Obtain the data to display
Function TA01-21000()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.XXX-RQT-DATA-FL != "Y" /* Data was not requested*/
   || TA01W00.TAW-ERROR-FLAG == "Y")

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* Clear the map display storage area and save the key fields.*/
   /* ------------------------------------------------------------*/

  set TA01W02 empty; /* M002 display storage*/

  TA01W02.XXX-RPT-FIL-ID = TA01M002.XXX-RPT-FIL-ID;

  TA01W02.IM-IMDS-SBJT = TA01M002.IM-IMDS-SBJT;

  TA01W02.IM-IMDS-ACT-TYP = TA01M002.IM-IMDS-ACT-TYP;

  TA01W02.IM-IMDS-FIL-NM-1 = TA01M002.IM-IMDS-FIL-NM-1;

  TA01W02.IM-IMDS-FIL-NM-2 = TA01M002.IM-IMDS-FIL-NM-2;

  TA01W02.IM-TRGTTNT-NETWK-ADDR-1 = TA01M002.IM-TRGTTNT-NETWK-ADDR-1;
  TA01W02.IM-TRGTTNT-NETWK-ADDR-2 = TA01M002.IM-TRGTTNT-NETWK-ADDR-2;

  TA01W02.IM-HOSTTNT-NETWK-ADDR-1 = TA01M002.IM-HOSTTNT-NETWK-ADDR-1;
  TA01W02.IM-HOSTTNT-NETWK-ADDR-2 = TA01M002.IM-HOSTTNT-NETWK-ADDR-2;

  TA01W02.IM-NTFYTNT-NETWK-ADDR-1 = TA01M002.IM-NTFYTNT-NETWK-ADDR-1;
  TA01W02.IM-NTFYTNT-NETWK-ADDR-2 = TA01M002.IM-NTFYTNT-NETWK-ADDR-2;

  TA01W02.IM-ERRTNT-NETWK-ADDR-1 = TA01M002.IM-ERRTNT-NETWK-ADDR-1;
  TA01W02.IM-ERRTNT-NETWK-ADDR-2 = TA01M002.IM-ERRTNT-NETWK-ADDR-2;

   /* ------------------------------------------------------------*/
   /* Edit fields based on action type.*/
   /* ------------------------------------------------------------*/

  /* TA01S-EDIT-FOR-ACTION;*/

   /* ------------------------------------------------------------*/
   /* Convert TNTNM's to IM-EMP-DEVICE-ID's.*/
   /* ------------------------------------------------------------*/

  TA01S-TNTNM-TO-ID();

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/

  TA01W00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/


end // end TA01-21000

// Format and converse the map
Function TA01-22000()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/
    TAWREC.XXX-MSG-NBR = converseVar.validationMsgNum; /* Message number*/
  end

   /* ------------------------------------------------------------*/
   /* If the map must be formatted, format the data in the map.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.XXX-MAP-FMAT-FL == "Y") /* Map must be formatted*/
    TA01-22100(); /* Format the data in the map*/
  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/

  TA01M002.XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/

  TA01M002.CATOAP = COMMAREA.CATOAP; /* TO application*/
  TA01M002.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

  if (TAWREC.XXX-MSG-NBR != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ")  /* or message text*/

    if (TAWREC.XXX-MSG-NBR != 0) /* Message to display*/

      if (TAWREC.XXX-MSG-NBR == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(TAWREC.XXX-MSG-NBR);
      end /* Message number*/
    else
      TA01M002.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    TA01M002.XXX-MSG-LOC-ID = TAWREC.XXX-MSG-LOC-ID;
                                   /* Message location*/

    set TA01M002 alarm; /* Indicate sound alarm*/

  end

  converse TA01M002 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/

  TAWREC.XXX-MSG-LOC-ID = " "; /* Message location*/
  TAWREC.XXX-MSG-NBR = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/
  TA01W00.TAW-ERROR-FLAG = "N";

  COMMAREA.CATOAP = TA01M002.CATOAP; /* TO application*/
  COMMAREA.CAITEM = TA01M002.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* Reset modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set TA01M002.CATOAP normal; /* TO application*/
  set TA01M002.CAITEM normal; /* Application data*/

  set TA01M002.XXX-RPT-FIL-ID normal;
  set TA01M002.IM-IMDS-SBJT normal;
  set TA01M002.IM-IMDS-ACT-TYP normal;
  set TA01M002.IM-TRGTTNT-NETWK-ADDR-1 normal;
  set TA01M002.IM-TRGTTNT-NETWK-ADDR-2 normal;
  set TA01M002.IM-HOSTTNT-NETWK-ADDR-1 normal;
  set TA01M002.IM-HOSTTNT-NETWK-ADDR-2 normal;
  set TA01M002.IM-NTFYTNT-NETWK-ADDR-1 normal;
  set TA01M002.IM-NTFYTNT-NETWK-ADDR-2 normal;
  set TA01M002.IM-ERRTNT-NETWK-ADDR-1 normal;
  set TA01M002.IM-ERRTNT-NETWK-ADDR-2 normal;
  set TA01M002.IM-IMDS-FIL-NM-1 normal;
  set TA01M002.IM-IMDS-FIL-NM-2 normal;


end // end TA01-22000

// Format the data in the map
Function TA01-22100()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/

  set TA01M002 initial; /* M002 input/output area*/

  TA01W00.XXX-MAP-FMAT-FL = "N"; /* Format the map flag*/

  TA01S-MOVE-TO-MAP2();

  TA01W00.XXX-RQT-DATA-FL = "Y"; /* Request for data flag*/
  TA01W00.XXX-MAP-DSP-CD = "2"; /* Map to be displayed*/


end // end TA01-22100

// Check attention identifier
Function TA01-23000()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (TA01W00.XXX-RQT-DATA-FL == "Y") /* Request for data pending*/
      set TA01W02 empty; /* M002 display storage*/
    end

    TA01W00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/
    TA01W00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (COMMAREA.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      TA01S-TA0050-P(); /* Purge any temporary storage*/

      XSTAS-EXIT(); /* Exit to another application*/

      set TA01M001.CATOAP cursor, bold; /* Highlight*/

      TA01W00.XXX-MAP-DSP-CD = "1"; /* Map to be displayed*/

    else

      TAWREC.XXX-MSG-LOC-ID = "23000"; /* Message location*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/
      TA01W00.TAW-ERROR-FLAG = "Y";

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If a PF key transfer requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4 /* or PF4 was pressed*/
   || converseVar.eventKey is pf15
   || converseVar.eventKey is pf16)

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "TA01M002"; /* Application find code*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/

      TA01W00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/
      TA01W00.XXX-MAP-DSP-CD = "1"; /* Map to be displayed*/

      exit stack; /* Exit to display map*/

    end

    if (converseVar.eventKey is pf15) /* PF15 was pressed*/
      COMMAREA.CATOAP = "TA0S"; /* Security table maint.*/
    end

    if (converseVar.eventKey is pf16) /* PF15 was pressed*/
      COMMAREA.CATOAP = "TA02"; /* Street talk table maint.*/
    end

    TA01S-TA0050-P(); /* Purge any temporary storage*/

    XSTAS-EXIT(); /* Exit to another application*/

    TA01W00.XXX-MAP-DSP-CD = "1"; /* Map to be displayed*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
  )                                 
    TAWREC.XXX-MSG-LOC-ID = "23000"; /* Message location*/
    converseLib.validationFailed(2); /* PA/PF key pressed is not valid*/
    TA01W00.TAW-ERROR-FLAG = "Y";

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end TA01-23000

// Verify data that was entered
Function TA01-24000()
   /* ------------------------------------------------------------*/
   /* Highlight any application data that was entered.*/
   /* ------------------------------------------------------------*/

  if (COMMAREA.CAITEM > " ") /* Application data was entered*/

    TAWREC.XXX-MSG-LOC-ID = "24000"; /* Message location*/
    converseLib.validationFailed(21); /* Enter only with FAST PATH*/
    set TA01M002.CAITEM cursor, bold; /* Highlight*/

  end

   /* ------------------------------------------------------------*/
   /* Find out if one of the three key fields has been changed.*/
   /* xxx-rpt-fil-id, im-imds-act-typ, and im-imds-trgt-id.*/
   /* rows cannot be updated if their key has been changed,*/
   /* they must be deleted and re-added instead.*/
   /* ------------------------------------------------------------*/

  if (TA01M002.XXX-RPT-FIL-ID != TA01W02.XXX-RPT-FIL-ID
   || TA01M002.IM-IMDS-ACT-TYP != TA01W02.IM-IMDS-ACT-TYP)

    TA01W00.TA01W-KEY-CHNGD-FL = "Y";

  else /* Used in adding or updating.*/

    TA01W00.TA01W-KEY-CHNGD-FL = "N";

  end

   /* ------------------------------------------------------------*/
   /* If adding or deleting or doing nothing :*/
   /* Convert and find street talk names.*/
   /*  */
   /* NOTE: ta01S-TNTNM-TO-ID may call pop-up window.  If it does,*/
   /* do not allow adding or updating on this converse.*/
   /*  */
   /* 'NOTHER NOTE: Don't do any editing if a pop-up is called.*/
   /*  */
   /* If a pop-up window is not called :*/
   /* Enforce rule that files must start with '\\',*/
   /* and that all backslashes must be doubled ( \ is \\ ).*/
   /* ------------------------------------------------------------*/

  TA01W00.TA01W-POPUP-CALLED-FL = "N"; /* No.*/

   /* ------------------------------------------------------------*/
   /* If no pop-up windows were called, edit action and report id,*/
   /* otherwise decide where to place the cursor*/
   /* ------------------------------------------------------------*/

  if (TA01W00.TA01W-POPUP-CALLED-FL == "N")

    TA01-24200(); /* Verify action.*/

    if (converseVar.validationMsgNum == 0
     || converseVar.validationMsgNum == 51) /* Default notification only*/
      TA01S-TNTNM-TO-ID(); /* Convert street talk names.*/

      TA01-24300(); /* Check Report ID.*/
    end

  else

    if (TA01W00.TAW-ERROR-FLAG == "N")

      TA01-24400(); /* Place cursor.*/

    end

  end

   /* ------------------------------------------------------------*/
   /* If any error was found, exit to display the map.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.TAW-ERROR-FLAG == "Y") /* Error was found*/
    exit stack; /* Exit to display map*/
  end


end // end TA01-24000

// Verify action type
Function TA01-24200()
   /* ------------------------------------------------------------*/
   /* Determine if action code is valid.*/
   /* ------------------------------------------------------------*/

  if (TA01M002.IM-IMDS-ACT-TYP == "FILE"
   || TA01M002.IM-IMDS-ACT-TYP == "PRINT"
   || TA01M002.IM-IMDS-ACT-TYP == "TEXT")

    TA01-24210(); /* Check if the entries exist for given*/
                                  /* action*/

  else

    /* ------------------------------------------------------------*/
    /* If not found, indicate missing or invalid.*/
    /* ------------------------------------------------------------*/

    TAWREC.XXX-MSG-LOC-ID = "24200"; /* Message location*/
    converseLib.validationFailed(25); /* Field missing or invalid*/
    set TA01M002.IM-IMDS-ACT-TYP cursor, bold; /* Highlight*/
    TA01W00.TAW-ERROR-FLAG = "Y";

  end


end // end TA01-24200

// Verify fields exist for action
Function TA01-24210()
   /* ---------------------------------------------------------------*/
   /* Check that fields entered match rules for action type*/
   /* ---------------------------------------------------------------*/
   /* First check error address (valid for all)*/
  if (TA01M002.IM-ERRTNT-NETWK-ADDR-1 == " ")
  /* next sentence*/
  else
    TA01S-CHECK-ERR();
  end
   /* NEXT  check action 'FILE'*/
  if (TA01M002.IM-IMDS-ACT-TYP == "FILE")
    if (TA01M002.IM-IMDS-FIL-NM-1 == " ")
      converseLib.validationFailed(47); /* Missing file name*/
      set TA01M002.IM-IMDS-FIL-NM-1 bold, cursor;
    else
      TA01S-CHECK-FNAME();
    end
    if (TA01M002.IM-NTFYTNT-NETWK-ADDR-1 > " ")
      TA01S-CHECK-NTFY();
    end
    if (TA01M002.IM-HOSTTNT-NETWK-ADDR-1 != "FTP://SWOOSHFTP/PUB/PRODFTP/")
      if (converseVar.validationMsgNum == 0)
        converseLib.validationFailed(51); /* default file server used*/
        set TA01M002.IM-HOSTTNT-NETWK-ADDR-1 bold;
      end
      TA01M002.IM-HOSTTNT-NETWK-ADDR-1 = "FTP://SWOOSHFTP/PUB/PRODFTP/";
    end
    if (TA01M002.IM-TRGTTNT-NETWK-ADDR-1 == " ")
      converseLib.validationFailed(50); /* Enter a recipient*/
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-1 bold, cursor;
    else
      TA01S-CHECK-TRGT();
    end
  end
   /* Now check action 'PRINT'*/
  if (TA01M002.IM-IMDS-ACT-TYP == "PRINT")
    if (TA01M002.IM-IMDS-FIL-NM-1 > " ")
      converseLib.validationFailed(15); /* Field must be blank*/
      set TA01M002.IM-IMDS-FIL-NM-1 bold, cursor;
    end
    if (TA01M002.IM-NTFYTNT-NETWK-ADDR-1 == " ")
      converseLib.validationFailed(13); /* Enter a notify address*/
      set TA01M002.IM-NTFYTNT-NETWK-ADDR-1 bold, cursor;
    else
      TA01S-CHECK-NTFY();
    end
    if (TA01M002.IM-HOSTTNT-NETWK-ADDR-1 == " ")
      converseLib.validationFailed(48); /* Enter a printer server name*/
      set TA01M002.IM-HOSTTNT-NETWK-ADDR-1 bold, cursor;
    else
      TA01S-CHECK-HOST();
    end
    if (TA01M002.IM-TRGTTNT-NETWK-ADDR-1 == " ")
      converseLib.validationFailed(49); /* Enter a printer name*/
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-1 bold, cursor;
    else
      TA01S-CHECK-TRGT();
    end
  end
   /* Now check action 'TEXT'*/
  if (TA01M002.IM-IMDS-ACT-TYP == "TEXT")
    if (TA01M002.IM-IMDS-FIL-NM-1 > " ")
      converseLib.validationFailed(15); /* Field must be blank*/
      set TA01M002.IM-IMDS-FIL-NM-1 bold, cursor;
    end
    if (TA01M002.IM-NTFYTNT-NETWK-ADDR-1 > " ")
      converseLib.validationFailed(15); /* FIELD MUST BE BLANK*/
      set TA01M002.IM-NTFYTNT-NETWK-ADDR-1 bold, cursor;
    end
    if (TA01M002.IM-HOSTTNT-NETWK-ADDR-1 > " ")
      converseLib.validationFailed(15); /* FIELD MUST BE BLANK*/
      set TA01M002.IM-HOSTTNT-NETWK-ADDR-1 bold, cursor;
    end
    if (TA01M002.IM-TRGTTNT-NETWK-ADDR-1 == " ")
      converseLib.validationFailed(50); /* Enter a recipient*/
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-1 bold, cursor;
    else
      TA01S-CHECK-TRGT();
    end
  end

  if (converseVar.validationMsgNum > 0)
    exit stack;
  end
end // end TA01-24210

// Check Report ID
Function TA01-24300()
   /* ------------------------------------------------------------*/
   /* Dataset/report id must exist, may not be 'security', or '*'.*/
   /* If the dataset/report id currently exists, the user must*/
   /* have a row in the vsecur01 table with report id to change it.*/
   /* ------------------------------------------------------------*/

  if (TA01M002.XXX-RPT-FIL-ID == "SECURITY"
   || TA01M002.XXX-RPT-FIL-ID <= "*"
   || TA01M002.XXX-RPT-FIL-ID <= " ")

    TAWREC.XXX-MSG-LOC-ID = "24300"; /* Message location*/
    converseLib.validationFailed(11); /* Invalid value entered.*/
    set TA01M002.XXX-RPT-FIL-ID cursor, bold;
    TA01W00.TAW-ERROR-FLAG = "Y";

  else

    if (TA01M002.TA01-ACTION == "A" 
     || TA01M002.TA01-ACTION == "D" /* 3 of 4 valid action codes*/
     || TA01M002.TA01-ACTION == "U") /* 4th below completes action check...*/

      TA01-24310(); /* Check user for authority to change*/
                                   /* report.*/
    else

      if (TA01M002.TA01-ACTION != " ") /* 4th valid action type.*/

        TAWREC.XXX-MSG-LOC-ID = "24300"; /* Message location*/
        converseLib.validationFailed(11); /* Invalid value entered.*/
        set TA01M002.TA01-ACTION cursor, bold;

      end

    end

  end


end // end TA01-24300

// Check user's authority
Function TA01-24310()
   /* ------------------------------------------------------------*/
   /* All user's may add new reports, but they may only alter*/
   /* reports that are linked to their scrty-user-id in the*/
   /* DIMDS.VSECUR01 table.  This record is set-up for the creating*/
   /* user, who can then give other's access to that report using*/
   /* TA0S security table maint. program.*/
   /* ------------------------------------------------------------*/

  TA01W00.TA01W-ADD-SCRTY-FLAG = "N";

  set VSECUR01 empty;

  VSECUR01.XXX-RPT-FIL-ID = TA01M002.XXX-RPT-FIL-ID;

  VSECUR01.XXX-SCRTY-USER-ID = "*";

  TA01S-IO7020-S1(); /* Select report*/

  TA01S-IO7020-N1(); /* Select report*/

  if (SQLCA.VAGen_SQLCODE != 100 /* Report exists...*/
  )                             
    TA01S-IO7020-C1(); /* Close set first*/

    VSECUR01.XXX-SCRTY-USER-ID = COMMAREA.CAUSERID;

    TA01S-IO7020-S(); /* Select row*/

    if (SQLCA.VAGen_SQLCODE != 0)

      converseLib.validationFailed(28); /* Not autherized to change this report.*/
      TAWREC.XXX-MSG-LOC-ID = "24300";
      set TA01M002.XXX-RPT-FIL-ID bold, cursor;
      TA01W00.TAW-ERROR-FLAG = "Y";

      TA01W00.XXX-MAP-FMAT-FL = "Y"; /* reset report from ta01w02.*/

    else

   /* ------------------------------------------------------------*/
   /* Check to see if VDIST key matches for new report determination.*/
   /* ------------------------------------------------------------*/

      set VDIST001 empty;

      VDIST001.XXX-RPT-FIL-ID = TA01M002.XXX-RPT-FIL-ID;
      VDIST001.IM-IMDS-ACT-TYP = TA01M002.IM-IMDS-ACT-TYP;
      VDIST001.IM-TRGT-STALK-ID = TA01W02.IM-TRGT-TNTNM-ID;

      TA01S-IO7010-S();

      if (SQLCA.VAGen_SQLCODE == 0)

        TA01W00.TA01W-ADD-FLAG = "N"; /* Not a new report.*/

      else

        TA01W00.TA01W-ADD-FLAG = "Y"; /* Yes, it is a new report.*/

      end

    end

  else
    TA01S-IO7020-C1(); /* Close set.*/

    TA01W00.TA01W-ADD-FLAG = "Y"; /* Add a new report.*/

    TA01W00.TA01W-ADD-SCRTY-FLAG = "Y"; /* Add a security row.*/

  end


end // end TA01-24310

// Place cursor
Function TA01-24400()
   /* ------------------------------------------------------------*/
   /* If pop-up window was called, place cursor at next address.*/
   /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum == 0)
    if (TA01M002.IM-HOSTTNT-NETWK-ADDR-1 > " ")

      set TA01M002.IM-NTFYTNT-NETWK-ADDR-1 cursor;

    else

      set TA01M002.IM-HOSTTNT-NETWK-ADDR-1 cursor;

    end

    if (TA01M002.IM-NTFYTNT-NETWK-ADDR-1 > " ")

      if (TA01M002.IM-ERRTNT-NETWK-ADDR-1 > " ") /* All fields filled.*/

        set TA01M002.TA01-ACTION cursor;

      else

        set TA01M002.IM-ERRTNT-NETWK-ADDR-1 cursor;

      end

    end

  end


end // end TA01-24400

// Process valid input request
Function TA01-25000()
   /* ------------------------------------------------------------*/
   /* If no modifiable data changed, search for next selection.*/
   /* ------------------------------------------------------------*/

  if (TA01M002.TA01-ACTION != "A" /* While not adding...*/
   && TA01M002.TA01-ACTION != "D" 
   && TA01M002.TA01-ACTION != "U" 
   && TA01M002.XXX-RPT-FIL-ID == TA01W02.XXX-RPT-FIL-ID
   && TA01M002.IM-IMDS-SBJT == TA01W02.IM-IMDS-SBJT
   && TA01M002.IM-IMDS-ACT-TYP == TA01W02.IM-IMDS-ACT-TYP
   && TA01M002.IM-TRGTTNT-NETWK-ADDR-1 == TA01W02.IM-TRGTTNT-NETWK-ADDR-1
   && TA01M002.IM-TRGTTNT-NETWK-ADDR-2 == TA01W02.IM-TRGTTNT-NETWK-ADDR-2
   && TA01M002.IM-HOSTTNT-NETWK-ADDR-1 == TA01W02.IM-HOSTTNT-NETWK-ADDR-1
   && TA01M002.IM-HOSTTNT-NETWK-ADDR-2 == TA01W02.IM-HOSTTNT-NETWK-ADDR-2
   && TA01M002.IM-NTFYTNT-NETWK-ADDR-1 == TA01W02.IM-NTFYTNT-NETWK-ADDR-1
   && TA01M002.IM-NTFYTNT-NETWK-ADDR-2 == TA01W02.IM-NTFYTNT-NETWK-ADDR-2
   && TA01M002.IM-ERRTNT-NETWK-ADDR-1 == TA01W02.IM-ERRTNT-NETWK-ADDR-1
   && TA01M002.IM-ERRTNT-NETWK-ADDR-2 == TA01W02.IM-ERRTNT-NETWK-ADDR-2
   && TA01M002.IM-IMDS-FIL-NM-1 == TA01W02.IM-IMDS-FIL-NM-1
   && TA01M002.IM-IMDS-FIL-NM-2 == TA01W02.IM-IMDS-FIL-NM-2)

    TA01-25100(); /* Search for next selection*/

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If the popup window was called do not add or update row.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.TA01W-POPUP-CALLED-FL == "Y")

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If adding a new row to VDIST, add one to VSECUR also,*/
   /* else update the changed data of an existing VDIST001 row.*/
   /* If deleting a report, delete all it's row's in the VSECUR01 table.*/
   /* ------------------------------------------------------------*/

  TA01W00.TA01W-DELETED-FLAG = "N"; /* Nothing deleted yet...*/

  TA01-25200();

   /* ------------------------------------------------------------*/
   /* Commit all changes and refresh the data to be displayed.*/
   /* ------------------------------------------------------------*/

  VGVar.handleSysLibraryErrors = 0;
  sysLib.commit(); /* Commit all changes*/

  if (TA01W00.TA01W-DELETED-FLAG != "Y") /* If row wasn't deleted...*/

    TA01W00.XXX-RQT-DATA-FL = "Y"; /* Request for data flag*/

  end


end // end TA01-25000

// Search for next selection
Function TA01-25100()
   /* ------------------------------------------------------------*/
   /* Determine if another line was selected for viewing.*/
   /* ------------------------------------------------------------*/

  set TA01M002 initial; /* Clean up map 2.*/

  if ("S" in TA01M001.XXX-LN-SEL-IND from XXX-INX-FLD-NBR) /* Line was selected*/

    TA01W00.XXX-INX-FLD-NBR = sysVar.arrayIndex; /* Map line index*/

    TA01S-MOVE-TO-MAP2(); /* Move selected data to map2.*/

    TA01W01.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] = "V";

    TA01W00.XXX-RQT-DATA-FL = "Y"; /* Request for data flag*/

  else

    TA01W00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/
    TA01W00.XXX-MAP-DSP-CD = "1"; /* Map to be displayed*/

  end


end // end TA01-25100

// Add, change, or delete report.
Function TA01-25200()
   /* ------------------------------------------------------------*/
   /* If adding a new row to VDIST, add one to VSECUR also,*/
   /* else update the changed data of an existing VDIST001 row.*/
   /*  */
   /* Update changes to map1 working storage.*/
   /* ------------------------------------------------------------*/

  set VDIST001 empty; /* Clear out table record.*/

  TA01-25210(); /* Get VDIST data*/

  if (TA01M002.TA01-ACTION == "A"
  )
    if (TA01W00.TA01W-ADD-FLAG == "Y")

      TA01-25230(); /* Add Vdist data.*/

      /* --------------------------------------------*/
      /* THE FOLLOWING IF LOGIC IS FOR NT CONVERSION*/
      /* --------------------------------------------*/
      if (TA01M002.XXX-RPT-FIL-ID != TA01W01.XXX-RPT-FIL-ID[XXX-INX-FLD-NBR]
       || TA01M002.IM-IMDS-ACT-TYP != 
      TA01W01.IM-IMDS-ACT-TYP[XXX-INX-FLD-NBR]
       || TA01W02.IM-TRGT-TNTNM-ID != 
      TA01W01.IM-TRGT-TNTNM-ID[XXX-INX-FLD-NBR])
        TA01S-DELETE-OLD();
      end

      TA01-25250(); /* Update ta01W01 row.*/

    else

      TAWREC.XXX-MSG-LOC-ID = "25200";
      converseLib.validationFailed(43); /* Duplicate row.*/
      TA01W00.TAW-ERROR-FLAG = "Y";

      set TA01M002.XXX-RPT-FIL-ID bold, cursor;
      set TA01M002.IM-IMDS-ACT-TYP bold;
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-1 bold;
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-2 bold;

    end

    return; /* Exit to next process.*/

  end


  if (TA01M002.TA01-ACTION == "U"
  )
    if (TA01W00.TA01W-KEY-CHNGD-FL == "N")

      TA01-25220(); /* Update Vdist data.*/

      TA01-25250(); /* Update ta01W01 row.*/

    else

      TAWREC.XXX-MSG-LOC-ID = "25200";
      converseLib.validationFailed(31);
      TA01W00.TAW-ERROR-FLAG = "Y";

      set TA01M002.XXX-RPT-FIL-ID bold, cursor;
      set TA01M002.IM-IMDS-ACT-TYP bold;
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-1 bold;
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-2 bold;

    end

    return; /* Flow to next process.*/

  end


  if (TA01M002.TA01-ACTION == "D") /* Not deleting*/

    TA01-25240(); /* Delete a Vdist row.*/

    TA01-25250(); /* Update ta01W01 row.*/

  end


end // end TA01-25200

