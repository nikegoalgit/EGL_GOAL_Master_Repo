// Convert TNTNM's to emp-dev-id
Function TA01S-TNTNM-TO-ID()
   /* ------------------------------------------------------------*/
   /* Convert and find out if any addresses were changed.*/
   /* ------------------------------------------------------------*/

  TA01W00.TA01W-SET-PAGE-FL = "N";

   /* ------------------------------------------------------------*/
   /* Check error address*/
   /* ------------------------------------------------------------*/

  if (TA01M002.IM-ERRTNT-NETWK-ADDR-1 == " ")
    TA01W02.IM-ERR-TNTNM-ID = "          ";
  else

    TA01W00.XXX-1PART-TNTNM-NM = TA01M002.IM-ERRTNT-NETWK-ADDR-1;
    TA01W00.XXX-2PART-TNTNM-NM = TA01M002.IM-ERRTNT-NETWK-ADDR-2;

    TA01W00.TA01W-TNTNM-FOUND-FLAG = "N";

    TA01W00.TAW-ADDRESS-NAME = "ERROR"; /* May be used in Popup window*/

    TA01S-CHECK-TNTNM(); /* Check for pop-up, *'s, & convert.*/

    if (TA01W00.TA01W-TNTNM-FOUND-FLAG == "Y") /* Flag set in ta01-24110*/
   /* Save IM-EMP-DEVICE-ID if good,*/
      TA01W02.IM-ERR-TNTNM-ID = TA01W00.IM-EMP-DEVICE-ID;

      TA01M002.IM-ERRTNT-NETWK-ADDR-1 = TA01W00.XXX-1PART-TNTNM-NM;
      TA01M002.IM-ERRTNT-NETWK-ADDR-2 = TA01W00.XXX-2PART-TNTNM-NM;

    else /* Otherwise indicate error.*/
      TAWREC.XXX-MSG-LOC-ID = "TNTNM>ID"; /* Message location*/
      set TA01M002.IM-ERRTNT-NETWK-ADDR-1 cursor, bold;
      set TA01M002.IM-ERRTNT-NETWK-ADDR-2 bold;
    end

  end

   /* ------------------------------------------------------------*/
   /* Check notify*/
   /* ------------------------------------------------------------*/

  if (TA01M002.IM-NTFYTNT-NETWK-ADDR-1 == " ")
    TA01W02.IM-NTFY-TNTNM-ID = "          ";
  else

    TA01W00.XXX-1PART-TNTNM-NM = TA01M002.IM-NTFYTNT-NETWK-ADDR-1;
    TA01W00.XXX-2PART-TNTNM-NM = TA01M002.IM-NTFYTNT-NETWK-ADDR-2;

    TA01W00.TA01W-TNTNM-FOUND-FLAG = "N";

    TA01W00.TAW-ADDRESS-NAME = "NOTIFY";
                                   /* May be used in Popup window*/

    TA01S-CHECK-TNTNM(); /* Check for pop-up, *'s, & convert.*/

    if (TA01W00.TA01W-TNTNM-FOUND-FLAG == "Y") /* Flag set in ta01-24110*/

      TA01M002.IM-NTFYTNT-NETWK-ADDR-1 = TA01W00.XXX-1PART-TNTNM-NM;
      TA01M002.IM-NTFYTNT-NETWK-ADDR-2 = TA01W00.XXX-2PART-TNTNM-NM;

   /* Save IM-EMP-DEVICE-ID if good,*/
      TA01W02.IM-NTFY-TNTNM-ID = TA01W00.IM-EMP-DEVICE-ID;

    else /* Otherwise indicate error.*/
      TAWREC.XXX-MSG-LOC-ID = "TNTNM>ID"; /* Message location*/
      set TA01M002.IM-NTFYTNT-NETWK-ADDR-1 cursor, bold;
      set TA01M002.IM-NTFYTNT-NETWK-ADDR-2 bold;
    end

  end

   /* ------------------------------------------------------------*/
   /* Check Host*/
   /* ------------------------------------------------------------*/

  if (TA01M002.IM-HOSTTNT-NETWK-ADDR-1 == " ")
    TA01W02.IM-HOST-TNTNM-ID = "          ";

  else
    TA01W00.XXX-1PART-TNTNM-NM = TA01M002.IM-HOSTTNT-NETWK-ADDR-1;
    TA01W00.XXX-2PART-TNTNM-NM = TA01M002.IM-HOSTTNT-NETWK-ADDR-2;

    TA01W00.TA01W-TNTNM-FOUND-FLAG = "N";

    TA01W00.TAW-ADDRESS-NAME = "AGENT ";
                                   /* May be used in Popup window*/

    TA01S-CHECK-TNTNM(); /* Check for pop-up, *'s, & convert.*/

    if (TA01W00.TA01W-TNTNM-FOUND-FLAG == "Y") /* Flag set in ta01-24110*/

      TA01M002.IM-HOSTTNT-NETWK-ADDR-1 = TA01W00.XXX-1PART-TNTNM-NM;
      TA01M002.IM-HOSTTNT-NETWK-ADDR-2 = TA01W00.XXX-2PART-TNTNM-NM;

   /* Save IM-EMP-DEVICE-ID if good,*/
      TA01W02.IM-HOST-TNTNM-ID = TA01W00.IM-EMP-DEVICE-ID;

    else /* Otherwise indicate error.*/
      TAWREC.XXX-MSG-LOC-ID = "TNTNM>ID"; /* Message location*/
      set TA01M002.IM-HOSTTNT-NETWK-ADDR-1 cursor, bold;
      set TA01M002.IM-HOSTTNT-NETWK-ADDR-2 bold;
    end

  end

   /* ------------------------------------------------------------*/
   /* Target TNTNM must exist.*/
   /* ------------------------------------------------------------*/

  if (TA01M002.IM-TRGTTNT-NETWK-ADDR-1 == " ")

    TAWREC.XXX-MSG-LOC-ID = "TNTNM>ID"; /* Message location*/
    TA01W00.TAW-ERROR-FLAG = "Y";
    converseLib.validationFailed(24); /* Report must have target address.*/
    set TA01M002.IM-TRGTTNT-NETWK-ADDR-1 cursor, bold;
    set TA01M002.IM-TRGTTNT-NETWK-ADDR-2 bold;

  else
    TA01W00.XXX-1PART-TNTNM-NM = TA01M002.IM-TRGTTNT-NETWK-ADDR-1; /* Move target TNTNM.*/
    TA01W00.XXX-2PART-TNTNM-NM = TA01M002.IM-TRGTTNT-NETWK-ADDR-2;

    TA01W00.TA01W-TNTNM-FOUND-FLAG = "N";

    TA01W00.TAW-ADDRESS-NAME = "TARGET";
                                   /* May be used in Popup window*/

    TA01S-CHECK-TNTNM(); /* Check for pop-up, *'s, & convert.*/

    if (TA01W00.TA01W-TNTNM-FOUND-FLAG == "Y") /* Flag set in ta01-24110*/

   /* Move target TNTNM.*/
      TA01M002.IM-TRGTTNT-NETWK-ADDR-1 = TA01W00.XXX-1PART-TNTNM-NM;
      TA01M002.IM-TRGTTNT-NETWK-ADDR-2 = TA01W00.XXX-2PART-TNTNM-NM;

        /* ------------------------------------------------------------*/
        /* If a new target name is being used, record amy not be updated,*/
        /* but msut be added as a new record.  For this purpose, if a*/
        /* pop-up window is called, do not reflect changes to ta01W01*/
        /* so that key change can be checked on following edit.*/
        /* ------------------------------------------------------------*/

      if (TA01W00.IM-EMP-DEVICE-ID != TA01W02.IM-TRGT-TNTNM-ID)

        TA01W00.TA01W-KEY-CHNGD-FL = "Y";

      end

      if (TA01W00.TA01W-POPUP-CALLED-FL == "N")

   /* Save IM-EMP-DEVICE-ID*/
        TA01W02.IM-TRGT-TNTNM-ID = TA01W00.IM-EMP-DEVICE-ID;

      end

    else /* Otherwise indicate error.*/
      TAWREC.XXX-MSG-LOC-ID = "TNTNM>ID"; /* Message location*/
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-1 cursor, bold;
      set TA01M002.IM-TRGTTNT-NETWK-ADDR-2 bold;
    end

  end

  if (TA01W00.TA01W-SET-PAGE-FL == "Y")

    converseLib.clearScreen();
     /* Clear out the pop-up map.*/

  end


end // end TA01S-TNTNM-TO-ID

// tnt pop-up window
Function XPTA-2000()
   /* ************************************************************/
   /* *  nt network processing is done accordingly:            **/
   /* *    - open cursor DIMDS.VTNTNM01                         **/
   /* *    - fetch each row and save TNTNM and IM-EMP-DEVICE-ID **/
   /* *      into the array (90 occurrences)                    **/
   /* *    - close cursor                                       **/
   /* *    - format and display XPTAM001 map                    **/
   /* *    - check if selection has been made before exiting    **/
   /* ************************************************************/

  TAMREC02.TAM-POPUP-INX-MAX = 10; /* Init window max.*/

  set VTNTNM01 empty; /* Initialize record area*/

  VTNTNM01.XXX-NTWK-ADDR = TAMREC02.TAM-PARTIAL-KEY;

  XSTAS-TATNTNM-S1(); /* Open DIMDS.VTNTNM01 cursor*/
                                  /* using "like" SQL option*/
  TAMREC02.TAM-TNTNM-INDEX = 1;

   /* -----------------------------------------------------------*/
   /* chg while stmt ... added "and" condition .... vcowan 11/29/94*/
   /* note:  this "while" stmt allows for 99 occurances;*/
         /* tamrec allows for 200 occurances*/
   /* -----------------------------------------------------------*/
  while (SQLCA.VAGen_SQLCODE == 0
   && TAMREC02.TAM-TNTNM-INDEX < 99)

    XSTAS-TATNTNM-N1(); /* Fetch next DIMDS.VTNTNM01 row*/

    if (SQLCA.VAGen_SQLCODE == 0)

      TAMREC02.IM-EMP-DEVICE-ID[TAM-TNTNM-INDEX] = VTNTNM01.IM-EMP-DEVICE-ID;
                                   /* Employee or device number*/
      TAMREC02.XXX-TNTNM-NM[TAM-TNTNM-INDEX] = VTNTNM01.XXX-NTWK-ADDR;
                                   /* Street talk name*/
      TAMREC02.TAM-TNTNM-MAX = TAMREC02.TAM-TNTNM-INDEX;

      TAMREC02.TAM-TNTNM-INDEX = TAMREC02.TAM-TNTNM-INDEX + 1;

    end

    if (TAMREC02.TAM-TNTNM-INDEX > 99)

      TA1REC.TA1TYPE = "DB2 ";
      TA1REC.TA1PGMNM = "XPTA-1000";
      TA1REC.TA1LOC = "TNTNM POPUP TABLE EXCEEDED";
      TA1REC.TA1MAP = "TA0MM002";
      TA1REC.TA1DBASE = "DIMDS   ";
      TA1REC.TA1TBLVU = "VTNTNM01";
      TA1REC.TA1TBLKE = VTNTNM01.VTNTNM01-KEY;

      call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes};

    end

  end

  XSTAS-TATNTNM-C1(); /* Close cursor*/

  TAMREC02.TAM-TNTNM-INDEX = 1; /* Initialize index*/

  if (TAMREC02.XXX-TNTNM-NM[TAM-TNTNM-INDEX] > " ") /* Found a TNTNM*/

    set XMTAM002 initial; /* Clear pop-up window*/

    while (TAMREC02.TAM-POPUP-WINDOW-DONE-FL != "Y")

      XPTA-2100(); /* Format display pop-up window*/

    end

  end


end // end XPTA-2000

// Format and display XMTAM002
Function XPTA-2100()
   /* ************************************************************/
   /* Format and display XMTAM002 map                        **/
   /* ************************************************************/

  if (converseVar.eventKey is pa2)

    TAWREC.XXX-MSG-LOC-ID = "XPTA-2100";
    TAMREC02.TAM-POPUP-WINDOW-DONE-FL = "Y"; /* Pop-up window done*/
    return;

  end

  XPTA-2110(); /* Check for selected STALK name*/

  if (converseVar.validationMsgNum == 0) /* If no error*/

    if (converseVar.eventKey is pf8) /* Forward browsing*/

      if (TAMREC02.TAM-TNTNM-SEL-FL == " ")
        if (TAMREC02.TAM-MAP-LAST-INDEX < TAMREC02.TAM-TNTNM-MAX)
                                  /* Continue fward brws*/
          TAMREC02.TAM-TNTNM-INDEX = TAMREC02.TAM-MAP-LAST-INDEX + 1;
        else
          converseLib.validationFailed(04); /* Last page*/
          TAWREC.XXX-MSG-LOC-ID = "XPTA-2100";
        end
      else
        converseLib.validationFailed(39); /* Select is done, no browsing*/
        TAWREC.XXX-MSG-LOC-ID = "XPTA-2100";
      end

    else

      if (converseVar.eventKey is pf7) /* Backward browsing*/

        if (TAMREC02.TAM-TNTNM-SEL-FL == " ")
          if (TAMREC02.TAM-MAP-FIRST-INDEX > TAMREC02.TAM-POPUP-INX-MAX)
                                   /* Continue backward browsing*/
            TAMREC02.TAM-TNTNM-INDEX = TAMREC02.TAM-MAP-FIRST-INDEX - TAMREC02.TAM-POPUP-INX-MAX;
          else
            converseLib.validationFailed(03); /* No scroll back past 1st page*/
            TAWREC.XXX-MSG-LOC-ID = "XPTA-2100";
          end
        else
          converseLib.validationFailed(39); /* Select is done, no browsing*/
          TAWREC.XXX-MSG-LOC-ID = "XPTA-2100";
        end

      else

        if (converseVar.eventKey is enter)

          if (TAMREC02.TAM-TNTNM-SEL-FL == " ")
            if (TAMREC02.TAM-TNTNM-INDEX == 1 /* First time around*/
             && XMTAM002.XXX-NTWK-ADDR[TAM-POPUP-WINDOW-IDX] == " ")
               /* Next sentence*/
            else
              converseLib.validationFailed(40); /* Need to select a line*/
              TAWREC.XXX-MSG-LOC-ID = "XPTA-2100";
            end
          else
            TAMREC02.TAM-POPUP-WINDOW-DONE-FL = "Y";
                                   /* Pop-up window done*/
            return;
          end

        else

          converseLib.validationFailed(02); /* Invalid attention key*/
          TAWREC.XXX-MSG-LOC-ID = "XPTA-2100";

        end

      end

    end

    if (converseVar.validationMsgNum == 0) /* No error.*/

      set XMTAM002 initial; /* Clear pop-up window*/

      XMTAM002.TAW-ADDRESS-NAME = TAMREC02.TAW-ADDRESS-NAME;

      TAMREC02.TAM-POPUP-WINDOW-IDX = 1;

      while (TAMREC02.TAM-TNTNM-INDEX <= TAMREC02.TAM-TNTNM-MAX
       && TAMREC02.TAM-POPUP-WINDOW-IDX <= TAMREC02.TAM-POPUP-INX-MAX)

        XMTAM002.XXX-NTWK-ADDR[TAM-POPUP-WINDOW-IDX] = TAMREC02.XXX-1PART-TNTNM-NM[TAM-TNTNM-INDEX];
        set XMTAM002.TAW-SELECT[TAM-POPUP-WINDOW-IDX] normal;

        if (TAMREC02.TAM-POPUP-WINDOW-IDX == 1)
          TAMREC02.TAM-MAP-FIRST-INDEX = TAMREC02.TAM-TNTNM-INDEX;
        end

        TAMREC02.TAM-MAP-LAST-INDEX = TAMREC02.TAM-TNTNM-INDEX;

        TAMREC02.TAM-TNTNM-INDEX = TAMREC02.TAM-TNTNM-INDEX + 1;
        TAMREC02.TAM-POPUP-WINDOW-IDX = TAMREC02.TAM-POPUP-WINDOW-IDX + 1;

      end

    end

  end

  XMTAM002.XXXERRID = TAWREC.XXX-MSG-LOC-ID;


  converse XMTAM002 ;
end // end XPTA-2100

// Check for selected STALK
Function XPTA-2110()
   /* ************************************************************/
   /* Check for selected street talk name                    **/
   /* ************************************************************/

  TAMREC02.TAM-POPUP-WINDOW-IDX = 1;

  while (TAMREC02.TAM-POPUP-WINDOW-IDX <= TAMREC02.TAM-POPUP-INX-MAX
   && XMTAM002.XXX-NTWK-ADDR[TAM-POPUP-WINDOW-IDX] > " ")

    if (XMTAM002.TAW-SELECT[TAM-POPUP-WINDOW-IDX] > " ")

      if (XMTAM002.TAW-SELECT[TAM-POPUP-WINDOW-IDX] == "S")

        if (TAMREC02.TAM-TNTNM-SEL-FL == "Y")
          TAMREC02.TAM-TNTNM-INDEX = TAMREC02.TAM-MAP-FIRST-INDEX +           TAMREC02.TAM-POPUP-WINDOW-IDX - 1;

          if (TAMREC02.XXX-TNTNM-NM[TAM-TNTNM-INDEX] == 
          TAMREC02.TAM-SELECTED-TNTNM-NM)

            if (converseVar.eventKey is enter)
               /* Next sentence*/
            else
              converseLib.validationFailed(38); /* Cannot select and scroll*/
              set XMTAM002.TAW-SELECT[TAM-POPUP-WINDOW-IDX] cursor, bold;
              TAWREC.XXX-MSG-LOC-ID = "XPTA-1110";
            end
          else
            converseLib.validationFailed(39); /* May only select one line*/
            set XMTAM002.TAW-SELECT[TAM-POPUP-WINDOW-IDX] cursor, bold;
            TAWREC.XXX-MSG-LOC-ID = "XPSR-1110";
          end
        else
          TAMREC02.TAM-TNTNM-SEL-FL = "Y";
          TAMREC02.TAM-TNTNM-INDEX = TAMREC02.TAM-MAP-FIRST-INDEX +           TAMREC02.TAM-POPUP-WINDOW-IDX - 1;
          TAMREC02.TAM-SELECTED-TNTNM-NM = TAMREC02.XXX-TNTNM-NM[TAM-TNTNM-INDEX];
          TAMREC02.TAM-SELECTED-EMP-DEVICE-ID = TAMREC02.IM-EMP-DEVICE-ID[TAM-TNTNM-INDEX];
        end

      else

        converseLib.validationFailed(5); /* Select value must be 's'*/
        set XMTAM002.TAW-SELECT[TAM-POPUP-WINDOW-IDX] cursor, bold;
        TAWREC.XXX-MSG-LOC-ID = "XPSR-1110";

      end

    else

      if (TAMREC02.TAM-TNTNM-SEL-FL == "Y")
        TAMREC02.TAM-TNTNM-INDEX = TAMREC02.TAM-MAP-FIRST-INDEX +         TAMREC02.TAM-POPUP-WINDOW-IDX - 1;
        if (TAMREC02.XXX-TNTNM-NM[TAM-TNTNM-INDEX] == 
        TAMREC02.TAM-SELECTED-TNTNM-NM)
          TAMREC02.TAM-TNTNM-SEL-FL = " ";
        end
      end

    end

    TAMREC02.TAM-POPUP-WINDOW-IDX = TAMREC02.TAM-POPUP-WINDOW-IDX + 1;

  end


end // end XPTA-2110

