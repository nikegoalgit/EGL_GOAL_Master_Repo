// Check for popup call, convert
Function TA01S-CHECK-TNTNM()
   /* ------------------------------------------------------------*/
   /* Move the current TNTNM into an array and search it's*/
   /* contents for a percent sign. if found, call the popup*/
   /* window to convert TNTNM, otherwise treat current TNTNM*/
   /* as a full NT ADDRESS  and call TANTNM02 to convert*/
   /* TNTNM into IM-EMP-DEVICE-ID.  In both cases, if no match is*/
   /* found, declare error.*/
   /* 1)  Do not allow wildcard '*' as a valid TNTNM name.*/
   /* 2) Pop up window needs all trailing blanks to be percent signs.*/
   /* ------------------------------------------------------------*/

  if (TA01W00.TA01W-TNTNM == "*")

    converseLib.validationFailed(30); /* Invalid value entered.*/
    TA01W00.TAW-ERROR-FLAG = "Y";

    return;

  end

   /* ------------------------------------------------------------*/
   /* Search TNTNM for percent signs.*/
   /* ------------------------------------------------------------*/

  TA01W00.TA01W-TNTNM-ARRAY = TA01W00.TA01W-TNTNM;

  TA01W00.TA01W-INDEX = 1;

  TA01W00.TA01W-CALL-POP-UP = "N";

  while (TA01W00.TA01W-INDEX <= 100
  )
    if (TA01W00.TA01W-TNTNM-CHAR[TA01W-INDEX] == "%"
     || TA01W00.TA01W-TNTNM-CHAR[TA01W-INDEX] == "?")

      TA01W-TNTNM-CHAR[TA01W-INDEX] = "%"; /* Change '?' to '%'.*/

      TA01W00.TA01W-CALL-POP-UP = "Y";

    end

    TA01W00.TA01W-INDEX = TA01W00.TA01W-INDEX + 1;

  end

  if (TA01W00.TA01W-CALL-POP-UP == "Y")

    TA01W00.TA01W-POPUP-CALLED-FL = "Y"; /* Yes.*/

    /* ------------------------------------------------------------*/
    /* Replace trailing blanks with percent signs.*/
    /* ------------------------------------------------------------*/

    TA01W00.TA01W-INDEX = 100;

    TA01W00.TA01W-FILL-BLANKS = "Y";

    while (TA01W00.TA01W-INDEX >= 1 /* Run back thru array*/
     && TA01W00.TA01W-FILL-BLANKS == "Y")

      if (TA01W00.TA01W-TNTNM-CHAR[TA01W-INDEX] == " ")
        TA01W00.TA01W-TNTNM-CHAR[TA01W-INDEX] = "%";
      else
        TA01W00.TA01W-FILL-BLANKS = "N";
      end

      TA01W00.TA01W-INDEX = TA01W00.TA01W-INDEX - 1;

    end

    TA01W00.TA01W-SET-PAGE-FL = "Y";

    TA01W00.TA01W-SAVE-MAP2-ERR = converseVar.validationMsgNum;
    converseLib.validationFailed(0); /* Clean ezemno for popup*/

    set TAMREC02 empty; /* Popup window record.*/

    TAMREC02.TAM-PARTIAL-KEY = TA01W00.TA01W-TNTNM-ARRAY;

    TAMREC02.TAW-ADDRESS-NAME = TA01W00.TAW-ADDRESS-NAME;

    XPTA-2000(); /* Call pop-up procs.*/

    if (TAMREC02.TAM-SELECTED-TNTNM-NM <= " ")

      converseLib.validationFailed(52); /* TNTNM not found...*/
      TA01W00.TAW-ERROR-FLAG = "Y";

    else

      TA01W00.IM-EMP-DEVICE-ID = TAMREC02.TAM-SELECTED-EMP-DEVICE-ID;

      TA01W00.TA01W-TNTNM = TAMREC02.TAM-SELECTED-TNTNM-NM;

      if (TA01W00.TA01W-SAVE-MAP2-ERR == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(TA01W00.TA01W-SAVE-MAP2-ERR);
      end /* Restore map2 err msg.*/

      TA01W00.TA01W-TNTNM-FOUND-FLAG = "Y";

    end

  else

    set VTNTNM01 empty;

    VTNTNM01.XXX-NTWK-ADDR = TA01W00.TA01W-TNTNM-ARRAY;

    VTNTNM01.IM-EMP-DEVICE-ID = "*";

    TA01S-TANTNM02-S(); /* VTNTNM01 table call.*/

    if (SQLCA.VAGen_SQLCODE == 100)

      converseLib.validationFailed(52); /* TNTNM not found...*/
      TA01W00.TAW-ERROR-FLAG = "Y";

      TA01W00.IM-EMP-DEVICE-ID = 0;

    else

      TA01W00.IM-EMP-DEVICE-ID = VTNTNM01.IM-EMP-DEVICE-ID;

      TA01W00.TA01W-TNTNM-FOUND-FLAG = "Y";

    end

  end


end // end TA01S-CHECK-TNTNM

