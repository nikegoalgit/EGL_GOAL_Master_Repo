  if (DG0TW01.XPLCD == DG0TM002.XPLCD
   && DG0TW01.XGPCD == DG0TM002.XGPCD
   && DG0TW01.XPLSDESC == DG0TM002.XPLSDESC
   && DG0TW01.XTVCD == DG0TM002.XTVCD
   && DG0TW01.XPLABRV == DG0TM002.XPLABRV
   && DG0TW01.XPLDESC == DG0TM002.XPLDESC
   && DG0TW01.XXXSTAT == DG0TM002.XXXSTAT
   && DG0TW01.DA-PLOG-TXT1 == DG0TM002.DA-PLOG-TXT1
   && DG0TW01.DA-PLOG-TXT2 == DG0TM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
  /* ***************** END DG0TP71 **********************************/
  /* **************/
end // end DG0TP71-COMPARE

