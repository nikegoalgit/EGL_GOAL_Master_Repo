// SET MAP 1 FIELD ATTRIBUTES
Function DG0TP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG0TM001.DAW-PAGE-LIT = "PAGE";
    set DG0TM001.DAW-CURR-PAGE initialAttributes;
    DG0TM001.DAW-OF-LIT = "OF";
    set DG0TM001.DAW-LAST-PAGE initialAttributes;
    DG0TM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG0TM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG0TM001.DAW-PAGE-LIT = " ";
    set DG0TM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG0TM001.DAW-OF-LIT = " ";
    set DG0TM001.DAW-LAST-PAGE skip, invisible;
    DG0TM001.DAW-PF7-LIT = " ";
    DG0TM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG0TM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG0TM001.XPLCD[sysVar.arrayIndex] == " ")
      set DG0TM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG0TM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG0TP25 **************************/
  /* **************/
end // end DG0TP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG0TP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DG0TM002.XPLCD initialAttributes;
    set DG0TM002.XGPCD initialAttributes;
  else
    set DG0TM002.XPLCD skip;
    set DG0TM002.XGPCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG0TM002.XPLSDESC skip;
    set DG0TM002.XTVCD skip;
    set DG0TM002.XPLABRV skip;
    set DG0TM002.XPLDESC skip;
    set DG0TM002.XXXSTAT skip;
  else
    set DG0TM002.XPLSDESC initialAttributes;
    set DG0TM002.XTVCD initialAttributes;
    set DG0TM002.XPLABRV initialAttributes;
    set DG0TM002.XPLDESC initialAttributes;
    set DG0TM002.XXXSTAT initialAttributes;
  end

  set DG0TM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DG0TM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0TM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DG0TM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DG0TM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DG0TM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DG0TM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DG0TM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DG0TP25 **************************/
  /* **************/
end // end DG0TP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DG0TP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DG0TP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DG0TM001.VAGen_EZEMSG = " ";
  DG0TM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DG0TP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DG0TP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DG0TP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG0TP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DG0TP30 *****************************/
  /* **************/
end // end DG0TP30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DG0TP30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DG0TP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DG0TM002.VAGen_EZEMSG = " ";
  DG0TM002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DG0TP25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DG0TP50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DG0TP60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG0TP70-PROC-MAP-2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DG0TP30 *****************************/
  /* **************/
end // end DG0TP30-MAIN-MAP-2

