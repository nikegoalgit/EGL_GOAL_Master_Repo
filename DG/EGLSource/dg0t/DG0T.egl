// BUILD NEW VDATRN01 RECORD
Function DG0TP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG0TM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG0TM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0TW01.DA-CCAT-KEY-TXT = " ";
  DG0TW01.DG0TW-KEY-XPLCD = DG0TM002.XPLCD;
  DG0TW01.DG0TW-KEY-XGPCD = DG0TM002.XGPCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0TW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0TW01.XPLCD = DG0TM002.XPLCD;
  DG0TW01.XGPCD = DG0TM002.XGPCD;
  DG0TW01.XPLSDESC = DG0TM002.XPLSDESC;
  DG0TW01.XTVCD = DG0TM002.XTVCD;
  DG0TW01.XPLABRV = DG0TM002.XPLABRV;
  DG0TW01.XPLDESC = DG0TM002.XPLDESC;
  DG0TW01.XXXSTAT = DG0TM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 56;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0TW01.XPLGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0TP72 **********************************/
  /* **************/
end // end DG0TP72-NEW-TRAN

