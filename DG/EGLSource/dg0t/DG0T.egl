// DATE CREATED: 04/30/97
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   07-08-98  ECHAN       CORRECTED XPLCD FROM CHAR(3) TO CHAR
//                         (2) IN DG0TM002.
//                         ADDED EDIT FOR INVALID 99 XPLCD IN
//                         DG0TP69-EDIT-KEY.
//   02-24-99  ECHAN       Add NOHILITE in DG0TP25-SET-ATTR-1.
//                         Chg TRAMSACTION to TRANSACTION in
//                         DG0TP70-PROC-MAP-1.
//                         Display status in DG0TM001.
//                         Check duplicate and deadlock in
//                         DG0TP70-PROC-MAP-2.
// *********************
Program DG0T type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0TW01 DG0TW01; // record
  DG0TW02 DG0TW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XPLTBL {deleteAfterUse = yes}; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use DG0TM.DG0TM001,  // forms
      DG0TM.DG0TM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0TP00-DRIVER: DG0TP00-DRIVER();
    exit program;
  end // end main
end // end DG0T

// DRIVER FOR APPLICATION
Function DG0TP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0TP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0TP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0TP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0TP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0TP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0TP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0TP00 *******************************/
  /* **************/
end // end DG0TP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0TP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0TW01 empty; /* Working storage record*/
  set DG0TW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0TM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0TM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0TM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0TM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0TM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0TM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0TM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0TP10 ******************************/
  /* **************/
end // end DG0TP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0TP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0TP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0TM001.DAW-CURR-PAGE)
    DG0TS-TS-READ-PAGE(); /* read temp storage page*/
    DG0TP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0TP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0TP20 **********************************/
  /* **************/
end // end DG0TP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0TP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0TM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0TM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0TW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0TM002.DA-PLOG-TXT1 = " ";
    DG0TM002.DA-PLOG-TXT2 = " ";
    DG0TM002.XPLCD = " ";
    DG0TM002.XGPCD = " ";
    DG0TM002.XPLSDESC = " ";
    DG0TM002.XTVCD = " ";
    DG0TM002.XPLABRV = " ";
    DG0TM002.XPLDESC = " ";
    DG0TM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0TW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0TW01.XPLGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0TW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0TW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0TM002.XPLCD = DG0TW01.XPLCD; /* move to the map*/
      DG0TM002.XGPCD = DG0TW01.XGPCD; /* move to the map*/
      DG0TM002.XPLSDESC = DG0TW01.XPLSDESC;
      DG0TM002.XTVCD = DG0TW01.XTVCD;
      DG0TM002.XPLABRV = DG0TW01.XPLABRV;
      DG0TM002.XPLDESC = DG0TW01.XPLDESC;
      DG0TM002.XXXSTAT = DG0TW01.XXXSTAT;
      DG0TM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0TM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0TM002.XPLCD = DG0TW02.XPLCD[DAW-ROW-NBR];
        DG0TM002.XGPCD = DG0TW02.XGPCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG0TM002.XPLCD in XPLTBL.XPLCD)
          if (XPLTBL.XGPCD[sysVar.arrayIndex] == DG0TM002.XGPCD)
            DG0TW01.DG0TW-FND-FL = "Y";
          else
            DG0TW01.DG0TW-FND-FL = "N";

            while (DG0TW01.DG0TW-FND-FL != "Y"
             && XPLTBL.XPLCD[sysVar.arrayIndex] != "99"
            )
              sysVar.arrayIndex = sysVar.arrayIndex + 1;
              if (XPLTBL.XPLCD[sysVar.arrayIndex] == DG0TM002.XPLCD
               && XPLTBL.XGPCD[sysVar.arrayIndex] == DG0TM002.XGPCD)
                DG0TW01.DG0TW-FND-FL = "Y";
              end
            end
          end
          if (DG0TW01.DG0TW-FND-FL == "Y")
            DG0TW01.XPLCD = XPLTBL.XPLCD[sysVar.arrayIndex];
            DG0TW01.XGPCD = XPLTBL.XGPCD[sysVar.arrayIndex]; /* save for*/
            DG0TW01.XPLSDESC = XPLTBL.XPLSDESC[sysVar.arrayIndex]; /* later*/
                                                               /* compare*/
            DG0TW01.XTVCD = XPLTBL.XTVCD[sysVar.arrayIndex];
            DG0TW01.XPLABRV = XPLTBL.XPLABRV[sysVar.arrayIndex];
            DG0TW01.XPLDESC = XPLTBL.XPLDESC[sysVar.arrayIndex];
            DG0TW01.XXXSTAT = XPLTBL.XXXSTAT[sysVar.arrayIndex];
            DG0TW01.ZZZSDT = XPLTBL.ZZZSDT[sysVar.arrayIndex];
            DG0TW01.ZZZCHGDT = XPLTBL.ZZZCHGDT[sysVar.arrayIndex];
            DG0TW01.ZZZCHGTM = XPLTBL.ZZZCHGTM[sysVar.arrayIndex];
            DG0TW01.ZZZCHGCT = XPLTBL.ZZZCHGCT[sysVar.arrayIndex];

            DG0TM002.XPLSDESC = XPLTBL.XPLSDESC[sysVar.arrayIndex];
            DG0TM002.XTVCD = XPLTBL.XTVCD[sysVar.arrayIndex];
            DG0TM002.XPLABRV = XPLTBL.XPLABRV[sysVar.arrayIndex];
            DG0TM002.XPLDESC = XPLTBL.XPLDESC[sysVar.arrayIndex];
            DG0TM002.XXXSTAT = XPLTBL.XXXSTAT[sysVar.arrayIndex];
          end
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0TW01.XPLGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0TW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0TW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0TM002.XPLCD = DG0TW01.XPLCD; /* move to the map*/
    DG0TM002.XGPCD = DG0TW01.XGPCD; /* move to the map*/
    DG0TM002.XPLSDESC = DG0TW01.XPLSDESC;
    DG0TM002.XTVCD = DG0TW01.XTVCD;
    DG0TM002.XPLABRV = DG0TW01.XPLABRV;
    DG0TM002.XPLDESC = DG0TW01.XPLDESC;
    DG0TM002.XXXSTAT = DG0TW01.XXXSTAT;
    DG0TM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0TM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0TM002.XXXSTAT == "A")
    DG0TM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0TM002.XXXSTAT == "I")
    DG0TM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0TM002.XXXSTAT == " ")
    DG0TM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0TP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0TP20 **********************************/
  /* **************/
end // end DG0TP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0TP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0TW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0TP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0TS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XPLTBL.XPLCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0TP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0TS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0TS-TS-ADD-PAGE();
    end

    DG0TM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0TM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG0TP22 **********************************/
  /* **************/
end // end DG0TP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0TP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0TW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG0TW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG0TW01.XPLGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG0TW02.XPLCD[DAW-ROW-NBR] = DG0TW01.XPLCD;
  DG0TW02.XGPCD[DAW-ROW-NBR] = DG0TW01.XGPCD;
  DG0TW02.XPLABRV[DAW-ROW-NBR] = DG0TW01.XPLABRV;
  DG0TW02.XPLDESC[DAW-ROW-NBR] = DG0TW01.XPLDESC;
  DG0TW02.XXXSTAT[DAW-ROW-NBR] = DG0TW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0TW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0TW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0TW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0TW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0TW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0TW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0TP221 ******************************/
  /* ***************/
end // end DG0TP23-DB2-TO-TSQ

