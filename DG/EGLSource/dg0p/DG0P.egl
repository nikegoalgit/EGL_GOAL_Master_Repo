//*** PROGRAM=DG0P ****
//   NIKE TABLE HANDLER - TABLE MAINTENANCE - ZCFTBL
// 
//   OVERVIEW: This application is used to maintain the NIL
//             COMMISION FEES table.
// 
//   DB2 TABLE: DCORP.VZCF0001
//   CSP TABLE: ZCFTBL
// 
//  DATE CREATED: 04/23/97
//        AUTHOR: Leslie Rainey
// 
//  MAINTENANCE HISTORY:
// 
//      date     by whom     overview of changes made
//    --------  ----------  -----------------------------------
//    05-20-99  ECHAN       ADD NOHILITE IN DG0PP25-SET-ATTR-1
//                          CHG TRAMSACTION TO TRANSACTION IN
//                          DG0PP70-PROC-MAP-1
//                          CHECK DUPLICATE AND DEADLOCK IN
//                          DG0PP70-PROC-MAP-2
//                          DISPLAY GRS-NET-IND, NET-BILL-FL,
//                          ZCHCD, STATUS IN DG0PM001
// *********************
Program DG0P type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0PW01 DG0PW01; // record
  DG0PW02 DG0PW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use ZCFTBL {deleteAfterUse = yes}; // table
  use ZCHTBL {deleteAfterUse = yes}; // table
  use DG0PM.DG0PM001,  // forms
      DG0PM.DG0PM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0PP00-DRIVER: DG0PP00-DRIVER();
    exit program;
  end // end main
end // end DG0P

// DRIVER FOR APPLICATION
Function DG0PP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0PP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0PP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0PP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0PP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0PP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0PP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0PP00 *******************************/
  /* **************/
end // end DG0PP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0PP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0PW01 empty; /* Working storage record*/
  set DG0PW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0PM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0PM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0PM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0PM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0PM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0PM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0PM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0PP10 ******************************/
  /* **************/
end // end DG0PP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0PP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0PP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0PM001.DAW-CURR-PAGE)
    DG0PS-TS-READ-PAGE(); /* read temp storage page*/
    DG0PP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0PP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0PP20 **********************************/
  /* **************/
end // end DG0PP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0PP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0PM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0PM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0PW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0PM002.DA-PLOG-TXT1 = " ";
    DG0PM002.DA-PLOG-TXT2 = " ";
    DG0PM002.ZCF-COMM-FEE-CD = " ";
    DG0PM002.ZCF-COMM-FEE-ABRV = " ";
    DG0PM002.ZCF-COMM-FEE-DESC = " ";
    DG0PM002.ZCF-COMM-FEE-EDT = 0;
    DG0PM002.ZCF-COMM-FEE-PCT = 0;
    DG0PM002.ZCF-GRS-NET-IND = " ";
    DG0PM002.ZCHCD = " ";
    DG0PM002.ZCF-NET-BILL-FL = " ";
    DG0PM002.ZCF-NISSHO-FL = " ";
    DG0PM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0PW01.ZCFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0PW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0PW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0PM002.ZCF-COMM-FEE-CD = DG0PW01.ZCF-COMM-FEE-CD;
                                                         /* move to the map*/
      DG0PM002.ZCF-COMM-FEE-ABRV = DG0PW01.ZCF-COMM-FEE-ABRV;
      DG0PM002.ZCF-COMM-FEE-DESC = DG0PW01.ZCF-COMM-FEE-DESC;
      DG0PM002.ZCF-COMM-FEE-EDT = DG0PW01.ZCF-COMM-FEE-EDT;
      DG0PM002.ZCF-COMM-FEE-PCT = DG0PW01.ZCF-COMM-FEE-PCT;
      DG0PM002.ZCF-GRS-NET-IND = DG0PW01.ZCF-GRS-NET-IND;
      DG0PM002.ZCHCD = DG0PW01.ZCHCD;
      DG0PM002.ZCF-NET-BILL-FL = DG0PW01.ZCF-NET-BILL-FL;
      DG0PM002.ZCF-NISSHO-FL = DG0PW01.ZCF-NISSHO-FL;
      DG0PM002.XXXSTAT = DG0PW01.XXXSTAT;
      DG0PM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0PM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0PM002.ZCF-COMM-FEE-CD = DG0PW02.ZCF-COMM-FEE-CD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG0PM002.ZCF-COMM-FEE-CD in ZCFTBL.ZCF-COMM-FEE-CD)
          DG0PW01.ZCF-COMM-FEE-CD = ZCFTBL.ZCF-COMM-FEE-CD[sysVar.arrayIndex];
                                                              /* save for a*/
          DG0PW01.ZCF-COMM-FEE-ABRV = ZCFTBL.ZCF-COMM-FEE-ABRV[sysVar.arrayIndex];
                                                           /* later compare*/
          DG0PW01.ZCF-COMM-FEE-DESC = ZCFTBL.ZCF-COMM-FEE-DESC[sysVar.arrayIndex];
          DG0PW01.ZCF-COMM-FEE-EDT = ZCFTBL.ZCF-COMM-FEE-EDT[sysVar.arrayIndex];
          DG0PW01.ZCF-COMM-FEE-PCT = ZCFTBL.ZCF-COMM-FEE-PCT[sysVar.arrayIndex];
          DG0PW01.ZCF-GRS-NET-IND = ZCFTBL.ZCF-GRS-NET-IND[sysVar.arrayIndex];
          DG0PW01.ZCHCD = ZCFTBL.ZCHCD[sysVar.arrayIndex];
          DG0PW01.ZCF-NET-BILL-FL = ZCFTBL.ZCF-NET-BILL-FL[sysVar.arrayIndex];
          DG0PW01.ZCF-NISSHO-FL = ZCFTBL.ZCF-NISSHO-FL[sysVar.arrayIndex];
          DG0PW01.XXXSTAT = ZCFTBL.XXXSTAT[sysVar.arrayIndex];
          DG0PW01.ZZZSDT = ZCFTBL.ZZZSDT[sysVar.arrayIndex];
          DG0PW01.ZZZCHGDT = ZCFTBL.ZZZCHGDT[sysVar.arrayIndex];
          DG0PW01.ZZZCHGTM = ZCFTBL.ZZZCHGTM[sysVar.arrayIndex];
          DG0PW01.ZZZCHGCT = ZCFTBL.ZZZCHGCT[sysVar.arrayIndex];

          DG0PM002.ZCF-COMM-FEE-ABRV = ZCFTBL.ZCF-COMM-FEE-ABRV[sysVar.arrayIndex];
                                                             /* move to map*/
          DG0PM002.ZCF-COMM-FEE-DESC = ZCFTBL.ZCF-COMM-FEE-DESC[sysVar.arrayIndex];
          DG0PM002.ZCF-COMM-FEE-EDT = ZCFTBL.ZCF-COMM-FEE-EDT[sysVar.arrayIndex];
          DG0PM002.ZCF-COMM-FEE-PCT = ZCFTBL.ZCF-COMM-FEE-PCT[sysVar.arrayIndex];
          DG0PM002.ZCF-GRS-NET-IND = ZCFTBL.ZCF-GRS-NET-IND[sysVar.arrayIndex];
          DG0PM002.ZCHCD = ZCFTBL.ZCHCD[sysVar.arrayIndex];
          DG0PM002.ZCF-NET-BILL-FL = ZCFTBL.ZCF-NET-BILL-FL[sysVar.arrayIndex];
          DG0PM002.ZCF-NISSHO-FL = ZCFTBL.ZCF-NISSHO-FL[sysVar.arrayIndex];
          DG0PM002.XXXSTAT = ZCFTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0PW01.ZCFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0PW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0PW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0PM002.ZCF-COMM-FEE-CD = DG0PW01.ZCF-COMM-FEE-CD;
                                                         /* move to the map*/
    DG0PM002.ZCF-COMM-FEE-ABRV = DG0PW01.ZCF-COMM-FEE-ABRV;
    DG0PM002.ZCF-COMM-FEE-DESC = DG0PW01.ZCF-COMM-FEE-DESC;
    DG0PM002.ZCF-COMM-FEE-EDT = DG0PW01.ZCF-COMM-FEE-EDT;
    DG0PM002.ZCF-COMM-FEE-PCT = DG0PW01.ZCF-COMM-FEE-PCT;
    DG0PM002.ZCF-GRS-NET-IND = DG0PW01.ZCF-GRS-NET-IND;
    DG0PM002.ZCHCD = DG0PW01.ZCHCD;
    DG0PM002.ZCF-NET-BILL-FL = DG0PW01.ZCF-NET-BILL-FL;
    DG0PM002.ZCF-NISSHO-FL = DG0PW01.ZCF-NISSHO-FL;
    DG0PM002.XXXSTAT = DG0PW01.XXXSTAT;
    DG0PM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0PM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0PM002.XXXSTAT == "A")
    DG0PM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0PM002.XXXSTAT == "I")
    DG0PM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0PM002.XXXSTAT == " ")
    DG0PM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0PP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0PP20 **********************************/
  /* **************/
end // end DG0PP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0PP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0PW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0PP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0PS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (ZCFTBL.ZCF-COMM-FEE-CD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0PP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0PS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0PS-TS-ADD-PAGE();
    end

    DG0PM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0PM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG0PP22 **********************************/
  /* **************/
end // end DG0PP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0PP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0PW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG0PW01.ZCFGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG0PW02.ZCF-COMM-FEE-CD[DAW-ROW-NBR] = DG0PW01.ZCF-COMM-FEE-CD;
  DG0PW02.ZCF-COMM-FEE-ABRV[DAW-ROW-NBR] = DG0PW01.ZCF-COMM-FEE-ABRV;
  DG0PW02.ZCF-COMM-FEE-DESC[DAW-ROW-NBR] = DG0PW01.ZCF-COMM-FEE-DESC;
  DG0PW02.ZCF-GRS-NET-IND[DAW-ROW-NBR] = DG0PW01.ZCF-GRS-NET-IND;
  DG0PW02.ZCF-NET-BILL-FL[DAW-ROW-NBR] = DG0PW01.ZCF-NET-BILL-FL;
  DG0PW02.ZCHCD[DAW-ROW-NBR] = DG0PW01.ZCHCD;
  DG0PW02.XXXSTAT[DAW-ROW-NBR] = DG0PW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0PW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0PW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0PW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0PW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0PW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0PP221 ******************************/
  /* ***************/
end // end DG0PP23-DB2-TO-TSQ

