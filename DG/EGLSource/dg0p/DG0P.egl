// BUILD NEW VDATRN01 RECORD
Function DG0PP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG0PM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG0PM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0PW01.DA-CCAT-KEY-TXT = " ";
  DG0PW01.DG0PW-KEY-ZCFCD = DG0PM002.ZCF-COMM-FEE-CD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0PW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0PW01.ZCF-COMM-FEE-CD = DG0PM002.ZCF-COMM-FEE-CD;
  DG0PW01.ZCF-COMM-FEE-ABRV = DG0PM002.ZCF-COMM-FEE-ABRV;
  DG0PW01.ZCF-COMM-FEE-DESC = DG0PM002.ZCF-COMM-FEE-DESC;
  DG0PW01.ZCF-COMM-FEE-EDT = DG0PM002.ZCF-COMM-FEE-EDT;
  DG0PW01.ZCF-COMM-FEE-PCT = DG0PM002.ZCF-COMM-FEE-PCT;
  DG0PW01.ZCF-GRS-NET-IND = DG0PM002.ZCF-GRS-NET-IND;
  DG0PW01.ZCHCD = DG0PM002.ZCHCD;
  DG0PW01.ZCF-NET-BILL-FL = DG0PM002.ZCF-NET-BILL-FL;
  DG0PW01.ZCF-NISSHO-FL = DG0PM002.ZCF-NISSHO-FL;
  DG0PW01.XXXSTAT = DG0PM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 53;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0PW01.ZCFGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0PP72 **********************************/
  /* **************/
end // end DG0PP72-NEW-TRAN

