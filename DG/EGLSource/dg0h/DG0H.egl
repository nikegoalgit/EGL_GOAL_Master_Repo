//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DG0HP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DG0HP70-PROC-MAP-2
//                         DISPLAY STATUS IN DG0HM001
// *********************
Program DG0H type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0HW01 DG0HW01; // record
  DG0HW02 DG0HW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XROTBL {deleteAfterUse = yes}; // table
  use XWHTBL {deleteAfterUse = yes}; // table
  use DG0HM.DG0HM001,  // forms
      DG0HM.DG0HM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0HP00-DRIVER: DG0HP00-DRIVER();
    exit program;
  end // end main
end // end DG0H

// DRIVER FOR APPLICATION
Function DG0HP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0HP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0HP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0HP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0HP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0HP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0HP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0HP00 *******************************/
  /* **************/
end // end DG0HP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0HP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0HW01 empty; /* Working storage record*/
  set DG0HW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0HM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0HM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0HM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0HM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0HM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0HM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0HM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0HP10 ******************************/
  /* **************/
end // end DG0HP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0HP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0HP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0HM001.DAW-CURR-PAGE)
    DG0HS-TS-READ-FIL(); /* read temp storage page*/
    DG0HP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0HP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0HP20 **********************************/
  /* **************/
end // end DG0HP20-BUILD-MAP1

