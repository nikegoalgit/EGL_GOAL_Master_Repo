   && DG0HW01.XROCRFL == DG0HM002.XROCRFL
   && DG0HW01.XROOPFL == DG0HM002.XROOPFL
   && DG0HW01.XROISFL == DG0HM002.XROISFL
   && DG0HW01.XROACFL == DG0HM002.XROACFL
   && DG0HW01.XROPRFL == DG0HM002.XROPRFL
   && DG0HW01.XROPFFL == DG0HM002.XROPFFL
   && DG0HW01.XROPKFL == DG0HM002.XROPKFL
   && DG0HW01.XROSLFL == DG0HM002.XROSLFL
   && DG0HW01.XROSRFL == DG0HM002.XROSRFL
