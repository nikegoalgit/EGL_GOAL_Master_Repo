// MOVE TEMP STORAGE PAGE TO MAP
Function DG0HP71-TSQ-TO-MAP()
  /* ************************************************************/
  /* Move temp storage page to map*/
  /* ************************************************************/

  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DG0HM001.DAW-SELECT-CD[sysVar.arrayIndex] = DG0HW02.DAW-SELECT-CD[sysVar.arrayIndex];
    DG0HM001.XROCD[sysVar.arrayIndex] = DG0HW02.XROCD[sysVar.arrayIndex];
    DG0HM001.XROABRV[sysVar.arrayIndex] = DG0HW02.XROABRV[sysVar.arrayIndex];
    DG0HM001.XRODESC[sysVar.arrayIndex] = DG0HW02.XRODESC[sysVar.arrayIndex];
    DG0HM001.XXXSTAT[sysVar.arrayIndex] = DG0HW02.XXXSTAT[sysVar.arrayIndex];
    DG0HM001.DAW-STATUS-DESC[sysVar.arrayIndex] = DG0HW02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DG0HM001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DG0HW02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DG0HM001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DG0HW02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

