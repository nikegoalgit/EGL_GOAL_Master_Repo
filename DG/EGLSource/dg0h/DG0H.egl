  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG0HM001.DAW-PAGE-LIT = "PAGE";
    set DG0HM001.DAW-CURR-PAGE initialAttributes;
    DG0HM001.DAW-OF-LIT = "OF";
    set DG0HM001.DAW-LAST-PAGE initialAttributes;
    DG0HM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG0HM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG0HM001.DAW-PAGE-LIT = " ";
    set DG0HM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG0HM001.DAW-OF-LIT = " ";
    set DG0HM001.DAW-LAST-PAGE skip, invisible;
    DG0HM001.DAW-PF7-LIT = " ";
    DG0HM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG0HM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG0HM001.XROCD[sysVar.arrayIndex] == " ")
      set DG0HM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG0HM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG0HP25 **************************/
  /* **************/
end // end DG0HP25-SET-ATTR-1

