   && DG0HW01.XROSMFL == DG0HM002.XROSMFL
   && DG0HW01.XWHCD1 == DG0HM002.XWHCD1
   && DG0HW01.XWHCD2 == DG0HM002.XWHCD2
   && DG0HW01.XWHCD3 == DG0HM002.XWHCD3
