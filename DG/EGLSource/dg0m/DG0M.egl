// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   10-27-97  ECHAN       Changed to eliminate duplicate if
//                         ADD or COPY in DG0MP69-EDIT-KEY
//   05-17-99  ECHAN       ADD NOHILITE IN DG0MP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DG0MP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DG0MP70-PROC-MAP-2
//                         DISPLAY STATUS IN DG0MM001
// *********************
Program DG0M type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0MW01 DG0MW01; // record
  DG0MW02 DG0MW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YMCTBL {deleteAfterUse = yes}; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use YMBTBL {deleteAfterUse = yes}; // table
  use DG0MM.DG0MM001,  // forms
      DG0MM.DG0MM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0MP00-DRIVER: DG0MP00-DRIVER();
    exit program;
  end // end main
end // end DG0M

// DRIVER FOR APPLICATION
Function DG0MP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0MP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0MP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0MP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0MP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0MP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0MP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0MP00 *******************************/
  /* **************/
end // end DG0MP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0MP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0MW01 empty; /* Working storage record*/
  set DG0MW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0MM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0MM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0MM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0MM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0MM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0MM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0MM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0MP10 ******************************/
  /* **************/
end // end DG0MP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0MP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0MP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0MM001.DAW-CURR-PAGE)
    DG0MS-TS-READ-PAGE(); /* read temp storage page*/
    DG0MP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0MP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0MP20 **********************************/
  /* **************/
end // end DG0MP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0MP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0MM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0MM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0MW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DG0MM002.XGPABRV = " ";
  DG0MM002.YMBABRV = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0MM002.DA-PLOG-TXT1 = " ";
    DG0MM002.DA-PLOG-TXT2 = " ";
    DG0MM002.YMCCD = " ";
    DG0MM002.XGPCD = " ";
    DG0MM002.YMCSDESC = " ";
    DG0MM002.YMBCD = " ";
    DG0MM002.YMCABRV = " ";
    DG0MM002.YMCDESC = " ";
    DG0MM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0MW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0MW01.YMCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0MW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0MW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0MM002.YMCCD = DG0MW01.YMCCD; /* move to the map*/
      DG0MM002.XGPCD = DG0MW01.XGPCD; /* move to the map*/
      DG0MM002.YMCSDESC = DG0MW01.YMCSDESC;
      DG0MM002.YMBCD = DG0MW01.YMBCD;
      DG0MM002.YMCABRV = DG0MW01.YMCABRV;
      DG0MM002.YMCDESC = DG0MW01.YMCDESC;
      DG0MM002.XXXSTAT = DG0MW01.XXXSTAT;
      DG0MM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0MM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0MM002.YMCCD = DG0MW02.YMCCD[DAW-ROW-NBR];
        DG0MM002.XGPCD = DG0MW02.XGPCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG0MM002.YMCCD in YMCTBL.YMCCD)
          if (YMCTBL.XGPCD[sysVar.arrayIndex] == DG0MM002.XGPCD)
            DG0MW01.DG0MW-FND-FL = "Y";
          else
            DG0MW01.DG0MW-FND-FL = "N";

            while (DG0MW01.DG0MW-FND-FL != "Y"
             && YMCTBL.YMCCD[sysVar.arrayIndex] != "99"
            )
              sysVar.arrayIndex = sysVar.arrayIndex + 1;
              if (YMCTBL.YMCCD[sysVar.arrayIndex] == DG0MM002.YMCCD
               && YMCTBL.XGPCD[sysVar.arrayIndex] == DG0MM002.XGPCD)
                DG0MW01.DG0MW-FND-FL = "Y";
              end
            end
          end
          if (DG0MW01.DG0MW-FND-FL == "Y")
            DG0MW01.YMCCD = YMCTBL.YMCCD[sysVar.arrayIndex];
            DG0MW01.XGPCD = YMCTBL.XGPCD[sysVar.arrayIndex]; /* save for*/
            DG0MW01.YMCSDESC = YMCTBL.YMCSDESC[sysVar.arrayIndex]; /* later*/
                                                               /* compare*/
            DG0MW01.YMBCD = YMCTBL.YMBCD[sysVar.arrayIndex];
            DG0MW01.YMCABRV = YMCTBL.YMCABRV[sysVar.arrayIndex];
            DG0MW01.YMCDESC = YMCTBL.YMCDESC[sysVar.arrayIndex];
            DG0MW01.XXXSTAT = YMCTBL.XXXSTAT[sysVar.arrayIndex];
            DG0MW01.ZZZSDT = YMCTBL.ZZZSDT[sysVar.arrayIndex];
            DG0MW01.ZZZCHGDT = YMCTBL.ZZZCHGDT[sysVar.arrayIndex];
            DG0MW01.ZZZCHGTM = YMCTBL.ZZZCHGTM[sysVar.arrayIndex];
            DG0MW01.ZZZCHGCT = YMCTBL.ZZZCHGCT[sysVar.arrayIndex];

            DG0MM002.YMCSDESC = YMCTBL.YMCSDESC[sysVar.arrayIndex];
            DG0MM002.YMBCD = YMCTBL.YMBCD[sysVar.arrayIndex];
            DG0MM002.YMCABRV = YMCTBL.YMCABRV[sysVar.arrayIndex];
            DG0MM002.YMCDESC = YMCTBL.YMCDESC[sysVar.arrayIndex];
            DG0MM002.XXXSTAT = YMCTBL.XXXSTAT[sysVar.arrayIndex];
          end
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0MW01.YMCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0MW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0MW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0MM002.YMCCD = DG0MW01.YMCCD; /* move to the map*/
    DG0MM002.XGPCD = DG0MW01.XGPCD; /* move to the map*/
    DG0MM002.YMCSDESC = DG0MW01.YMCSDESC;
    DG0MM002.YMBCD = DG0MW01.YMBCD;
    DG0MM002.YMCABRV = DG0MW01.YMCABRV;
    DG0MM002.YMCDESC = DG0MW01.YMCDESC;
    DG0MM002.XXXSTAT = DG0MW01.XXXSTAT;
    DG0MM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0MM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0MM002.XXXSTAT == "A")
    DG0MM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0MM002.XXXSTAT == "I")
    DG0MM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0MM002.XXXSTAT == " ")
    DG0MM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0MP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0MP20 **********************************/
  /* **************/
end // end DG0MP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0MP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0MW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0MP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0MS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YMCTBL.YMCCD[sysVar.arrayIndex] != "999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0MP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0MS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0MS-TS-ADD-PAGE();
    end

    DG0MM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0MM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG0MP22 **********************************/
  /* **************/
end // end DG0MP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0MP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0MW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG0MW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG0MW01.YMCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG0MW02.YMCCD[DAW-ROW-NBR] = DG0MW01.YMCCD;
  DG0MW02.YMBCD[DAW-ROW-NBR] = DG0MW01.YMBCD;
  DG0MW02.XGPCD[DAW-ROW-NBR] = DG0MW01.XGPCD;
  DG0MW02.YMCABRV[DAW-ROW-NBR] = DG0MW01.YMCABRV;
  DG0MW02.YMCDESC[DAW-ROW-NBR] = DG0MW01.YMCDESC;
  DG0MW02.XXXSTAT[DAW-ROW-NBR] = DG0MW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0MW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0MW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0MW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0MW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0MW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0MW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0MW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0MW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0MW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0MW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0MP221 ******************************/
  /* ***************/
end // end DG0MP23-DB2-TO-TSQ

