// BUILD NEW VDATRN01 RECORD
Function DG0MP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG0MM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG0MM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0MW01.DA-CCAT-KEY-TXT = " ";
  DG0MW01.DG0MW-KEY-YMCCD = DG0MM002.YMCCD;
  DG0MW01.DG0MW-KEY-YMBCD = DG0MM002.YMBCD;
  DG0MW01.DG0MW-KEY-XGPCD = DG0MM002.XGPCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0MW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0MW01.YMCCD = DG0MM002.YMCCD;
  DG0MW01.XGPCD = DG0MM002.XGPCD;
  DG0MW01.YMCSDESC = DG0MM002.YMCSDESC;
  DG0MW01.YMBCD = DG0MM002.YMBCD;
  DG0MW01.YMCABRV = DG0MM002.YMCABRV;
  DG0MW01.YMCDESC = DG0MM002.YMCDESC;
  DG0MW01.XXXSTAT = DG0MM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 59;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0MW01.YMCGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0MP72 **********************************/
  /* **************/
end // end DG0MP72-NEW-TRAN

