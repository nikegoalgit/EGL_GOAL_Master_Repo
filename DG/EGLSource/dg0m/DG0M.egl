   && DG0MW01.XGPCD == DG0MM002.XGPCD
   && DG0MW01.YMCSDESC == DG0MM002.YMCSDESC
   && DG0MW01.YMBCD == DG0MM002.YMBCD
   && DG0MW01.YMCABRV == DG0MM002.YMCABRV
   && DG0MW01.YMCDESC == DG0MM002.YMCDESC
   && DG0MW01.XXXSTAT == DG0MM002.XXXSTAT
   && DG0MW01.DA-PLOG-TXT1 == DG0MM002.DA-PLOG-TXT1
   && DG0MW01.DA-PLOG-TXT2 == DG0MM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
  /* ***************** END DG0MP71 **********************************/
  /* **************/
