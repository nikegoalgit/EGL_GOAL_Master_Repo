// SET MAP 1 FIELD ATTRIBUTES
Function DG01P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG01M001.DAW-PAGE-LIT = "PAGE";
    set DG01M001.DAW-CURR-PAGE initialAttributes;
    DG01M001.DAW-OF-LIT = "OF";
    set DG01M001.DAW-LAST-PAGE initialAttributes;
    DG01M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG01M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG01M001.DAW-PAGE-LIT = " ";
    set DG01M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG01M001.DAW-OF-LIT = " ";
    set DG01M001.DAW-LAST-PAGE skip, invisible;
    DG01M001.DAW-PF7-LIT = " ";
    DG01M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG01M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG01M001.YCEAOVRF[sysVar.arrayIndex] == " ")
      set DG01M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG01M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG01P25 **************************/
  /* **************/
end // end DG01P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG01P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DG01M002.YCECD initialAttributes;
    set DG01M002.YCEDESC initialAttributes;
    set DG01M002.YCEAOVRF initialAttributes;
    set DG01M002.YCEADJRQ initialAttributes;
    set DG01M002.YCEKPHIS initialAttributes;
    set DG01M002.YCERPTDY initialAttributes;
    set DG01M002.YCESOFCF initialAttributes;
    set DG01M002.YCETRFCF initialAttributes;
    set DG01M002.YCEPRFTF initialAttributes;
    set DG01M002.YCEWHSIN initialAttributes;
    set DG01M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG01M002.YCECD skip;
    set DG01M002.YCEDESC skip;
    set DG01M002.YCEAOVRF skip;
    set DG01M002.YCEADJRQ skip;
    set DG01M002.YCEKPHIS skip;
    set DG01M002.YCERPTDY skip;
    set DG01M002.YCESOFCF skip;
    set DG01M002.YCETRFCF skip;
    set DG01M002.YCEPRFTF skip;
    set DG01M002.YCEWHSIN skip;
    set DG01M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DG01M002.YCECD skip;
