// BUILD NEW VDATRN01 RECORD
Function DG01P72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG01M002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG01M002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG01W01.DA-CCAT-KEY-TXT = " ";
  DG01W01.DG01W-KEY-YCECD = DG01M002.YCECD;
  VDATRN01.DA-CCAT-KEY-TXT = DG01W01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG01W01.YCECD = DG01M002.YCECD;
  DG01W01.YCEDESC = DG01M002.YCEDESC;
  DG01W01.YCEAOVRF = DG01M002.YCEAOVRF;
  DG01W01.YCEADJRQ = DG01M002.YCEADJRQ;
  DG01W01.YCEKPHIS = DG01M002.YCEKPHIS;
  DG01W01.YCERPTDY = DG01M002.YCERPTDY;
  DG01W01.YCESOFCF = DG01M002.YCESOFCF;
  DG01W01.YCETRFCF = DG01M002.YCETRFCF;
  DG01W01.YCEPRFTF = DG01M002.YCEPRFTF;
  DG01W01.YCEWHSIN = DG01M002.YCEWHSIN;
  DG01W01.XXXSTAT = DG01M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 74;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG01W01.YCEGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG01P72 **********************************/
  /* **************/
end // end DG01P72-NEW-TRAN

