// NIKE TABLE HANDLER - TABLE MAINTENANCE - CLAIM EXCEPTION
// 
// OVERVIEW: This application is the maintenance application
//           for the claim exception table
// 
// DB2 TBL: DCORP.VYCE0001
// CSP TBL: YCETBL
// 
// DATE CREATED: 12/04/97
//       AUTHOR: Eric Chan
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   05-12-99  ECHAN       ADD NOHILITE IN DG01P25-SET-ATTR-1
//                         CHG TRAMASACTION TO TRANSACTION IN
//                         DG01P70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DG01P70-PROC-MAP-2
//                         DISPLAY STATUS IN DG01M001
// *********************
Program DG01 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG01W01 DG01W01; // record
  DG01W02 DG01W02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YCETBL {deleteAfterUse = yes}; // table
  use DG01M.DG01M001,  // forms
      DG01M.DG01M002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG01P00-DRIVER: DG01P00-DRIVER();
    exit program;
  end // end main
end // end DG01

// DRIVER FOR APPLICATION
Function DG01P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG01P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG01P20-BUILD-MAP2(); /* Build the second map*/
  else
    DG01P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG01P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG01P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG01P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG01P00 *******************************/
  /* **************/
end // end DG01P00-DRIVER

// INITIALIZE THE APPLICATION
Function DG01P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG01W01 empty; /* Working storage record*/
  set DG01W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG01M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG01M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG01M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG01M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG01M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG01M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG01M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG01P10 ******************************/
  /* **************/
end // end DG01P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG01P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG01P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG01M001.DAW-CURR-PAGE)
    DG01S-TS-READ-PAGE(); /* read temp storage page*/
    DG01P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG01P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG01P20 **********************************/
  /* **************/
end // end DG01P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG01P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG01M002.DA-CHNG-USER-ID = sysVar.userID;
  DG01M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG01W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG01M002.DA-PLOG-TXT1 = " ";
    DG01M002.DA-PLOG-TXT2 = " ";
    DG01M002.YCECD = " ";
    DG01M002.YCEDESC = " ";
    DG01M002.YCEAOVRF = " ";
    DG01M002.YCEADJRQ = " ";
    DG01M002.YCEKPHIS = " ";
    DG01M002.YCERPTDY = " ";
    DG01M002.YCESOFCF = " ";
    DG01M002.YCETRFCF = " ";
    DG01M002.YCEPRFTF = " ";
    DG01M002.YCEWHSIN = " ";
    DG01M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG01W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG01W01.YCEGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG01W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG01W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG01M002.YCECD = DG01W01.YCECD; /* move to the map*/
      DG01M002.YCEDESC = DG01W01.YCEDESC;
      DG01M002.YCEAOVRF = DG01W01.YCEAOVRF;
      DG01M002.YCEADJRQ = DG01W01.YCEADJRQ;
      DG01M002.YCEKPHIS = DG01W01.YCEKPHIS;
      DG01M002.YCERPTDY = DG01W01.YCERPTDY;
      DG01M002.YCESOFCF = DG01W01.YCESOFCF;
      DG01M002.YCETRFCF = DG01W01.YCETRFCF;
      DG01M002.YCEPRFTF = DG01W01.YCEPRFTF;
      DG01M002.YCEWHSIN = DG01W01.YCEWHSIN;
      DG01M002.XXXSTAT = DG01W01.XXXSTAT;
      DG01M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG01M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG01M002.YCECD = DG01W02.YCECD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG01M002.YCECD in YCETBL.YCECD)
          DG01W01.YCECD = YCETBL.YCECD[sysVar.arrayIndex]; /* save for a*/
          DG01W01.YCEDESC = YCETBL.YCEDESC[sysVar.arrayIndex]; /* later compare*/
          DG01W01.YCEAOVRF = YCETBL.YCEAOVRF[sysVar.arrayIndex];
          DG01W01.YCEADJRQ = YCETBL.YCEADJRQ[sysVar.arrayIndex];
          DG01W01.YCEKPHIS = YCETBL.YCEKPHIS[sysVar.arrayIndex];
          DG01W01.YCERPTDY = YCETBL.YCERPTDY[sysVar.arrayIndex];
          DG01W01.YCESOFCF = YCETBL.YCESOFCF[sysVar.arrayIndex];
          DG01W01.YCETRFCF = YCETBL.YCETRFCF[sysVar.arrayIndex];
          DG01W01.YCEPRFTF = YCETBL.YCEPRFTF[sysVar.arrayIndex];
          DG01W01.YCEWHSIN = YCETBL.YCEWHSIN[sysVar.arrayIndex];
          DG01W01.XXXSTAT = YCETBL.XXXSTAT[sysVar.arrayIndex];
          DG01W01.ZZZSDT = YCETBL.ZZZSDT[sysVar.arrayIndex];
          DG01W01.ZZZCHGDT = YCETBL.ZZZCHGDT[sysVar.arrayIndex];
          DG01W01.ZZZCHGTM = YCETBL.ZZZCHGTM[sysVar.arrayIndex];
          DG01W01.ZZZCHGCT = YCETBL.ZZZCHGCT[sysVar.arrayIndex];

          DG01M002.YCEDESC = YCETBL.YCEDESC[sysVar.arrayIndex]; /* move to map*/
          DG01M002.YCEDESC = YCETBL.YCEDESC[sysVar.arrayIndex];
          DG01M002.YCEAOVRF = YCETBL.YCEAOVRF[sysVar.arrayIndex];
          DG01M002.YCEADJRQ = YCETBL.YCEADJRQ[sysVar.arrayIndex];
          DG01M002.YCEKPHIS = YCETBL.YCEKPHIS[sysVar.arrayIndex];
          DG01M002.YCERPTDY = YCETBL.YCERPTDY[sysVar.arrayIndex];
          DG01M002.YCESOFCF = YCETBL.YCESOFCF[sysVar.arrayIndex];
          DG01M002.YCETRFCF = YCETBL.YCETRFCF[sysVar.arrayIndex];
          DG01M002.YCEPRFTF = YCETBL.YCEPRFTF[sysVar.arrayIndex];
          DG01M002.YCEWHSIN = YCETBL.YCEWHSIN[sysVar.arrayIndex];
          DG01M002.XXXSTAT = YCETBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG01W01.YCEGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG01W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG01W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG01M002.YCECD = DG01W01.YCECD; /* move to the map*/
    DG01M002.YCEDESC = DG01W01.YCEDESC;
    DG01M002.YCEAOVRF = DG01W01.YCEAOVRF;
    DG01M002.YCEADJRQ = DG01W01.YCEADJRQ;
    DG01M002.YCEKPHIS = DG01W01.YCEKPHIS;
    DG01M002.YCERPTDY = DG01W01.YCERPTDY;
    DG01M002.YCESOFCF = DG01W01.YCESOFCF;
    DG01M002.YCETRFCF = DG01W01.YCETRFCF;
    DG01M002.YCEPRFTF = DG01W01.YCEPRFTF;
    DG01M002.YCEWHSIN = DG01W01.YCEWHSIN;
    DG01M002.XXXSTAT = DG01W01.XXXSTAT;
    DG01M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG01M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG01M002.XXXSTAT == "A")
    DG01M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG01M002.XXXSTAT == "I")
    DG01M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG01M002.XXXSTAT == " ")
    DG01M002.DAW-TBL-STATUS-DESC = " ";
  end

  DG01P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG01P20 **********************************/
  /* **************/
end // end DG01P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG01P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG01W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG01P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG01S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YCETBL.YCECD[sysVar.arrayIndex] != "999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG01P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG01S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG01S-TS-ADD-PAGE();
    end

    DG01M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG01M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG01P22 **********************************/
  /* **************/
end // end DG01P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG01P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG01W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG01W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG01W01.YCEGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG01W02.YCECD[DAW-ROW-NBR] = DG01W01.YCECD;
  DG01W02.YCEAOVRF[DAW-ROW-NBR] = DG01W01.YCEAOVRF;
  DG01W02.YCEADJRQ[DAW-ROW-NBR] = DG01W01.YCEADJRQ;
  DG01W02.YCEKPHIS[DAW-ROW-NBR] = DG01W01.YCEKPHIS;
  DG01W02.YCERPTDY[DAW-ROW-NBR] = DG01W01.YCERPTDY;
  DG01W02.YCESOFCF[DAW-ROW-NBR] = DG01W01.YCESOFCF;
  DG01W02.YCETRFCF[DAW-ROW-NBR] = DG01W01.YCETRFCF;
  DG01W02.YCEPRFTF[DAW-ROW-NBR] = DG01W01.YCEPRFTF;
  DG01W02.YCEWHSIN[DAW-ROW-NBR] = DG01W01.YCEWHSIN;
  DG01W02.XXXSTAT[DAW-ROW-NBR] = DG01W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG01W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG01W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG01W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG01W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG01W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG01W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG01P221 ******************************/
  /* ***************/
end // end DG01P23-DB2-TO-TSQ

