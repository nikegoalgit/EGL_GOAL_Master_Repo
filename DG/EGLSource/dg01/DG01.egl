   && DG01W01.YCESOFCF == DG01M002.YCESOFCF
   && DG01W01.YCETRFCF == DG01M002.YCETRFCF
   && DG01W01.YCEPRFTF == DG01M002.YCEPRFTF
   && DG01W01.YCEWHSIN == DG01M002.YCEWHSIN
   && DG01W01.XXXSTAT == DG01M002.XXXSTAT
   && DG01W01.DA-PLOG-TXT1 == DG01M002.DA-PLOG-TXT1
