// BUILD NEW VDATRN01 RECORD
Function DG0GP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG0GM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG0GM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0GW01.DA-CCAT-KEY-TXT = " ";
  DG0GW01.DG0GW-KEY-XOWCD = DG0GM002.XOWCD;
  DG0GW01.DG0GW-KEY-XWHCD = DG0GM002.XWHCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0GW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0GW01.XOWCD = DG0GM002.XOWCD;
  DG0GW01.XWHCD = DG0GM002.XWHCD;
  DG0GW01.XOWSSEGC = DG0GM002.XOWSSEGC;
  DG0GW01.XOWSWHSE = DG0GM002.XOWSWHSE;
  DG0GW01.XOWALUSR = DG0GM002.XOWALUSR;
  DG0GW01.XOWDESC = DG0GM002.XOWDESC;
  DG0GW01.XOWABRV = DG0GM002.XOWABRV;
  DG0GW01.XXXSTAT = DG0GM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 47;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0GW01.XOWGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0GP72 **********************************/
  /* **************/
end // end DG0GP72-NEW-TRAN

