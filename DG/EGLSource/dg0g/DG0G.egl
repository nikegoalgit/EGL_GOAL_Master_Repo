// SET MAP 1 FIELD ATTRIBUTES
Function DG0GP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG0GM001.DAW-PAGE-LIT = "PAGE";
    set DG0GM001.DAW-CURR-PAGE initialAttributes;
    DG0GM001.DAW-OF-LIT = "OF";
    set DG0GM001.DAW-LAST-PAGE initialAttributes;
    DG0GM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG0GM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG0GM001.DAW-PAGE-LIT = " ";
    set DG0GM001.DAW-CURR-PAGE skip, invisible;
    DG0GM001.DAW-OF-LIT = " ";
    set DG0GM001.DAW-LAST-PAGE skip, invisible;
    DG0GM001.DAW-PF7-LIT = " ";
    DG0GM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG0GM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG0GM001.XOWCD[sysVar.arrayIndex] == " ")
      set DG0GM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG0GM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG0GP25 **************************/
  /* **************/
end // end DG0GP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG0GP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DG0GM002.XOWCD initialAttributes;
    set DG0GM002.XWHCD initialAttributes;
  else
    set DG0GM002.XOWCD skip;
    set DG0GM002.XWHCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG0GM002.XOWSSEGC skip;
    set DG0GM002.XOWSWHSE skip;
    set DG0GM002.XOWALUSR skip;
    set DG0GM002.XOWDESC skip;
    set DG0GM002.XOWABRV skip;
    set DG0GM002.XXXSTAT skip;
  else
    set DG0GM002.XOWSSEGC initialAttributes;
    set DG0GM002.XOWSWHSE initialAttributes;
    set DG0GM002.XOWALUSR initialAttributes;
    set DG0GM002.XOWDESC initialAttributes;
    set DG0GM002.XOWABRV initialAttributes;
    set DG0GM002.XXXSTAT initialAttributes;
  end

  set DG0GM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DG0GM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0GM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DG0GM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DG0GM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DG0GM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DG0GM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DG0GM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DG0GP25 **************************/
  /* **************/
end // end DG0GP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DG0GP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DG0GP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DG0GM001.VAGen_EZEMSG = " ";
  DG0GM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DG0GP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DG0GP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DG0GP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG0GP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
