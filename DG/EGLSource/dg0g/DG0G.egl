  DG0GP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DG0GM002.VAGen_EZEMSG = " ";
  DG0GM002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DG0GP25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DG0GP50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DG0GP60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG0GP70-PROC-MAP-2(); /* Do any processing needed for MAP 2*/
