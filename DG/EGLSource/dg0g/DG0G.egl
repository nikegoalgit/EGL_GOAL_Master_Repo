   && DG0GW01.XOWDESC == DG0GM002.XOWDESC
   && DG0GW01.XOWABRV == DG0GM002.XOWABRV
   && DG0GW01.XXXSTAT == DG0GM002.XXXSTAT
   && DG0GW01.DA-PLOG-TXT1 == DG0GM002.DA-PLOG-TXT1
   && DG0GW01.DA-PLOG-TXT2 == DG0GM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

