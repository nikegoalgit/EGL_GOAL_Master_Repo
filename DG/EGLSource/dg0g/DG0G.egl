//                         DISPLAY STATUS IN DG0GM001
// *********************
Program DG0G type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0GW01 DG0GW01; // record
  DG0GW02 DG0GW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XOWTBL {deleteAfterUse = yes}; // table
  use XWHTBL {deleteAfterUse = yes}; // table
  use DG0GM.DG0GM001,  // forms
      DG0GM.DG0GM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0GP00-DRIVER: DG0GP00-DRIVER();
    exit program;
  end // end main
end // end DG0G

// DRIVER FOR APPLICATION
Function DG0GP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0GP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0GP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0GP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0GP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0GP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0GP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0GP00 *******************************/
  /* **************/
end // end DG0GP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0GP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0GW01 empty; /* Working storage record*/
  set DG0GW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0GM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0GM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0GM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0GM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0GM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0GM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0GM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0GP10 ******************************/
  /* **************/
end // end DG0GP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0GP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0GP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0GM001.DAW-CURR-PAGE)
    DG0GS-TS-READ-PAGE(); /* read temp storage page*/
    DG0GP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0GP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0GP20 **********************************/
  /* **************/
end // end DG0GP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0GP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0GM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0GM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0GW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DG0GM002.XWHABRV = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0GM002.DA-PLOG-TXT1 = " ";
    DG0GM002.DA-PLOG-TXT2 = " ";
    DG0GM002.XOWCD = " ";
    DG0GM002.XWHCD = " ";
    DG0GM002.XOWSSEGC = " ";
    DG0GM002.XOWSWHSE = " ";
    DG0GM002.XOWALUSR = " ";
    DG0GM002.XOWDESC = " ";
    DG0GM002.XOWABRV = " ";
    DG0GM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0GW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0GW01.XOWGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0GW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0GW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0GM002.XOWCD = DG0GW01.XOWCD; /* move to the map*/
      DG0GM002.XWHCD = DG0GW01.XWHCD; /* move to the map*/
      DG0GM002.XOWSSEGC = DG0GW01.XOWSSEGC;
      DG0GM002.XOWSWHSE = DG0GW01.XOWSWHSE;
      DG0GM002.XOWALUSR = DG0GW01.XOWALUSR;
      DG0GM002.XOWDESC = DG0GW01.XOWDESC;
      DG0GM002.XOWABRV = DG0GW01.XOWABRV;
      DG0GM002.XXXSTAT = DG0GW01.XXXSTAT;
      DG0GM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0GM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0GM002.XOWCD = DG0GW02.XOWCD[DAW-ROW-NBR];
        DG0GM002.XWHCD = DG0GW02.XWHCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        DG0GW01.DG0GW-FND-FL = "N";
        sysVar.arrayIndex = 1;
        while (DG0GW01.DG0GW-FND-FL != "Y"
         && XOWTBL.XOWKEY[sysVar.arrayIndex] != "9999")
          if (XOWTBL.XOWCD[sysVar.arrayIndex] == DG0GM002.XOWCD
           && XOWTBL.XWHCD[sysVar.arrayIndex] == DG0GM002.XWHCD)
            DG0GW01.DG0GW-FND-FL = "Y";
          else
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
          end
        end

        if (DG0GW01.DG0GW-FND-FL == "Y")
          DG0GW01.XOWCD = XOWTBL.XOWCD[sysVar.arrayIndex];
          DG0GW01.XWHCD = XOWTBL.XWHCD[sysVar.arrayIndex]; /* save for*/
          DG0GW01.XOWSSEGC = XOWTBL.XOWSSEGC[sysVar.arrayIndex]; /* later*/
                                                            /* compare*/
          DG0GW01.XOWSWHSE = XOWTBL.XOWSWHSE[sysVar.arrayIndex];
          DG0GW01.XOWALUSR = XOWTBL.XOWALUSR[sysVar.arrayIndex];
          DG0GW01.XOWDESC = XOWTBL.XOWDESC[sysVar.arrayIndex];
          DG0GW01.XOWABRV = XOWTBL.XOWABRV[sysVar.arrayIndex];
          DG0GW01.XXXSTAT = XOWTBL.XXXSTAT[sysVar.arrayIndex];
          DG0GW01.ZZZSDT = XOWTBL.ZZZSDT[sysVar.arrayIndex];
          DG0GW01.ZZZCHGDT = XOWTBL.ZZZCHGDT[sysVar.arrayIndex];
          DG0GW01.ZZZCHGTM = XOWTBL.ZZZCHGTM[sysVar.arrayIndex];
          DG0GW01.ZZZCHGCT = XOWTBL.ZZZCHGCT[sysVar.arrayIndex];

          DG0GM002.XOWSSEGC = XOWTBL.XOWSSEGC[sysVar.arrayIndex];
          DG0GM002.XOWSWHSE = XOWTBL.XOWSWHSE[sysVar.arrayIndex];
          DG0GM002.XOWALUSR = XOWTBL.XOWALUSR[sysVar.arrayIndex];
          DG0GM002.XOWDESC = XOWTBL.XOWDESC[sysVar.arrayIndex];
          DG0GM002.XOWABRV = XOWTBL.XOWABRV[sysVar.arrayIndex];
          DG0GM002.XXXSTAT = XOWTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0GW01.XOWGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0GW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0GW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0GM002.XOWCD = DG0GW01.XOWCD; /* move to the map*/
    DG0GM002.XWHCD = DG0GW01.XWHCD; /* move to the map*/
    DG0GM002.XOWSSEGC = DG0GW01.XOWSSEGC;
    DG0GM002.XOWSWHSE = DG0GW01.XOWSWHSE;
    DG0GM002.XOWALUSR = DG0GW01.XOWALUSR;
    DG0GM002.XOWDESC = DG0GW01.XOWDESC;
    DG0GM002.XOWABRV = DG0GW01.XOWABRV;
    DG0GM002.XXXSTAT = DG0GW01.XXXSTAT;
    DG0GM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0GM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0GM002.XXXSTAT == "A")
    DG0GM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0GM002.XXXSTAT == "I")
    DG0GM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0GM002.XXXSTAT == " ")
    DG0GM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0GP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0GP20 **********************************/
  /* **************/
end // end DG0GP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0GP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0GW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0GP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0GS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XOWTBL.XOWKEY[sysVar.arrayIndex] != "9999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0GP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0GS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0GS-TS-ADD-PAGE();
    end

    DG0GM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0GM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG0GP22 **********************************/
  /* **************/
end // end DG0GP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0GP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0GW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG0GW01.XOWGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG0GW02.XOWCD[DAW-ROW-NBR] = DG0GW01.XOWCD;
  DG0GW02.XWHCD[DAW-ROW-NBR] = DG0GW01.XWHCD;
  DG0GW02.XOWABRV[DAW-ROW-NBR] = DG0GW01.XOWABRV;
  DG0GW02.XOWDESC[DAW-ROW-NBR] = DG0GW01.XOWDESC;
  DG0GW02.XXXSTAT[DAW-ROW-NBR] = DG0GW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0GW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0GW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0GW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0GP221 ******************************/
  /* ***************/
end // end DG0GP23-DB2-TO-TSQ

