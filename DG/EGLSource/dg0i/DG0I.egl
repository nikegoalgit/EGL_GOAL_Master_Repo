   && DG0IW01.XPTEOMPY == DG0IM002.XPTEOMPY
   && DG0IW01.XPTEOMMO == DG0IM002.XPTEOMMO
   && DG0IW01.XPTDISDT == DG0IM002.XPTDISDT
   && DG0IW01.XPTDFRDT == DG0IM002.XPTDFRDT
   && DG0IW01.XPTCTRLI == DG0IM002.XPTCTRLI
   && DG0IW01.XPTSHDTI == DG0IM002.XPTSHDTI
   && DG0IW01.XPTLCFL == DG0IM002.XPTLCFL
   && DG0IW01.XPTSPND == DG0IM002.XPTSPND
   && DG0IW01.XPTABRV == DG0IM002.XPTABRV
   && DG0IW01.XPTDESC == DG0IM002.XPTDESC
   && DG0IW01.XXXSTAT == DG0IM002.XXXSTAT
