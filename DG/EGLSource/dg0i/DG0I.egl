// BUILD NEW VDATRN01 RECORD
Function DG0IP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG0IM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG0IM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0IW01.DA-CCAT-KEY-TXT = " ";
  DG0IW01.DG0IW-KEY-XPTCD = DG0IM002.XPTCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0IW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0IW01.XPTCD = DG0IM002.XPTCD;
  DG0IW01.XPTPCT = DG0IM002.XPTPCT;
  DG0IW01.XPTDAYS = DG0IM002.XPTDAYS;
  DG0IW01.XPTDFRD = DG0IM002.XPTDFRD;
  DG0IW01.XPTEOMFL = DG0IM002.XPTEOMFL;
  DG0IW01.XPTEOMDY = DG0IM002.XPTEOMDY;
  DG0IW01.XPTEOMPY = DG0IM002.XPTEOMPY;
  DG0IW01.XPTEOMMO = DG0IM002.XPTEOMMO;
  DG0IW01.XPTDISDT = DG0IM002.XPTDISDT;
  DG0IW01.XPTDFRDT = DG0IM002.XPTDFRDT;
  DG0IW01.XPTCTRLI = DG0IM002.XPTCTRLI;
  DG0IW01.XPTSHDTI = DG0IM002.XPTSHDTI;
  DG0IW01.XPTLCFL = DG0IM002.XPTLCFL;
  DG0IW01.XPTSPND = DG0IM002.XPTSPND;
  DG0IW01.XPTABRV = DG0IM002.XPTABRV;
  DG0IW01.XPTDESC = DG0IM002.XPTDESC;
  DG0IW01.XXXSTAT = DG0IM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 130;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0IW01.XPTGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0IP72 **********************************/
  /* **************/
end // end DG0IP72-NEW-TRAN

