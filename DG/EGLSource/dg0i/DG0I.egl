  if (DG0IW01.XPTCD == DG0IM002.XPTCD
   && DG0IW01.XPTPCT == DG0IM002.XPTPCT
   && DG0IW01.XPTDAYS == DG0IM002.XPTDAYS
   && DG0IW01.XPTDFRD == DG0IM002.XPTDFRD
