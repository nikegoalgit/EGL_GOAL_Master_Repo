// 
// OVERVIEW: This application is the application used to
//           maintain the xpttbl.
// 
// DB2 TABLE: DCORP.XPT0001
// CSP TABLE: XPTTBL
// DATE CREATED: 04/14/97
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   06-17-97    SJOHN6    COMMENT OUT DATE EDITS.
//   02-18-98    ECHAN     CHG EZEMNO TO 340 AND 16 IN DG0IP69-
//                         EDIT-KEY
//   11-18-98    ECHAN     CORRECTED PCT FIELD TO 6 BYTES.
//   02-24-99    ECHAN     Add NOHLITE in DG0IP25-SET-ATTR-1.
//                         Chg TRAMSACTION to TRANSACTION in
//                         DG0IP70-PROC-MAP-1.
//                         Display status in DG0IM001.
//                         Check duplicate and deadlock in
//                         DG0IP70-PROC-MAP-2.
// *********************
Program DG0I type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0IW01 DG0IW01; // record
  DG0IW02 DG0IW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XPTTBL {deleteAfterUse = yes}; // table
  use DG0IM.DG0IM001,  // forms
      DG0IM.DG0IM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0IP00-DRIVER: DG0IP00-DRIVER();
    exit program;
  end // end main
end // end DG0I

// DRIVER FOR APPLICATION
Function DG0IP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0IP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0IP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0IP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0IP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0IP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0IP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0IP00 *******************************/
  /* **************/
end // end DG0IP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0IP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0IW01 empty; /* Working storage record*/
  set DG0IW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0IM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0IM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0IM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0IM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0IM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0IM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0IM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0IP10 ******************************/
  /* **************/
end // end DG0IP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0IP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0IP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0IM001.DAW-CURR-PAGE)
    DG0IS-TS-READ-FIL(); /* read temp storage page*/
    DG0IP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0IP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0IP20 **********************************/
  /* **************/
end // end DG0IP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0IP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0IM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0IM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0IW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0IM002.DA-PLOG-TXT1 = " ";
    DG0IM002.DA-PLOG-TXT2 = " ";
    DG0IM002.XPTCD = " ";
    DG0IM002.XPTPCT = 0;
    DG0IM002.XPTDAYS = 0;
    DG0IM002.XPTDFRD = 0;
    DG0IM002.XPTEOMFL = " ";
    DG0IM002.XPTEOMDY = 0;
    DG0IM002.XPTEOMPY = 0;
    DG0IM002.XPTEOMMO = 0;
    DG0IM002.XPTDISDT = 0;
    DG0IM002.XPTDFRDT = 0;
    DG0IM002.XPTCTRLI = " ";
    DG0IM002.XPTSHDTI = " ";
    DG0IM002.XPTLCFL = " ";
    DG0IM002.XPTSPND = " ";
    DG0IM002.XPTABRV = " ";
    DG0IM002.XPTDESC = " ";
    DG0IM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0IW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0IW01.XPTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0IW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0IW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0IM002.XPTCD = DG0IW01.XPTCD; /* move to the map*/
      DG0IM002.XPTPCT = DG0IW01.XPTPCT;
      DG0IM002.XPTDAYS = DG0IW01.XPTDAYS;
      DG0IM002.XPTDFRD = DG0IW01.XPTDFRD;
      DG0IM002.XPTEOMFL = DG0IW01.XPTEOMFL;
      DG0IM002.XPTEOMDY = DG0IW01.XPTEOMDY;
      DG0IM002.XPTEOMPY = DG0IW01.XPTEOMPY;
      DG0IM002.XPTEOMMO = DG0IW01.XPTEOMMO;
      DG0IM002.XPTDISDT = DG0IW01.XPTDISDT;
      DG0IM002.XPTDFRDT = DG0IW01.XPTDFRDT;
      DG0IM002.XPTCTRLI = DG0IW01.XPTCTRLI;
      DG0IM002.XPTSHDTI = DG0IW01.XPTSHDTI;
      DG0IM002.XPTLCFL = DG0IW01.XPTLCFL;
      DG0IM002.XPTSPND = DG0IW01.XPTSPND;
      DG0IM002.XPTABRV = DG0IW01.XPTABRV;
      DG0IM002.XPTDESC = DG0IW01.XPTDESC;
      DG0IM002.XXXSTAT = DG0IW01.XXXSTAT;
      DG0IM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0IM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0IM002.XPTCD = DG0IW02.XPTCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG0IM002.XPTCD in XPTTBL.XPTCD)
          DG0IW01.XPTCD = XPTTBL.XPTCD[sysVar.arrayIndex]; /* save for a*/
          DG0IW01.XPTPCT = XPTTBL.XPTPCT[sysVar.arrayIndex];
          DG0IW01.XPTDAYS = XPTTBL.XPTDAYS[sysVar.arrayIndex];
          DG0IW01.XPTDFRD = XPTTBL.XPTDFRD[sysVar.arrayIndex];
          DG0IW01.XPTEOMFL = XPTTBL.XPTEOMFL[sysVar.arrayIndex];
          DG0IW01.XPTEOMDY = XPTTBL.XPTEOMDY[sysVar.arrayIndex];
          DG0IW01.XPTEOMPY = XPTTBL.XPTEOMPY[sysVar.arrayIndex];
          DG0IW01.XPTEOMMO = XPTTBL.XPTEOMMO[sysVar.arrayIndex];
          DG0IW01.XPTDISDT = XPTTBL.XPTDISDT[sysVar.arrayIndex];
          DG0IW01.XPTDFRDT = XPTTBL.XPTDFRDT[sysVar.arrayIndex];
          DG0IW01.XPTCTRLI = XPTTBL.XPTCTRLI[sysVar.arrayIndex];
          DG0IW01.XPTSHDTI = XPTTBL.XPTSHDTI[sysVar.arrayIndex];
          DG0IW01.XPTLCFL = XPTTBL.XPTLCFL[sysVar.arrayIndex];
          DG0IW01.XPTSPND = XPTTBL.XPTSPND[sysVar.arrayIndex];
          DG0IW01.XPTABRV = XPTTBL.XPTABRV[sysVar.arrayIndex]; /* later compare*/
          DG0IW01.XPTDESC = XPTTBL.XPTDESC[sysVar.arrayIndex];
          DG0IW01.XXXSTAT = XPTTBL.XXXSTAT[sysVar.arrayIndex];
          DG0IW01.ZZZSDT = XPTTBL.ZZZSDT[sysVar.arrayIndex];
          DG0IW01.ZZZCHGDT = XPTTBL.ZZZCHGDT[sysVar.arrayIndex];
          DG0IW01.ZZZCHGTM = XPTTBL.ZZZCHGTM[sysVar.arrayIndex];
          DG0IW01.ZZZCHGCT = XPTTBL.ZZZCHGCT[sysVar.arrayIndex];

          DG0IM002.XPTPCT = XPTTBL.XPTPCT[sysVar.arrayIndex];
          DG0IM002.XPTDAYS = XPTTBL.XPTDAYS[sysVar.arrayIndex];
          DG0IM002.XPTDFRD = XPTTBL.XPTDFRD[sysVar.arrayIndex];
          DG0IM002.XPTEOMFL = XPTTBL.XPTEOMFL[sysVar.arrayIndex];
          DG0IM002.XPTEOMDY = XPTTBL.XPTEOMDY[sysVar.arrayIndex];
          DG0IM002.XPTEOMPY = XPTTBL.XPTEOMPY[sysVar.arrayIndex];
          DG0IM002.XPTEOMMO = XPTTBL.XPTEOMMO[sysVar.arrayIndex];
          DG0IM002.XPTDISDT = XPTTBL.XPTDISDT[sysVar.arrayIndex];
          DG0IM002.XPTDFRDT = XPTTBL.XPTDFRDT[sysVar.arrayIndex];
          DG0IM002.XPTCTRLI = XPTTBL.XPTCTRLI[sysVar.arrayIndex];
          DG0IM002.XPTSHDTI = XPTTBL.XPTSHDTI[sysVar.arrayIndex];
          DG0IM002.XPTLCFL = XPTTBL.XPTLCFL[sysVar.arrayIndex];
          DG0IM002.XPTSPND = XPTTBL.XPTSPND[sysVar.arrayIndex];
          DG0IM002.XPTABRV = XPTTBL.XPTABRV[sysVar.arrayIndex]; /* move to map*/
          DG0IM002.XPTDESC = XPTTBL.XPTDESC[sysVar.arrayIndex];
          DG0IM002.XXXSTAT = XPTTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0IW01.XPTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0IW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0IW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0IM002.XPTCD = DG0IW01.XPTCD; /* move to the map*/
    DG0IM002.XPTPCT = DG0IW01.XPTPCT;
    DG0IM002.XPTDAYS = DG0IW01.XPTDAYS;
    DG0IM002.XPTDFRD = DG0IW01.XPTDFRD;
    DG0IM002.XPTEOMFL = DG0IW01.XPTEOMFL;
    DG0IM002.XPTEOMDY = DG0IW01.XPTEOMDY;
    DG0IM002.XPTEOMPY = DG0IW01.XPTEOMPY;
