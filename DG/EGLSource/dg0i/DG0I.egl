   && DG0IW01.DA-PLOG-TXT2 == DG0IM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
  /* ***************** END DG0IP71 **********************************/
  /* **************/
end // end DG0IP71-COMPARE

