    DG0IM002.XPTSPND = DG0IW01.XPTSPND;
    DG0IM002.XPTABRV = DG0IW01.XPTABRV;
    DG0IM002.XPTDESC = DG0IW01.XPTDESC;
    DG0IM002.XXXSTAT = DG0IW01.XXXSTAT;
    DG0IM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0IM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0IM002.XXXSTAT == "A")
    DG0IM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0IM002.XXXSTAT == "I")
    DG0IM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0IM002.XXXSTAT == " ")
    DG0IM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0IP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0IP20 **********************************/
  /* **************/
end // end DG0IP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0IP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0IW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0IP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0IS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XPTTBL.XPTCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0IP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0IS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0IS-TS-ADD-PAGE();
    end

    DG0IM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0IM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG0IP22 **********************************/
  /* **************/
end // end DG0IP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0IP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0IW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG0IW01.XPTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG0IW02.XPTCD[DAW-ROW-NBR] = DG0IW01.XPTCD;
  DG0IW02.XPTABRV[DAW-ROW-NBR] = DG0IW01.XPTABRV;
  DG0IW02.XPTDESC[DAW-ROW-NBR] = DG0IW01.XPTDESC;
  DG0IW02.XXXSTAT[DAW-ROW-NBR] = DG0IW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0IW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0IW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0IW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0IP221 ******************************/
  /* ***************/
end // end DG0IP23-DB2-TO-TSQ

