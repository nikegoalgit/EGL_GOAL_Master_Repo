// SET MAP 1 FIELD ATTRIBUTES
Function DG0IP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG0IM001.DAW-PAGE-LIT = "PAGE";
    set DG0IM001.DAW-CURR-PAGE initialAttributes;
    DG0IM001.DAW-OF-LIT = "OF";
    set DG0IM001.DAW-LAST-PAGE initialAttributes;
    DG0IM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG0IM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG0IM001.DAW-PAGE-LIT = " ";
    set DG0IM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG0IM001.DAW-OF-LIT = " ";
    set DG0IM001.DAW-LAST-PAGE skip, invisible;
    DG0IM001.DAW-PF7-LIT = " ";
    DG0IM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG0IM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG0IM001.XPTCD[sysVar.arrayIndex] == " ")
      set DG0IM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG0IM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG0IP25 **************************/
  /* **************/
end // end DG0IP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG0IP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG0IM002.XPTCD initialAttributes;
  else
    set DG0IM002.XPTCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG0IM002.XPTPCT skip;
    set DG0IM002.XPTDAYS skip;
    set DG0IM002.XPTDFRD skip;
