// BUILD NEW VDATRN01 RECORD
Function DG0CP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG0CM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG0CM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0CW01.DA-CCAT-KEY-TXT = " ";
  DG0CW01.DG0CW-KEY-XHDCD = DG0CM002.XHDCD;
  DG0CW01.DG0CW-KEY-XGPCD = DG0CM002.XGPCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0CW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0CW01.XHDCD = DG0CM002.XHDCD;
  DG0CW01.XGPCD = DG0CM002.XGPCD;
  DG0CW01.XHDCHG = DG0CM002.XHDCHG;
  DG0CW01.XHDUCHFL = DG0CM002.XHDUCHFL;
  DG0CW01.GLCD = DG0CM002.GLCD;
  DG0CW01.XHDSHMM = DG0CM002.XHDSHMM;
  DG0CW01.XHDLPIND = DG0CM002.XHDLPIND;
  DG0CW01.XHD-HNDL-CST = DG0CM002.XHD-HNDL-CST;
  DG0CW01.ZFC-CRCY-CD = DG0CM002.ZFC-CRCY-CD;
  DG0CW01.XHDABRV = DG0CM002.XHDABRV;
  DG0CW01.XHDDESC = DG0CM002.XHDDESC;
  DG0CW01.XXXSTAT = DG0CM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 75;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0CW01.XHDGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0CP72 **********************************/
  /* **************/
end // end DG0CP72-NEW-TRAN

