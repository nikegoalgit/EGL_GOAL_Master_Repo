//*** PROGRAM=DG0C ****
// NIKE TABLE HANDLER - TABLE MAINTENANCE - SPECIAL HANDLING
// 
// OVERVIEW: This application is used to maintain the special
//           handling table
// 
// DB2 TABLE: DCORP.VXHD0001
// CSP TABLE: XHDTBL
// 
// DATE CREATED: 04/07/97
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   02/18/98  ECHAN       Add DG0CW-FND-FL TO DG0CW01
//                         Fix to avoid diplicate row inserted
//                         when COPY/ADD in DG0CP69-EDIT-KEY
//                         Fix to locate the row when COPY or
//                         UPDATE in DG0CP20-BUILD-MAP2
//   04/29/99  ECHAN       ADD NOHILITE IN DG0CP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DG0CP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DG0CP70-PROC-MAP-1
//                         DISPLAY STATUS IN DG0CM001
// *********************
Program DG0C type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0CW01 DG0CW01; // record
  DG0CW02 DG0CW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XHDTBL {deleteAfterUse = yes}; // table
  use XGPTBL {deleteAfterUse = yes}; // table
  use ZFCTBL {deleteAfterUse = yes}; // table
  use DG0CM.DG0CM001,  // forms
      DG0CM.DG0CM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0CP00-DRIVER: DG0CP00-DRIVER();
    exit program;
  end // end main
end // end DG0C

// DRIVER FOR APPLICATION
Function DG0CP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0CP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0CP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0CP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0CP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0CP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0CP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0CP00 *******************************/
  /* **************/
end // end DG0CP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0CP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0CW01 empty; /* Working storage record*/
  set DG0CW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0CM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0CM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0CM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0CM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0CM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0CM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0CM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0CP10 ******************************/
  /* **************/
end // end DG0CP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0CP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0CP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0CM001.DAW-CURR-PAGE)
    DG0CS-TS-READ-PAGE(); /* read temp storage page*/
    DG0CP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0CP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0CP20 **********************************/
  /* **************/
end // end DG0CP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0CP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0CM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0CM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0CW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0CM002.DA-PLOG-TXT1 = " ";
    DG0CM002.DA-PLOG-TXT2 = " ";
    DG0CM002.XHDCD = " ";
    DG0CM002.XGPCD = " ";
    DG0CM002.XHDCHG = 0;
    DG0CM002.XHDUCHFL = " ";
    DG0CM002.GLCD = " ";
    DG0CM002.XHDSHMM = 0;
    DG0CM002.XHDLPIND = " ";
    DG0CM002.XHD-HNDL-CST = 0;
    DG0CM002.ZFC-CRCY-CD = " ";
    DG0CM002.XHDABRV = " ";
    DG0CM002.XHDDESC = " ";
    DG0CM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0CW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0CW01.XHDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0CW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0CW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0CM002.XHDCD = DG0CW01.XHDCD; /* move to the map*/
      DG0CM002.XGPCD = DG0CW01.XGPCD; /* move to map*/
      DG0CM002.XHDABRV = DG0CW01.XHDABRV;
      DG0CM002.XHDCHG = DG0CW01.XHDCHG;
      DG0CM002.XHDUCHFL = DG0CW01.XHDUCHFL;
      DG0CM002.GLCD = DG0CW01.GLCD;
      DG0CM002.XHDSHMM = DG0CW01.XHDSHMM;
      DG0CM002.XHDLPIND = DG0CW01.XHDLPIND;
      DG0CM002.XHD-HNDL-CST = DG0CW01.XHD-HNDL-CST;
      DG0CM002.ZFC-CRCY-CD = DG0CW01.ZFC-CRCY-CD;
      DG0CM002.XHDDESC = DG0CW01.XHDDESC;
      DG0CM002.XXXSTAT = DG0CW01.XXXSTAT;
      DG0CM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0CM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0CM002.XHDCD = DG0CW02.XHDCD[DAW-ROW-NBR];
        DG0CM002.XGPCD = DG0CW02.XGPCD[DAW-ROW-NBR];
                                   /* get the tbl row*/

        DG0CW01.DG0CW-FND-FL = "N";
        sysVar.arrayIndex = 1;
        while (DG0CW01.DG0CW-FND-FL != "Y"
         && XHDTBL.XHDCD[sysVar.arrayIndex] != "999")
          if (XHDTBL.XHDCD[sysVar.arrayIndex] == DG0CM002.XHDCD
           && XHDTBL.XGPCD[sysVar.arrayIndex] == DG0CM002.XGPCD)
            DG0CW01.DG0CW-FND-FL = "Y";
          else
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
          end
        end

        if (DG0CW01.DG0CW-FND-FL == "Y")
          DG0CW01.XHDCD = XHDTBL.XHDCD[sysVar.arrayIndex];
          DG0CW01.XGPCD = XHDTBL.XGPCD[sysVar.arrayIndex];

          DG0CW01.XHDABRV = XHDTBL.XHDABRV[sysVar.arrayIndex];
          DG0CW01.XHDCHG = XHDTBL.XHDCHG[sysVar.arrayIndex];
          DG0CW01.XHDUCHFL = XHDTBL.XHDUCHFL[sysVar.arrayIndex];
          DG0CW01.GLCD = XHDTBL.GLCD[sysVar.arrayIndex];
          DG0CW01.XHDSHMM = XHDTBL.XHDSHMM[sysVar.arrayIndex];
          DG0CW01.XHDLPIND = XHDTBL.XHDLPIND[sysVar.arrayIndex];
          DG0CW01.XHD-HNDL-CST = XHDTBL.XHD-HNDL-CST[sysVar.arrayIndex];
          DG0CW01.ZFC-CRCY-CD = XHDTBL.ZFC-CRCY-CD[sysVar.arrayIndex];
          DG0CW01.XHDDESC = XHDTBL.XHDDESC[sysVar.arrayIndex];
          DG0CW01.XXXSTAT = XHDTBL.XXXSTAT[sysVar.arrayIndex];
          DG0CW01.ZZZSDT = XHDTBL.ZZZSDT[sysVar.arrayIndex];
          DG0CW01.ZZZCHGTM = XHDTBL.ZZZCHGTM[sysVar.arrayIndex];
          DG0CW01.ZZZCHGDT = XHDTBL.ZZZCHGDT[sysVar.arrayIndex];
          DG0CW01.ZZZCHGCT = XHDTBL.ZZZCHGCT[sysVar.arrayIndex];

          DG0CM002.XHDABRV = XHDTBL.XHDABRV[sysVar.arrayIndex];
          DG0CM002.XHDCHG = XHDTBL.XHDCHG[sysVar.arrayIndex];
          DG0CM002.XHDUCHFL = XHDTBL.XHDUCHFL[sysVar.arrayIndex];
          DG0CM002.GLCD = XHDTBL.GLCD[sysVar.arrayIndex];
          DG0CM002.XHDSHMM = XHDTBL.XHDSHMM[sysVar.arrayIndex];
          DG0CM002.XHDLPIND = XHDTBL.XHDLPIND[sysVar.arrayIndex];
          DG0CM002.XHD-HNDL-CST = XHDTBL.XHD-HNDL-CST[sysVar.arrayIndex];
          DG0CM002.ZFC-CRCY-CD = XHDTBL.ZFC-CRCY-CD[sysVar.arrayIndex];
          DG0CM002.XHDDESC = XHDTBL.XHDDESC[sysVar.arrayIndex];
          DG0CM002.XXXSTAT = XHDTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0CW01.XHDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0CW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0CW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0CM002.XHDCD = DG0CW01.XHDCD; /* move to the map*/
    DG0CM002.XGPCD = DG0CW01.XGPCD;
                                   /* move to the map*/
    DG0CM002.XHDABRV = DG0CW01.XHDABRV;
    DG0CM002.XHDCHG = DG0CW01.XHDCHG;
    DG0CM002.XHDUCHFL = DG0CW01.XHDUCHFL;
    DG0CM002.GLCD = DG0CW01.GLCD;
    DG0CM002.XHDSHMM = DG0CW01.XHDSHMM;
    DG0CM002.XHDLPIND = DG0CW01.XHDLPIND;
    DG0CM002.XHD-HNDL-CST = DG0CW01.XHD-HNDL-CST;
    DG0CM002.ZFC-CRCY-CD = DG0CW01.ZFC-CRCY-CD;
    DG0CM002.XHDDESC = DG0CW01.XHDDESC;
    DG0CM002.XXXSTAT = DG0CW01.XXXSTAT;
    DG0CM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0CM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0CM002.XXXSTAT == "A")
    DG0CM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0CM002.XXXSTAT == "I")
    DG0CM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0CM002.XXXSTAT == " ")
    DG0CM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0CP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0CP20 **********************************/
  /* **************/
end // end DG0CP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0CP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0CW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0CP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0CS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XHDTBL.XHDCD[sysVar.arrayIndex] != "999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0CP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0CS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0CS-TS-ADD-PAGE();
    end

    DG0CM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0CM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG0CP22 **********************************/
  /* **************/
end // end DG0CP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0CP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0CW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG0CW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG0CW01.XHDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG0CW02.XHDCD[DAW-ROW-NBR] = DG0CW01.XHDCD;
  DG0CW02.XGPCD[DAW-ROW-NBR] = DG0CW01.XGPCD;
  DG0CW02.XHDDESC[DAW-ROW-NBR] = DG0CW01.XHDDESC;
  DG0CW02.XXXSTAT[DAW-ROW-NBR] = DG0CW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0CW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0CW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0CW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0CW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0CW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0CP221 ******************************/
  /* ***************/
end // end DG0CP23-DB2-TO-TSQ

// MOVE TO TEMP STORAGE PAGE
