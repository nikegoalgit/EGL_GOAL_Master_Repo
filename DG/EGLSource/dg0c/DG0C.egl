// SET MAP 1 FIELD ATTRIBUTES
Function DG0CP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG0CM001.DAW-PAGE-LIT = "PAGE";
    set DG0CM001.DAW-CURR-PAGE initialAttributes;
    DG0CM001.DAW-OF-LIT = "OF";
    set DG0CM001.DAW-LAST-PAGE initialAttributes;
    DG0CM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG0CM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG0CM001.DAW-PAGE-LIT = " ";
    set DG0CM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG0CM001.DAW-OF-LIT = " ";
    set DG0CM001.DAW-LAST-PAGE skip, invisible;
    DG0CM001.DAW-PF7-LIT = " ";
    DG0CM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG0CM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DG0CM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DG0CM001.XHDCD[sysVar.arrayIndex] == " "
       && DG0CM001.XGPCD[sysVar.arrayIndex] == " ")
        set DG0CM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
      else
        set DG0CM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG0CP25 **************************/
  /* **************/
end // end DG0CP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG0CP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DG0CM002.XHDCD initialAttributes;
    set DG0CM002.XGPCD initialAttributes;
  else
    set DG0CM002.XHDCD skip;
    set DG0CM002.XGPCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG0CM002.XHDABRV skip;
    set DG0CM002.XHDCHG skip;
    set DG0CM002.XHDUCHFL skip;
    set DG0CM002.GLCD skip;
    set DG0CM002.XHDSHMM skip;
    set DG0CM002.XHDLPIND skip;
    set DG0CM002.XHD-HNDL-CST skip;
    set DG0CM002.ZFC-CRCY-CD skip;
    set DG0CM002.XHDDESC skip;
    set DG0CM002.XXXSTAT skip;
  else
    set DG0CM002.XHDABRV initialAttributes;
    set DG0CM002.XHDCHG initialAttributes;
    set DG0CM002.XHDUCHFL initialAttributes;
    set DG0CM002.GLCD initialAttributes;
    set DG0CM002.XHDSHMM initialAttributes;
    set DG0CM002.XHDLPIND initialAttributes;
    set DG0CM002.XHD-HNDL-CST initialAttributes;
    set DG0CM002.ZFC-CRCY-CD initialAttributes;
    set DG0CM002.XHDDESC initialAttributes;
    set DG0CM002.XXXSTAT initialAttributes;
  end

  DG0CM002.XGPABRV = " ";

  set DG0CM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DG0CM002.DA-PLOG-TXT2 initialAttributes;

