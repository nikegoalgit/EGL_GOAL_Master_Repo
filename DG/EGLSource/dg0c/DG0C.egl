  DG0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = " ";
  DG0CW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = " ";
  DG0CW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = " ";

  /* ***************/
  /* ****************** END DG0CP221 ******************************/
