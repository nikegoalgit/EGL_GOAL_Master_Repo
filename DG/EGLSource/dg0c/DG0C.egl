  DG0CP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DG0CP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DG0CP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG0CP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
