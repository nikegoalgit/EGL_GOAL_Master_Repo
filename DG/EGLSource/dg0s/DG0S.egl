// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   06/04/97  LRAINE      FIX DEFAULT ATTRIBUTE EDIT
//   05/14/99  ECHAN       ADD NOHILITE IN DG0SP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DG0SP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DG0SP70-PROC-MAP-2
//                         DISPLAY YISPFFL, YFTCD, STATUS IN
//                         DG0SM001
// *********************
Program DG0S type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0SW01 DG0SW01; // record
  DG0SW02 DG0SW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YISTBL {deleteAfterUse = yes}; // table
  use YTYTBL {deleteAfterUse = yes}; // table
  use YFTTBL {deleteAfterUse = yes}; // table
  use DG0SM.DG0SM001,  // forms
      DG0SM.DG0SM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0SP00-DRIVER: DG0SP00-DRIVER();
    exit program;
  end // end main
end // end DG0S

// DRIVER FOR APPLICATION
Function DG0SP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0SP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0SP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0SP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0SP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0SP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0SP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0SP00 *******************************/
  /* **************/
end // end DG0SP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0SP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0SW01 empty; /* Working storage record*/
  set DG0SW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0SM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0SM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0SM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0SM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0SM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0SM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0SM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0SP10 ******************************/
  /* **************/
end // end DG0SP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0SP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0SP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0SM001.DAW-CURR-PAGE)
    DG0SS-TS-READ-PAGE(); /* read temp storage page*/
    DG0SP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0SP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0SP20 **********************************/
  /* **************/
end // end DG0SP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0SP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0SM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0SM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0SW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0SM002.DA-PLOG-TXT1 = " ";
    DG0SM002.DA-PLOG-TXT2 = " ";
    DG0SM002.YISCD = 0;
    DG0SM002.YTYCD = " ";
    DG0SM002.YISSDESC = " ";
    DG0SM002.YISGLIND = " ";
    DG0SM002.YISPFFL = " ";
    DG0SM002.YFTCD = " ";
    DG0SM002.XXXGLAC1 = " ";
    DG0SM002.XXXGLAC2 = " ";
    DG0SM002.YIS-DFLT-ATTR-IND = " ";
    DG0SM002.YISABRV = " ";
    DG0SM002.YISDESC = " ";
    DG0SM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0SW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0SW01.YISGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0SW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0SW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0SM002.YTYCD = DG0SW01.YTYCD; /* move to the map*/
      DG0SM002.YISCD = DG0SW01.YISCD; /* move to map*/
      DG0SM002.YISABRV = DG0SW01.YISABRV;
      DG0SM002.YISSDESC = DG0SW01.YISSDESC;
      DG0SM002.YISGLIND = DG0SW01.YISGLIND;
      DG0SM002.YISPFFL = DG0SW01.YISPFFL;
      DG0SM002.YFTCD = DG0SW01.YFTCD;
      DG0SM002.XXXGLAC1 = DG0SW01.XXXGLAC1;
      DG0SM002.XXXGLAC2 = DG0SW01.XXXGLAC2;
      DG0SM002.YIS-DFLT-ATTR-IND = DG0SW01.YIS-DFLT-ATTR-IND;
      DG0SM002.YISDESC = DG0SW01.YISDESC;
      DG0SM002.XXXSTAT = DG0SW01.XXXSTAT;
      DG0SM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0SM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0SM002.YTYCD = DG0SW02.YTYCD[DAW-ROW-NBR];
        DG0SM002.YISCD = DG0SW02.YISCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG0SM002.YTYCD in YISTBL.YTYCD
         && DG0SM002.YISCD in YISTBL.YISCD)
          DG0SW01.YTYCD = YISTBL.YTYCD[sysVar.arrayIndex];
          DG0SW01.YISCD = YISTBL.YISCD[sysVar.arrayIndex];

          DG0SW01.YISABRV = YISTBL.YISABRV[sysVar.arrayIndex];
          DG0SW01.YISSDESC = YISTBL.YISSDESC[sysVar.arrayIndex];
          DG0SW01.YISGLIND = YISTBL.YISGLIND[sysVar.arrayIndex];
          DG0SW01.YISPFFL = YISTBL.YISPFFL[sysVar.arrayIndex];
          DG0SW01.YFTCD = YISTBL.YFTCD[sysVar.arrayIndex];
          DG0SW01.XXXGLAC1 = YISTBL.XXXGLAC1[sysVar.arrayIndex];
          DG0SW01.XXXGLAC2 = YISTBL.XXXGLAC2[sysVar.arrayIndex];
          DG0SW01.YIS-DFLT-ATTR-IND = YISTBL.YIS-DFLT-ATTR-IND[sysVar.arrayIndex];
          DG0SW01.YISDESC = YISTBL.YISDESC[sysVar.arrayIndex];
          DG0SW01.XXXSTAT = YISTBL.XXXSTAT[sysVar.arrayIndex];
          DG0SW01.ZZZSDT = YISTBL.ZZZSDT[sysVar.arrayIndex];
          DG0SW01.ZZZCHGTM = YISTBL.ZZZCHGTM[sysVar.arrayIndex];
          DG0SW01.ZZZCHGDT = YISTBL.ZZZCHGDT[sysVar.arrayIndex];
          DG0SW01.ZZZCHGCT = YISTBL.ZZZCHGCT[sysVar.arrayIndex];

          DG0SM002.YISABRV = YISTBL.YISABRV[sysVar.arrayIndex];
          DG0SM002.YISSDESC = YISTBL.YISSDESC[sysVar.arrayIndex];
          DG0SM002.YISGLIND = YISTBL.YISGLIND[sysVar.arrayIndex];
          DG0SM002.YISPFFL = YISTBL.YISPFFL[sysVar.arrayIndex];
          DG0SM002.YFTCD = YISTBL.YFTCD[sysVar.arrayIndex];
          DG0SM002.XXXGLAC1 = YISTBL.XXXGLAC1[sysVar.arrayIndex];
          DG0SM002.XXXGLAC2 = YISTBL.XXXGLAC2[sysVar.arrayIndex];
          DG0SM002.YIS-DFLT-ATTR-IND = YISTBL.YIS-DFLT-ATTR-IND[sysVar.arrayIndex];
          DG0SM002.YISDESC = YISTBL.YISDESC[sysVar.arrayIndex];
          DG0SM002.XXXSTAT = YISTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0SW01.YISGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0SW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0SW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0SM002.YTYCD = DG0SW01.YTYCD; /* move to the map*/
    DG0SM002.YISCD = DG0SW01.YISCD;
                                   /* move to the map*/
    DG0SM002.YISABRV = DG0SW01.YISABRV;
    DG0SM002.YISSDESC = DG0SW01.YISSDESC;
    DG0SM002.YISGLIND = DG0SW01.YISGLIND;
    DG0SM002.YISPFFL = DG0SW01.YISPFFL;
    DG0SM002.YFTCD = DG0SW01.YFTCD;
    DG0SM002.XXXGLAC1 = DG0SW01.XXXGLAC1;
    DG0SM002.XXXGLAC2 = DG0SW01.XXXGLAC2;
    DG0SM002.YIS-DFLT-ATTR-IND = DG0SW01.YIS-DFLT-ATTR-IND;
    DG0SM002.YISDESC = DG0SW01.YISDESC;
    DG0SM002.XXXSTAT = DG0SW01.XXXSTAT;
    DG0SM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0SM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0SM002.XXXSTAT == "A")
    DG0SM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0SM002.XXXSTAT == "I")
    DG0SM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0SM002.XXXSTAT == " ")
    DG0SM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0SP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0SP20 **********************************/
  /* **************/
end // end DG0SP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0SP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0SW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0SP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0SS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YISTBL.YISCD[sysVar.arrayIndex] != 9999) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0SP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0SS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0SS-TS-ADD-PAGE();
    end

    DG0SM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0SM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG0SP22 **********************************/
  /* **************/
end // end DG0SP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0SP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0SW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG0SW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG0SW01.YISGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG0SW02.YISCD[DAW-ROW-NBR] = DG0SW01.YISCD;
  DG0SW02.YTYCD[DAW-ROW-NBR] = DG0SW01.YTYCD;
  DG0SW02.YISSDESC[DAW-ROW-NBR] = DG0SW01.YISSDESC;
  DG0SW02.YISPFFL[DAW-ROW-NBR] = DG0SW01.YISPFFL;
  DG0SW02.YFTCD[DAW-ROW-NBR] = DG0SW01.YFTCD;
  DG0SW02.XXXSTAT[DAW-ROW-NBR] = DG0SW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0SW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0SW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0SW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0SW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0SW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0SW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0SW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0SW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0SW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0SW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0SP221 ******************************/
  /* ***************/
end // end DG0SP23-DB2-TO-TSQ

