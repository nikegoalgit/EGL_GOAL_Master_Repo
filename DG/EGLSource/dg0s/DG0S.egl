// SET MAP 1 FIELD ATTRIBUTES
Function DG0SP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG0SM001.DAW-PAGE-LIT = "PAGE";
    set DG0SM001.DAW-CURR-PAGE initialAttributes;
    DG0SM001.DAW-OF-LIT = "OF";
    set DG0SM001.DAW-LAST-PAGE initialAttributes;
    DG0SM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG0SM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG0SM001.DAW-PAGE-LIT = " ";
    set DG0SM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG0SM001.DAW-OF-LIT = " ";
    set DG0SM001.DAW-LAST-PAGE skip, invisible;
    DG0SM001.DAW-PF7-LIT = " ";
    DG0SM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG0SM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG0SM001.YTYCD[sysVar.arrayIndex] == " ")
      set DG0SM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG0SM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG0SP25 **************************/
  /* **************/
end // end DG0SP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG0SP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DG0SM002.YTYCD initialAttributes;
    set DG0SM002.YISCD initialAttributes;
  else
    set DG0SM002.YTYCD skip;
    set DG0SM002.YISCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG0SM002.YISABRV skip;
    set DG0SM002.YISSDESC skip;
    set DG0SM002.YISGLIND skip;
    set DG0SM002.YISPFFL skip;
    set DG0SM002.YFTCD skip;
    set DG0SM002.XXXGLAC1 skip;
    set DG0SM002.XXXGLAC2 skip;
    set DG0SM002.YIS-DFLT-ATTR-IND skip;
    set DG0SM002.YISDESC skip;
    set DG0SM002.XXXSTAT skip;
  else
    set DG0SM002.YISABRV initialAttributes;
    set DG0SM002.YISSDESC initialAttributes;
    set DG0SM002.YISGLIND initialAttributes;
    set DG0SM002.YISPFFL initialAttributes;
    set DG0SM002.YFTCD initialAttributes;
    set DG0SM002.XXXGLAC1 initialAttributes;
    set DG0SM002.XXXGLAC2 initialAttributes;
    set DG0SM002.YIS-DFLT-ATTR-IND initialAttributes;
    set DG0SM002.YISDESC initialAttributes;
    set DG0SM002.XXXSTAT initialAttributes;
  end

  DG0SM002.YTYABRV = " ";
  DG0SM002.YFTSDESC = " ";

  set DG0SM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DG0SM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0SM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DG0SM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DG0SM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
