// BUILD NEW VDATRN01 RECORD
Function DG0SP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG0SM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG0SM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0SW01.DA-CCAT-KEY-TXT = " ";
  DG0SW01.DG0SW-KEY-YTYCD = DG0SM002.YTYCD;
  DG0SW01.DG0SW-KEY-YISCD = DG0SM002.YISCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0SW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0SW01.YTYCD = DG0SM002.YTYCD;
  DG0SW01.YISCD = DG0SM002.YISCD;
  DG0SW01.YISSDESC = DG0SM002.YISSDESC;
  DG0SW01.YISGLIND = DG0SM002.YISGLIND;
  DG0SW01.YISPFFL = DG0SM002.YISPFFL;
  DG0SW01.YFTCD = DG0SM002.YFTCD;
  DG0SW01.XXXGLAC1 = DG0SM002.XXXGLAC1;
  DG0SW01.XXXGLAC2 = DG0SM002.XXXGLAC2;
  DG0SW01.YIS-DFLT-ATTR-IND = DG0SM002.YIS-DFLT-ATTR-IND;
  DG0SW01.YISABRV = DG0SM002.YISABRV;
  DG0SW01.YISDESC = DG0SM002.YISDESC;
  DG0SW01.XXXSTAT = DG0SM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 139;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0SW01.YISGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0SP72 **********************************/
  /* **************/
end // end DG0SP72-NEW-TRAN

