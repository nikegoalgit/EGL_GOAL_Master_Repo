// SET MAP 1 FIELD ATTRIBUTES
Function DG02P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG02M001.DAW-PAGE-LIT = "PAGE";
    set DG02M001.DAW-CURR-PAGE initialAttributes;
    DG02M001.DAW-OF-LIT = "OF";
    set DG02M001.DAW-LAST-PAGE initialAttributes;
    DG02M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG02M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG02M001.DAW-PAGE-LIT = " ";
    set DG02M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG02M001.DAW-OF-LIT = " ";
    set DG02M001.DAW-LAST-PAGE skip, invisible;
    DG02M001.DAW-PF7-LIT = " ";
    DG02M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG02M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DG02M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DG02M001.YCPCD[sysVar.arrayIndex] == 0
       && DG02M001.YCPNM[sysVar.arrayIndex] == " ")
        set DG02M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
        set DG02M001.YCPCD[sysVar.arrayIndex] skip, invisible;
      else
        set DG02M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
        set DG02M001.YCPCD[sysVar.arrayIndex] initialAttributes;
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG02P25 **************************/
  /* **************/
end // end DG02P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG02P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DG02M002.YCPCD initialAttributes;
    set DG02M002.YCPNM initialAttributes;
    set DG02M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG02M002.YCPCD skip;
    set DG02M002.YCPNM skip;
    set DG02M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DG02M002.YCPCD skip;
    set DG02M002.YCPNM initialAttributes;
    set DG02M002.XXXSTAT initialAttributes;
  end

  set DG02M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DG02M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG02M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DG02M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DG02M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DG02M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DG02M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DG02M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DG02P25 **************************/
  /* **************/
end // end DG02P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DG02P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DG02P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DG02M001.VAGen_EZEMSG = " ";
  DG02M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DG02P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DG02P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DG02P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG02P70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DG02P30 *****************************/
  /* **************/
end // end DG02P30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DG02P30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DG02P40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DG02M002.VAGen_EZEMSG = " ";
  DG02M002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DG02P25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DG02P50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DG02P60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG02P70-PROC-MAP-2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DG02P30 *****************************/
  /* **************/
end // end DG02P30-MAIN-MAP-2

