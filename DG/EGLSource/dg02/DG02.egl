Program DG02 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG02W01 DG02W01; // record
  DG02W02 DG02W02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YCPTBL {deleteAfterUse = yes}; // table
  use DG02M.DG02M001,  // forms
      DG02M.DG02M002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG02P00-DRIVER: DG02P00-DRIVER();
    exit program;
  end // end main
end // end DG02

// DRIVER FOR APPLICATION
Function DG02P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG02P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG02P20-BUILD-MAP2(); /* Build the second map*/
  else
    DG02P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG02P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG02P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG02P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG02P00 *******************************/
  /* **************/
end // end DG02P00-DRIVER

// INITIALIZE THE APPLICATION
Function DG02P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG02W01 empty; /* Working storage record*/
  set DG02W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG02M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG02M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG02M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG02M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG02M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG02M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG02M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG02P10 ******************************/
  /* **************/
end // end DG02P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG02P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG02P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG02M001.DAW-CURR-PAGE)
    DG02S-TS-READ-PAGE(); /* read temp storage page*/
    DG02P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG02P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG02P20 **********************************/
  /* **************/
end // end DG02P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG02P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG02M002.DA-CHNG-USER-ID = sysVar.userID;
  DG02M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG02W01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG02M002.DA-PLOG-TXT1 = " ";
    DG02M002.DA-PLOG-TXT2 = " ";
    DG02M002.YCPCD = " ";
    DG02M002.YCPNM = " ";
    DG02M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG02W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG02W01.YCPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG02W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG02W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG02M002.YCPCD = DG02W01.YCPCD; /* move to the map*/
      DG02M002.YCPNM = DG02W01.YCPNM;
      DG02M002.XXXSTAT = DG02W01.XXXSTAT;
      DG02M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG02M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG02M002.YCPCD = DG02W02.YCPCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG02M002.YCPCD in YCPTBL.YCPCD)
          DG02W01.YCPCD = YCPTBL.YCPCD[sysVar.arrayIndex]; /* save for*/
          DG02W01.YCPNM = YCPTBL.YCPNM[sysVar.arrayIndex];
          DG02W01.XXXSTAT = YCPTBL.XXXSTAT[sysVar.arrayIndex];
          DG02W01.ZZZSDT = YCPTBL.ZZZSDT[sysVar.arrayIndex];
          DG02W01.ZZZCHGDT = YCPTBL.ZZZCHGDT[sysVar.arrayIndex];
          DG02W01.ZZZCHGTM = YCPTBL.ZZZCHGTM[sysVar.arrayIndex];
          DG02W01.ZZZCHGCT = YCPTBL.ZZZCHGCT[sysVar.arrayIndex];

          DG02M002.YCPNM = YCPTBL.YCPNM[sysVar.arrayIndex]; /* move to map*/
          DG02M002.XXXSTAT = YCPTBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG02W01.YCPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG02W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG02W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG02M002.YCPCD = DG02W01.YCPCD; /* move to the map*/
    DG02M002.YCPNM = DG02W01.YCPNM;
    DG02M002.XXXSTAT = DG02W01.XXXSTAT;
    DG02M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG02M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG02M002.XXXSTAT == "A")
    DG02M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG02M002.XXXSTAT == "I")
    DG02M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG02M002.XXXSTAT == " ")
    DG02M002.DAW-TBL-STATUS-DESC = " ";
  end

  DG02P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG02P20 **********************************/
  /* **************/
end // end DG02P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG02P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG02W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG02P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG02S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (YCPTBL.YCPCD[sysVar.arrayIndex] != 9999) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG02P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG02S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG02S-TS-ADD-PAGE();
    end

    DG02M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG02M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG02P22 **********************************/
  /* **************/
end // end DG02P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG02P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG02W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG02W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG02W01.YCPGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG02W02.YCPCD[DAW-ROW-NBR] = DG02W01.YCPCD;
  DG02W02.YCPNM[DAW-ROW-NBR] = DG02W01.YCPNM;
  DG02W02.XXXSTAT[DAW-ROW-NBR] = DG02W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG02W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG02W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG02W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG02W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG02W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG02W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG02P221 ******************************/
  /* ***************/
end // end DG02P23-DB2-TO-TSQ

