// SET MAP 1 FIELD ATTRIBUTES
Function DG0NP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG0NM001.DAW-PAGE-LIT = "PAGE";
    set DG0NM001.DAW-CURR-PAGE initialAttributes;
    DG0NM001.DAW-OF-LIT = "OF";
    set DG0NM001.DAW-LAST-PAGE initialAttributes;
    DG0NM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG0NM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG0NM001.DAW-PAGE-LIT = " ";
    set DG0NM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG0NM001.DAW-OF-LIT = " ";
    set DG0NM001.DAW-LAST-PAGE skip, invisible;
    DG0NM001.DAW-PF7-LIT = " ";
    DG0NM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG0NM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG0NM001.ZCHCD[sysVar.arrayIndex] == " ")
      set DG0NM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG0NM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG0NP25 **************************/
  /* **************/
end // end DG0NP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG0NP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DG0NM002.ZCHCD initialAttributes;
  else
    set DG0NM002.ZCHCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG0NM002.ZCHABRV skip;
    set DG0NM002.ZCHPIFL skip;
    set DG0NM002.ZCHDCIND skip;
    set DG0NM002.ZRTCD skip;
    set DG0NM002.XLBCD1 skip;
    set DG0NM002.XLBCD2 skip;
    set DG0NM002.XLBCD3 skip;
    set DG0NM002.XLBCD4 skip;
    set DG0NM002.YSMTBLPF skip;
    set DG0NM002.ZCHDESC skip;
    set DG0NM002.XXXSTAT skip;
  else
    set DG0NM002.ZCHABRV initialAttributes;
    set DG0NM002.ZCHPIFL initialAttributes;
    set DG0NM002.ZCHDCIND initialAttributes;
    set DG0NM002.ZRTCD initialAttributes;
    set DG0NM002.XLBCD1 initialAttributes;
    set DG0NM002.XLBCD2 initialAttributes;
    set DG0NM002.XLBCD3 initialAttributes;
    set DG0NM002.XLBCD4 initialAttributes;
    set DG0NM002.YSMTBLPF initialAttributes;
    set DG0NM002.ZCHDESC initialAttributes;
    set DG0NM002.XXXSTAT initialAttributes;
  end

  DG0NM002.ZRTABRV = " ";

  set DG0NM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DG0NM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0NM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DG0NM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DG0NM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DG0NM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DG0NM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DG0NM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DG0NP25 **************************/
  /* **************/
