Program DG0N type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG0NW01 DG0NW01; // record
  DG0NW02 DG0NW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use ZCHTBL {deleteAfterUse = yes}; // table
  use ZRTTBL {deleteAfterUse = yes}; // table
  use XLBTBL {deleteAfterUse = yes}; // table
  use DG0NM.DG0NM001,  // forms
      DG0NM.DG0NM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG0NP00-DRIVER: DG0NP00-DRIVER();
    exit program;
  end // end main
end // end DG0N

// DRIVER FOR APPLICATION
Function DG0NP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG0NP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0NP20-BUILD-MAP2(); /* Build the second map*/
  else
    DG0NP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0NP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0NP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0NP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0NP00 *******************************/
  /* **************/
end // end DG0NP00-DRIVER

// INITIALIZE THE APPLICATION
Function DG0NP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG0NW01 empty; /* Working storage record*/
  set DG0NW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG0NM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0NM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0NM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG0NM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG0NM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG0NM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG0NM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG0NP10 ******************************/
  /* **************/
end // end DG0NP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG0NP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG0NP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG0NM001.DAW-CURR-PAGE)
    DG0NS-TS-READ-PAGE(); /* read temp storage page*/
    DG0NP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG0NP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0NP20 **********************************/
  /* **************/
end // end DG0NP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0NP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0NM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0NM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0NW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0NM002.DA-PLOG-TXT1 = " ";
    DG0NM002.DA-PLOG-TXT2 = " ";
    DG0NM002.ZCHCD = " ";
    DG0NM002.ZCHPIFL = " ";
    DG0NM002.ZCHDCIND = " ";
    DG0NM002.ZRTCD = " ";
    DG0NM002.XLBCD1 = " ";
    DG0NM002.XLBCD2 = " ";
    DG0NM002.XLBCD3 = " ";
    DG0NM002.XLBCD4 = " ";
    DG0NM002.YSMTBLPF = " ";
    DG0NM002.ZCHABRV = " ";
    DG0NM002.ZCHDESC = " ";
    DG0NM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0NW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0NW01.ZCHGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0NW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0NW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0NM002.ZCHCD = DG0NW01.ZCHCD; /* move to map*/
      DG0NM002.ZCHABRV = DG0NW01.ZCHABRV;
      DG0NM002.ZCHPIFL = DG0NW01.ZCHPIFL;
      DG0NM002.ZCHDCIND = DG0NW01.ZCHDCIND;
      DG0NM002.ZRTCD = DG0NW01.ZRTCD; /* move to the map*/
      DG0NM002.XLBCD1 = DG0NW01.XLBCD1;
      DG0NM002.XLBCD2 = DG0NW01.XLBCD2;
      DG0NM002.XLBCD3 = DG0NW01.XLBCD3;
      DG0NM002.XLBCD4 = DG0NW01.XLBCD4;
      DG0NM002.YSMTBLPF = DG0NW01.YSMTBLPF;
      DG0NM002.ZCHDESC = DG0NW01.ZCHDESC;
      DG0NM002.XXXSTAT = DG0NW01.XXXSTAT;
      DG0NM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0NM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0NM002.ZCHCD = DG0NW02.ZCHCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG0NM002.ZCHCD in ZCHTBL.ZCHCD)
          DG0NW01.ZCHCD = ZCHTBL.ZCHCD[sysVar.arrayIndex];

          DG0NW01.ZCHABRV = ZCHTBL.ZCHABRV[sysVar.arrayIndex];
          DG0NW01.ZCHPIFL = ZCHTBL.ZCHPIFL[sysVar.arrayIndex];
          DG0NW01.ZCHDCIND = ZCHTBL.ZCHDCIND[sysVar.arrayIndex];
          DG0NW01.ZRTCD = ZCHTBL.ZRTCD[sysVar.arrayIndex];
          DG0NW01.XLBCD1 = ZCHTBL.XLBCD1[sysVar.arrayIndex];
          DG0NW01.XLBCD2 = ZCHTBL.XLBCD2[sysVar.arrayIndex];
          DG0NW01.XLBCD3 = ZCHTBL.XLBCD3[sysVar.arrayIndex];
          DG0NW01.XLBCD4 = ZCHTBL.XLBCD4[sysVar.arrayIndex];
          DG0NW01.YSMTBLPF = ZCHTBL.YSMTBLPF[sysVar.arrayIndex];
          DG0NW01.ZCHDESC = ZCHTBL.ZCHDESC[sysVar.arrayIndex];
          DG0NW01.XXXSTAT = ZCHTBL.XXXSTAT[sysVar.arrayIndex];
          DG0NW01.ZZZSDT = ZCHTBL.ZZZSDT[sysVar.arrayIndex];
          DG0NW01.ZZZCHGTM = ZCHTBL.ZZZCHGTM[sysVar.arrayIndex];
          DG0NW01.ZZZCHGDT = ZCHTBL.ZZZCHGDT[sysVar.arrayIndex];
          DG0NW01.ZZZCHGCT = ZCHTBL.ZZZCHGCT[sysVar.arrayIndex];

          DG0NM002.ZCHABRV = ZCHTBL.ZCHABRV[sysVar.arrayIndex];
          DG0NM002.ZCHPIFL = ZCHTBL.ZCHPIFL[sysVar.arrayIndex];
          DG0NM002.ZCHDCIND = ZCHTBL.ZCHDCIND[sysVar.arrayIndex];
          DG0NM002.ZRTCD = ZCHTBL.ZRTCD[sysVar.arrayIndex];
          DG0NM002.XLBCD1 = ZCHTBL.XLBCD1[sysVar.arrayIndex];
          DG0NM002.XLBCD2 = ZCHTBL.XLBCD2[sysVar.arrayIndex];
          DG0NM002.XLBCD3 = ZCHTBL.XLBCD3[sysVar.arrayIndex];
          DG0NM002.XLBCD4 = ZCHTBL.XLBCD4[sysVar.arrayIndex];
          DG0NM002.YSMTBLPF = ZCHTBL.YSMTBLPF[sysVar.arrayIndex];
          DG0NM002.ZCHDESC = ZCHTBL.ZCHDESC[sysVar.arrayIndex];
          DG0NM002.XXXSTAT = ZCHTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0NW01.ZCHGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0NW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0NW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0NM002.ZCHCD = DG0NW01.ZCHCD;
                                   /* move to the map*/
    DG0NM002.ZCHABRV = DG0NW01.ZCHABRV;
    DG0NM002.ZCHPIFL = DG0NW01.ZCHPIFL;
    DG0NM002.ZCHDCIND = DG0NW01.ZCHDCIND;
    DG0NM002.ZRTCD = DG0NW01.ZRTCD; /* move to the map*/
    DG0NM002.XLBCD1 = DG0NW01.XLBCD1;
    DG0NM002.XLBCD2 = DG0NW01.XLBCD2;
    DG0NM002.XLBCD3 = DG0NW01.XLBCD3;
    DG0NM002.XLBCD4 = DG0NW01.XLBCD4;
    DG0NM002.YSMTBLPF = DG0NW01.YSMTBLPF;
    DG0NM002.ZCHDESC = DG0NW01.ZCHDESC;
    DG0NM002.XXXSTAT = DG0NW01.XXXSTAT;
    DG0NM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0NM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0NM002.XXXSTAT == "A")
    DG0NM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0NM002.XXXSTAT == "I")
    DG0NM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0NM002.XXXSTAT == " ")
    DG0NM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0NP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0NP20 **********************************/
  /* **************/
end // end DG0NP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0NP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0NW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0NP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0NS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (ZCHTBL.ZCHCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0NP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0NS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0NS-TS-ADD-PAGE();
    end

    DG0NM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0NM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG0NP22 **********************************/
  /* **************/
end // end DG0NP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0NP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0NW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG0NW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG0NW01.ZCHGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG0NW02.ZCHCD[DAW-ROW-NBR] = DG0NW01.ZCHCD;
  DG0NW02.ZCHABRV[DAW-ROW-NBR] = DG0NW01.ZCHABRV;
  DG0NW02.ZCHDESC[DAW-ROW-NBR] = DG0NW01.ZCHDESC;
  DG0NW02.XXXSTAT[DAW-ROW-NBR] = DG0NW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0NW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0NW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0NW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0NW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0NW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0NP221 ******************************/
  /* ***************/
end // end DG0NP23-DB2-TO-TSQ

