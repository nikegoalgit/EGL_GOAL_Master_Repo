  VDATRN01.DA-PLOG-TXT2 = DG0NM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0NW01.DA-CCAT-KEY-TXT = " ";
  DG0NW01.DG0NW-KEY-ZCHCD = DG0NM002.ZCHCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0NW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0NW01.ZCHCD = DG0NM002.ZCHCD;
  DG0NW01.ZCHPIFL = DG0NM002.ZCHPIFL;
  DG0NW01.ZCHDCIND = DG0NM002.ZCHDCIND;
  DG0NW01.ZRTCD = DG0NM002.ZRTCD;
  DG0NW01.XLBCD1 = DG0NM002.XLBCD1;
  DG0NW01.XLBCD2 = DG0NM002.XLBCD2;
  DG0NW01.XLBCD3 = DG0NM002.XLBCD3;
  DG0NW01.XLBCD4 = DG0NM002.XLBCD4;
  DG0NW01.YSMTBLPF = DG0NM002.YSMTBLPF;
  DG0NW01.ZCHABRV = DG0NM002.ZCHABRV;
  DG0NW01.ZCHDESC = DG0NM002.ZCHDESC;
  DG0NW01.XXXSTAT = DG0NM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 66;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0NW01.ZCHGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0NP72 **********************************/
  /* **************/
