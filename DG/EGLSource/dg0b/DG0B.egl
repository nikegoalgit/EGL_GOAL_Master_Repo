// BUILD MAP 2 - TBLE ROW DETAILS
Function DG0BP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG0BM002.DA-CHNG-USER-ID = sysVar.userID;
  DG0BM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG0BW01 empty;
  set VDATRN01 empty;

  DG0BM002.XOBABRV = " ";
  DG0BM002.XDVABRV = " ";
  DG0BM002.XBCABRV = " ";
  DG0BM002.XGPABRV = " ";
  DG0BM002.XSOABRV = " ";
  DG0BM002.XPCABRV = " ";
  DG0BM002.XOTABRV = " ";

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG0BM002.DA-PLOG-TXT1 = " ";
    DG0BM002.DA-PLOG-TXT2 = " ";
    DG0BM002.XOBCD = " ";
    DG0BM002.XDVCD = " ";
    DG0BM002.XBCCD = " ";
    DG0BM002.XGPCD = " ";
    DG0BM002.XSOCD = " ";
    DG0BM002.XPC-PROD-CAT-CD = " ";
    DG0BM002.XOTCD = " ";
    DG0BM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG0BW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG0BW01.XVOGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG0BW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG0BW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG0BM002.XOBCD = DG0BW01.XOBCD; /* move to the map*/
      DG0BM002.XDVCD = DG0BW01.XDVCD;
      DG0BM002.XBCCD = DG0BW01.XBCCD;
      DG0BM002.XGPCD = DG0BW01.XGPCD;
      DG0BM002.XSOCD = DG0BW01.XSOCD;
      DG0BM002.XPC-PROD-CAT-CD = DG0BW01.XPC-PROD-CAT-CD;
                                   /* move to the map*/
      DG0BM002.XOTCD = DG0BW01.XOTCD;
      DG0BM002.XXXSTAT = DG0BW01.XXXSTAT;
      DG0BM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG0BM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG0BM002.XOBCD = DG0BW02.XOBCD[DAW-ROW-NBR];
        DG0BM002.XDVCD = DG0BW02.XDVCD[DAW-ROW-NBR];
        DG0BM002.XBCCD = DG0BW02.XBCCD[DAW-ROW-NBR];
        DG0BM002.XGPCD = DG0BW02.XGPCD[DAW-ROW-NBR];
        DG0BM002.XSOCD = DG0BW02.XSOCD[DAW-ROW-NBR];
        DG0BM002.XPC-PROD-CAT-CD = DG0BW02.XPC-PROD-CAT-CD[DAW-ROW-NBR];
        DG0BM002.XOTCD = DG0BW02.XOTCD[DAW-ROW-NBR];
        DG0BM002.XXXSTAT = DG0BW02.XXXSTAT[DAW-ROW-NBR];
                                   /* get the tbl row*/

        DG0BW01.DG0BW-FND-FL = "N";
  /* Singleton lookup based on PK against DMIROR.XVO*/
        VXVO0001.XOBCD = DG0BM002.XOBCD;
        VXVO0001.XDVCD = DG0BM002.XDVCD;
        VXVO0001.XBCCD = DG0BM002.XBCCD;
        VXVO0001.XGPCD = DG0BM002.XGPCD;
        VXVO0001.XSOCD = DG0BM002.XSOCD;
        VXVO0001.XPC-PROD-CAT-CD = DG0BM002.XPC-PROD-CAT-CD;
        VXVO0001.XOTCD = DG0BM002.XOTCD;

        call "DA1150" ("S ", SQLCA, VXVO0001) {isNoRefresh = yes, isExternal = yes};
        if (SQLCA.VAGen_SQLCODE == 100) /* IF not found*/
          /* next sentence*/
        else
          if (SQLCA.VAGen_SQLCODE == 0) /* IF found*/
            DG0BW01.DG0BW-FND-FL = "Y";
            DG0BM002.XXXSTAT = VXVO0001.XXXSTAT;
          else
            if (SQLCA.VAGen_SQLCODE == -911) /* database busy*/
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
              DAWREC.DAW-INFO-MESSAGE = "DATABASE BUSY - PLEASE TRY AGAIN ";
            else
              DAWREC.DAW-EDIT-ERROR-FL = "Y";

              TA1REC.TA1DBASE = "DMIROR  "; /* data base name*/
              TA1REC.TA1TBLVU = "VXVO0001"; /* table view*/
              TA1REC.TA1LOCAT[1] = "SELECT  ";
              XSDAS-DB2-ABEND-RB();
            end
          end
        end
        DG0BW01.DG0BW-FND-FL = "Y";

        if (DG0BW01.DG0BW-FND-FL == "Y")
          DG0BW01.XOTCD = VXVO0001.XOBCD; /* Save for*/
          DG0BW01.XDVCD = VXVO0001.XDVCD; /* later*/
          DG0BW01.XBCCD = VXVO0001.XBCCD;
          DG0BW01.XGPCD = VXVO0001.XGPCD;
          DG0BW01.XSOCD = VXVO0001.XSOCD;
          DG0BW01.XPC-PROD-CAT-CD = VXVO0001.XPC-PROD-CAT-CD;
          DG0BW01.XOTCD = VXVO0001.XOTCD;
          DG0BW01.XXXSTAT = VXVO0001.XXXSTAT;
          DG0BW01.ZZZSDT = VXVO0001.ZZZSDT;
          DG0BW01.ZZZCHGDT = VXVO0001.ZZZCHGDT;
          DG0BW01.ZZZCHGTM = VXVO0001.ZZZCHGTM;
          DG0BW01.ZZZCHGCT = VXVO0001.ZZZCHGCT;
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG0BW01.XVOGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG0BW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG0BW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG0BM002.XOBCD = DG0BW01.XOBCD; /* move to the map*/
    DG0BM002.XDVCD = DG0BW01.XDVCD;
    DG0BM002.XBCCD = DG0BW01.XBCCD;
    DG0BM002.XGPCD = DG0BW01.XGPCD;
    DG0BM002.XSOCD = DG0BW01.XSOCD;
    DG0BM002.XPC-PROD-CAT-CD = DG0BW01.XPC-PROD-CAT-CD;
    DG0BM002.XOTCD = DG0BW01.XOTCD;
    DG0BM002.XXXSTAT = DG0BW01.XXXSTAT;
    DG0BM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG0BM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG0BM002.XXXSTAT == "A")
    DG0BM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG0BM002.XXXSTAT == "I")
    DG0BM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG0BM002.XXXSTAT == " ")
    DG0BM002.DAW-TBL-STATUS-DESC = " ";
  end

  DG0BP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG0BP20 **********************************/
  /* **************/
end // end DG0BP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG0BP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG0BW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0BP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0BS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/

  /* Move the selected XVO rows to temp storage*/
  DG0B-OPEN-XVO();
  DG0B-FETCH-XVO(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG0BW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TBL ";
    DG0BW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
    DG0BW02.XOBCD[DAW-ROW-NBR] = VXVO0001.XOBCD;
    DG0BW02.XDVCD[DAW-ROW-NBR] = VXVO0001.XDVCD;
    DG0BW02.XBCCD[DAW-ROW-NBR] = VXVO0001.XBCCD;
    DG0BW02.XGPCD[DAW-ROW-NBR] = VXVO0001.XGPCD;
    DG0BW02.XSOCD[DAW-ROW-NBR] = VXVO0001.XSOCD;
    DG0BW02.XPC-PROD-CAT-CD[DAW-ROW-NBR] = VXVO0001.XPC-PROD-CAT-CD;
    DG0BW02.XOTCD[DAW-ROW-NBR] = VXVO0001.XOTCD;
    DG0BW02.XXXSTAT[DAW-ROW-NBR] = VXVO0001.XXXSTAT;

    DG0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = " ";
    DG0BW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = " ";
    DG0BW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = " ";
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG0BS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    DG0B-FETCH-XVO(); /* fetch next row*/
  end

  DG0B-CLOSE-XVO();

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG0BS-TS-ADD-PAGE();
    end

    DG0BM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG0BM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end
end // end DG0BP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG0BP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG0BW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
