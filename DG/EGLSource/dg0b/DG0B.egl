// EDIT MAP PAGE NUMBER
Function DG0BP62-EDIT-PAGE()
  /* ***********************************************************/
  /* Edit map current page number*/
  /* ***********************************************************/

  if (DG0BM001.DAW-CURR-PAGE == DAWREC.DAW-CURR-PAGE)
    return;
  end

  /* if select codes entered and page number changed*/
  /* then reset page number*/

  if (DAWREC.DAW-ADD-ROW-FL == "Y"
   || DAWREC.DAW-DELETE-ROW-FL == "Y"
   || DAWREC.DAW-SELECT-ROW-FL == "Y"
   || DAWREC.DAW-COPY-ROW-FL == "Y")
    set DG0BM001.DAW-CURR-PAGE bold;
    DG0BM001.VAGen_EZEMSG = "CANNOT MAINTAIN OR ADD A ROW AND CHANGE PAGE AT THE SAME TIME";
    DG0BM001.DAW-CURR-PAGE = DAWREC.DAW-CURR-PAGE;
    DG0BM001.DAW-INFO-MESSAGE = "PAGE NUMBER HAS BEEN RESET - HIT ENTER OR PF11 TO CONTINUE";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
  end

  if (DG0BM001.DAW-CURR-PAGE > 1)
    if (DG0BM001.DAW-CURR-PAGE > DG0BM001.DAW-LAST-PAGE)
      set DG0BM001.DAW-CURR-PAGE cursor, bold;
      converseLib.validationFailed(006); /* page number entered is greater than*/
      DAWREC.DAW-EDIT-ERROR-FL = "Y"; /* last page available*/
    end
  end

  if (DG0BM001.DAW-CURR-PAGE == 0)
    set DG0BM001.DAW-CURR-PAGE cursor, bold;
    converseLib.validationFailed(007); /* page number entered is zero - page*/
    DAWREC.DAW-EDIT-ERROR-FL = "Y"; /* does not exist*/
  end

  /* **************/
  /* ****************** END DG0BP62 ****************************/
  /* **************/
end // end DG0BP62-EDIT-PAGE

