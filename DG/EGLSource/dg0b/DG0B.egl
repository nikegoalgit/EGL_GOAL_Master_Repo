// DRIVER FOR APPLICATION
Function DG0BP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  /* WJBTEST;                         /* test statement group*/
  DG0BP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG0BP20-BUILD-MAP2(); /* Build the second map*/
  else
  /* PERFORM DG0BP20-BUILD-MAP1;    /* Build the first map*/
  end

  DG0BW01.XOB-SRCH = "ZZ";
  DG0BW01.XDV-SRCH = "ZZ";
  DG0BW01.XBC-SRCH = "ZZ";
  DG0BW01.XGP-SRCH = "ZZ";
  DG0BW01.XSO-SRCH = "ZZ";

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG0BP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG0BP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG0BP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG0BP00 *******************************/
  /* **************/
end // end DG0BP00-DRIVER

