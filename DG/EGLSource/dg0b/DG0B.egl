  DG0BW02.XOBCD[DAW-ROW-NBR] = DG0BW01.XOBCD;
  DG0BW02.XDVCD[DAW-ROW-NBR] = DG0BW01.XDVCD;
  DG0BW02.XBCCD[DAW-ROW-NBR] = DG0BW01.XBCCD;
  DG0BW02.XGPCD[DAW-ROW-NBR] = DG0BW01.XGPCD;
  DG0BW02.XSOCD[DAW-ROW-NBR] = DG0BW01.XSOCD;
  DG0BW02.XPC-PROD-CAT-CD[DAW-ROW-NBR] = DG0BW01.XPC-PROD-CAT-CD;
  DG0BW02.XOTCD[DAW-ROW-NBR] = DG0BW01.XOTCD;
  DG0BW02.XXXSTAT[DAW-ROW-NBR] = DG0BW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG0BW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG0BW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG0BW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG0BW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG0BW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG0BP221 ******************************/
  /* ***************/
end // end DG0BP23-DB2-TO-TSQ

// SET MAP 1 FIELD ATTRIBUTES
Function DG0BP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG0BM001.DAW-PAGE-LIT = "PAGE";
    set DG0BM001.DAW-CURR-PAGE initialAttributes;
    DG0BM001.DAW-OF-LIT = "OF";
    set DG0BM001.DAW-LAST-PAGE initialAttributes;
    DG0BM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG0BM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG0BM001.DAW-PAGE-LIT = " ";
    set DG0BM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG0BM001.DAW-OF-LIT = " ";
    set DG0BM001.DAW-LAST-PAGE skip, invisible;
    DG0BM001.DAW-PF7-LIT = " ";
    DG0BM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG0BM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG0BM001.XOBCD[sysVar.arrayIndex] == " ")
      set DG0BM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG0BM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG0BP25 **************************/
  /* **************/
end // end DG0BP25-SET-ATTR-1

