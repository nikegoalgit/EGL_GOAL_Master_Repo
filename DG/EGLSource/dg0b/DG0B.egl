// BUILD NEW VDATRN01 RECORD
Function DG0BP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG0BM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG0BM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG0BW01.DA-CCAT-KEY-TXT = " ";
  DG0BW01.DG0BW-KEY-XOBCD = DG0BM002.XOBCD;
  DG0BW01.DG0BW-KEY-XDVCD = DG0BM002.XDVCD;
  DG0BW01.DG0BW-KEY-XBCCD = DG0BM002.XBCCD;
  DG0BW01.DG0BW-KEY-XGPCD = DG0BM002.XGPCD;
  DG0BW01.DG0BW-KEY-XSOCD = DG0BM002.XSOCD;
  DG0BW01.DG0BW-KEY-XPCCD = DG0BM002.XPC-PROD-CAT-CD;
  DG0BW01.DG0BW-KEY-XOTCD = DG0BM002.XOTCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG0BW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG0BW01.XOBCD = DG0BM002.XOBCD;
  DG0BW01.XDVCD = DG0BM002.XDVCD;
  DG0BW01.XBCCD = DG0BM002.XBCCD;
  DG0BW01.XGPCD = DG0BM002.XGPCD;
  DG0BW01.XSOCD = DG0BM002.XSOCD;
  DG0BW01.XPC-PROD-CAT-CD = DG0BM002.XPC-PROD-CAT-CD;
  DG0BW01.XOTCD = DG0BM002.XOTCD;
  DG0BW01.XXXSTAT = DG0BM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 27;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG0BW01.XVOGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG0BP72 **********************************/
  /* **************/
end // end DG0BP72-NEW-TRAN

