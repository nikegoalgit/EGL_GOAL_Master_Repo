// BUILD NEW VDATRN01 RECORD
Function DG09P72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DG09M002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DG09M002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DG09W01.DA-CCAT-KEY-TXT = " ";
  DG09W01.DG09W-KEY-XOMCD = DG09M002.XOMCD;
  VDATRN01.DA-CCAT-KEY-TXT = DG09W01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DG09W01.XOMCD = DG09M002.XOMCD;
  DG09W01.XOMDESC = DG09M002.XOMDESC;
  DG09W01.XOMABRV = DG09M002.XOMABRV;
  DG09W01.XOMCANCF = DG09M002.XOMCANCF;
  DG09W01.XOMAPCFL = DG09M002.XOMAPCFL;
  DG09W01.XOMFPCFL = DG09M002.XOMFPCFL;
  DG09W01.XMRCD = DG09M002.XMRCD;
  DG09W01.XOMRBKFL = DG09M002.XOMRBKFL;
  DG09W01.XXXSTAT = DG09M002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 47;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DG09W01.XOMGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DG09P72 **********************************/
  /* **************/
end // end DG09P72-NEW-TRAN

