// NIKE TABLE HANDLER - TABLE MAINTENANCE - ORDER MAINTENANCE
//                                          REASON
// 
// OVERVIEW: This application is the maintenance application
//           for the order maintenance reason table
// 
// DB2 TBL: DCORP.VXOM0001
// CSP TBL: XOMTBL
// 
// DATE CREATED: 12/18/97
//       AUTHOR: Eric Chan
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   05-06-99  ECHAN       ADD NOHILITE IN DG09P25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DG09P70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DG09P70-PROC-MAP-2
//                         DISPLAY STATUS IN DG09M001
// *********************
Program DG09 type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DG09W01 DG09W01; // record
  DG09W02 DG09W02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XMRTBL {deleteAfterUse = yes}; // table
  use XOMTBL {deleteAfterUse = yes}; // table
  use DG09M.DG09M001,  // forms
      DG09M.DG09M002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DG09P00-DRIVER: DG09P00-DRIVER();
    exit program;
  end // end main
end // end DG09

// DRIVER FOR APPLICATION
Function DG09P00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DG09P10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DG09P20-BUILD-MAP2(); /* Build the second map*/
  else
    DG09P20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DG09P30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DG09P30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DG09P99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DG09P00 *******************************/
  /* **************/
end // end DG09P00-DRIVER

// INITIALIZE THE APPLICATION
Function DG09P10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DG09W01 empty; /* Working storage record*/
  set DG09W02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DG09M001 initial;
  XSDAS-FMT-DB2-MDCY();
  DG09M001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG09M001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DG09M002 initial;
  XSDAS-FMT-DB2-MDCY();
  DG09M002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DG09M002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DG09M001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DG09P10 ******************************/
  /* **************/
end // end DG09P10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DG09P20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DG09P22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DG09M001.DAW-CURR-PAGE)
    DG09S-TS-READ-PAGE(); /* read temp storage page*/
    DG09P71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DG09P25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG09P20 **********************************/
  /* **************/
end // end DG09P20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DG09P20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DG09M002.DA-CHNG-USER-ID = sysVar.userID;
  DG09M002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DG09W01 empty;
  set VDATRN01 empty;
  DG09M002.XMRABRV = " ";

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG09M002.DA-PLOG-TXT1 = " ";
    DG09M002.DA-PLOG-TXT2 = " ";
    DG09M002.XOMCD = " ";
    DG09M002.XOMDESC = " ";
    DG09M002.XOMABRV = " ";
    DG09M002.XOMCANCF = " ";
    DG09M002.XOMAPCFL = " ";
    DG09M002.XOMFPCFL = " ";
    DG09M002.XMRCD = " ";
    DG09M002.XOMRBKFL = " ";
    DG09M002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DG09W02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DG09W01.XOMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DG09W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DG09W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DG09M002.XOMCD = DG09W01.XOMCD; /* move to the map*/
      DG09M002.XOMDESC = DG09W01.XOMDESC;
      DG09M002.XOMABRV = DG09W01.XOMABRV;
      DG09M002.XOMCANCF = DG09W01.XOMCANCF;
      DG09M002.XOMAPCFL = DG09W01.XOMAPCFL;
      DG09M002.XOMFPCFL = DG09W01.XOMFPCFL;
      DG09M002.XMRCD = DG09W01.XMRCD;
      DG09M002.XOMRBKFL = DG09W01.XOMRBKFL;
      DG09M002.XXXSTAT = DG09W01.XXXSTAT;
      DG09M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DG09M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DG09M002.XOMCD = DG09W02.XOMCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DG09M002.XOMCD in XOMTBL.XOMCD)
          DG09W01.XOMCD = XOMTBL.XOMCD[sysVar.arrayIndex]; /* save for a*/
          DG09W01.XOMDESC = XOMTBL.XOMDESC[sysVar.arrayIndex]; /* later compare*/
          DG09W01.XOMABRV = XOMTBL.XOMABRV[sysVar.arrayIndex];
          DG09W01.XOMCANCF = XOMTBL.XOMCANCF[sysVar.arrayIndex];
          DG09W01.XOMAPCFL = XOMTBL.XOMAPCFL[sysVar.arrayIndex];
          DG09W01.XOMFPCFL = XOMTBL.XOMFPCFL[sysVar.arrayIndex];
          DG09W01.XMRCD = XOMTBL.XMRCD[sysVar.arrayIndex];
          DG09W01.XOMRBKFL = XOMTBL.XOMRBKFL[sysVar.arrayIndex];
          DG09W01.XXXSTAT = XOMTBL.XXXSTAT[sysVar.arrayIndex];
          DG09W01.ZZZSDT = XOMTBL.ZZZSDT[sysVar.arrayIndex];
          DG09W01.ZZZCHGDT = XOMTBL.ZZZCHGDT[sysVar.arrayIndex];
          DG09W01.ZZZCHGTM = XOMTBL.ZZZCHGTM[sysVar.arrayIndex];
          DG09W01.ZZZCHGCT = XOMTBL.ZZZCHGCT[sysVar.arrayIndex];

          DG09M002.XOMDESC = XOMTBL.XOMDESC[sysVar.arrayIndex]; /* move to map*/
          DG09M002.XOMABRV = XOMTBL.XOMABRV[sysVar.arrayIndex];
          DG09M002.XOMCANCF = XOMTBL.XOMCANCF[sysVar.arrayIndex];
          DG09M002.XOMAPCFL = XOMTBL.XOMAPCFL[sysVar.arrayIndex];
          DG09M002.XOMFPCFL = XOMTBL.XOMFPCFL[sysVar.arrayIndex];
          DG09M002.XMRCD = XOMTBL.XMRCD[sysVar.arrayIndex];
          DG09M002.XOMRBKFL = XOMTBL.XOMRBKFL[sysVar.arrayIndex];
          DG09M002.XXXSTAT = XOMTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DG09W01.XOMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DG09W01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DG09W01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DG09M002.XOMCD = DG09W01.XOMCD; /* move to the map*/
    DG09M002.XOMDESC = DG09W01.XOMDESC;
    DG09M002.XOMABRV = DG09W01.XOMABRV;
    DG09M002.XOMCANCF = DG09W01.XOMCANCF;
    DG09M002.XOMAPCFL = DG09W01.XOMAPCFL;
    DG09M002.XOMFPCFL = DG09W01.XOMFPCFL;
    DG09M002.XMRCD = DG09W01.XMRCD;
    DG09M002.XOMRBKFL = DG09W01.XOMRBKFL;
    DG09M002.XXXSTAT = DG09W01.XXXSTAT;
    DG09M002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DG09M002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DG09M002.XXXSTAT == "A")
    DG09M002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DG09M002.XXXSTAT == "I")
    DG09M002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DG09M002.XXXSTAT == " ")
    DG09M002.DAW-TBL-STATUS-DESC = " ";
  end

  DG09P25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DG09P20 **********************************/
  /* **************/
end // end DG09P20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DG09P22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DG09W02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG09P23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG09S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XOMTBL.XOMCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DG09P24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DG09S-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DG09S-TS-ADD-PAGE();
    end

    DG09M001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DG09M001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DG09P22 **********************************/
  /* **************/
end // end DG09P22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DG09P23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DG09W02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DG09W02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DG09W01.XOMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DG09W02.XOMCD[DAW-ROW-NBR] = DG09W01.XOMCD;
  DG09W02.XOMDESC[DAW-ROW-NBR] = DG09W01.XOMDESC;
  DG09W02.XOMCANCF[DAW-ROW-NBR] = DG09W01.XOMCANCF;
  DG09W02.XOMAPCFL[DAW-ROW-NBR] = DG09W01.XOMAPCFL;
  DG09W02.XOMFPCFL[DAW-ROW-NBR] = DG09W01.XOMFPCFL;
  DG09W02.XMRCD[DAW-ROW-NBR] = DG09W01.XMRCD;
  DG09W02.XOMRBKFL[DAW-ROW-NBR] = DG09W01.XOMRBKFL;
  DG09W02.XXXSTAT[DAW-ROW-NBR] = DG09W01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DG09W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DG09W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DG09W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DG09W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DG09W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DG09W02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DG09W02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DG09W02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DG09W02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DG09W02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DG09P221 ******************************/
  /* ***************/
end // end DG09P23-DB2-TO-TSQ

