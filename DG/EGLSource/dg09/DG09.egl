      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      set DG09M002.XXXSTAT cursor, bold;
    end
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL"
   && DG09M002.XXXSTAT != "I")
    DG09M002.DAW-INFO-MESSAGE = "CANNOT DELETE TABLE ROW; STATUS IS ACTIVE. PF3 TO RETURN TO LIST";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
  end

  /* **************/
  /* ***************** END DG09P61 **********************************/
  /* **************/
end // end DG09P61-EDIT-STAT

