// SET MAP 1 FIELD ATTRIBUTES
Function DG09P25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DG09M001.DAW-PAGE-LIT = "PAGE";
    set DG09M001.DAW-CURR-PAGE initialAttributes;
    DG09M001.DAW-OF-LIT = "OF";
    set DG09M001.DAW-LAST-PAGE initialAttributes;
    DG09M001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DG09M001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DG09M001.DAW-PAGE-LIT = " ";
    set DG09M001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DG09M001.DAW-OF-LIT = " ";
    set DG09M001.DAW-LAST-PAGE skip, invisible;
    DG09M001.DAW-PF7-LIT = " ";
    DG09M001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DG09M001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DG09M001.XOMCD[sysVar.arrayIndex] == " ")
      set DG09M001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DG09M001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DG09P25 **************************/
  /* **************/
end // end DG09P25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DG09P25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DG09M002.XOMCD initialAttributes;
    set DG09M002.XOMDESC initialAttributes;
    set DG09M002.XOMABRV initialAttributes;
    set DG09M002.XOMCANCF initialAttributes;
    set DG09M002.XOMAPCFL initialAttributes;
    set DG09M002.XOMFPCFL initialAttributes;
    set DG09M002.XMRCD initialAttributes;
    set DG09M002.XOMRBKFL initialAttributes;
    set DG09M002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DG09M002.XOMCD skip;
    set DG09M002.XOMDESC skip;
    set DG09M002.XOMABRV skip;
    set DG09M002.XOMCANCF skip;
    set DG09M002.XOMAPCFL skip;
    set DG09M002.XOMFPCFL skip;
    set DG09M002.XMRCD skip;
    set DG09M002.XOMRBKFL skip;
    set DG09M002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DG09M002.XOMCD skip;
    set DG09M002.XOMDESC initialAttributes;
    set DG09M002.XOMABRV initialAttributes;
    set DG09M002.XOMCANCF initialAttributes;
    set DG09M002.XOMAPCFL initialAttributes;
    set DG09M002.XOMFPCFL initialAttributes;
    set DG09M002.XMRCD initialAttributes;
    set DG09M002.XOMRBKFL initialAttributes;
    set DG09M002.XXXSTAT initialAttributes;
  end

  set DG09M002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DG09M002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DG09M002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DG09M002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DG09M002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DG09M002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DG09M002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DG09M002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end

  /* **************/
  /* ****************** END DG09P25 **************************/
  /* **************/
end // end DG09P25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DG09P30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DG09P40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DG09M001.VAGen_EZEMSG = " ";
  DG09M001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DG09P25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DG09P50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DG09P60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG09P70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end

  /* **************/
  /* ****************** END DG09P30 *****************************/
  /* **************/
end // end DG09P30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DG09P30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DG09P40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DG09M002.VAGen_EZEMSG = " ";
  DG09M002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DG09P25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DG09P50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DG09P60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DG09P70-PROC-MAP-2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DG09P30 *****************************/
  /* **************/
end // end DG09P30-MAIN-MAP-2

