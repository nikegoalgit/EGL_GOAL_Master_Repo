    return; /* Return to previous process*/
  end


   /* ------------------------------------------------------------*/
   /* If scroll forward was requested, increment the page number.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf8) /* Scroll forward requested*/
    EP0DW00.XXX-CURR-PAGE-NBR = EP0DW00.XXX-CURR-PAGE-NBR + 1;
                                   /* Increment page number*/
    EP0DW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

    return; /* Return to previous process*/
  end


   /* ------------------------------------------------------------*/
   /* Process delete requests*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf12) /* Delete*/
    EP0DW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/
    while (EP0DW00.XXX-INX-FLD-NBR <= EP0DW00.XXX-INX-LMT-NBR)
                                   /* Until end of lines*/
      if (EP0DM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "D") /* Delete*/

        EP0D-5300(); /* Process valid delete request*/
      end
      EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-FLD-NBR + 1;
    end
    if (converseVar.validationMsgNum == 0) /* No errors found*/
      EP0DW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/
      while (EP0DW00.XXX-INX-FLD-NBR <= EP0DW00.XXX-INX-LMT-NBR)
                                   /* Until end of lines*/
        if (EP0DM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] == "D") /* Delete*/
          EP0DM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] = " ";
          EP0DM001.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] = " ";
          EP0DM001.XSUCD[XXX-INX-FLD-NBR] = " ";
          EP0DM001.EP0DM-CU-CO-ID[XXX-INX-FLD-NBR] = " ";
          EP0DM001.EP0DM-INCL-EXCL-FL[XXX-INX-FLD-NBR] = " ";
          EP0DM001.EP0DM-EP-TEXT[XXX-INX-FLD-NBR] = " ";
          EP0DM001.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] = " ";
          EP0DM001.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR] = " ";
        end
        EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-FLD-NBR + 1;
      end
    end
  end


   /* ------------------------------------------------------------*/
   /* Do actual DB adds or updates requested.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is enter)
    EP0DW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

    while (EP0DW00.XXX-INX-FLD-NBR <= EP0DW00.XXX-INX-LMT-NBR)
                                   /* Until end of lines*/
      if (EP0DM001.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] != 
      EP0DW01.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR]    /* Goal spec hndl cd*/
       || EP0DM001.XSUCD[XXX-INX-FLD-NBR] !=      
      EP0DW01.XSUCD[XXX-INX-FLD-NBR]              /* Silhouette code*/
       || EP0DM001.EP0DM-CU-CO-ID[XXX-INX-FLD-NBR] != 
      EP0DW01.EP0DW-CU-CO-ID[XXX-INX-FLD-NBR]     /* Company*/
       || EP0DM001.EP0DM-INCL-EXCL-FL[XXX-INX-FLD-NBR] != 
      EP0DW01.EP0DW-INCL-EXCL-FL[XXX-INX-FLD-NBR] /* Incl/excl fl*/
       || EP0DM001.EP0DM-EP-TEXT[XXX-INX-FLD-NBR] != 
      EP0DW01.EP0DW-EP-TEXT[XXX-INX-FLD-NBR]      /* Text*/
       || EP0DM001.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] != 
      EP0DW01.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR]    /* Dcms spec hndl code*/
       || EP0DM001.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR] != 
      EP0DW01.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR]) /* Dcms spec instruc*/

        if (EP0DW01.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] == "  "
         && EP0DW01.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] == "  ") /* Dcms cd*/

          EP0D-5100(); /* Process valid add request*/
        else

          EP0D-5200(); /* Process valid change request*/
        end
        EP0DW00.XXX-PAGE-UPD-FL = "Y"; /* Page update flag*/
      end

      EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-FLD-NBR + 1;
    end
  end


   /* -----------------------------------------*/
   /* Rollback if any errors found*/
   /* -----------------------------------------*/
  if (converseVar.validationMsgNum > 0) /* Errors found*/
    VGVar.handleSysLibraryErrors = 0;
    sysLib.rollback(); /* Rollback DB changes done on this page*/

    exit stack; /* Exit to display map*/
  end


   /* -----------------------------------------*/
   /* Update temporary storage*/
   /* -----------------------------------------*/
  EP0DW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (EP0DW00.XXX-INX-FLD-NBR <= EP0DW00.XXX-INX-LMT-NBR)
                                   /* Until end of lines*/
    EP0DW01.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] = EP0DM001.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR]; /* Goal spec hndl cd*/
    EP0DW01.XSUCD[XXX-INX-FLD-NBR] = EP0DM001.XSUCD[XXX-INX-FLD-NBR]; /* Silhouette code*/
    EP0DW01.EP0DW-CU-CO-ID[XXX-INX-FLD-NBR] = EP0DM001.EP0DM-CU-CO-ID[XXX-INX-FLD-NBR]; /* Company*/
    EP0DW01.EP0DW-INCL-EXCL-FL[XXX-INX-FLD-NBR] = EP0DM001.EP0DM-INCL-EXCL-FL[XXX-INX-FLD-NBR]; /* Incl/excl fl*/
    EP0DW01.EP0DW-EP-TEXT[XXX-INX-FLD-NBR] = EP0DM001.EP0DM-EP-TEXT[XXX-INX-FLD-NBR]; /* Text*/
    EP0DW01.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] = EP0DM001.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR]; /* Dcms code*/
    EP0DW01.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR] = EP0DM001.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR]; /* Dcms spec instruc*/

    EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-FLD-NBR + 1;
  end


   /* -----------------------------------------*/
   /* Return to main process*/
   /* -----------------------------------------*/
  EP0DW00.XXX-PAGE-UPD-FL = "Y"; /* Page update flag*/
  converseLib.validationFailed(20); /* Transaction competed*/

  return; /* Return to previous process*/


end // end EP0D-5000

