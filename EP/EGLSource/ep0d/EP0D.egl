//*** PROGRAM=EP0D ****
// This application mainains the label attachment variables for
// DCMS vas. For vas orders, we use cross reference information
// to determine the 4 digit DCMS special handling code to use
// on the order.
// 
// This application allows a user to inquire and maintain rows
// in a DB2 table.
// 
// There are two different screens that may be displayed.
// 
// The first screen displayed to the user allows specification
// of the search criteria to be used to list the rows in the
// table.  The list is displayed and the user may page backward
// and forward to view the information.
// 
// When a line is selected on the screen, the second screen is
// displayed, showing all information for that table row.
// 
// If the user has sufficient security to change the data, the
// modifiable fields remain unprotected.  Otherwise, they are
// protected from modification by the user.
// 
// If any data is modified, it is verified and, if complete and
// correct, the table row is updated with the changed data.
// The screen is redisplayed to the user, indicating that the
// update is completed.
// 
// If more than one line was selected on the first screen, the
// next selection is displayed when the user presses the ENTER
// key without making any change to the displayed data.
// 
// When the user exits the second screen (either via PF3 key or
// after all selections have been viewed) the first screen is
// redisplayed, indicating the selections that were viewed.
// 
// -------  --------  ------  ---------------------------------
// CONTROL    DATE    USERID  DESCRIPTION OF CHANGE
// -------  --------  ------  ---------------------------------
// NEW      06/03/98  schen   New application.
// multgpc  08/03/99  OENFIE  modify map to make the column
//                            heading literal for 'silh code'
//                            be a variable field.  Also added
//                            a variable field on the main
//                            heading to identify whether apprl
//                            or equipment.
//                            Also, modified logic to now
//                            check the sel-num passed from
//                            EP0A before moving either 'ATCH'
//                            or 'ATEQ' to the PK-GSPRC-SRCE-CD
//                            OF VSPRCD01.
// TT#6347  06/02/00  LPATTN  INCLUDED V00F AS COMPANY NAME.
// *********************
Program EP0D type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "EP00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  EP0DW00 EP0DW00; // record
  EP0DW01 EP0DW01; // record
  EP0DW02 EP0DW02; // record
  EPWREC EPWREC; // record
  KUCREC KUCREC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VSPRCD01 VSPRCD01; // record
  XPCREC XPCREC; // record
  XX0XW01 XX0XW01; // record
  XX0XW02 XX0XW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YLATBL {deleteAfterUse = yes}; // table
  use XSUTBL {deleteAfterUse = yes}; // table
  use XPCTBL; // table
  use EP0DM.EP0DM001,  // forms
      EP0DM.XX0XM001
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    XPEP-00000: XPEP-00000();
     /* ------------------------------------------------------------*/
     /* Initialize working storage and display storage areas.*/
     /* ------------------------------------------------------------*/

    set EP0DM001 initial; /* M001 input/output*/
    set EP0DW00 empty; /* Working storage*/
    set EP0DW01 empty; /* M001 display storage*/
    set EP0DW02 empty; /* M002 display storage*/
    set SQLCA empty; /* SQL communication area*/
    set TAEREC empty; /* Error diagnosis information*/
    set TA1REC empty; /* Error diagnosis parameters*/
    set TA5REC empty; /* Temporary storage parameters*/
    set XPCREC empty; /* Product category table row*/

    EP0DW00.XXX-RQT-DATA-FL = "Y"; /* Request for data flag*/
    EP0DW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

    EP0DW00.XXX-MAP-DSP-CD = "1"; /* Map to be displayed*/

    EP0DW00.XXX-TEMP-STG-LNTH = 1028; /* Temporary storage length*/
    EP0DW00.XXX-INX-LMT-NBR = 16; /* Number of lines on a page*/

     /* ------------------------------------------------------------*/
     /* Initialize any fields passed from another application.*/
     /* ------------------------------------------------------------*/

    if (COMMAREA.CAITEM > " ") /* Data was passed*/

      COMMAREA.CAITEM = " "; /* Clear passed data*/

      EP0DW00.XXX-RQT-DATA-FL = "Y"; /* Request for data flag*/

    end

     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto EP0D-0000; /* Converse the main process*/


    EP0D-0000: EP0D-0000();
     /* ------------------------------------------------------------*/
     /* Flow to converse the main process used to display a map.*/
     /* ------------------------------------------------------------*/

    goto EP0D-0000; /* Converse the main process*/


  end // end main
end // end EP0D

// Main process to display a map
Function EP0D-0000()
   /* ------------------------------------------------------------*/
   /* This is the main process used to display a map.  Processing*/
   /* always returns to the top of this process and falls through*/
   /* subordinate processes until an error is found or a valid*/
   /* request is processed.  Processing sequence is as follows:*/

   /* 1) Obtain the data to display.*/
   /* 2) Format the map output area and display the map.*/
   /* 3) Check the attention identifier used to respond.*/
   /* 4) Verify the data that was entered.*/
   /* 5) Process a valid input request.*/

   /* In all subordinate processes, "EZEFLO" is used to exit to*/
   /* redisplay the map when an error is found.  This is done to*/
   /* minimize the coding effort required to skip subsequent*/
   /* processing under error conditions.*/
   /* ------------------------------------------------------------*/

  EP0D-1000(); /* Obtain the data to display*/

  EP0D-2000(); /* Format and display the map*/

  EP0D-3000(); /* Check attention identifier*/

  EP0D-4000(); /* Verify the entered data*/

  EP0D-5000(); /* Process valid input request*/


end // end EP0D-0000

// Obtain the data to display
Function EP0D-1000()
   /* ------------------------------------------------------------*/
   /* If data was not requested or an error was found, return to*/
   /* the previous process.*/
   /* ------------------------------------------------------------*/
  if (EP0DW00.XXX-RQT-DATA-FL != "Y" /* Data was not requested*/
   || converseVar.validationMsgNum != 0) /* or an error was found*/

    return; /* Return to previous process*/
  end

   /* ------------------------------------------------------------*/
   /* If no message to display, reset the page number to display.*/
   /* ------------------------------------------------------------*/
  if (EPWREC.EPW-MSG-NO == 0) /* No message to display*/
    EP0DW00.XXX-CURR-PAGE-NBR = 1; /* Page number to display*/
  end

   /* ------------------------------------------------------------*/
   /* Purge any pages from temporary storage and save key fields.*/
   /* ------------------------------------------------------------*/
  EP0DS-TA0050-P(); /* Purge any temporary storage*/

  EP0DW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

   /* ------------------------------------------------------------*/
   /* Select and open the cursor for the set to be formatted.*/
   /* ------------------------------------------------------------*/
  set VSPRCD01 empty; /* Clear row storage area*/

  if (EPWREC.EPW-FROM-MAP-SEL-NUM == 3) /* Apparel*/
    VSPRCD01.PK-GSPRC-SRCE-CD = "ATCH"; /* Table key*/
  else
    VSPRCD01.PK-GSPRC-SRCE-CD = "ATEQ"; /* Attachment for Equip*/
  end

  EP0DS-VSPRCD01-S1(); /* Select set and open cursor*/

   /* ------------------------------------------------------------*/
   /* Fetch and format the data into pages until end of cursor.*/
   /* ------------------------------------------------------------*/
  EP0DW00.XXX-INX-FLD-NBR = 1; /* Display line index*/
  EP0DW00.EP0DW-COLOR-EOF-FL = "N"; /* End of cursor flag*/

  while (EP0DW00.EP0DW-COLOR-EOF-FL == "N") /* Until end of cursor*/

    EP0DS-VSPRCD01-N1(); /* Fetch next row in cursor*/

    if (EP0DW00.EP0DW-COLOR-EOF-FL == "N") /* Not end of cursor*/

      /* ------------------------------------------------------------*/
      /* Save the data from the table row in display storage.*/
      /* ------------------------------------------------------------*/
      EP0DW01.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] = VSPRCD01.PK-GOAL-SPRC-CD; /* Hanger style*/

      if (EPWREC.EPW-FROM-MAP-SEL-NUM == 3) /* Apparel*/
        EP0DW01.XSUCD[XXX-INX-FLD-NBR] = VSPRCD01.PK-SPCL-PROC-VAR1; /* Sil cd*/
      else /* equipment*/
        EP0DW01.XPCCD[XXX-INX-FLD-NBR] = VSPRCD01.PK-SPCL-PROC-VAR1; /* Prod cat cd*/
      end

      EP0DW00.PK-SPCL-PROC-VAR2 = VSPRCD01.PK-SPCL-PROC-VAR2;
      EP0DW01.EP0DW-CU-CO-ID[XXX-INX-FLD-NBR] = EP0DW00.PK-SPCL-PROC-VAR2-3-10; /* Company*/
      EP0DW01.EP0DW-INCL-EXCL-FL[XXX-INX-FLD-NBR] = VSPRCD01.PK-SPCL-PROC-VAR3; /* Incl/excl cd*/
      EP0DW01.EP0DW-EP-TEXT[XXX-INX-FLD-NBR] = VSPRCD01.PK-SPCL-PROC-VAR4; /* Text*/
      EP0DW01.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] = VSPRCD01.PK-DCMS-SPRC-CD; /* Dcms cd*/
      EP0DW01.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR] = VSPRCD01.PK-DCMS-SINST-TXT; /* Instructions*/

      EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-FLD-NBR + 1;

      /* ------------------------------------------------------------*/
      /* If the page is full, add the page to the storage queue.*/
      /* ------------------------------------------------------------*/
      if (EP0DW00.XXX-INX-FLD-NBR > EP0DW00.XXX-INX-LMT-NBR) /* Page is full*/

        EP0DS-TA0050-A(); /* Add page to temporary storage*/

        EP0DW00.XXX-INX-FLD-NBR = 1; /* Display line index*/
      end
    end
  end

   /* ------------------------------------------------------------*/
   /* Add any partially formatted last page to the queue.*/
   /* (need empty lines for add's).*/
   /* ------------------------------------------------------------*/
  EP0DS-TA0050-A(); /* Add page to temporary storage*/

   /* ------------------------------------------------------------*/
   /* Close the cursor for the selected set.*/
   /* ------------------------------------------------------------*/
  EP0DS-VSPRCD01-C1(); /* Close selected set cursor*/

   /* ------------------------------------------------------------*/
   /* If no pages were formatted, indicate no data to display.*/
   /* ------------------------------------------------------------*/
  if (EP0DW00.XXX-HI-PAGE-NBR == 0) /* No pages formatted*/
    EPWREC.XXXERRID = "1000"; /* Message location*/
    converseLib.validationFailed(12); /* No data was found to display*/

    exit stack; /* Exit to display map*/
  end

   /* ------------------------------------------------------------*/
   /* Indicate that the request for data is complete.*/
   /* ------------------------------------------------------------*/
  EP0DW00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/


end // end EP0D-1000

// Format and converse the map
Function EP0D-2000()
   /* ------------------------------------------------------------*/
   /* If an error was found, save the error message number.*/
   /* ------------------------------------------------------------*/
  if (converseVar.validationMsgNum != 0) /* Error was found*/
    EPWREC.EPW-MSG-NO = converseVar.validationMsgNum; /* Message number*/
  end

   /* ------------------------------------------------------------*/
   /* If the map must be formatted, format the data in the map.*/
   /* ------------------------------------------------------------*/
  if (EP0DW00.XXX-MAP-FMAT-FL == "Y") /* Map must be formatted*/
    EP0D-2100(); /* Format the data in the map*/
  end

   /* ------------------------------------------------------------*/
   /* Format the current date and any FAST PATH information.*/
   /* ------------------------------------------------------------*/
  EP0DM001.XXX-CURR-DT = VGVar.currentShortGregorianDate; /* Current date*/

   /* ------------------------------------------------------------*/
   /* Format the main heading & column headings*/
   /* ------------------------------------------------------------*/
  if (EPWREC.EPW-FROM-MAP-SEL-NUM == 3) /* Apparel*/
    EP0DM001.EP0DM-HEAD-VAR = "(APPAREL)";
    EP0DM001.EP0DM-COLM-HEAD-VAR1 = "SILH";
    EP0DM001.EP0DM-COLM-HEAD-VAR2 = "CODE;";
  else /* sel num = 4*/
    EP0DM001.EP0DM-HEAD-VAR = "(EQUIPMENT)";
    EP0DM001.EP0DM-COLM-HEAD-VAR1 = "PROD";
    EP0DM001.EP0DM-COLM-HEAD-VAR2 = "CAT; ";
  end

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/
  if (EPWREC.EPW-MSG-NO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (EPWREC.EPW-MSG-NO != 0) /* Message to display*/
      if (EPWREC.EPW-MSG-NO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(EPWREC.EPW-MSG-NO);
      end /* Message number*/
    else
      EP0DM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/
    end
    EP0DM001.XXX-MSG-LOC-ID = EPWREC.XXXERRID;
    set EP0DM001 alarm; /* Indicate sound alarm*/
  end

  converse EP0DM001 ;

   /* ------------------------------------------------------------*/
   /* Clear the message fields and save FAST PATH information.*/
   /* ------------------------------------------------------------*/
  EPWREC.XXXERRID = " "; /* Message location*/
  EPWREC.EPW-MSG-NO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/

   /* ------------------------------------------------------------*/
   /* Reset modifiable field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/
  EP0DW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (EP0DW00.XXX-INX-FLD-NBR <= EP0DW00.XXX-INX-LMT-NBR)
                                   /* Until end of screen*/
                                   /* or end of formatted lines*/
    if (EP0DM001.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] > " "
     || EP0DM001.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] > " ")
      set EP0DM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] normal; /* Sel ind*/
      set EP0DM001.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] normal;
      set EP0DM001.XSUCD[XXX-INX-FLD-NBR] normal;
      set EP0DM001.EP0DM-CU-CO-ID[XXX-INX-FLD-NBR] normal;
      set EP0DM001.EP0DM-INCL-EXCL-FL[XXX-INX-FLD-NBR] normal;
      set EP0DM001.EP0DM-EP-TEXT[XXX-INX-FLD-NBR] normal;
      set EP0DM001.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] normal;
      set EP0DM001.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR] normal;
    end

    EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-FLD-NBR + 1;
  end


end // end EP0D-2000

// Format the data in the map
Function EP0D-2100()
   /* ------------------------------------------------------------*/
   /* Clear the fields in the map storage area to be formatted.*/
   /* ------------------------------------------------------------*/
  set EP0DM001 initial; /* M001 input/output area*/

  EP0DW00.XXX-MAP-FMAT-FL = "N"; /* Format the map flag*/

   /* ------------------------------------------------------------*/
   /* If the page in storage is not the page to be displayed,*/
   /* obtain the desired page from temporary storage.*/
   /* ------------------------------------------------------------*/
  if (EP0DW00.XXX-HI-PAGE-NBR > 0) /* Pages were formatted*/

    if (EP0DW00.XXX-CURR-PAGE-NBR != TA5REC.TA5TSQIX)
                                   /* Desired page not current*/
      /* ------------------------------------------------------------*/
      /* If the current page has been changed, update the page in*/
      /* storage before obtaining the next one to display.*/
      /* ------------------------------------------------------------*/
      if (EP0DW00.XXX-PAGE-UPD-FL == "Y") /* Current page was changed*/

        EP0DS-TA0050-U(); /* Update temporary storage page*/

        EP0DW00.XXX-PAGE-UPD-FL = "N"; /* Page update flag*/
      end

      /* ------------------------------------------------------------*/
      /* If the desired page number is greater than the highest page*/
      /* in storage, default to the highest page number.*/
      /* ------------------------------------------------------------*/
      if (EP0DW00.XXX-CURR-PAGE-NBR > EP0DW00.XXX-HI-PAGE-NBR)
                                   /* Desired greater highest*/
        EP0DW00.XXX-CURR-PAGE-NBR = EP0DW00.XXX-HI-PAGE-NBR;
                                   /* Default to highest*/
      end

      EP0DS-TA0050-R(); /* Read temporary storage page*/
    end

    /* ------------------------------------------------------------*/
    /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/
    EP0DM001.XXX-CURR-PAGE-NBR = EP0DW00.XXX-CURR-PAGE-NBR;
                                   /* Current page*/
    EP0DM001.XXX-HI-PAGE-NBR = EP0DW00.XXX-HI-PAGE-NBR;
                                   /* Highest page*/
  end

   /* ------------------------------------------------------------*/
   /* Format the map fields from the display storage area.*/
   /* ------------------------------------------------------------*/
  EP0DW00.XXX-INX-FLD-NBR = 1; /* Initialize line index*/

  while (EP0DW00.XXX-INX-FLD-NBR <= EP0DW00.XXX-INX-LMT-NBR)
                                   /* Until screen is full*/
    if (EP0DW01.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] > " "
     || EP0DW01.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] > " ")
      if (EP0DW00.XXX-INX-FLD-NBR == 1) /* First select field*/
        set EP0DM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] cursor, normal;
      else
        set EP0DM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] normal;
      end
    end

    EP0DM001.XXX-LN-SEL-IND[XXX-INX-FLD-NBR] = EP0DW01.XXX-LN-SEL-IND[XXX-INX-FLD-NBR];
    EP0DM001.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] = EP0DW01.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR]; /* Hanger style*/

    /* <----  MULTGPC (BEGIN)  ---->*/
    /* NOTE:  The map field name still remains as XSUCD*/
    if (EPWREC.EPW-FROM-MAP-SEL-NUM == 3) /* Apparel*/
      EP0DM001.XSUCD[XXX-INX-FLD-NBR] = EP0DW01.XSUCD[XXX-INX-FLD-NBR]; /* Sil cd*/
    else
      EP0DM001.XSUCD[XXX-INX-FLD-NBR] = EP0DW01.XPCCD[XXX-INX-FLD-NBR]; /* Sil cd*/
    end
    /* <----  MULTGPC (END)  ---->*/

    EP0DM001.EP0DM-CU-CO-ID[XXX-INX-FLD-NBR] = EP0DW01.EP0DW-CU-CO-ID[XXX-INX-FLD-NBR]; /* Company*/
    EP0DM001.EP0DM-INCL-EXCL-FL[XXX-INX-FLD-NBR] = EP0DW01.EP0DW-INCL-EXCL-FL[XXX-INX-FLD-NBR]; /* Incl/excl cd*/
    EP0DM001.EP0DM-EP-TEXT[XXX-INX-FLD-NBR] = EP0DW01.EP0DW-EP-TEXT[XXX-INX-FLD-NBR]; /* Text*/
    EP0DM001.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR] = EP0DW01.PK-DCMS-SPRC-CD[XXX-INX-FLD-NBR]; /* Dcms cd*/
    EP0DM001.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR] = EP0DW01.PK-DCMS-SINST-TXT[XXX-INX-FLD-NBR]; /* Instructions*/

    EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-FLD-NBR + 1;
                                   /* Increment line index*/
  end


end // end EP0D-2100

// Check attention identifier
Function EP0D-3000()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pa2) /* Pa2 was pressed*/
    if (EP0DW00.XXX-RQT-DATA-FL == "Y") /* Request for data pending*/
      set EP0DW01 empty; /* M001 display storage*/
    end

    EP0DW00.XXX-RQT-DATA-FL = "N"; /* Request for data flag*/
    EP0DW00.XXX-MAP-FMAT-FL = "Y"; /* Format the map flag*/

    exit stack; /* Exit to display map*/
  end

   /* ------------------------------------------------------------*/
   /* If a PF key transfer requested, exit to another application.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf2 /* or PF2 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4) /* or PF4 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "EP0DM001"; /* Application find code*/
    end

    if (converseVar.eventKey is pf2) /* PF2 was pressed*/
      COMMAREA.CATOAP = "EP0C"; /* Update spec processing cd*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "EP0A"; /* System Menu*/
    end

    if (converseVar.eventKey is pf4) /* PF4 was pressed*/
      COMMAREA.CATOAP = "MN0M"; /* GOAL   Menu*/
    end

    EP0DS-TA0050-P(); /* Purge any temporary storage*/

    EP0DS-EXIT(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf9   /* and PF9 was not pressed*/
   && converseVar.eventKey not pf11  /* and PF11 was not pressed*/
   && converseVar.eventKey not pf12) /* and PF12 was not pressed*/

    EPWREC.XXXERRID = "3000"; /* Message location*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end EP0D-3000

// Verify data that was entered
Function EP0D-4000()
   /* ------------------------------------------------------------*/
   /* If scroll requested, determine if any other field changed.*/
   /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8 /* or scroll forward request*/
   || converseVar.eventKey is pf12) /* or delete request*/

    EP0D-4100(); /* Highlight other changed fields*/
  end

    /* ------------------------------------------------------------*/
    /* Determine whether a valid request to scroll was entered.*/
    /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    EP0D-4200(); /* Verify scroll request*/
  end /* ;*/

    /* ------------------------------------------------------------*/
    /* If key field changed, determine if any other field changed.*/
    /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf12) /* Delete request*/

    EP0D-4400(); /* Verify delete request*/
  end

      /* ------------------------------------------------------------*/
      /* Check for pop up window.*/
      /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf9) /* Pop-up request*/

    EP0D-4500(); /* Do pop-up request*/
  end


      /* ------------------------------------------------------------*/
      /* <enter>, verify any modifiable data that was changed.*/
      /* ------------------------------------------------------------*/
  if (converseVar.eventKey is enter) /* <enter> - change request*/
    EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-LMT-NBR;

    while (EP0DW00.XXX-INX-FLD-NBR > 0) /* Until end of lines*/

      EP0D-4600(); /* Verify data in the line*/

      EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-FLD-NBR - 1;
    end
  end

   /* ------------------------------------------------------------*/
   /* If any error was found, exit to display the map.*/
   /* ------------------------------------------------------------*/
  if (converseVar.validationMsgNum != 0) /* Error was found*/
    exit stack; /* Exit to display map*/
  end


end // end EP0D-4000

// Highlight other changed fields
Function EP0D-4100()
   /* ------------------------------------------------------------*/
   /* If a modifiable field was changed, indicate that field may*/
   /* not be changed when key is changed or a pf key is pressed.*/
   /* ------------------------------------------------------------*/
  EP0DW00.XXX-INX-FLD-NBR = EP0DW00.XXX-INX-LMT-NBR;

  while (EP0DW00.XXX-INX-FLD-NBR > 0) /* Until end of lines*/

    if (EP0DM001.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] != 
    EP0DW01.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR]) /* Goal code*/
      EPWREC.XXXERRID = "4100"; /* Message location*/
      converseLib.validationFailed(61); /* Update valid only with <enter>*/
      set EP0DM001.PK-GOAL-SPRC-CD[XXX-INX-FLD-NBR] cursor, bold;
    end

