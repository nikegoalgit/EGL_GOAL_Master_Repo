package sr0k;
//*** DATATABLE=SR0KT ****
// This table is used for decoding shiprec batch statuses
// ************************
DataTable SR0KT type basicTable  {
  shared = yes, resident = no
  }
  10 SR0KTST SR0KTST; 
  10 SR0KTDSC SR0KTDSC; 
   {contents = [
    ["E ", "ENTERED "],
    ["P ", "PREPARED"],
    ["B ", "BALANCED"],
    ["R ", "RELEASED"],
    ["H ", "ON HOLD "]
  ]}
end // end SR0KT

