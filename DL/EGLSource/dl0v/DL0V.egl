// SET MAP 1 FIELD ATTRIBUTES
Function DL0VP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DL0VM001.DAW-PAGE-LIT = "PAGE";
    set DL0VM001.DAW-CURR-PAGE initialAttributes;
    DL0VM001.DAW-OF-LIT = "OF";
    set DL0VM001.DAW-LAST-PAGE initialAttributes;
    DL0VM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DL0VM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DL0VM001.DAW-PAGE-LIT = " ";
    set DL0VM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DL0VM001.DAW-OF-LIT = " ";
    set DL0VM001.DAW-LAST-PAGE skip, invisible;
    DL0VM001.DAW-PF7-LIT = " ";
    DL0VM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DL0VM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DL0VM001.XGPCD[sysVar.arrayIndex] == " ")
      set DL0VM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DL0VM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DL0VP25 **************************/
  /* **************/
end // end DL0VP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DL0VP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DL0VM002.XGPCD initialAttributes;
    set DL0VM002.XXX-ADDR1 initialAttributes;
    set DL0VM002.XXX-ADDR2 initialAttributes;
    set DL0VM002.XXX-ADDR3 initialAttributes;
    set DL0VM002.XXXCITY initialAttributes;
    set DL0VM002.XSTCD initialAttributes;
    set DL0VM002.XXXZIP initialAttributes;
    set DL0VM002.XXXPHN initialAttributes;
    set DL0VM002.XXXTWX initialAttributes;
    set DL0VM002.XXXFAX initialAttributes;
    set DL0VM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DL0VM002.XGPCD skip;
    set DL0VM002.XXX-ADDR1 skip;
    set DL0VM002.XXX-ADDR2 skip;
    set DL0VM002.XXX-ADDR3 skip;
    set DL0VM002.XXXCITY skip;
    set DL0VM002.XSTCD skip;
    set DL0VM002.XXXZIP skip;
    set DL0VM002.XXXPHN skip;
    set DL0VM002.XXXTWX skip;
    set DL0VM002.XXXFAX skip;
    set DL0VM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DL0VM002.XGPCD skip;
    set DL0VM002.XXX-ADDR1 initialAttributes;
    set DL0VM002.XXX-ADDR2 initialAttributes;
    set DL0VM002.XXX-ADDR3 initialAttributes;
    set DL0VM002.XXXCITY initialAttributes;
    set DL0VM002.XSTCD initialAttributes;
    set DL0VM002.XXXZIP initialAttributes;
    set DL0VM002.XXXPHN initialAttributes;
    set DL0VM002.XXXTWX initialAttributes;
    set DL0VM002.XXXFAX initialAttributes;
    set DL0VM002.XXXSTAT initialAttributes;
  end

  set DL0VM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DL0VM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DL0VM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DL0VM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DL0VM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DL0VM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DL0VM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
