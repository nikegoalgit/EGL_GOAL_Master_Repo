// NIKE TABLE HANDLER - TABLE MAINTENANCE - PO BUYER ADDRESS
// 
// OVERVIEW: This application is the maintenance application
//           for the PO buyer address table
// 
// DB2 TBL: DCORP.VXPO0001
// CSP TBL: XPOTBL
// 
// DATE CREATED: 09/09/98
//       AUTHOR: Eric Chan
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   03-18-99  ECHAN       ADD NOHILITE IN DL0VP25-SET-ATTR-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DL0VP70-PROC-MAP-2
// *********************
Program DL0V type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DL0VW01 DL0VW01; // record
  DL0VW02 DL0VW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XGPTBL {deleteAfterUse = yes}; // table
  use XPOTBL {deleteAfterUse = yes}; // table
  use XSTTBL {deleteAfterUse = yes}; // table
  use DL0VM.DL0VM001,  // forms
      DL0VM.DL0VM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DL0VP00-DRIVER: DL0VP00-DRIVER();
    exit program;
  end // end main
end // end DL0V

// DRIVER FOR APPLICATION
Function DL0VP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DL0VP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DL0VP20-BUILD-MAP2(); /* Build the second map*/
  else
    DL0VP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DL0VP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DL0VP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DL0VP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DL0VP00 *******************************/
  /* **************/
end // end DL0VP00-DRIVER

// INITIALIZE THE APPLICATION
Function DL0VP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DL0VW01 empty; /* Working storage record*/
  set DL0VW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DL0VM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DL0VM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DL0VM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DL0VM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DL0VM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DL0VM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DL0VM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DL0VP10 ******************************/
  /* **************/
end // end DL0VP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DL0VP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DL0VP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DL0VM001.DAW-CURR-PAGE)
    DL0VS-TS-READ-PAGE(); /* read temp storage page*/
    DL0VP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DL0VP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DL0VP20 **********************************/
  /* **************/
end // end DL0VP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DL0VP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DL0VM002.DA-CHNG-USER-ID = sysVar.userID;
  DL0VM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DL0VW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DL0VM002.XGPABRV = " ";
  DL0VM002.XSTDESC = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DL0VM002.DA-PLOG-TXT1 = " ";
    DL0VM002.DA-PLOG-TXT2 = " ";
    DL0VM002.XGPCD = " ";
    DL0VM002.XXX-ADDR1 = " ";
    DL0VM002.XXX-ADDR2 = " ";
    DL0VM002.XXX-ADDR3 = " ";
    DL0VM002.XXXCITY = " ";
    DL0VM002.XSTCD = " ";
    DL0VM002.XXXZIP = " ";
    DL0VM002.XXXPHN = " ";
    DL0VM002.XXXTWX = " ";
    DL0VM002.XXXFAX = " ";
    DL0VM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DL0VW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DL0VW01.XPOGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DL0VW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DL0VW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DL0VM002.XGPCD = DL0VW01.XGPCD; /* move to the map*/
      DL0VM002.XXX-ADDR1 = DL0VW01.XXX-ADDR1;
      DL0VM002.XXX-ADDR2 = DL0VW01.XXX-ADDR2;
      DL0VM002.XXX-ADDR3 = DL0VW01.XXX-ADDR3;
      DL0VM002.XXXCITY = DL0VW01.XXXCITY;
      DL0VM002.XSTCD = DL0VW01.XSTCD;
      DL0VM002.XXXZIP = DL0VW01.XXXZIP;
      DL0VM002.XXXPHN = DL0VW01.XXXPHN;
      DL0VM002.XXXTWX = DL0VW01.XXXTWX;
      DL0VM002.XXXFAX = DL0VW01.XXXFAX;
      DL0VM002.XXXSTAT = DL0VW01.XXXSTAT;
      DL0VM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DL0VM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DL0VM002.XGPCD = DL0VW02.XGPCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DL0VM002.XGPCD in XPOTBL.XGPCD)
          DL0VW01.XGPCD = XPOTBL.XGPCD[sysVar.arrayIndex]; /* save for a later*/
          DL0VW01.XXX-ADDR1 = XPOTBL.XXX-ADDR1[sysVar.arrayIndex]; /* compare*/
          DL0VW01.XXX-ADDR2 = XPOTBL.XXX-ADDR2[sysVar.arrayIndex];
          DL0VW01.XXX-ADDR3 = XPOTBL.XXX-ADDR3[sysVar.arrayIndex];
          DL0VW01.XXXCITY = XPOTBL.XXXCITY[sysVar.arrayIndex];
          DL0VW01.XSTCD = XPOTBL.XSTCD[sysVar.arrayIndex];
          DL0VW01.XXXZIP = XPOTBL.XXXZIP[sysVar.arrayIndex];
          DL0VW01.XXXPHN = XPOTBL.XXXPHN[sysVar.arrayIndex];
          DL0VW01.XXXTWX = XPOTBL.XXXTWX[sysVar.arrayIndex];
          DL0VW01.XXXFAX = XPOTBL.XXXFAX[sysVar.arrayIndex];
          DL0VW01.XXXSTAT = XPOTBL.XXXSTAT[sysVar.arrayIndex];
          DL0VW01.ZZZSDT = XPOTBL.ZZZSDT[sysVar.arrayIndex];
          DL0VW01.ZZZCHGDT = XPOTBL.ZZZCHGDT[sysVar.arrayIndex];
          DL0VW01.ZZZCHGTM = XPOTBL.ZZZCHGTM[sysVar.arrayIndex];
          DL0VW01.ZZZCHGCT = XPOTBL.ZZZCHGCT[sysVar.arrayIndex];

          DL0VM002.XXX-ADDR1 = XPOTBL.XXX-ADDR1[sysVar.arrayIndex]; /* move to map*/
          DL0VM002.XXX-ADDR2 = XPOTBL.XXX-ADDR2[sysVar.arrayIndex];
          DL0VM002.XXX-ADDR3 = XPOTBL.XXX-ADDR3[sysVar.arrayIndex];
          DL0VM002.XXXCITY = XPOTBL.XXXCITY[sysVar.arrayIndex];
          DL0VM002.XSTCD = XPOTBL.XSTCD[sysVar.arrayIndex];
          DL0VM002.XXXZIP = XPOTBL.XXXZIP[sysVar.arrayIndex];
          DL0VM002.XXXPHN = XPOTBL.XXXPHN[sysVar.arrayIndex];
          DL0VM002.XXXTWX = XPOTBL.XXXTWX[sysVar.arrayIndex];
          DL0VM002.XXXFAX = XPOTBL.XXXFAX[sysVar.arrayIndex];
          DL0VM002.XXXSTAT = XPOTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DL0VW01.XPOGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DL0VW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DL0VW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DL0VM002.XGPCD = DL0VW01.XGPCD; /* move to the map*/
    DL0VM002.XXX-ADDR1 = DL0VW01.XXX-ADDR1;
    DL0VM002.XXX-ADDR2 = DL0VW01.XXX-ADDR2;
    DL0VM002.XXX-ADDR3 = DL0VW01.XXX-ADDR3;
    DL0VM002.XXXCITY = DL0VW01.XXXCITY;
    DL0VM002.XSTCD = DL0VW01.XSTCD;
    DL0VM002.XXXZIP = DL0VW01.XXXZIP;
    DL0VM002.XXXPHN = DL0VW01.XXXPHN;
    DL0VM002.XXXTWX = DL0VW01.XXXTWX;
    DL0VM002.XXXFAX = DL0VW01.XXXFAX;
    DL0VM002.XXXSTAT = DL0VW01.XXXSTAT;
    DL0VM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DL0VM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DL0VM002.XXXSTAT == "A")
    DL0VM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DL0VM002.XXXSTAT == "I")
    DL0VM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DL0VM002.XXXSTAT == " ")
    DL0VM002.DAW-TBL-STATUS-DESC = " ";
  end

  DL0VP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DL0VP20 **********************************/
  /* **************/
end // end DL0VP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DL0VP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DL0VW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DL0VP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DL0VS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XPOTBL.XGPCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DL0VP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DL0VS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DL0VS-TS-ADD-PAGE();
    end

    DL0VM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DL0VM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DL0VP22 **********************************/
  /* **************/
end // end DL0VP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DL0VP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DL0VW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DL0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DL0VW01.XPOGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DL0VW02.XGPCD[DAW-ROW-NBR] = DL0VW01.XGPCD;
  DL0VW02.XXX-ADDR1[DAW-ROW-NBR] = DL0VW01.XXX-ADDR1;
  DL0VW02.XXXSTAT[DAW-ROW-NBR] = DL0VW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DL0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DL0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DL0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DL0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DL0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DL0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DL0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DL0VW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DL0VW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DL0VW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DL0VP221 ******************************/
  /* ***************/
end // end DL0VP23-DB2-TO-TSQ

