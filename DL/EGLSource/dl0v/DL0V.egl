// BUILD NEW VDATRN01 RECORD
Function DL0VP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DL0VM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DL0VM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DL0VW01.DA-CCAT-KEY-TXT = " ";
  DL0VW01.DL0VW-KEY-XGPCD = DL0VM002.XGPCD;
  VDATRN01.DA-CCAT-KEY-TXT = DL0VW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DL0VW01.XGPCD = DL0VM002.XGPCD;
  DL0VW01.XXX-ADDR1 = DL0VM002.XXX-ADDR1;
  DL0VW01.XXX-ADDR2 = DL0VM002.XXX-ADDR2;
  DL0VW01.XXX-ADDR3 = DL0VM002.XXX-ADDR3;
  DL0VW01.XXXCITY = DL0VM002.XXXCITY;
  DL0VW01.XSTCD = DL0VM002.XSTCD;
  DL0VW01.XXXZIP = DL0VM002.XXXZIP;
  DL0VW01.XXXPHN = DL0VM002.XXXPHN;
  DL0VW01.XXXTWX = DL0VM002.XXXTWX;
  DL0VW01.XXXFAX = DL0VM002.XXXFAX;
  DL0VW01.XXXSTAT = DL0VM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 208;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DL0VW01.XPOGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DL0VP72 **********************************/
  /* **************/
end // end DL0VP72-NEW-TRAN

