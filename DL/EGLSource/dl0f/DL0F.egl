// SET MAP 1 FIELD ATTRIBUTES
Function DL0FP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DL0FM001.DAW-PAGE-LIT = "PAGE";
    set DL0FM001.DAW-CURR-PAGE initialAttributes;
    DL0FM001.DAW-OF-LIT = "OF";
    set DL0FM001.DAW-LAST-PAGE initialAttributes;
    DL0FM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DL0FM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DL0FM001.DAW-PAGE-LIT = " ";
    set DL0FM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DL0FM001.DAW-OF-LIT = " ";
    set DL0FM001.DAW-LAST-PAGE skip, invisible;
    DL0FM001.DAW-PF7-LIT = " ";
    DL0FM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DL0FM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DL0FM001.XROPDEST[sysVar.arrayIndex] == " ")
      set DL0FM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DL0FM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DL0FP25 **************************/
  /* **************/
end // end DL0FP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DL0FP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DL0FM002.XROPDEST initialAttributes;
    set DL0FM002.XRQUEUE initialAttributes;
    set DL0FM002.XRQCODE initialAttributes;
    set DL0FM002.XRQDESC initialAttributes;
    set DL0FM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DL0FM002.XROPDEST skip;
    set DL0FM002.XRQUEUE skip;
    set DL0FM002.XRQCODE skip;
    set DL0FM002.XRQDESC skip;
    set DL0FM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DL0FM002.XROPDEST skip;
    set DL0FM002.XRQUEUE skip;
    set DL0FM002.XRQCODE initialAttributes;
    set DL0FM002.XRQDESC initialAttributes;
    set DL0FM002.XXXSTAT initialAttributes;
  end

  set DL0FM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DL0FM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DL0FM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DL0FM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DL0FM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DL0FM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DL0FM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DL0FM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DL0FP25 **************************/
  /* **************/
end // end DL0FP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DL0FP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DL0FP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DL0FM001.VAGen_EZEMSG = " ";
  DL0FM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DL0FP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DL0FP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DL0FP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DL0FP70-PROC-MAP1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DL0FP30 *****************************/
  /* **************/
end // end DL0FP30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DL0FP30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DL0FP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DL0FM002.VAGen_EZEMSG = " ";
  DL0FM002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DL0FP25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DL0FP50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DL0FP60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DL0FP70-PROC-MAP2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DL0FP30 *****************************/
  /* **************/
end // end DL0FP30-MAIN-MAP-2

