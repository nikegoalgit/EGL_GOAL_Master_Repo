  if (DL0FW01.XROPDEST == DL0FM002.XROPDEST
   && DL0FW01.XRQUEUE == DL0FM002.XRQUEUE
   && DL0FW01.XRQCODE == DL0FM002.XRQCODE
   && DL0FW01.XRQDESC == DL0FM002.XRQDESC
   && DL0FW01.XXXSTAT == DL0FM002.XXXSTAT
   && DL0FW01.DA-PLOG-TXT1 == DL0FM002.DA-PLOG-TXT1
   && DL0FW01.DA-PLOG-TXT2 == DL0FM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
  /* ***************** END DL0FP71 **********************************/
  /* **************/
end // end DL0FP71-COMPARE

