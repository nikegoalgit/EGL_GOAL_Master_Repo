    DL0FM001.DAW-STATUS-DESC[sysVar.arrayIndex] = DL0FW02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DL0FM001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DL0FW02.DA-CHNG-USER-ID[sysVar.arrayIndex];
