//   NIKE TABLE HANDLER - TABLE MAINTENANCE - REMOTE VS VAX
//                                            PRINTER QUEUE
//   OVERVIEW: This application is used to maintain the force
//             remote vs VAX printer queue table
// 
//   DB2 TABLE: DCORP.VXRQ0001
//   CSP TABLE: XRQTBL
// 
//  DATE CREATED: 08/04/98
//        AUTHOR: Eric Chan
// 
//  MAINTENANCE HISTORY:
// 
//      date     by whom     overview of changes made
//    --------  ----------  -----------------------------------
//    03-16-99  ECHAN       ADD NOHILITE IN DL0FP25-SET-ATTR-1
//                          CHG TRAMSACTION TO TRANSACTION IN
//                          DL0FP70-PROC-MAP-1
//                          CHECK DUPLICATE AND DEADLOCK IN
//                          DL0FP70-PROC-MAP-2
// *********************
Program DL0F type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DL0FW01 DL0FW01; // record
  DL0FW02 DL0FW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XRQTBL {deleteAfterUse = yes}; // table
  use DL0FM.DL0FM001,  // forms
      DL0FM.DL0FM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DL0FP00-DRIVER: DL0FP00-DRIVER();
    exit program;
  end // end main
end // end DL0F

// DRIVER FOR APPLICATION
Function DL0FP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DL0FP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DL0FP20-BUILD-MAP2(); /* Build the second map*/
  else
    DL0FP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DL0FP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DL0FP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DL0FP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DL0FP00 *******************************/
  /* **************/
end // end DL0FP00-DRIVER

// INITIALIZE THE APPLICATION
Function DL0FP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DL0FW01 empty; /* Working storage record*/
  set DL0FW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DL0FM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DL0FM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DL0FM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DL0FM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DL0FM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DL0FM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DL0FM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DL0FP10 ******************************/
  /* **************/
end // end DL0FP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DL0FP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DL0FP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DL0FM001.DAW-CURR-PAGE)
    DL0FS-TS-READ-PAGE(); /* read temp storage page*/
    DL0FP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DL0FP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DL0FP20 **********************************/
  /* **************/
end // end DL0FP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DL0FP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DL0FM002.DA-CHNG-USER-ID = sysVar.userID;
  DL0FM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DL0FW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DL0FM002.DA-PLOG-TXT1 = " ";
    DL0FM002.DA-PLOG-TXT2 = " ";
    DL0FM002.XROPDEST = " ";
    DL0FM002.XRQUEUE = " ";
    DL0FM002.XRQCODE = " ";
    DL0FM002.XRQDESC = " ";
    DL0FM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DL0FW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DL0FW01.XRQGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DL0FW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DL0FW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DL0FM002.XROPDEST = DL0FW01.XROPDEST; /* move to the map*/
      DL0FM002.XRQUEUE = DL0FW01.XRQUEUE;
      DL0FM002.XRQCODE = DL0FW01.XRQCODE;
      DL0FM002.XRQDESC = DL0FW01.XRQDESC;
      DL0FM002.XXXSTAT = DL0FW01.XXXSTAT;
      DL0FM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DL0FM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DL0FM002.XROPDEST = DL0FW02.XROPDEST[DAW-ROW-NBR];
        DL0FM002.XRQUEUE = DL0FW02.XRQUEUE[DAW-ROW-NBR];
                                   /* get the tbl row*/
        DL0FW01.DL0FW-FND-FL = "N";
        sysVar.arrayIndex = 1;
        while (DL0FW01.DL0FW-FND-FL != "Y"
         && XRQTBL.XROPDEST[sysVar.arrayIndex] != "EOT") /* until last row*/
          if (XRQTBL.XROPDEST[sysVar.arrayIndex] == DL0FM002.XROPDEST
           && XRQTBL.XRQUEUE[sysVar.arrayIndex] == DL0FM002.XRQUEUE)
            DL0FW01.DL0FW-FND-FL = "Y";
          else
            sysVar.arrayIndex = sysVar.arrayIndex + 1;
          end
        end

        if (DL0FW01.DL0FW-FND-FL == "Y")
          DL0FW01.XROPDEST = XRQTBL.XROPDEST[sysVar.arrayIndex]; /* save for a*/
          DL0FW01.XRQUEUE = XRQTBL.XRQUEUE[sysVar.arrayIndex]; /* later compare*/
          DL0FW01.XRQCODE = XRQTBL.XRQCODE[sysVar.arrayIndex];
          DL0FW01.XRQDESC = XRQTBL.XRQDESC[sysVar.arrayIndex];
          DL0FW01.XXXSTAT = XRQTBL.XXXSTAT[sysVar.arrayIndex];
          DL0FW01.ZZZSDT = XRQTBL.ZZZSDT[sysVar.arrayIndex];
          DL0FW01.ZZZCHGDT = XRQTBL.ZZZCHGDT[sysVar.arrayIndex];
          DL0FW01.ZZZCHGTM = XRQTBL.ZZZCHGTM[sysVar.arrayIndex];
          DL0FW01.ZZZCHGCT = XRQTBL.ZZZCHGCT[sysVar.arrayIndex];

          DL0FM002.XRQUEUE = XRQTBL.XRQUEUE[sysVar.arrayIndex]; /* move to map*/
          DL0FM002.XRQCODE = XRQTBL.XRQCODE[sysVar.arrayIndex];
          DL0FM002.XRQDESC = XRQTBL.XRQDESC[sysVar.arrayIndex];
          DL0FM002.XXXSTAT = XRQTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DL0FW01.XRQGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DL0FW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DL0FW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DL0FM002.XROPDEST = DL0FW01.XROPDEST; /* move to the map*/
    DL0FM002.XRQUEUE = DL0FW01.XRQUEUE;
    DL0FM002.XRQCODE = DL0FW01.XRQCODE;
    DL0FM002.XRQDESC = DL0FW01.XRQDESC;
    DL0FM002.XXXSTAT = DL0FW01.XXXSTAT;
    DL0FM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DL0FM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DL0FM002.XXXSTAT == "A")
    DL0FM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DL0FM002.XXXSTAT == "I")
    DL0FM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DL0FM002.XXXSTAT == " ")
    DL0FM002.DAW-TBL-STATUS-DESC = " ";
  end

  DL0FP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DL0FP20 **********************************/
  /* **************/
end // end DL0FP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DL0FP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DL0FW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DL0FP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DL0FS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XRQTBL.XROPDEST[sysVar.arrayIndex] != "99999999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DL0FP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DL0FS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DL0FS-TS-ADD-PAGE();
    end

    DL0FM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DL0FM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DL0FP22 **********************************/
  /* **************/
end // end DL0FP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DL0FP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DL0FW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DL0FW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DL0FW01.XRQGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DL0FW02.XROPDEST[DAW-ROW-NBR] = DL0FW01.XROPDEST;
  DL0FW02.XRQUEUE[DAW-ROW-NBR] = DL0FW01.XRQUEUE;
  DL0FW02.XRQCODE[DAW-ROW-NBR] = DL0FW01.XRQCODE;
  DL0FW02.XXXSTAT[DAW-ROW-NBR] = DL0FW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DL0FW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DL0FW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DL0FW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DL0FW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DL0FW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DL0FW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DL0FW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DL0FW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DL0FW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DL0FW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DL0FP221 ******************************/
  /* ***************/
end // end DL0FP23-DB2-TO-TSQ

