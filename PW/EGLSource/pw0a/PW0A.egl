    if (PW0AM030.PWCCAVTR[PW0AWNX2] != PW0AW01.PWCCAVTR[PW0AWNDX]
     || PW0AM030.PWCCAVTR[PW0AWNX2] is modified)
      set PW0AM030.PWCCAVTR[PW0AWNX2] modified;
