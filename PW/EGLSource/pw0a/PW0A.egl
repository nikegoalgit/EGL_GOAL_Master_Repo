// check for duplicates
Function PW0A059()

       /* --------------------------------------*/
       /* -       check for duplicates         -*/
       /* --------------------------------------*/






   /* ----- pw0awnx2 is index for added or changed record*/
   /* -----   values 1 to 11*/
   /* ----- pw0awnx3 is index for existing records*/
   /* -----   values 1 to end*/
   /* ----- pw0awndx is index for existing records*/
   /* -----   values 1 to end*/


   /* ----- if record added or changed has spur, carrier*/
   /* ----- and schedule available range 'from & thru'*/
   /* ----- that duplicate other records that exist*/
   /* ----- (on any page), it is an error*/


  PW0AW01.PW0AWNX3 = 1;
  while (PW0AW01.PW0AWNX3 <= PW0AW01.PW0AWMAX)
    PW0AW01.PW0AWFLG = "N"; /* check schedule available range*/
    PW0AW01.PW0AWEMF = "N"; /* error message flag*/


     /* ----- check if record added or changed*/
     /* ----- duplicates an existing record*/


    if (PW0AW01.PW0AWNX3 == PW0AW01.PW0AWNDX
     && PW0AM030.PW0AMACT[PW0AWNX2] == "C")
       /* dont want to look at self when changing info*/
    else
      if (PW0AM030.TF1CARCD[PW0AWNX2] == PW0AW01.TF1CARCD[PW0AWNX3])
        if (PW0AM030.PWCHLDFL[PW0AWNX2] == PW0AW01.PWCHLDFL[PW0AWNX3])
          if (PW0AM030.PKVSSPNB[PW0AWNX2] == PW0AW01.PKVSSPNB[PW0AWNX3])
            if (PW0AM030.PWCCAVFR[PW0AWNX2] == PW0AW01.PWCCAVFR[PW0AWNX3]
             && PW0AM030.PWCCAVTR[PW0AWNX2] == PW0AW01.PWCCAVTR[PW0AWNX3])
              PW0AW01.PW0AWEMF = "Y"; /* error message flag*/
              converseLib.validationFailed(263);
               /* record added or changed already exists*/
            else
              PW0AW01.PW0AWFLG = "Y";
               /* check schedule available range*/
            end
          else /* new spur number not equal existing spur number*/
            PW0AW01.PW0AWFLG = "Y"; /* check schedule available range*/
          end
        end
      end


       /* ----- check if schedule available 'from & thru'*/
       /* ----- falls within the range of an existing record*/

      if (PW0AW01.PW0AWFLG == "Y") /* check schedule available range*/

          /* ; check schedule range (from)*/

        PW0AW01.PW0AWHLD = PW0AW01.PWCCAVFR[PW0AWNX3];
        while (PW0AW01.PW0AWHLD <= PW0AW01.PWCCAVTR[PW0AWNX3])
          if (PW0AW01.PW0AWHLD == PW0AM030.PWCCAVFR[PW0AWNX2])
            PW0AW01.PW0AWHLD = PW0AW01.PWCCAVTR[PW0AWNX3];
            PW0AW01.PW0AWEMF = "Y"; /* error message flag*/
            converseLib.validationFailed(282); /* record exists within schedule range*/
                                   /* ; for carrier & spur, reenter*/
          end
          PW0AW01.PW0AWHLD = PW0AW01.PW0AWHLD + 1;
        end
        if (PW0AW01.PW0AWEMF != "Y") /* error message flag*/
          PW0AW01.PW0AWHLD = PW0AM030.PWCCAVFR[PW0AWNX2];
          while (PW0AW01.PW0AWHLD <= PW0AM030.PWCCAVTR[PW0AWNX2])
            if (PW0AW01.PW0AWHLD == PW0AW01.PWCCAVFR[PW0AWNX3])
              PW0AW01.PW0AWHLD = PW0AM030.PWCCAVTR[PW0AWNX2];
              PW0AW01.PW0AWEMF = "Y"; /* error message flag*/
              converseLib.validationFailed(282); /* record exists within schedule range*/
                                     /* ; for carrier & spur, reenter*/
            end
            PW0AW01.PW0AWHLD = PW0AW01.PW0AWHLD + 1;
          end
        end

          /* ; check schedule range (thru)*/

        if (PW0AW01.PW0AWEMF != "Y") /* error message flag*/
          PW0AW01.PW0AWHLD = PW0AW01.PWCCAVTR[PW0AWNX3];
          while (PW0AW01.PW0AWHLD >= PW0AW01.PWCCAVFR[PW0AWNX3])
            if (PW0AW01.PW0AWHLD == PW0AM030.PWCCAVTR[PW0AWNX2])
              PW0AW01.PW0AWHLD = PW0AW01.PWCCAVFR[PW0AWNX3];
              PW0AW01.PW0AWEMF = "Y"; /* error message flag*/
              converseLib.validationFailed(282); /* record exists within schedule range*/
                                   /* ; for carrier & spur, reenter*/
            end
            PW0AW01.PW0AWHLD = PW0AW01.PW0AWHLD - 1;
          end
          if (PW0AW01.PW0AWEMF != "Y") /* error message flag*/
            PW0AW01.PW0AWHLD = PW0AM030.PWCCAVTR[PW0AWNX2];
            while (PW0AW01.PW0AWHLD >= PW0AM030.PWCCAVFR[PW0AWNX2])
              if (PW0AW01.PW0AWHLD == PW0AW01.PWCCAVTR[PW0AWNX3])
                PW0AW01.PW0AWHLD = PW0AM030.PWCCAVFR[PW0AWNX2];
                PW0AW01.PW0AWEMF = "Y"; /* error message flag*/
                converseLib.validationFailed(282); /* record exists within schedule range*/
                                     /* ; for carrier & spur, reenter*/
              end
              PW0AW01.PW0AWHLD = PW0AW01.PW0AWHLD - 1;
            end
          end
        end
      end




      if (PW0AW01.PW0AWEMF == "Y") /* error message flag*/
        set PW0AM030.TF1CARCD[PW0AWNX2] modified, bold;
        set PW0AM030.PKVSSPNB[PW0AWNX2] cursor, modified, bold;
        set PW0AM030.PWCCAVFR[PW0AWNX2] modified, bold;
        set PW0AM030.PWCCAVTR[PW0AWNX2] modified, bold;
        set PW0AM030.PWCHLDFL[PW0AWNX2] modified, bold;
      end
    end
    PW0AW01.PW0AWNX3 = PW0AW01.PW0AWNX3 + 1;
  end



end // end PW0A059

