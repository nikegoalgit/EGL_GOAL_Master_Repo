    if (PW0AM030.PWCCAVFR[PW0AWNX2] != PW0AW01.PWCCAVFR[PW0AWNDX]
     || PW0AM030.PWCCAVFR[PW0AWNX2] is modified)
      set PW0AM030.PWCCAVFR[PW0AWNX2] modified;
