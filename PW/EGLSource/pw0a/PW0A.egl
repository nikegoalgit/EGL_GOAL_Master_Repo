// 
// 
// 
// 
// 
// 
//   maintenance history:
// 
//    date      name      description
//  --------  ---------  --------------------------------------
//  10-10-87  PRM        created
//  12-07-88  jwood      added NONCSP option on calls
//  12-07-88  jwood      added XSSEGTR logic
//  05-25-89  m walton   allow default pack & hold spur to
//                       equal reject spur
//  06-05-89  m walton   check schedule available range when
//                       add or change a record (PW0A059)
//  06-22-89  jcampb     fix schedule available range when
//                       add or change a record (PW0A059)
//                       which includes the packhold flag.
//  03-19-91  BHARRI     Restrict warehouse 51 from PW0A020
//  04-07-92  SHENDE     Temporary change to allow whse 2 & 4
//                       to return (PF3) to PW0W.
//  04-27-92  SHENDE     Allow whse 4 TO PF3 to PW0W.
//  05-07-92  SHENDE     Take out temp fix for whse '04'.
//  05-11-93  kande1     Restrict whse 02,04,07 from pw0a020
//  08-29-95  Ryonke     Added warehouse specific logic for
//                       Canada warehouse (whse=42).
// Y2KFIX  02/18/98  DKENNY  Y2K MODIFICATIONS MADE TO:
//                            PW0A084, PW0A094
// ************************************************************
// 
// 
// *********************
Program PW0A type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "PK00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  PKWREC PKWREC; // record
  PW0AW01 PW0AW01; // record
  PW0AW02 PW0AW02; // record
  PWCREC PWCREC; // record
  PWDREC PWDREC; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TF1REC TF1REC; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XWHTBL {deleteAfterUse = yes}; // table
  use PW0AM.PW0AM030 // form
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    PW0A005: PW0A005();

    goto PW0A010;



    PW0A010: PW0A010();

    goto PW0A010;


  end // end main
end // end PW0A

// main process
Function PW0A005()

   /* ----------------------------------------------------------*/
   /* initialization process....*/
   /* ----------------------------------------------------------*/






  set PKWREC empty; /* clear work records*/
  set PW0AW01 empty;
  set PW0AW02 empty;
  set SQLCA empty;


  XPPK001(); /* app initial.*/
  PW0AM030.XWHCD = PKWREC.XWHCD;
  if (converseVar.validationMsgNum == 2 /* invalid  warehouse*/
   || converseVar.validationMsgNum == 3 /* inactive warehouse*/
   || converseVar.validationMsgNum == 4) /* warehouse required*/
    set PW0AM030.XWHCD cursor, modified, bold; /* hilite warehouse*/
    PKWREC.XWHCD = " "; /* warehouse*/
  end
  if (converseVar.validationMsgNum == 0) /* valid warehouse*/
    PW0AM030.XWHCD = PKWREC.XWHCD;
    PW0A021(); /* Format map heading area*/
    PW0AW01.PW0AWTOP = 1; /* set top of page subscript*/
    PW0A022(); /* ss carrier avail rows*/
    if (converseVar.validationMsgNum == 0) /* no database errors*/
      PW0A024(); /* ss wilsonville controls rows*/
      if (converseVar.validationMsgNum == 0) /* no database errors*/
        PW0A023(); /* format screen*/
      end
    end
  end





  COMMAREA.CAFROMAP = COMMAREA.CACURRAP; /* save current application*/





  PKWREC.PKWINDEX = 0;
  PKWREC.PKWCNTRL = " ";




end // end PW0A005

// carrier/spur pre-assignment
Function PW0A010()


  PW0A020(); /* format output*/



  PW0A030(); /* converse map*/




   /* *********************************************/
   /* ** if table overflow has occured           **/
   /* **     go back to main selection menu      **/
   /* *********************************************/
  if (PW0AW01.PW0AWFL1 == "Y") /* table overflow flag*/
    converseLib.validationFailed(24);
    PW0A030(); /* converse map*/
    COMMAREA.CATOAP = "PK0S"; /* planning Menu*/
    PKWREC.XWHCD = PW0AM030.XWHCD; /* Save in work area*/
    COMMAREA.CAUSERA = PKWREC.PKWGRPLV; /* COMMAREA user area*/
    XSEXIT(); /* Invoke selected application*/
    PW0AM030.VAGen_EZEMSG = COMMAREA.CAMSG; /* Returned error message*/
    exit stack; /* ; Exit to redisplay map*/
  end





  if (PKWREC.XWHCD == " " /* receiving warehouse from map*/
   && converseVar.eventKey not pf3)
    exit stack;
  end





  PW0AW01.PW0AWUPD = "N"; /* database update flag.*/

  PW0A040(); /* validate pfkeys keys*/




  if (converseVar.validationMsgNum == 0
   && converseVar.eventKey is enter)
    PW0A045(); /* check for fastpath*/
  end




  if (converseVar.validationMsgNum == 0)
    PW0A050(); /* validate inputs*/
  end


  if (converseVar.validationMsgNum == 254) /* warning message, default p&h spur =*/
                                   /* reject spur*/
    PW0A030(); /* converse map to display message*/
    if (PW0AM030.PWDDFPHS != PW0AM030.PWDRJTSS) /* if spurs not =*/
      PW0A050(); /* check new p&h spur for errors*/
    end
  end


  if (converseVar.validationMsgNum == 0) /* ; no errors*/
    if (PKWREC.XWHCD == PW0AM030.XWHCD)
      if (converseVar.eventKey is pf12)
            /* no more processing*/
      else
        if (PW0AW01.PW0AWED1 == "N" /* data modified*/
         && PW0AW01.PW0AWED2 == "N") /* data modified*/
          /* no more processing*/
        else
          if (PW0AW01.PW0AWED1 == "Y") /* edits-all-ok-flag*/
            PW0A080(); /* update databases*/
          end
          if (PW0AW01.PW0AWED2 == "Y") /* edits-all-ok-flag*/
            PW0A090(); /* update databases*/
          end
        end
      end
    end
  end



end // end PW0A010

// Format the output display
Function PW0A020()


   /* ------------------------------------------------------------*/
   /* first time through or a change in warehouse, the map is*/
   /* reformatted with new data.*/
   /* warehouse is highlited if invalid.*/
   /* ------------------------------------------------------------*/





     /* ***********************************/
     /* *  if changed warehouse          **/
     /* *      get warehouse abbrev.     **/
     /* *      if lookup not successful  **/
     /* *         get out                **/
     /* *      else                      **/
     /* *         load new warehouse's   **/
     /* *         data into map and array**/
     /* ***********************************/
  if (PW0AM030.XWHCD == "51" /* If warehouse 51*/
   || PW0AM030.XWHCD == "02" /* or warehouse 02*/
   || PW0AM030.XWHCD == "04" /* or warehouse 04*/
   || PW0AM030.XWHCD == "07" /* or warehouse 07*/
   || PW0AM030.XWHCD == "42") /* or canada*/
    set PW0AM030.XWHCD cursor, modified, bold; /* highlight warehouse*/
    PKWREC.XWHCD = " "; /* Init work rec field*/
    converseLib.validationFailed(370); /* 'application unavailable'*/
    return; /* return*/
  end
                                   /* end if*/
  if (PKWREC.XWHCD != PW0AM030.XWHCD) /* change in warehouse*/
    set PW0AW01 empty;
    PKWREC.XWHCD = PW0AM030.XWHCD; /* warehouse*/
    XPPK002(); /* validate warehouse*/
    if (converseVar.validationMsgNum == 2 /* invalid  warehouse*/
     || converseVar.validationMsgNum == 3 /* inactive warehouse*/
     || converseVar.validationMsgNum == 4) /* warehouse required*/
      set PW0AM030.XWHCD cursor, modified, bold; /* hilite warehouse*/
      PKWREC.XWHCD = " "; /* warehouse*/
      return;
    end
    set PW0AM030.XWHCD normal;
    if (converseVar.validationMsgNum == 0) /* valid warehouse*/
      PW0A021(); /* Format map heading area*/
      PW0AW01.PW0AWTOP = 1; /* set top of page subscript*/
      PW0A022(); /* ss carrier avail rows*/
      if (converseVar.validationMsgNum == 0) /* no database errors*/
        PW0A024(); /* ss wilsonville controls rows*/
        if (converseVar.validationMsgNum == 0) /* no database errors*/
          PW0A023(); /* format screen*/
        end
      end
    end
  end

     /* ***********************************/
     /* *   * database was updated.      **/
     /* *   * refresh the array,         **/
     /* *   * the map, set ezemno        **/
     /* *   * to 'updated' and get out   **/
     /* ***********************************/
  if (PW0AW01.PW0AWUPD == "Y") /* database updated*/
    move PW0AW02 to PW0AW01 withV60Compat; /* initialize data base save area*/
    PW0A022(); /* ss carrier avail*/
    PW0A024(); /* ss wilsonville controls*/
    PW0A021(); /* format headings*/
    PW0A023(); /* format screen*/
    if (converseVar.eventKey not pa2)
      converseLib.validationFailed(32);
    end
    return;
  end




     /* ***********************************/
     /* *   * only paging is required *  **/
     /* *   *                            **/
     /* *   * if at the beginning or end **/
     /* *   * of the map, is's an error. **/
     /* *   * subtract 13 from variable  **/
     /* *   * holding top of map value   **/
     /* *   * when page backward is      **/
     /* *   * requested.                 **/
     /* *   * add 13 to variable         **/
     /* *   * holding top of map value   **/
     /* *   * when page forward is       **/
     /* *   * requested.                 **/
     /* ***********************************/

  if (converseVar.eventKey is pf7) /* page backward*/
    if (PW0AW01.PW0AWTOP == 1)
      converseLib.validationFailed(37);
    end
    if (PW0AW01.PW0AWTOP != 1)
      PW0AW01.PW0AWTOP = PW0AW01.PW0AWTOP - 11; /* set top of page*/
      PW0A021(); /* format map heading*/
      PW0A023(); /* format screen*/
    end
  end

  if (converseVar.eventKey is pf8) /* page forward*/
    PW0AW01.PW0AWTOP = PW0AW01.PW0AWTOP + 11; /* set top of page*/
    if (PW0AW01.PW0AWTOP <= PW0AW01.PW0AWMAX)
      PW0A021(); /* format map heading*/
      PW0A023(); /* format screen*/
    end
    if (PW0AW01.PW0AWTOP > PW0AW01.PW0AWMAX)
      PW0AW01.PW0AWTOP = PW0AW01.PW0AWTOP - 11; /* reset top of page*/
      converseLib.validationFailed(38); /* no more pages to display*/
    end
  end




  if (converseVar.validationMsgNum == 2 /* invalid  warehouse*/
   || converseVar.validationMsgNum == 3 /* inactive warehouse*/
   || converseVar.validationMsgNum == 4) /* warehouse required*/
    set PW0AM030.XWHCD cursor, modified, bold; /* hilite warehouse*/
  end





end // end PW0A020

// format map heading
Function PW0A021()
   /* ---------------------------------------------------*/
      /* format map headings...........*/
   /* ---------------------------------------------------*/


  set PW0AM030 initial; /* clear map area*/
  set PWCREC empty; /* empty carrier availability rec*/
  set PWDREC empty; /* empty wilsonville controls rec*/

  PW0AM030.XXXERRID = "PW0A021"; /* format location*/
  PW0AM030.XXXCURDT = VGVar.currentShortGregorianDate; /* date to map*/
  PW0AM030.XWHCD = PKWREC.XWHCD; /* warehouse code*/
  PW0AM030.XWHABRV = PKWREC.XWHABRV; /* warehouse abbrev.*/
  PW0AM030.XXXRVNBR = "00"; /* warehouse abbrev.*/
  converseLib.validationFailed(0); /* reset any error*/



end // end PW0A021

// obtain set carrier avail
Function PW0A022()
   /* ----------------------------------------------------*/
     /* load working storage with carrier available rows*/
             /* -  planned only   -*/
   /* ----------------------------------------------------*/

  set PWCREC empty; /* empty carrier availability rec*/

  PWCREC.XWHCD = PKWREC.XWHCD;
  PWCREC.PKURUNMD = "P"; /* ; non-planned mode*/

  call "IO2180" ("S1", SQLCA, PWCREC) {isNoRefresh = yes, isExternal = yes}; /* ss on carrier avail*/

  if (SQLCA.VAGen_SQLCODE != 0) /* ; unexpected return*/
    if (SQLCA.VAGen_SQLCODE == -911)
      TA1REC.TA1FUNC = "POST"; /* ; term. function*/
      TA1REC.TA1LOC = "SELECT";
      TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
      TA1REC.TA1PGMNM = "PW0A022"; /* process name*/
      TA1REC.TA1MAP = "PW0AM030"; /* map name*/
      TA1REC.TA1DBASE = "DPICK   "; /* database name*/
      TA1REC.TA1TBLVU = "VCARRA01"; /* table view name*/
      call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
      converseLib.validationFailed(28); /* contention error*/
    else
      if (SQLCA.VAGen_SQLCODE != 100)
        converseLib.validationFailed(29); /* ; indicate system error*/
        TA1REC.TA1LOC = "SELECT";
        TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
        TA1REC.TA1PGMNM = "PW0A022"; /* process name*/
        TA1REC.TA1MAP = "PW0AM030"; /* map name*/
        TA1REC.TA1DBASE = "DPICK   "; /* database name*/
        TA1REC.TA1TBLVU = "VCARRA01"; /* table view name*/
        TA1REC.TA1FUNC = "RETN"; /* ; term. function*/
        call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
        return;
      end
    end
  end



  if (SQLCA.VAGen_SQLCODE == 0) /* ; good return from s1 above*/
    PW0AW01.PW0AWMAX = 0; /* ; records read in counter*/
    while (SQLCA.VAGen_SQLCODE == 0 /* ; good return within loop*/
     && PW0AW01.PW0AWMAX <= 700) /* ; table maximum*/
      call "IO2180" ("N1", SQLCA, PWCREC) {isNoRefresh = yes, isExternal = yes}; /* fetch next record*/

      if (SQLCA.VAGen_SQLCODE == 0) /* ; good fetch*/
        PW0AW01.PW0AWMAX = PW0AW01.PW0AWMAX + 1; /* ;*/
        PW0AW01.TF1CARCD[PW0AWMAX] = PWCREC.TF1CARCD; /* ;*/
        PW0AW01.PW0AWCCR = PWCREC.TF1CARCD;
        PW0A026(); /* get carrier abbreviation*/
        PW0AW01.TF1CARNM[PW0AWMAX] = PW0AW01.PW0AWACR;
        set SQLCA empty;
        PW0AW01.PKVSSPNB[PW0AWMAX] = PWCREC.PKVSSPNB;
        PW0AW01.PWCCAVFR[PW0AWMAX] = PWCREC.PWCCAVFR;
        PW0AW01.PWCCAVTR[PW0AWMAX] = PWCREC.PWCCAVTR;
        PW0AW01.PWCHLDFL[PW0AWMAX] = PWCREC.PWCHLDFL;
        PW0AW01.PW0AWCCT[PW0AWMAX] = PWCREC.ZZZCHGCT;
      else
        if (SQLCA.VAGen_SQLCODE == -911)
          TA1REC.TA1FUNC = "POST"; /* ; term. function*/
          TA1REC.TA1LOC = "SELECT NEXT";
          TA1REC.TA1PGMNM = "PW0A022"; /* process name*/
          TA1REC.TA1MAP = "PW0AM030"; /* map name*/
          TA1REC.TA1DBASE = "DPICK   "; /* database name*/
          TA1REC.TA1TBLVU = "VCARRA01"; /* table view name*/
          call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
          converseLib.validationFailed(28); /* contention error*/
        else
          if (SQLCA.VAGen_SQLCODE != 100) /* ; not end of data*/
            converseLib.validationFailed(29); /* ; indicate system error*/
            TA1REC.TA1FUNC = "RETN"; /* ; term. function*/
            TA1REC.TA1LOC = "SELECT NEXT";
            TA1REC.TA1TYPE = "DB2 "; /* ; type of error*/
            TA1REC.TA1PGMNM = "PW0A022"; /* process name*/
            TA1REC.TA1MAP = "PW0AM030"; /* map name*/
            TA1REC.TA1DBASE = "DPICK   "; /* database name*/
            TA1REC.TA1TBLVU = "VCARRA01"; /* table view name*/
            call "TA0020" (TA1REC, SQLCA) {isNoRefresh = yes, isExternal = yes}; /* error diag routine*/
            return;
          end
        end
      end
    end
  end



  if (SQLCA.VAGen_SQLCODE == 0 /* ; good return from s1 above*/
   && PW0AW01.PW0AWMAX > 700)  /* ; table maximum was reached*/
    converseLib.validationFailed(24); /* overflow*/
    PW0AW01.PW0AWFL1 = "Y"; /* table overflow flag*/
    return;
  end



end // end PW0A022

// load map page
Function PW0A023()
   /* ----------------------------------------------------*/
     /* format map.....*/
      /* pk0lwtop - location in ws. array of the top of screen*/
      /* pk0lwndx - this keeps track of location in working*/
                 /* storage array for moving to map*/
      /* pk0lwnx2 - locates position in map - pk0lm030*/
      /* pk0lwmax - end of valid data in working storage array*/
   /* ----------------------------------------------------*/



  PW0AM030.PWDNBRSS = PW0AW01.PWDNBRSS;
  PW0AM030.PWDRJTSS = PW0AW01.PWDRJTSS;
  PW0AM030.PWDDFPHS = PW0AW01.PWDDFPHS;
  PW0AM030.PWDNBRSH = PW0AW01.PWDNBRSH;
  PW0AM030.PWDMIPSH = PW0AW01.PWDMIPSH;

  PW0AW01.PW0AWNDX = PW0AW01.PW0AWTOP; /* hold current top of page*/

  PW0AW01.PW0AWNX2 = 1; /* map display subscript*/

  while (PW0AW01.PW0AWNX2 <= 11) /* only 11 displayed*/
    if (PW0AW01.PW0AWNDX <= PW0AW01.PW0AWMAX) /* place in ws < data end*/
      PW0AM030.TF1CARCD[PW0AWNX2] = PW0AW01.TF1CARCD[PW0AWNDX];
      PW0AM030.TF1CARNM[PW0AWNX2] = PW0AW01.TF1CARNM[PW0AWNDX];
      PW0AM030.PKVSSPNB[PW0AWNX2] = PW0AW01.PKVSSPNB[PW0AWNDX];
      PW0AM030.PWCCAVFR[PW0AWNX2] = PW0AW01.PWCCAVFR[PW0AWNDX];
      PW0AM030.PWCCAVTR[PW0AWNX2] = PW0AW01.PWCCAVTR[PW0AWNDX];
      PW0AM030.PWCHLDFL[PW0AWNX2] = PW0AW01.PWCHLDFL[PW0AWNDX];
    end
    PW0AW01.PW0AWNX2 = PW0AW01.PW0AWNX2 + 1; /* set-up map subscript*/
    PW0AW01.PW0AWNDX = PW0AW01.PW0AWNDX + 1; /* set-up place in work.stor.*/
  end


  set PW0AM030.PW0AMACT cursor;
end // end PW0A023

// obtain set spur whse controls
Function PW0A024()
   /* ----------------------------------------------------*/
     /* load  working storage with pack and hold units*/
             /* -  planned only   -*/
   /* ----------------------------------------------------*/


  set PWDREC empty;
  PWDREC.XWHCD = PKWREC.XWHCD;
  PWDREC.PKURUNMD = "P"; /* planned mode*/

  call "IO2190" ("S ", SQLCA, PWDREC) {isNoRefresh = yes, isExternal = yes}; /* s wilsonville cntrl*/
  if (SQLCA.VAGen_SQLCODE == 0) /* record found*/
    PW0AW01.PWDNBRSS = PWDREC.PWDNBRSS;
    PW0AW01.PWDDFPHS = PWDREC.PWDDFPHS;
    PW0AW01.PWDNBRSH = PWDREC.PWDNBRSH;
    PW0AW01.PWDMIPSH = PWDREC.PWDMIPSH;
    PW0AW01.PWDSMDYS = PWDREC.PWDSMDYS;
    PW0AW01.PW0AWDCT = PWDREC.ZZZCHGCT;
    PW0AW01.PW0AWDWH = PWDREC.XWHCD;
  else
    if (SQLCA.VAGen_SQLCODE == 100) /* no records found*/
      /* next sentence*/
