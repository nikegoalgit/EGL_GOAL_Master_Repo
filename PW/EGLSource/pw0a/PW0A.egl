// edit entered fields
Function PW0A058()

       /* --------------------------------------*/
       /* -  edit line items                   -*/
       /* --------------------------------------*/

   /* -----  on an add or change*/
   /* -----      verify that pack & hold only flag is correct*/
   /* -----      (defaults to 'n')*/
  if (PW0AM030.PW0AMACT[PW0AWNX2] == "A"
   || PW0AM030.PW0AMACT[PW0AWNX2] == "C")
    if (PW0AM030.PWCHLDFL[PW0AWNX2] != "Y"
     && PW0AM030.PWCHLDFL[PW0AWNX2] != "N")
      if (PW0AM030.PWCHLDFL[PW0AWNX2] == " ")
        PW0AM030.PWCHLDFL[PW0AWNX2] = "N";
      else
        set PW0AM030.PWCHLDFL[PW0AWNX2] cursor, modified, bold;
        converseLib.validationFailed(255);
      end
    end
  end





   /* -----  on an add or change*/
   /* -----      verify availibale thru*/
  if (converseVar.validationMsgNum == 0)
    if (PW0AM030.PW0AMACT[PW0AWNX2] == "A"
     || PW0AM030.PW0AMACT[PW0AWNX2] == "C")
      if (PW0AM030.PWCCAVTR[PW0AWNX2] < 1
       || PW0AM030.PWCCAVTR[PW0AWNX2] < PW0AM030.PWCCAVFR[PW0AWNX2]
       || PW0AM030.PWCCAVTR[PW0AWNX2] > PW0AM030.PWDNBRSH)
        set PW0AM030.PWCCAVTR[PW0AWNX2] cursor, modified, bold;
        converseLib.validationFailed(261); /* invalid*/
      end
    end
  end







   /* -----  on an add or change*/
   /* -----      verify availibale from*/
  if (converseVar.validationMsgNum == 0)
    if (PW0AM030.PW0AMACT[PW0AWNX2] == "A"
     || PW0AM030.PW0AMACT[PW0AWNX2] == "C")
      if (PW0AM030.PWCCAVFR[PW0AWNX2] < 1
       || PW0AM030.PWCCAVFR[PW0AWNX2] > PW0AM030.PWDNBRSH)
        set PW0AM030.PWCCAVFR[PW0AWNX2] cursor, modified, bold;
        converseLib.validationFailed(260); /* invalid*/
      end
    end
  end






   /* -----  on an add or change*/
   /* -----      check for valid carrier (get description)*/
  if (converseVar.validationMsgNum == 0)
    if (PW0AM030.PW0AMACT[PW0AWNX2] == "A"
     || PW0AM030.PW0AMACT[PW0AWNX2] == "C")
      PW0AW01.PW0AWCCR = PW0AM030.TF1CARCD[PW0AWNX2];
      PW0A026(); /* get carrier abbreviation*/
      if (SQLCA.VAGen_SQLCODE == 0) /* a row not found*/
        PW0AM030.TF1CARNM[PW0AWNX2] = PW0AW01.PW0AWACR;
        if (PW0AM030.XWHCD != TF1REC.XWHCD[1]
         && PW0AM030.XWHCD != TF1REC.XWHCD[2]
         && PW0AM030.XWHCD != TF1REC.XWHCD[3]
         && PW0AM030.XWHCD != TF1REC.XWHCD[4]
         && PW0AM030.XWHCD != TF1REC.XWHCD[5]
         && PW0AM030.XWHCD != TF1REC.XWHCD[6]
         && PW0AM030.XWHCD != TF1REC.XWHCD[7]
         && PW0AM030.XWHCD != TF1REC.XWHCD[8]
         && PW0AM030.XWHCD != TF1REC.XWHCD[9]
         && PW0AM030.XWHCD != TF1REC.XWHCD[10]
         && PW0AM030.XWHCD != TF1REC.XWHCD[11]
         && PW0AM030.XWHCD != TF1REC.XWHCD[12]
         && PW0AM030.XWHCD != TF1REC.XWHCD[13]
         && PW0AM030.XWHCD != TF1REC.XWHCD[14]
         && PW0AM030.XWHCD != TF1REC.XWHCD[15])
          set PW0AM030.TF1CARCD[PW0AWNX2] cursor, modified, bold;
          converseLib.validationFailed(149);
        end
      end
      if (SQLCA.VAGen_SQLCODE != 0) /* a row was not found*/
        set PW0AM030.TF1CARCD[PW0AWNX2] cursor, modified, bold;
        converseLib.validationFailed(77);
      end
    end
  end








   /* -----  on an add or change*/
   /* -----      verify spur*/
  if (converseVar.validationMsgNum == 0)
    if (PW0AM030.PW0AMACT[PW0AWNX2] == "A"
     || PW0AM030.PW0AMACT[PW0AWNX2] == "C")
      if (PW0AM030.PKVSSPNB[PW0AWNX2] is modified)
        PW0AW01.PW0AWSPR = PW0AM030.PKVSSPNB[PW0AWNX2];
        PW0AW01.PW0AWFL2 = "N"; /* spur had errors flag*/
        PW0A054(); /* validate spur*/
        if (PW0AW01.PW0AWFL2 == "Y") /* spur had errors flag*/
          set PW0AM030.PKVSSPNB[PW0AWNX2] cursor, modified, bold;
          converseLib.validationFailed(250); /* invalid spur*/
        end
        if (PW0AM030.PKVSSPNB[PW0AWNX2] == PW0AM030.PWDDFPHS
         || PW0AM030.PKVSSPNB[PW0AWNX2] == PW0AM030.PWDRJTSS
         || PW0AM030.PKVSSPNB[PW0AWNX2] > PW0AM030.PWDNBRSS)
          set PW0AM030.PKVSSPNB[PW0AWNX2] cursor, modified, bold;
          converseLib.validationFailed(250); /* invalid spur*/
        end
      end
    end
  end




   /* -----  on an add or change*/
   /* -----      check for duplicates*/
  if (converseVar.validationMsgNum == 0)
    if (PW0AM030.PW0AMACT[PW0AWNX2] == "A"
     || PW0AM030.PW0AMACT[PW0AWNX2] == "C")
      PW0A059(); /* check for duplicates*/
    end
  end





   /* -----  verify action field*/
  if (converseVar.validationMsgNum == 0)
    if (PW0AM030.PW0AMACT[PW0AWNX2] == "A"
     || PW0AM030.PW0AMACT[PW0AWNX2] == "C"
     || PW0AM030.PW0AMACT[PW0AWNX2] == "D"
     || PW0AM030.PW0AMACT[PW0AWNX2] == " ")
      /* next sentence*/
    else
      set PW0AM030.PW0AMACT[PW0AWNX2] cursor, modified, bold;
      converseLib.validationFailed(35);
    end
  end




   /* -----  if fields have changed*/
   /* -----  and action field is not equal to add or change*/
   /* -----      it's an error.*/
  if (converseVar.validationMsgNum == 0)
    if (PW0AM030.PWCCAVFR[PW0AWNX2] is modified
