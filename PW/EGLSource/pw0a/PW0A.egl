    if (PW0AM030.PWCCAVTR[PW0AWNX2] > PW0AM030.PWDNBRSH)
      set PW0AM030.PWDNBRSH cursor, modified, bold;
      converseLib.validationFailed(251); /* number of schedules invalid*/
    end
    PW0AW01.PW0AWNX2 = PW0AW01.PW0AWNX2 - 1;
  end

  PW0AW01.PW0AWNDX = 1;
  while (PW0AW01.PW0AWNDX <= PW0AW01.PW0AWMAX)

    if (PW0AW01.PWCCAVFR[PW0AWNDX] > PW0AM030.PWDNBRSH)
      set PW0AM030.PWDNBRSH cursor, modified, bold;
      converseLib.validationFailed(251); /* number of schedules invalid*/
    end

    if (PW0AW01.PWCCAVTR[PW0AWNDX] > PW0AM030.PWDNBRSH)
      set PW0AM030.PWDNBRSH cursor, modified, bold;
      converseLib.validationFailed(251); /* number of schedules invalid*/
    end
    PW0AW01.PW0AWNDX = PW0AW01.PW0AWNDX + 1;
  end









  if (PW0AM030.PWDNBRSS is modified
   && PW0AM030.PWDNBRSS <= 0)
