  end

  PW0AW01.PW0AWNDX = 1;
  while (PW0AW01.PW0AWNDX <= PW0AW01.PW0AWMAX)

    if (PW0AW01.PKVSSPNB[PW0AWNDX] == PW0AM030.PWDDFPHS)
      set PW0AM030.PWDDFPHS cursor, modified, bold;
      converseLib.validationFailed(265); /* default pack & hold spur invalid*/
