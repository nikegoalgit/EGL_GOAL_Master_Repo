    set PW0AM030.PWDMIPSH cursor, modified, bold;
    converseLib.validationFailed(252); /* minutes per schedule invalid*/
  end





  PW0AW01.PW0AWNX2 = 11;
  while (PW0AW01.PW0AWNX2 >= 1)

    if (PW0AM030.PKVSSPNB[PW0AWNX2] == PW0AM030.PWDDFPHS)
      set PW0AM030.PWDDFPHS cursor, modified, bold;
      converseLib.validationFailed(265); /* default pack & hold spur invalid*/
    end
