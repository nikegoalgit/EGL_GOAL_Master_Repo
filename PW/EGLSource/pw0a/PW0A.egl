    PW0AW01.PW0AWNDX = PW0AW01.PW0AWNDX + 1;
  end







  if (PW0AM030.PWDNBRSH is modified
   && PW0AM030.PWDNBRSH <= 0)
    set PW0AM030.PWDNBRSH cursor, modified, bold;
    converseLib.validationFailed(251); /* number of schedules invalid*/
  end


  PW0AW01.PW0AWNX2 = 11;
  while (PW0AW01.PW0AWNX2 >= 1)

    if (PW0AM030.PWCCAVFR[PW0AWNX2] > PW0AM030.PWDNBRSH)
      set PW0AM030.PWDNBRSH cursor, modified, bold;
      converseLib.validationFailed(251); /* number of schedules invalid*/
