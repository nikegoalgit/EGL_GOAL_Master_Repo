    return;
  end




  if (PW0AM030.PWDDFPHS is modified)
    if (PW0AM030.PWDDFPHS == PW0AM030.PWDRJTSS)
      set PW0AM030.PWDDFPHS cursor, modified, bold;
      converseLib.validationFailed(254); /* warning: pack & hold spur equals reject spur*/
    end
  end




  if (PW0AM030.PWDDFPHS is modified)
    PW0AW01.PW0AWSPR = PW0AM030.PWDDFPHS;
    PW0AW01.PW0AWFL2 = "N"; /* spur had errors flag*/
    PW0A054(); /* validate spur*/
    if (PW0AW01.PW0AWFL2 == "Y") /* spur had errors flag*/
      set PW0AM030.PWDDFPHS cursor, modified, bold;
      converseLib.validationFailed(265); /* default pack & hold spur invalid*/
    end
