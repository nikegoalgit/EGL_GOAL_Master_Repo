// DRIVER FOR APPLICATION
Function DK0UP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DK0UP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DK0UP20-BUILD-MAP2(); /* Build the second map*/
  else
    DK0UP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DK0UP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DK0UP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DK0UP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DK0UP00 *******************************/
  /* **************/
end // end DK0UP00-DRIVER

// INITIALIZE THE APPLICATION
Function DK0UP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DK0UW01 empty; /* Working storage record*/
  set DK0UW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DK0UM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DK0UM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DK0UM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DK0UM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DK0UM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DK0UM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DK0UM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DK0UP10 ******************************/
  /* **************/
end // end DK0UP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DK0UP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DK0UP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DK0UM001.DAW-CURR-PAGE)
    DK0US-TS-READ-PAGE(); /* read temp storage page*/
    DK0UP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DK0UP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DK0UP20 **********************************/
  /* **************/
end // end DK0UP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DK0UP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DK0UM002.DA-CHNG-USER-ID = sysVar.userID;
  DK0UM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DK0UW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DK0UM002.DA-PLOG-TXT1 = " ";
    DK0UM002.DA-PLOG-TXT2 = " ";
    DK0UM002.XDACD = " ";
    DK0UM002.XDADESC = " ";
    DK0UM002.XDAABRV = " ";
    DK0UM002.XDAPCT = 0;
    DK0UM002.GLBCD = " ";
    DK0UM002.GLFCD = " ";
    DK0UM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DK0UW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DK0UW01.XDAGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DK0UW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DK0UW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DK0UM002.XDACD = DK0UW01.XDACD; /* move to the map*/
      DK0UM002.XDADESC = DK0UW01.XDADESC;
      DK0UM002.XDAABRV = DK0UW01.XDAABRV;
      DK0UM002.XDAPCT = DK0UW01.XDAPCT;
      DK0UM002.GLBCD = DK0UW01.GLBCD;
      DK0UM002.GLFCD = DK0UW01.GLFCD;
      DK0UM002.XXXSTAT = DK0UW01.XXXSTAT;
      DK0UM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DK0UM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DK0UM002.XDACD = DK0UW02.XDACD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DK0UM002.XDACD in XDATBL.XDACD)
          DK0UW01.XDACD = XDATBL.XDACD[sysVar.arrayIndex]; /* save for a*/
          DK0UW01.XDADESC = XDATBL.XDADESC[sysVar.arrayIndex]; /* later compare*/
          DK0UW01.XDAABRV = XDATBL.XDAABRV[sysVar.arrayIndex];
          DK0UW01.XDAPCT = XDATBL.XDAPCT[sysVar.arrayIndex];
          DK0UW01.GLBCD = XDATBL.GLBCD[sysVar.arrayIndex];
          DK0UW01.GLFCD = XDATBL.GLFCD[sysVar.arrayIndex];
          DK0UW01.XXXSTAT = XDATBL.XXXSTAT[sysVar.arrayIndex];
          DK0UW01.ZZZSDT = XDATBL.ZZZSDT[sysVar.arrayIndex];
          DK0UW01.ZZZCHGDT = XDATBL.ZZZCHGDT[sysVar.arrayIndex];
          DK0UW01.ZZZCHGTM = XDATBL.ZZZCHGTM[sysVar.arrayIndex];
          DK0UW01.ZZZCHGCT = XDATBL.ZZZCHGCT[sysVar.arrayIndex];

          DK0UM002.XDADESC = XDATBL.XDADESC[sysVar.arrayIndex]; /* move to map*/
          DK0UM002.XDAABRV = XDATBL.XDAABRV[sysVar.arrayIndex];
          DK0UM002.XDAPCT = XDATBL.XDAPCT[sysVar.arrayIndex];
          DK0UM002.GLBCD = XDATBL.GLBCD[sysVar.arrayIndex];
          DK0UM002.GLFCD = XDATBL.GLFCD[sysVar.arrayIndex];
          DK0UM002.XXXSTAT = XDATBL.XXXSTAT[sysVar.arrayIndex];
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DK0UW01.XDAGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DK0UW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DK0UW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DK0UM002.XDACD = DK0UW01.XDACD; /* move to the map*/
    DK0UM002.XDADESC = DK0UW01.XDADESC;
    DK0UM002.XDAABRV = DK0UW01.XDAABRV;
    DK0UM002.XDAPCT = DK0UW01.XDAPCT;
    DK0UM002.GLBCD = DK0UW01.GLBCD;
    DK0UM002.GLFCD = DK0UW01.GLFCD;
    DK0UM002.XXXSTAT = DK0UW01.XXXSTAT;
    DK0UM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DK0UM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DK0UM002.XXXSTAT == "A")
    DK0UM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DK0UM002.XXXSTAT == "I")
    DK0UM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DK0UM002.XXXSTAT == " ")
    DK0UM002.DAW-TBL-STATUS-DESC = " ";
  end

  DK0UP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DK0UP20 **********************************/
  /* **************/
end // end DK0UP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DK0UP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DK0UW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DK0UP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DK0US-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XDATBL.XDACD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DK0UP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DK0US-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DK0US-TS-ADD-PAGE();
    end

    DK0UM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DK0UM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DK0UP22 **********************************/
  /* **************/
end // end DK0UP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DK0UP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DK0UW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DK0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DK0UW01.XDAGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DK0UW02.XDACD[DAW-ROW-NBR] = DK0UW01.XDACD;
  DK0UW02.XDADESC[DAW-ROW-NBR] = DK0UW01.XDADESC;
  DK0UW02.XDAABRV[DAW-ROW-NBR] = DK0UW01.XDAABRV;
  DK0UW02.XXXSTAT[DAW-ROW-NBR] = DK0UW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DK0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DK0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DK0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DK0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DK0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DK0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DK0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DK0UW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DK0UW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DK0UW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DK0UP221 ******************************/
  /* ***************/
end // end DK0UP23-DB2-TO-TSQ

