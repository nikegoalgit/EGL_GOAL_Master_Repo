  if (DK0UW01.XDACD == DK0UM002.XDACD
   && DK0UW01.XDADESC == DK0UM002.XDADESC
   && DK0UW01.XDAABRV == DK0UM002.XDAABRV
   && DK0UW01.XDAPCT == DK0UM002.XDAPCT
   && DK0UW01.GLBCD == DK0UM002.GLBCD
   && DK0UW01.GLFCD == DK0UM002.GLFCD
   && DK0UW01.XXXSTAT == DK0UM002.XXXSTAT
   && DK0UW01.DA-PLOG-TXT1 == DK0UM002.DA-PLOG-TXT1
   && DK0UW01.DA-PLOG-TXT2 == DK0UM002.DA-PLOG-TXT2)
    DAWREC.DAW-DATA-CHANGED-FL = "N";
  else
    DAWREC.DAW-DATA-CHANGED-FL = "Y";
  end

  /* **************/
