// SET MAP 1 FIELD ATTRIBUTES
Function DK0UP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DK0UM001.DAW-PAGE-LIT = "PAGE";
    set DK0UM001.DAW-CURR-PAGE initialAttributes;
    DK0UM001.DAW-OF-LIT = "OF";
    set DK0UM001.DAW-LAST-PAGE initialAttributes;
    DK0UM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DK0UM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DK0UM001.DAW-PAGE-LIT = " ";
    set DK0UM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DK0UM001.DAW-OF-LIT = " ";
    set DK0UM001.DAW-LAST-PAGE skip, invisible;
    DK0UM001.DAW-PF7-LIT = " ";
    DK0UM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DK0UM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DK0UM001.XDACD[sysVar.arrayIndex] == " ")
      set DK0UM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DK0UM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DK0UP25 **************************/
  /* **************/
end // end DK0UP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DK0UP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DK0UM002.XDACD initialAttributes;
    set DK0UM002.XDADESC initialAttributes;
    set DK0UM002.XDAABRV initialAttributes;
    set DK0UM002.XDAPCT initialAttributes;
    set DK0UM002.GLBCD initialAttributes;
    set DK0UM002.GLFCD initialAttributes;
    set DK0UM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DK0UM002.XDACD skip;
    set DK0UM002.XDADESC skip;
    set DK0UM002.XDAABRV skip;
    set DK0UM002.XDAPCT skip;
    set DK0UM002.GLBCD skip;
    set DK0UM002.GLFCD skip;
    set DK0UM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DK0UM002.XDACD skip;
    set DK0UM002.XDADESC initialAttributes;
    set DK0UM002.XDAABRV initialAttributes;
    set DK0UM002.XDAPCT initialAttributes;
    set DK0UM002.GLBCD initialAttributes;
    set DK0UM002.GLFCD initialAttributes;
    set DK0UM002.XXXSTAT initialAttributes;
  end

  set DK0UM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DK0UM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DK0UM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DK0UM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DK0UM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DK0UM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DK0UM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DK0UM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end

  /* **************/
  /* ****************** END DK0UP25 **************************/
  /* **************/
end // end DK0UP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
