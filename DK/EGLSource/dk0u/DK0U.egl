// BUILD NEW VDATRN01 RECORD
Function DK0UP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DK0UM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DK0UM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DK0UW01.DA-CCAT-KEY-TXT = " ";
  DK0UW01.DK0UW-KEY-XDACD = DK0UM002.XDACD;
  VDATRN01.DA-CCAT-KEY-TXT = DK0UW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DK0UW01.XDACD = DK0UM002.XDACD;
  DK0UW01.XDADESC = DK0UM002.XDADESC;
  DK0UW01.XDAABRV = DK0UM002.XDAABRV;
  DK0UW01.XDAPCT = DK0UM002.XDAPCT;
  DK0UW01.GLBCD = DK0UM002.GLBCD;
  DK0UW01.GLFCD = DK0UM002.GLFCD;
  DK0UW01.XXXSTAT = DK0UM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 55;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DK0UW01.XDAGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DK0UP72 **********************************/
  /* **************/
end // end DK0UP72-NEW-TRAN

