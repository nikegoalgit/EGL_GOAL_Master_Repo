// SET MAP 1 FIELD ATTRIBUTES
Function DK0JP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DK0JM001.DAW-PAGE-LIT = "PAGE";
    set DK0JM001.DAW-CURR-PAGE initialAttributes;
    DK0JM001.DAW-OF-LIT = "OF";
    set DK0JM001.DAW-LAST-PAGE initialAttributes;
    DK0JM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DK0JM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DK0JM001.DAW-PAGE-LIT = " ";
    set DK0JM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DK0JM001.DAW-OF-LIT = " ";
    set DK0JM001.DAW-LAST-PAGE skip, invisible;
    DK0JM001.DAW-PF7-LIT = " ";
    DK0JM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DK0JM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DK0JM001.YCTCD[sysVar.arrayIndex] == " ")
      set DK0JM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DK0JM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DK0JP25 **************************/
  /* **************/
end // end DK0JP25-SET-ATTR-1

