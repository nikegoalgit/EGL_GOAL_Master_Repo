//                          DK0IP70-PROC-MAP-1
//                          CHECK DUPLICATE AND DEADLOCK IN
//                          DK0IP70-PROC-MAP-2
// *********************
Program DK0I type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DK0IW01 DK0IW01; // record
  DK0IW02 DK0IW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use YCSTBL {deleteAfterUse = yes}; // table
  use DK0IM.DK0IM001,  // forms
      DK0IM.DK0IM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DK0IP00-DRIVER: DK0IP00-DRIVER();
    exit program;
  end // end main
end // end DK0I

// DRIVER FOR APPLICATION
Function DK0IP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DK0IP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DK0IP20-BUILD-MAP2(); /* Build the second map*/
  else
    DK0IP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DK0IP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DK0IP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DK0IP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DK0IP00 *******************************/
  /* **************/
end // end DK0IP00-DRIVER

// INITIALIZE THE APPLICATION
Function DK0IP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DK0IW01 empty; /* Working storage record*/
  set DK0IW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DK0IM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DK0IM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DK0IM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DK0IM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DK0IM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DK0IM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DK0IM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DK0IP10 ******************************/
  /* **************/
end // end DK0IP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DK0IP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DK0IP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DK0IM001.DAW-CURR-PAGE)
    DK0IS-TS-READ-PAGE(); /* read temp storage page*/
    DK0IP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DK0IP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DK0IP20 **********************************/
  /* **************/
end // end DK0IP20-BUILD-MAP1

