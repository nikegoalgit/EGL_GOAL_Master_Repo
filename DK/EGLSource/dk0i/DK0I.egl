// MAIN PROCESS LOOP FOR MAP 1
Function DK0IP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DK0IP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DK0IM001.VAGen_EZEMSG = " ";
  DK0IM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DK0IP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DK0IP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DK0IP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DK0IP70-PROC-MAP1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DK0IP30 *****************************/
  /* **************/
end // end DK0IP30-MAIN-MAP-1

