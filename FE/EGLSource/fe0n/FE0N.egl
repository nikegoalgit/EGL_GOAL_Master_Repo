// Check attention identifier
Function FE0N-300()
   /* ------------------------------------------------------------*/
   /* If CANCEL was requested, clear the map fields and exit.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    converseLib.clearScreen();
     /* refresh map*/

    FE0NW00.FE0NW-SWF = "Y"; /* format screen m001*/
    FE0NW00.FE0NW-SWR = "N"; /* request for data*/

    COMMAREA.CATOAP = " "; /* to application*/
    COMMAREA.CAITEM = " "; /* passed data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If FAST PATH was requested, transfer to the application.*/
   /* ------------------------------------------------------------*/

  if (FE0NM001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      XSFFS01(); /* Exit to another application*/

      FE0NW00.FE0NW-SWF = "Y"; /* format screen m001*/
      FE0NW00.FE0NW-SWR = "N"; /* request for data*/

      set FE0NM001.CATOAP cursor, bold; /* Highlight*/

    else

      FFWREC.XXXMODID = "300"; /* Module identification*/
      converseLib.validationFailed(36); /* Indicate PF key not valid*/

    end

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* If HELP, exit to the previous level or exit to the Master*/
   /* Menu was requested, exit to another application.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf5) /* or PF5 was pressed*/

    if (converseVar.eventKey is pf1) /* PF1 was pressed*/
      COMMAREA.CAITEM = "FE0NM001"; /* Application find code*/
    end

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/
      COMMAREA.CATOAP = "FF03"; /* TO application*/
    end

    XSFFS01(); /* Exit to another application*/

    FE0NW00.FE0NW-SWF = "Y"; /* format screen m001*/
    FE0NW00.FE0NW-SWR = "N"; /* request for data*/

    exit stack; /* Exit to display map*/

  end

   /* ------------------------------------------------------------*/
   /* Determine whether a valid attention identifier was used to*/
   /* invoke the transaction.  Display an error message to the*/
   /* user if the PF key used is not supported.*/
   /* ------------------------------------------------------------*/

  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf15) /* pf15 was not pressed*/

    FFWREC.XXXMODID = "300"; /* Module identification*/
    converseLib.validationFailed(3); /* Indicate invalid attention key*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  end


end // end FE0N-300

// Reset highlighted fields
Function FE0N-400()
   /* ------------------------------------------------------------*/
   /* Reset field attributes to normal intensity.*/
   /* ------------------------------------------------------------*/

  set FE0NM001.FE0NM-BRG initialAttributes; /* begin range*/
  set FE0NM001.FE0NM-ERG initialAttributes; /* end range*/
  set FE0NM001.XPLCD initialAttributes; /* line code*/
  set FE0NM001.XPCCD initialAttributes; /* category code*/
  set FE0NM001.YPGCD initialAttributes; /* product type group*/
  set FE0NM001.YSMXDV initialAttributes; /* division group*/
  set FE0NM001.FE0NSESN initialAttributes; /* division group*/

  FE0NW00.FE0NW-IDX = 1;

  while (FE0NW00.FE0NW-IDX <= FFZREC.FFZNUMMO)

    set FE0NM001.FE0NM-ADJ[FE0NW-IDX] normal;

    FE0NW00.FE0NW-IDX = FE0NW00.FE0NW-IDX + 1;

  end

  set FE0NM001.CATOAP initialAttributes;
  set FE0NM001.CAITEM initialAttributes;

   /* ------------------------------------------------------------*/
   /* If no lists were requested, return to the previous process.*/
   /* ------------------------------------------------------------*/

  if (FE0NM001.XPLCD == "?" /* line code*/
   || FE0NM001.XPCCD == "?" /* category code*/
   || FE0NM001.YPGCD == "?") /* product type group*/

    if (converseVar.eventKey not enter) /* ENTER was not pressed*/

      FFWREC.XXXMODID = "400"; /* Module identification*/
      converseLib.validationFailed(19); /* Indicate PF key not valid*/

      exit stack; /* Exit to display map*/

    end

  else

    return; /* Return to previous process*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of line codes*/
   /* ------------------------------------------------------------*/

  if (FE0NM001.XPLCD == "?") /* product category list*/

    XPXX001(); /* window initialization*/
    XX0XW02.XX0XWHDR = "LINE CODES "; /* Heading*/

    XX0XW02.XGPCD = COMMAREA.XGPCD;
    while (XX0XW02.XX0XWEND == "N") /* Until end of request*/
      XPXPL01(); /* Display the list*/
    end

    if (XX0XW02.XX0XWPIX == 0) /* No selection made*/

      FE0NM001.XPLCD = FFWREC.XPLCD; /* product line*/
      XPLREC.XPLCD = FFWREC.XPLCD; /* product line*/
      XPLREC.XGPCD = COMMAREA.XGPCD; /* gpc code*/
      if (XPLREC.XPLKEY in XPLTBL.XPLKEY)
        FE0NM001.XPLABRV = XPLTBL.XPLABRV[sysVar.arrayIndex];
      end
    else
      FE0NM001.XPLCD = XPLTBL.XPLCD[XX0XWPIX]; /* Selected code*/
      FE0NM001.XPLABRV = XPLTBL.XPLABRV[XX0XWPIX]; /* Selected code*/

    end

    set FE0NM001.XPLCD cursor; /* Establish cursor position*/

  end

   /* ------------------------------------------------------------*/
   /* If requested, display the list of category codes*/
   /* ------------------------------------------------------------*/

  if (FE0NM001.XPCCD == "?") /* product category list*/

    XPXX001(); /* window initialization*/
