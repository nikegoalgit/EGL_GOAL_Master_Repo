        /* ------------------------------------------------------------*/
         /* Format fe0nm001 using passed data*/
        /* ------------------------------------------------------------*/

      if (FE0NW00.FE0NW-DATE-ENTERED == "Y")
        FE0NW00.FE0NW-WDT = FFWREC.FFABEGMO; /* w/s date field*/
