        FE0NM001.FE0NM-ERG = FE0NW00.FE0NW-WYM; /* end month range*/
      else
        FE0NM001.FE0NM-BRG = 0000;
        FE0NM001.FE0NM-ERG = 0000;
      end

      FE0NM001.YPGCD = FFWREC.YPGCD; /* product type group*/
      FE0NM001.YPGABRV = FFWREC.YPGABRV; /* product type group abrv*/

      FE0NM001.XPLCD = FFWREC.XPLCD; /* line code*/
      FE0NM001.XPLABRV = FFWREC.XPLABRV; /* line abrv*/

      FE0NM001.XPCCD = FFWREC.XPCCD; /* category code*/
      FE0NM001.XPCABRV = FFWREC.XPCABRV; /* category abrv*/

      FE0NM001.YSMXDV = FFWREC.YSMXDV; /* division group*/
      FE0NM001.YSMSMDSC = FFWREC.YSMSMDSC; /* div grp desc*/

      if (FE0NW00.FE0NW-SESN-ENTERED == "Y")
        FE0NM001.FE0NSESN = FE0NW00.FE0NSESN;
      else
        FE0NM001.FE0NSESN = "    ";
      end
      FE0N-210(); /* Format the data on the map*/

    end

  else

    FFWREC.XXXMSGNO = converseVar.validationMsgNum; /* load message number*/

  end

  FE0NM001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  FE0NM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  FE0NM001.CAITEM = COMMAREA.CAITEM; /* Application data*/

   /* ------------------------------------------------------------*/
   /* If a message to display, indicate sound audible alarm.*/
   /* ------------------------------------------------------------*/

