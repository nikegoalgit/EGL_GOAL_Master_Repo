// VALIDATE SEASON CODE
Function FE0NS-VAL-SEASON()
  FE0NW00.FE0NW-SESN-YR = FE0NM001.FE0NSESN;
  if (FE0NW00.FE0NW-YR not numeric)
    FFWREC.XXXMODID = "510";
    converseLib.validationFailed(537);
    return;
  end

  if (FE0NW00.FE0NW-SESN in FE0DTBL.FE0DSESN)
    FE0NW00.FE0NW-BRG-MM = FE0DTBL.FE0DBMON[sysVar.arrayIndex];
  end
  if (sysVar.arrayIndex == 0)
    FFWREC.XXXMODID = "510";
    converseLib.validationFailed(537);
    return;
  else
    FE0NW00.FE0NW-ERG-MM = FE0DTBL.FE0DEMON[sysVar.arrayIndex];
    if (FE0NW00.FE0NW-SESN == "SP")
      if (FE0NW00.FE0NW-YR == 00)
        FE0NW00.FE0NW-BRG-YY = 99;
      else
        FE0NW00.FE0NW-TEMP-YR = FE0NW00.FE0NW-YR;
        FE0NW00.FE0NW-TEMP-YR = FE0NW00.FE0NW-TEMP-YR - 1;
        FE0NW00.FE0NW-BRG-YY = FE0NW00.FE0NW-TEMP-YR;
      end
    else
      FE0NW00.FE0NW-BRG-YY = FE0NW00.FE0NW-YR;
    end
    FE0NW00.FE0NW-ERG-YY = FE0NW00.FE0NW-YR;
  end

  if (FE0NW00.FE0NW-BRG-YY > 57)
    FE0NW00.FE0NW-BRG-CC = 19;
  else
    FE0NW00.FE0NW-BRG-CC = 20;
  end

  if (FE0NW00.FE0NW-ERG-YY > 57)
    FE0NW00.FE0NW-ERG-CC = 19;
  else
    FE0NW00.FE0NW-ERG-CC = 20;
  end

  FE0NW00.FE0NW-BUYMO-PLUS1 = FE0NW00.FE0NW-CYM;
  FE0NW00.FE0NW-BUYMO-PLUS1-MM = FE0NW00.FE0NW-BUYMO-PLUS1-MM + 1;
  if (FE0NW00.FE0NW-BUYMO-PLUS1-MM > 12)
    FE0NW00.FE0NW-BUYMO-PLUS1-MM = FE0NW00.FE0NW-BUYMO-PLUS1-MM - 12;
    if (FE0NW00.FE0NW-BUYMO-PLUS1-YY == 99)
      FE0NW00.FE0NW-BUYMO-PLUS1-YY = 00;
      FE0NW00.FE0NW-BUYMO-PLUS1-CC = FE0NW00.FE0NW-BUYMO-PLUS1-CC + 1;
    else
      FE0NW00.FE0NW-BUYMO-PLUS1-YY = FE0NW00.FE0NW-BUYMO-PLUS1-YY + 1;
    end
  end

  if (FE0NW00.FE0NW-BRG-CYM < FE0NW00.FE0NW-BUYMO-PLUS1)
    if (FE0NW00.FE0NW-ERG-CYM < FE0NW00.FE0NW-BUYMO-PLUS1)
      FFWREC.XXXMODID = "510";
      converseLib.validationFailed(540);
      set FE0NM001.FE0NSESN cursor, bold;
      return;
    else
      FE0NW00.FE0NW-BRG-CYM = FE0NW00.FE0NW-BUYMO-PLUS1;
    end
  else
    FE0NW00.FE0NW-BRG-CYM = FE0NW00.FE0NW-BUYMO-PLUS1;
  end

  set FFZREC empty;

  if (FF7REC.FF7CBYFL == "N")
    FFZREC.FFZMONTH = 5;
  else
    FFZREC.FFZMONTH = 6;
  end
  FFZREC.FFZBYYMM = FE0NW00.FE0NW-BRG;
  FFZREC.FFZEYYMM = FE0NW00.FE0NW-ERG;

  XSFFS35(); /* edit date range*/

  if (FFZREC.FFZENDRC == "1"
   || FFZREC.FFZENDRC == "2")
    set FE0NM001.FE0NSESN cursor, bold; /* Highlight*/
    converseLib.validationFailed(541); /* Indicate missing or invalid*/
    FFWREC.XXXMODID = "520";
  else
    if (FFZREC.FFZENDRC == "3")
      set FE0NM001.FE0NSESN cursor, bold; /* Highlight*/
      converseLib.validationFailed(542); /* Indicate missing or invalid*/
      FFWREC.XXXMODID = "520";
    end
  end

  if (FFZREC.FFZBEGRC == "1"
   || FFZREC.FFZBEGRC == "2")
    set FE0NM001.FE0NSESN cursor, bold; /* Highlight*/
    converseLib.validationFailed(541); /* Indicate missing or invalid*/
    FFWREC.XXXMODID = "520";
  else
    if (FFZREC.FFZBEGRC == "3")
      set FE0NM001.FE0NSESN cursor, bold; /* Highlight*/
      converseLib.validationFailed(542); /* Indicate missing or invalid*/
      FFWREC.XXXMODID = "520";
    end
  end
end // end FE0NS-VAL-SEASON

