// SET MAP 1 FIELD ATTRIBUTES
Function DA0JP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DA0JM001.DAW-PAGE-LIT = "PAGE";
    set DA0JM001.DAW-CURR-PAGE initialAttributes;
    DA0JM001.DAW-OF-LIT = "OF";
    set DA0JM001.DAW-LAST-PAGE initialAttributes;
    DA0JM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DA0JM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DA0JM001.DAW-PAGE-LIT = " ";
    set DA0JM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DA0JM001.DAW-OF-LIT = " ";
    set DA0JM001.DAW-LAST-PAGE skip, invisible;
    DA0JM001.DAW-PF7-LIT = " ";
    DA0JM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DA0JM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DA0JM001.XCTCD[sysVar.arrayIndex] == " ")
      set DA0JM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DA0JM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DA0JP25 **************************/
  /* **************/
end // end DA0JP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DA0JP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0JM002.XCTCD initialAttributes;
  else
    set DA0JM002.XCTCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0JM002.XCTABRV skip;
    set DA0JM002.XCTDESC skip;
    set DA0JM002.XCT-ISO-CTRY-CD skip;
    set DA0JM002.XCTSPND skip;
    set DA0JM002.XCTFRND skip;
    set DA0JM002.XCTFARTL skip;
    set DA0JM002.ZPSCD skip;
    set DA0JM002.XCTCPCD skip;
    set DA0JM002.XCTRTADY skip;
    set DA0JM002.XXXSTAT skip;
  else
    set DA0JM002.XCTABRV initialAttributes;
    set DA0JM002.XCTDESC initialAttributes;
    set DA0JM002.XCT-ISO-CTRY-CD initialAttributes;
    set DA0JM002.XCTSPND initialAttributes;
    set DA0JM002.XCTFRND initialAttributes;
    set DA0JM002.XCTFARTL initialAttributes;
    set DA0JM002.ZPSCD initialAttributes;
    set DA0JM002.XCTCPCD initialAttributes;
    set DA0JM002.XCTRTADY initialAttributes;
    set DA0JM002.XXXSTAT initialAttributes;
  end

  set DA0JM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DA0JM002.DA-PLOG-TXT2 initialAttributes;

  DA0JM002.ZPSABRV = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DA0JM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DA0JM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DA0JM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DA0JM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DA0JM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DA0JM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DA0JP25 **************************/
  /* **************/
end // end DA0JP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DA0JP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
