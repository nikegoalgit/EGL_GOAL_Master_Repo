//                         DISPLAY STATUS IN DA0JM001
// *********************
Program DA0J type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DA0JW01 DA0JW01; // record
  DA0JW02 DA0JW02; // record
  DAWREC DAWREC; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XCTTBL {deleteAfterUse = yes}; // table
  use ZPSTBL {deleteAfterUse = yes}; // table
  use DA0JM.DA0JM001,  // forms
      DA0JM.DA0JM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DA0JP00-DRIVER: DA0JP00-DRIVER();
    exit program;
  end // end main
end // end DA0J

// DRIVER FOR APPLICATION
Function DA0JP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DA0JP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DA0JP20-BUILD-MAP2(); /* Build the second map*/
  else
    DA0JP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DA0JP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DA0JP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DA0JP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DA0JP00 *******************************/
  /* **************/
end // end DA0JP00-DRIVER

// INITIALIZE THE APPLICATION
Function DA0JP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DA0JW01 empty; /* Working storage record*/
  set DA0JW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DA0JM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0JM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0JM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DA0JM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0JM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0JM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DA0JM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DA0JP10 ******************************/
  /* **************/
end // end DA0JP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DA0JP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DA0JP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DA0JM001.DAW-CURR-PAGE)
    DA0JS-TS-READ-PAGE(); /* read temp storage page*/
    DA0JP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DA0JP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0JP20 **********************************/
  /* **************/
end // end DA0JP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DA0JP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DA0JM002.DA-CHNG-USER-ID = sysVar.userID;
  DA0JM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DA0JW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DA0JM002.DA-PLOG-TXT1 = " ";
    DA0JM002.DA-PLOG-TXT2 = " ";
    DA0JM002.XCTCD = " ";
    DA0JM002.XCTABRV = " ";
    DA0JM002.XCTDESC = " ";
    DA0JM002.XCT-ISO-CTRY-CD = " ";
    DA0JM002.XCTSPND = " ";
    DA0JM002.XCTFRND = " ";
    DA0JM002.XCTFARTL = " ";
    DA0JM002.ZPSCD = " ";
    DA0JM002.XCTCPCD = " ";
    DA0JM002.XCTRTADY = " ";
    DA0JM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DA0JW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DA0JW01.XCTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DA0JW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DA0JW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DA0JM002.XCTCD = DA0JW01.XCTCD; /* move to the map*/
      DA0JM002.XCTABRV = DA0JW01.XCTABRV;
      DA0JM002.XCTDESC = DA0JW01.XCTDESC;
      DA0JM002.XCT-ISO-CTRY-CD = DA0JW01.XCT-ISO-CTRY-CD;
      DA0JM002.XCTSPND = DA0JW01.XCTSPND;
      DA0JM002.XCTFRND = DA0JW01.XCTFRND;
      DA0JM002.XCTFARTL = DA0JW01.XCTFARTL;
      DA0JM002.ZPSCD = DA0JW01.ZPSCD;
      DA0JM002.XCTCPCD = DA0JW01.XCTCPCD;
      DA0JM002.XCTRTADY = DA0JW01.XCTRTADY;
      DA0JM002.XXXSTAT = DA0JW01.XXXSTAT;
      DA0JM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DA0JM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DA0JM002.XCTCD = DA0JW02.XCTCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DA0JM002.XCTCD in XCTTBL.XCTCD)
          DA0JW01.XCTCD = XCTTBL.XCTCD[sysVar.arrayIndex]; /* save for a*/
          DA0JW01.XCTABRV = XCTTBL.XCTABRV[sysVar.arrayIndex]; /* later compare*/
          DA0JW01.XCTDESC = XCTTBL.XCTDESC[sysVar.arrayIndex];
          DA0JW01.XCT-ISO-CTRY-CD = XCTTBL.XCT-ISO-CTRY-CD[sysVar.arrayIndex];
          DA0JW01.XCTSPND = XCTTBL.XCTSPND[sysVar.arrayIndex];
          DA0JW01.XCTFRND = XCTTBL.XCTFRND[sysVar.arrayIndex];
          DA0JW01.XCTFARTL = XCTTBL.XCTFARTL[sysVar.arrayIndex];
          DA0JW01.ZPSCD = XCTTBL.ZPSCD[sysVar.arrayIndex];
          DA0JW01.XCTCPCD = XCTTBL.XCTCPCD[sysVar.arrayIndex];
          DA0JW01.XCTRTADY = XCTTBL.XCTRTADY[sysVar.arrayIndex];
          DA0JW01.XXXSTAT = XCTTBL.XXXSTAT[sysVar.arrayIndex];
          DA0JW01.ZZZSDT = XCTTBL.ZZZSDT[sysVar.arrayIndex];
          DA0JW01.ZZZCHGDT = XCTTBL.ZZZCHGDT[sysVar.arrayIndex];
          DA0JW01.ZZZCHGTM = XCTTBL.ZZZCHGTM[sysVar.arrayIndex];
          DA0JW01.ZZZCHGCT = XCTTBL.ZZZCHGCT[sysVar.arrayIndex];

          DA0JM002.XCTABRV = XCTTBL.XCTABRV[sysVar.arrayIndex]; /* move to map*/
          DA0JM002.XCTDESC = XCTTBL.XCTDESC[sysVar.arrayIndex];
          DA0JM002.XCT-ISO-CTRY-CD = XCTTBL.XCT-ISO-CTRY-CD[sysVar.arrayIndex];
          DA0JM002.XCTSPND = XCTTBL.XCTSPND[sysVar.arrayIndex];
          DA0JM002.XCTFRND = XCTTBL.XCTFRND[sysVar.arrayIndex];
          DA0JM002.XCTFARTL = XCTTBL.XCTFARTL[sysVar.arrayIndex];
          DA0JM002.ZPSCD = XCTTBL.ZPSCD[sysVar.arrayIndex];
          DA0JM002.XCTCPCD = XCTTBL.XCTCPCD[sysVar.arrayIndex];
          DA0JM002.XCTRTADY = XCTTBL.XCTRTADY[sysVar.arrayIndex];
          DA0JM002.XXXSTAT = XCTTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DA0JW01.XCTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DA0JW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DA0JW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DA0JM002.XCTCD = DA0JW01.XCTCD; /* move to the map*/
    DA0JM002.XCTABRV = DA0JW01.XCTABRV;
    DA0JM002.XCTDESC = DA0JW01.XCTDESC;
    DA0JM002.XCT-ISO-CTRY-CD = DA0JW01.XCT-ISO-CTRY-CD;
    DA0JM002.XCTSPND = DA0JW01.XCTSPND;
    DA0JM002.XCTFRND = DA0JW01.XCTFRND;
    DA0JM002.XCTFARTL = DA0JW01.XCTFARTL;
    DA0JM002.ZPSCD = DA0JW01.ZPSCD;
    DA0JM002.XCTCPCD = DA0JW01.XCTCPCD;
    DA0JM002.XCTRTADY = DA0JW01.XCTRTADY;
    DA0JM002.XXXSTAT = DA0JW01.XXXSTAT;
    DA0JM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DA0JM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DA0JM002.XXXSTAT == "A")
    DA0JM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DA0JM002.XXXSTAT == "I")
    DA0JM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DA0JM002.XXXSTAT == " ")
    DA0JM002.DAW-TBL-STATUS-DESC = " ";
  end

  DA0JP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0JP20 **********************************/
  /* **************/
end // end DA0JP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DA0JP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DA0JW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DA0JP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DA0JS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XCTTBL.XCTCD[sysVar.arrayIndex] != "9999") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DA0JP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DA0JS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DA0JS-TS-ADD-PAGE();
    end

    DA0JM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DA0JM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DA0JP22 **********************************/
  /* **************/
end // end DA0JP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DA0JP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DA0JW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DA0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DA0JW01.XCTGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DA0JW02.XCTCD[DAW-ROW-NBR] = DA0JW01.XCTCD;
  DA0JW02.XCTABRV[DAW-ROW-NBR] = DA0JW01.XCTABRV;
  DA0JW02.XCTDESC[DAW-ROW-NBR] = DA0JW01.XCTDESC;
  DA0JW02.XXXSTAT[DAW-ROW-NBR] = DA0JW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DA0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DA0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DA0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DA0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DA0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DA0JW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DA0JW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DA0JW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DA0JW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DA0JW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DA0JP221 ******************************/
  /* ***************/
end // end DA0JP23-DB2-TO-TSQ

