// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   10-03-96  sjohn6      regen to pick up change in sgrp:
//                         XSDAS-PRINT-TABLE
//   05-11-99  ECHAN       ADD NOHILITE IN DA0HP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DA0HP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DA0HP70-PROC-MAP-2
//                         DISPLAY STATUS IN DA0HM001
// *********************
Program DA0H type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DA0HW01 DA0HW01; // record
  DA0HW02 DA0HW02; // record
  DAWREC DAWREC; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XSOTBL {deleteAfterUse = yes}; // table
  use XCTTBL {deleteAfterUse = yes}; // table
  use XDVTBL {deleteAfterUse = yes}; // table
  use XSTTBL {deleteAfterUse = yes}; // table
  use DA0HM.DA0HM001,  // forms
      DA0HM.DA0HM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DA0HP00-DRIVER: DA0HP00-DRIVER();
    exit program;
  end // end main
end // end DA0H

// DRIVER FOR APPLICATION
Function DA0HP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DA0HP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DA0HP20-BUILD-MAP2(); /* Build the second map*/
  else
    DA0HP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DA0HP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DA0HP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DA0HP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DA0HP00 *******************************/
  /* **************/
end // end DA0HP00-DRIVER

// INITIALIZE THE APPLICATION
Function DA0HP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DA0HW01 empty; /* Working storage record*/
  set DA0HW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DA0HM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0HM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0HM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DA0HM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0HM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0HM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DA0HM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DA0HP10 ******************************/
  /* **************/
end // end DA0HP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DA0HP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DA0HP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DA0HM001.DAW-CURR-PAGE)
    DA0HS-TS-READ-FIL(); /* read temp storage page*/
    DA0HP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DA0HP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0HP20 **********************************/
  /* **************/
end // end DA0HP20-BUILD-MAP1

