          DA0HW01.XXX-ADDR3 = XSOTBL.XXX-ADDR3[sysVar.arrayIndex];
          DA0HW01.XXXCITY = XSOTBL.XXXCITY[sysVar.arrayIndex];
          DA0HW01.XSTCD = XSOTBL.XSTCD[sysVar.arrayIndex];
          DA0HW01.XXXZIP = XSOTBL.XXXZIP[sysVar.arrayIndex];
          DA0HW01.XCTCD = XSOTBL.XCTCD[sysVar.arrayIndex];
          DA0HW01.XXXPHN = XSOTBL.XXXPHN[sysVar.arrayIndex];
          DA0HW01.XSOJBSCD = XSOTBL.XSOJBSCD[sysVar.arrayIndex];
          DA0HW01.XSOPDEST = XSOTBL.XSOPDEST[sysVar.arrayIndex];
