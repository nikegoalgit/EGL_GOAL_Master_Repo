      DA0HM002.XSORPRDI = DA0HW01.XSORPRDI;
      DA0HM002.XSOABRV = DA0HW01.XSOABRV;
      DA0HM002.XSONM = DA0HW01.XSONM;
      DA0HM002.XXXSTAT = DA0HW01.XXXSTAT;
      DA0HM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DA0HM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
