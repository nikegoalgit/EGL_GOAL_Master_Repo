          DA0HW01.ZZZCHGTM = XSOTBL.ZZZCHGTM[sysVar.arrayIndex];
          DA0HW01.ZZZCHGCT = XSOTBL.ZZZCHGCT[sysVar.arrayIndex];

          DA0HM002.XSOSNM = XSOTBL.XSOSNM[sysVar.arrayIndex];
          DA0HM002.XDVCD = XSOTBL.XDVCD[sysVar.arrayIndex];
          DA0HM002.XXX-ADDR1 = XSOTBL.XXX-ADDR1[sysVar.arrayIndex];
          DA0HM002.XXX-ADDR2 = XSOTBL.XXX-ADDR2[sysVar.arrayIndex];
          DA0HM002.XXX-ADDR3 = XSOTBL.XXX-ADDR3[sysVar.arrayIndex];
