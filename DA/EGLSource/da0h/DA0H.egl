// BUILD NEW VDATRN01 RECORD
Function DA0HP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DA0HM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DA0HM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DA0HW01.DA-CCAT-KEY-TXT = " ";
  DA0HW01.DA0HW-KEY-XSOCD = DA0HM002.XSOCD;
  VDATRN01.DA-CCAT-KEY-TXT = DA0HW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DA0HW01.XSOCD = DA0HM002.XSOCD;
  DA0HW01.XSOSNM = DA0HM002.XSOSNM;
  DA0HW01.XDVCD = DA0HM002.XDVCD;
  DA0HW01.XXX-ADDR1 = DA0HM002.XXX-ADDR1;
  DA0HW01.XXX-ADDR2 = DA0HM002.XXX-ADDR2;
  DA0HW01.XXX-ADDR3 = DA0HM002.XXX-ADDR3;
  DA0HW01.XXXCITY = DA0HM002.XXXCITY;
  DA0HW01.XSTCD = DA0HM002.XSTCD;
  DA0HW01.XXXZIP = DA0HM002.XXXZIP;
  DA0HW01.XCTCD = DA0HM002.XCTCD;
  DA0HW01.XXXPHN = DA0HM002.XXXPHN;
  DA0HW01.XSOJBSCD = DA0HM002.XSOJBSCD;
  DA0HW01.XSOPDEST = DA0HM002.XSOPDEST;
  DA0HW01.XSORCUSI = DA0HM002.XSORCUSI;
  DA0HW01.XSORPRDI = DA0HM002.XSORPRDI;
  DA0HW01.XSOABRV = DA0HM002.XSOABRV;
  DA0HW01.XSONM = DA0HM002.XSONM;
  DA0HW01.XXXSTAT = DA0HM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 241;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DA0HW01.XSOGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DA0HP72 **********************************/
  /* **************/
end // end DA0HP72-NEW-TRAN

