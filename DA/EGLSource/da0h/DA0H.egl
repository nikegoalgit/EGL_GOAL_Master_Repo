// SET MAP 1 FIELD ATTRIBUTES
Function DA0HP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DA0HM001.DAW-PAGE-LIT = "PAGE";
    set DA0HM001.DAW-CURR-PAGE initialAttributes;
    DA0HM001.DAW-OF-LIT = "OF";
    set DA0HM001.DAW-LAST-PAGE initialAttributes;
    DA0HM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DA0HM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DA0HM001.DAW-PAGE-LIT = " ";
    set DA0HM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DA0HM001.DAW-OF-LIT = " ";
    set DA0HM001.DAW-LAST-PAGE skip, invisible;
    DA0HM001.DAW-PF7-LIT = " ";
    DA0HM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DA0HM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DA0HM001.XSOCD[sysVar.arrayIndex] == " ")
      set DA0HM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DA0HM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DA0HP25 **************************/
  /* **************/
end // end DA0HP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DA0HP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0HM002.XSOCD initialAttributes;
  else
    set DA0HM002.XSOCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0HM002.XSOSNM skip;
    set DA0HM002.XDVCD skip;
