    DA0HM002.DA-PLOG-TXT2 = " ";
    DA0HM002.XSOCD = " ";
    DA0HM002.XSOSNM = " ";
    DA0HM002.XDVCD = " ";
    DA0HM002.XXX-ADDR1 = " ";
    DA0HM002.XXX-ADDR2 = " ";
    DA0HM002.XXX-ADDR3 = " ";
