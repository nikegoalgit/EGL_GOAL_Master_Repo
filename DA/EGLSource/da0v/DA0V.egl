// SET MAP 1 FIELD ATTRIBUTES
Function DA0VP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DA0VM001.DAW-PAGE-LIT = "PAGE";
    set DA0VM001.DAW-CURR-PAGE initialAttributes;
    DA0VM001.DAW-OF-LIT = "OF";
    set DA0VM001.DAW-LAST-PAGE initialAttributes;
    DA0VM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DA0VM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DA0VM001.DAW-PAGE-LIT = " ";
    set DA0VM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DA0VM001.DAW-OF-LIT = " ";
    set DA0VM001.DAW-LAST-PAGE skip, invisible;
    DA0VM001.DAW-PF7-LIT = " ";
    DA0VM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DA0VM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DA0VM001.XCCCD[sysVar.arrayIndex] == " ")
      set DA0VM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DA0VM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DA0VP25 **************************/
  /* **************/
end // end DA0VP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DA0VP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0VM002.XCCCD initialAttributes;
    set DA0VM002.XCCNCRU initialAttributes;
    set DA0VM002.XCCNCRL initialAttributes;
  else
    set DA0VM002.XCCCD skip;
    set DA0VM002.XCCNCRU skip;
    set DA0VM002.XCCNCRL skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0VM002.XCCENTFL skip;
    set DA0VM002.XCCRELFL skip;
    set DA0VM002.XCCPOL skip;
    set DA0VM002.XCCPPD30 skip;
    set DA0VM002.XCCPPD60 skip;
    set DA0VM002.XCCABRV skip;
    set DA0VM002.XCCDESC skip;
    set DA0VM002.XXXSTAT skip;
  else
    set DA0VM002.XCCENTFL initialAttributes;
    set DA0VM002.XCCRELFL initialAttributes;
    set DA0VM002.XCCPOL initialAttributes;
    set DA0VM002.XCCPPD30 initialAttributes;
    set DA0VM002.XCCPPD60 initialAttributes;
    set DA0VM002.XCCABRV initialAttributes;
    set DA0VM002.XCCDESC initialAttributes;
    set DA0VM002.XXXSTAT initialAttributes;
  end

  set DA0VM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DA0VM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DA0VM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
