// BUILD NEW VDATRN01 RECORD
Function DA0VP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DA0VM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DA0VM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DA0VW01.DA-CCAT-KEY-TXT = " ";
  DA0VW01.DA0VW-KEY-XCCCD = DA0VM002.XCCCD;
  DA0VW01.DA0VW-KEY-XCCNCRU = DA0VM002.XCCNCRU;
  DA0VW01.DA0VW-KEY-XCCNCRL = DA0VM002.XCCNCRL;
  VDATRN01.DA-CCAT-KEY-TXT = DA0VW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DA0VW01.XCCCD = DA0VM002.XCCCD;
  DA0VW01.XCCNCRU = DA0VM002.XCCNCRU;
  DA0VW01.XCCNCRL = DA0VM002.XCCNCRL;
  DA0VW01.XCCENTFL = DA0VM002.XCCENTFL;
  DA0VW01.XCCRELFL = DA0VM002.XCCRELFL;
  DA0VW01.XCCPOL = DA0VM002.XCCPOL;
  DA0VW01.XCCPPD30 = DA0VM002.XCCPPD30;
  DA0VW01.XCCPPD60 = DA0VM002.XCCPPD60;
  DA0VW01.XCCABRV = DA0VM002.XCCABRV;
  DA0VW01.XCCDESC = DA0VM002.XCCDESC;
  DA0VW01.XXXSTAT = DA0VM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 55;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DA0VW01.XCCGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DA0VP72 **********************************/
  /* **************/
end // end DA0VP72-NEW-TRAN

