//*** PROGRAM=DA0V ****
// NIKE TABLE HANDLER - TABLE MAINTENANCE
// 
// OVERVIEW: This application is the table handler maintenance
//           program for the CUSTOMER CREDIT STATUS table.
// 
// DB2 NAME: DCORP.VXCC0001
// CSP NAME: XCCTBL
// 
// DATE CREATED: 07/16/96
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   10-03-96  sjohn6      regen to pick up change in sgrp:
//                         XSDAS-PRINT-TABLE
//   04-14-99  ECHAN       ADD NOHILITE IN DA0VP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DA0VP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DA0VP70-PROC-MAP-2
//                         DISPLAY STATUS IN DA0VM001
// *********************
Program DA0V type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DA0VW01 DA0VW01; // record
  DA0VW02 DA0VW02; // record
  DAWREC DAWREC; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XCCTBL {deleteAfterUse = yes}; // table
  use DA0VM.DA0VM001,  // forms
      DA0VM.DA0VM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DA0VP00-DRIVER: DA0VP00-DRIVER();
    exit program;
  end // end main
end // end DA0V

// DRIVER FOR APPLICATION
Function DA0VP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DA0VP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DA0VP20-BUILD-MAP2(); /* Build the second map*/
  else
    DA0VP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DA0VP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DA0VP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DA0VP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DA0VP00 *******************************/
  /* **************/
end // end DA0VP00-DRIVER

// INITIALIZE THE APPLICATION
Function DA0VP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DA0VW01 empty; /* Working storage record*/
  set DA0VW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  if (DAWREC.DAW-MAP-TITLE == " ")
    DAWREC.DAW-MAP-TITLE = "TABLE HANDLER TEST TABLE";
    DAWREC.XXX-TBL-PFX = "DAM";
  end

  /* Initialize and move standard information to the map*/

  set DA0VM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0VM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0VM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DA0VM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0VM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0VM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DA0VM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DA0VP10 ******************************/
  /* **************/
end // end DA0VP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DA0VP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DA0VP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DA0VM001.DAW-CURR-PAGE)
    DA0VS-TS-READ-PAGE(); /* read temp storage page*/
    DA0VP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DA0VP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0VP20 **********************************/
  /* **************/
end // end DA0VP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DA0VP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DA0VM002.DA-CHNG-USER-ID = sysVar.userID;
  DA0VM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DA0VW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DA0VM002.DA-PLOG-TXT1 = " ";
    DA0VM002.DA-PLOG-TXT2 = " ";
    DA0VM002.XCCCD = " ";
    DA0VM002.XCCNCRU = " ";
    DA0VM002.XCCNCRL = " ";
    DA0VM002.XCCENTFL = " ";
    DA0VM002.XCCRELFL = " ";
    DA0VM002.XCCPOL = " ";
    DA0VM002.XCCPPD30 = " ";
    DA0VM002.XCCPPD60 = " ";
    DA0VM002.XCCABRV = " ";
    DA0VM002.XCCDESC = " ";
    DA0VM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DA0VW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DA0VW01.XCCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DA0VW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DA0VW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DA0VM002.XCCCD = DA0VW01.XCCCD; /* move to the map*/
      DA0VM002.XCCNCRU = DA0VW01.XCCNCRU;
      DA0VM002.XCCNCRL = DA0VW01.XCCNCRL;
      DA0VM002.XCCENTFL = DA0VW01.XCCENTFL;
      DA0VM002.XCCRELFL = DA0VW01.XCCRELFL;
      DA0VM002.XCCPOL = DA0VW01.XCCPOL;
      DA0VM002.XCCPPD30 = DA0VW01.XCCPPD30;
      DA0VM002.XCCPPD60 = DA0VW01.XCCPPD60;
      DA0VM002.XCCABRV = DA0VW01.XCCABRV;
      DA0VM002.XCCDESC = DA0VW01.XCCDESC;
      DA0VM002.XXXSTAT = DA0VW01.XXXSTAT;
      DA0VM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DA0VM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DA0VM002.XCCCD = DA0VW02.XCCCD[DAW-ROW-NBR];
        DA0VM002.XCCNCRU = DA0VW02.XCCNCRU[DAW-ROW-NBR];
        DA0VM002.XCCNCRL = DA0VW02.XCCNCRL[DAW-ROW-NBR];
                                   /* get the tbl row*/
        sysVar.arrayIndex = 1;
        while (XCCTBL.XCCCD[sysVar.arrayIndex] <= DA0VM002.XCCCD
         && XCCTBL.XCCCD[sysVar.arrayIndex] != "99")

          if (DA0VM002.XCCCD == XCCTBL.XCCCD[sysVar.arrayIndex]
           && DA0VM002.XCCNCRU == XCCTBL.XCCNCRU[sysVar.arrayIndex]
           && DA0VM002.XCCNCRL == XCCTBL.XCCNCRL[sysVar.arrayIndex])
            DA0VW01.XCCCD = XCCTBL.XCCCD[sysVar.arrayIndex]; /* save for a*/
            DA0VW01.XCCNCRU = XCCTBL.XCCNCRU[sysVar.arrayIndex]; /* later compare*/
            DA0VW01.XCCNCRL = XCCTBL.XCCNCRL[sysVar.arrayIndex];
            DA0VW01.XCCENTFL = XCCTBL.XCCENTFL[sysVar.arrayIndex];
            DA0VW01.XCCRELFL = XCCTBL.XCCRELFL[sysVar.arrayIndex];
            DA0VW01.XCCPOL = XCCTBL.XCCPOL[sysVar.arrayIndex];
            DA0VW01.XCCPPD30 = XCCTBL.XCCPPD30[sysVar.arrayIndex];
            DA0VW01.XCCPPD60 = XCCTBL.XCCPPD60[sysVar.arrayIndex];
            DA0VW01.XCCABRV = XCCTBL.XCCABRV[sysVar.arrayIndex];
            DA0VW01.XCCDESC = XCCTBL.XCCDESC[sysVar.arrayIndex];
            DA0VW01.XXXSTAT = XCCTBL.XXXSTAT[sysVar.arrayIndex];
            DA0VW01.ZZZSDT = XCCTBL.ZZZSDT[sysVar.arrayIndex];
            DA0VW01.ZZZCHGDT = XCCTBL.ZZZCHGDT[sysVar.arrayIndex];
            DA0VW01.ZZZCHGTM = XCCTBL.ZZZCHGTM[sysVar.arrayIndex];
            DA0VW01.ZZZCHGCT = XCCTBL.ZZZCHGCT[sysVar.arrayIndex];

            DA0VM002.XCCENTFL = XCCTBL.XCCENTFL[sysVar.arrayIndex];
            DA0VM002.XCCRELFL = XCCTBL.XCCRELFL[sysVar.arrayIndex];
            DA0VM002.XCCPOL = XCCTBL.XCCPOL[sysVar.arrayIndex];
            DA0VM002.XCCPPD30 = XCCTBL.XCCPPD30[sysVar.arrayIndex];
            DA0VM002.XCCPPD60 = XCCTBL.XCCPPD60[sysVar.arrayIndex];
            DA0VM002.XCCABRV = XCCTBL.XCCABRV[sysVar.arrayIndex]; /* move to map*/
            DA0VM002.XCCDESC = XCCTBL.XCCDESC[sysVar.arrayIndex];
            DA0VM002.XXXSTAT = XCCTBL.XXXSTAT[sysVar.arrayIndex];
          end
          sysVar.arrayIndex = sysVar.arrayIndex + 1;
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DA0VW01.XCCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DA0VW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DA0VW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DA0VM002.XCCCD = DA0VW01.XCCCD; /* move to the map*/
    DA0VM002.XCCNCRU = DA0VW01.XCCNCRU;
    DA0VM002.XCCNCRL = DA0VW01.XCCNCRL;
    DA0VM002.XCCENTFL = DA0VW01.XCCENTFL;
    DA0VM002.XCCRELFL = DA0VW01.XCCRELFL;
    DA0VM002.XCCPOL = DA0VW01.XCCPOL;
    DA0VM002.XCCPPD30 = DA0VW01.XCCPPD30;
    DA0VM002.XCCPPD60 = DA0VW01.XCCPPD60;
    DA0VM002.XCCABRV = DA0VW01.XCCABRV;
    DA0VM002.XCCDESC = DA0VW01.XCCDESC;
    DA0VM002.XXXSTAT = DA0VW01.XXXSTAT;
    DA0VM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DA0VM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DA0VM002.XXXSTAT == "A")
    DA0VM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DA0VM002.XXXSTAT == "I")
    DA0VM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DA0VM002.XXXSTAT == " ")
    DA0VM002.DAW-TBL-STATUS-DESC = " ";
  end

  DA0VP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0VP20 **********************************/
  /* **************/
end // end DA0VP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DA0VP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DA0VW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DA0VP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DA0VS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XCCTBL.XCCCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DA0VP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DA0VS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DA0VS-TS-ADD-PAGE();
    end

    DA0VM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DA0VM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DA0VP22 **********************************/
  /* **************/
end // end DA0VP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DA0VP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DA0VW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DA0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DA0VW01.XCCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DA0VW02.XCCCD[DAW-ROW-NBR] = DA0VW01.XCCCD;
  DA0VW02.XCCNCRU[DAW-ROW-NBR] = DA0VW01.XCCNCRU;
  DA0VW02.XCCNCRL[DAW-ROW-NBR] = DA0VW01.XCCNCRL;
  DA0VW02.XCCABRV[DAW-ROW-NBR] = DA0VW01.XCCABRV;
  DA0VW02.XCCDESC[DAW-ROW-NBR] = DA0VW01.XCCDESC;
  DA0VW02.XXXSTAT[DAW-ROW-NBR] = DA0VW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DA0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DA0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DA0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DA0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DA0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DA0VW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DA0VW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DA0VW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DA0VW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DA0VW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DA0VP221 ******************************/
  /* ***************/
end // end DA0VP23-DB2-TO-TSQ

