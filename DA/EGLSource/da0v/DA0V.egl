  if (DA0VW01.XCCCD == DA0VM002.XCCCD
   && DA0VW01.XCCNCRU == DA0VM002.XCCNCRU
   && DA0VW01.XCCNCRL == DA0VM002.XCCNCRL
   && DA0VW01.XCCENTFL == DA0VM002.XCCENTFL
   && DA0VW01.XCCRELFL == DA0VM002.XCCRELFL
   && DA0VW01.XCCPOL == DA0VM002.XCCPOL
   && DA0VW01.XCCPPD30 == DA0VM002.XCCPPD30
   && DA0VW01.XCCPPD60 == DA0VM002.XCCPPD60
   && DA0VW01.XCCABRV == DA0VM002.XCCABRV
   && DA0VW01.XCCDESC == DA0VM002.XCCDESC
   && DA0VW01.XXXSTAT == DA0VM002.XXXSTAT
   && DA0VW01.DA-PLOG-TXT1 == DA0VM002.DA-PLOG-TXT1
