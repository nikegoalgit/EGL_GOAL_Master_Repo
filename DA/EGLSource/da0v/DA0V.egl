Function DA0VP67-EDIT-NCR()
  /* ****************************************************************/
  /* Edit the Credit rating low nbr and credit rating upper nbr\*/
  /* a) required - can't be spaces*/
  /* b) can't be within the same range as another row with the*/
  /* same credit status code*/
  /* ****************************************************************/

  if (DA0VM002.XCCCD == " ")
    return; /* can't edit without the xcccd*/
  end

  if (DA0VM002.XCCNCRU < DA0VM002.XCCNCRL)
    converseLib.validationFailed(067); /* upper range less than lower range*/
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    if (DAWREC.DAW-CURR-FUNCTION-CD != "UPD")
      set DA0VM002.XCCNCRL cursor, bold;
    end
    return;
  end

  if (DA0VM002.XXXSTAT == "I")
    return; /* Skip the remaining edits*/
  end

  sysVar.arrayIndex = 1;
  while (XCCTBL.XCCCD[sysVar.arrayIndex] <= DA0VM002.XCCCD
   && XCCTBL.XCCCD[sysVar.arrayIndex] != "99")

    if (DA0VM002.XCCCD == XCCTBL.XCCCD[sysVar.arrayIndex]
     && XCCTBL.XXXSTAT[sysVar.arrayIndex] == "A")

      if (DA0VM002.XCCNCRU == XCCTBL.XCCNCRU[sysVar.arrayIndex]
       && DA0VM002.XCCNCRL == XCCTBL.XCCNCRL[sysVar.arrayIndex]
       && DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
      /* found current record*/
      else
        if (DA0VM002.XCCNCRU >= XCCTBL.XCCNCRU[sysVar.arrayIndex]
         && DA0VM002.XCCNCRL >= XCCTBL.XCCNCRU[sysVar.arrayIndex])
      /* not within existing range*/
        else
          if (DA0VM002.XCCNCRU <= XCCTBL.XCCNCRL[sysVar.arrayIndex]
           && DA0VM002.XCCNCRL <= XCCTBL.XCCNCRL[sysVar.arrayIndex])
        /* not within existing range*/
          else
            converseLib.validationFailed(070); /* WITHIN RANGE OF EXIST CREDIT STATUS*/
            DA0VM002.DAW-INFO-MESSAGE = "REVIEW TABLE LIST TO DETERMINE CORRECT CREDIT RATING RANGE";
            DAWREC.DAW-EDIT-ERROR-FL = "Y";
            if (DAWREC.DAW-CURR-FUNCTION-CD != "UPD")
              set DA0VM002.XCCNCRU cursor, bold;
              set DA0VM002.XCCNCRL bold;
            end
          end
        end
      end

    end

    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  /* **************/
  /* ***************** END DA0VP66 **********************************/
  /* **************/
end // end DA0VP67-EDIT-NCR

