// SET MAP 1 FIELD ATTRIBUTES
Function DA0UP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DA0UM001.DAW-PAGE-LIT = "PAGE";
    set DA0UM001.DAW-CURR-PAGE initialAttributes;
    DA0UM001.DAW-OF-LIT = "OF";
    set DA0UM001.DAW-LAST-PAGE initialAttributes;
    DA0UM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DA0UM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DA0UM001.DAW-PAGE-LIT = " ";
    set DA0UM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DA0UM001.DAW-OF-LIT = " ";
    set DA0UM001.DAW-LAST-PAGE skip, invisible;
    DA0UM001.DAW-PF7-LIT = " ";
    DA0UM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DA0UM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DA0UM001.XBDCD[sysVar.arrayIndex] == " ")
      set DA0UM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DA0UM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DA0UP25 **************************/
  /* **************/
end // end DA0UP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DA0UP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0UM002.XBDCD initialAttributes;
  else
    set DA0UM002.XBDCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0UM002.XBDABRV skip;
    set DA0UM002.XBDDESC skip;
    set DA0UM002.XBDBKOFL skip;
    set DA0UM002.XBDRWOFL skip;
    set DA0UM002.XBDSETFL skip;
    set DA0UM002.XBDXRFCD skip;
    set DA0UM002.XBDAALOC skip;
    set DA0UM002.XBDRVRFL skip;
    set DA0UM002.XBDCRCFL skip;
    set DA0UM002.XXXSTAT skip;
  else
    set DA0UM002.XBDABRV initialAttributes;
    set DA0UM002.XBDDESC initialAttributes;
    set DA0UM002.XBDBKOFL initialAttributes;
    set DA0UM002.XBDRWOFL initialAttributes;
    set DA0UM002.XBDSETFL initialAttributes;
    set DA0UM002.XBDXRFCD initialAttributes;
    set DA0UM002.XBDAALOC initialAttributes;
    set DA0UM002.XBDRVRFL initialAttributes;
    set DA0UM002.XBDCRCFL initialAttributes;
    set DA0UM002.XXXSTAT initialAttributes;
  end

  set DA0UM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DA0UM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DA0UM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DA0UM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DA0UM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DA0UM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DA0UM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DA0UM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DA0UP25 **************************/
  /* **************/
end // end DA0UP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DA0UP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DA0UP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
