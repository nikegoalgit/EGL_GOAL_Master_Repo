// BUILD NEW VDATRN01 RECORD
Function DA0UP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DA0UM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DA0UM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DA0UW01.DA-CCAT-KEY-TXT = " ";
  DA0UW01.DA0UW-KEY-XBDCD = DA0UM002.XBDCD;
  VDATRN01.DA-CCAT-KEY-TXT = DA0UW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DA0UW01.XBDCD = DA0UM002.XBDCD;
  DA0UW01.XBDABRV = DA0UM002.XBDABRV;
  DA0UW01.XBDDESC = DA0UM002.XBDDESC;
  DA0UW01.XBDBKOFL = DA0UM002.XBDBKOFL;
  DA0UW01.XBDRWOFL = DA0UM002.XBDRWOFL;
  DA0UW01.XBDSETFL = DA0UM002.XBDSETFL;
  DA0UW01.XBDXRFCD = DA0UM002.XBDXRFCD;
  DA0UW01.XBDAALOC = DA0UM002.XBDAALOC;
  DA0UW01.XBDRVRFL = DA0UM002.XBDRVRFL;
  DA0UW01.XBDCRCFL = DA0UM002.XBDCRCFL;
  DA0UW01.XXXSTAT = DA0UM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 47;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DA0UW01.XBDGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DA0UP72 **********************************/
  /* **************/
end // end DA0UP72-NEW-TRAN

