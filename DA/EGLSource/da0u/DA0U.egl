// DRIVER FOR APPLICATION
Function DA0UP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DA0UP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DA0UP20-BUILD-MAP2(); /* Build the second map*/
  else
    DA0UP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DA0UP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DA0UP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DA0UP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DA0UP00 *******************************/
  /* **************/
end // end DA0UP00-DRIVER

// INITIALIZE THE APPLICATION
Function DA0UP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DA0UW01 empty; /* Working storage record*/
  set DA0UW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DA0UM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0UM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0UM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DA0UM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0UM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0UM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DA0UM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DA0UP10 ******************************/
  /* **************/
end // end DA0UP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DA0UP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DA0UP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DA0UM001.DAW-CURR-PAGE)
    DA0US-TS-READ-PAGE(); /* read temp storage page*/
    DA0UP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DA0UP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0UP20 **********************************/
  /* **************/
end // end DA0UP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DA0UP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DA0UM002.DA-CHNG-USER-ID = sysVar.userID;
  DA0UM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DA0UW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DA0UM002.DA-PLOG-TXT1 = " ";
    DA0UM002.DA-PLOG-TXT2 = " ";
    DA0UM002.XBDCD = " ";
    DA0UM002.XBDABRV = " ";
    DA0UM002.XBDDESC = " ";
    DA0UM002.XBDBKOFL = " ";
    DA0UM002.XBDRWOFL = " ";
    DA0UM002.XBDSETFL = " ";
    DA0UM002.XBDXRFCD = " ";
    DA0UM002.XBDAALOC = " ";
    DA0UM002.XBDRVRFL = " ";
    DA0UM002.XBDCRCFL = " ";
    DA0UM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DA0UW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DA0UW01.XBDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DA0UW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DA0UW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DA0UM002.XBDCD = DA0UW01.XBDCD; /* move to the map*/
      DA0UM002.XBDABRV = DA0UW01.XBDABRV;
      DA0UM002.XBDDESC = DA0UW01.XBDDESC;
      DA0UM002.XBDBKOFL = DA0UW01.XBDBKOFL;
      DA0UM002.XBDRWOFL = DA0UW01.XBDRWOFL;
      DA0UM002.XBDSETFL = DA0UW01.XBDSETFL;
      DA0UM002.XBDXRFCD = DA0UW01.XBDXRFCD;
      DA0UM002.XBDAALOC = DA0UW01.XBDAALOC;
      DA0UM002.XBDRVRFL = DA0UW01.XBDRVRFL;
      DA0UM002.XBDCRCFL = DA0UW01.XBDCRCFL;
      DA0UM002.XXXSTAT = DA0UW01.XXXSTAT;
      DA0UM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DA0UM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DA0UM002.XBDCD = DA0UW02.XBDCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DA0UM002.XBDCD in XBDTBL.XBDCD)
          DA0UW01.XBDCD = XBDTBL.XBDCD[sysVar.arrayIndex]; /* save for a*/
          DA0UW01.XBDABRV = XBDTBL.XBDABRV[sysVar.arrayIndex]; /* later compare*/
          DA0UW01.XBDDESC = XBDTBL.XBDDESC[sysVar.arrayIndex];
          DA0UW01.XBDBKOFL = XBDTBL.XBDBKOFL[sysVar.arrayIndex];
          DA0UW01.XBDRWOFL = XBDTBL.XBDRWOFL[sysVar.arrayIndex];
          DA0UW01.XBDSETFL = XBDTBL.XBDSETFL[sysVar.arrayIndex];
          DA0UW01.XBDXRFCD = XBDTBL.XBDXRFCD[sysVar.arrayIndex];
          DA0UW01.XBDAALOC = XBDTBL.XBDAALOC[sysVar.arrayIndex];
          DA0UW01.XBDRVRFL = XBDTBL.XBDRVRFL[sysVar.arrayIndex];
          DA0UW01.XBDCRCFL = XBDTBL.XBDCRCFL[sysVar.arrayIndex];
          DA0UW01.XXXSTAT = XBDTBL.XXXSTAT[sysVar.arrayIndex];
          DA0UW01.ZZZSDT = XBDTBL.ZZZSDT[sysVar.arrayIndex];
          DA0UW01.ZZZCHGDT = XBDTBL.ZZZCHGDT[sysVar.arrayIndex];
          DA0UW01.ZZZCHGTM = XBDTBL.ZZZCHGTM[sysVar.arrayIndex];
          DA0UW01.ZZZCHGCT = XBDTBL.ZZZCHGCT[sysVar.arrayIndex];

          DA0UM002.XBDABRV = XBDTBL.XBDABRV[sysVar.arrayIndex]; /* move to map*/
          DA0UM002.XBDDESC = XBDTBL.XBDDESC[sysVar.arrayIndex];
          DA0UM002.XBDBKOFL = XBDTBL.XBDBKOFL[sysVar.arrayIndex];
          DA0UM002.XBDRWOFL = XBDTBL.XBDRWOFL[sysVar.arrayIndex];
          DA0UM002.XBDSETFL = XBDTBL.XBDSETFL[sysVar.arrayIndex];
          DA0UM002.XBDXRFCD = XBDTBL.XBDXRFCD[sysVar.arrayIndex];
          DA0UM002.XBDAALOC = XBDTBL.XBDAALOC[sysVar.arrayIndex];
          DA0UM002.XBDRVRFL = XBDTBL.XBDRVRFL[sysVar.arrayIndex];
          DA0UM002.XBDCRCFL = XBDTBL.XBDCRCFL[sysVar.arrayIndex];
          DA0UM002.XXXSTAT = XBDTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DA0UW01.XBDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DA0UW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DA0UW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DA0UM002.XBDCD = DA0UW01.XBDCD; /* move to the map*/
    DA0UM002.XBDABRV = DA0UW01.XBDABRV;
    DA0UM002.XBDDESC = DA0UW01.XBDDESC;
    DA0UM002.XBDBKOFL = DA0UW01.XBDBKOFL;
    DA0UM002.XBDRWOFL = DA0UW01.XBDRWOFL;
    DA0UM002.XBDSETFL = DA0UW01.XBDSETFL;
    DA0UM002.XBDXRFCD = DA0UW01.XBDXRFCD;
    DA0UM002.XBDAALOC = DA0UW01.XBDAALOC;
    DA0UM002.XBDRVRFL = DA0UW01.XBDRVRFL;
    DA0UM002.XBDCRCFL = DA0UW01.XBDCRCFL;
    DA0UM002.XXXSTAT = DA0UW01.XXXSTAT;
    DA0UM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DA0UM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DA0UM002.XXXSTAT == "A")
    DA0UM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DA0UM002.XXXSTAT == "I")
    DA0UM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DA0UM002.XXXSTAT == " ")
    DA0UM002.DAW-TBL-STATUS-DESC = " ";
  end

  DA0UP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0UP20 **********************************/
  /* **************/
end // end DA0UP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DA0UP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DA0UW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DA0UP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DA0US-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XBDTBL.XBDCD[sysVar.arrayIndex] != "9") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DA0UP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DA0US-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DA0US-TS-ADD-PAGE();
    end

    DA0UM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DA0UM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DA0UP22 **********************************/
  /* **************/
end // end DA0UP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DA0UP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DA0UW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DA0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DA0UW01.XBDGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DA0UW02.XBDCD[DAW-ROW-NBR] = DA0UW01.XBDCD;
  DA0UW02.XBDABRV[DAW-ROW-NBR] = DA0UW01.XBDABRV;
  DA0UW02.XBDDESC[DAW-ROW-NBR] = DA0UW01.XBDDESC;
  DA0UW02.XXXSTAT[DAW-ROW-NBR] = DA0UW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DA0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DA0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DA0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DA0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DA0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DA0UW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DA0UW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DA0UW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DA0UW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DA0UW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DA0UP221 ******************************/
  /* ***************/
end // end DA0UP23-DB2-TO-TSQ

