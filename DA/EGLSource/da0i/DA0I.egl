//*** PROGRAM=DA0I ****
// NIKE TABLE HANDLER - TABLE MAINTENANCE - WAREHOUSE AREA TBL
// 
// OVERVIEW: This application is used to maintain the warehouse
//           area code table
// 
// DB2 TABLE: DCORP.VXAR0001
// CSP TABLE: XARTBL
// 
// DATE CREATED: 07/19/96
//       AUTHOR: Leslie Rainey
// 
// MAINTENANCE HISTORY:
// 
//     date     by whom     overview of changes made
//   --------  ----------  ------------------------------------
//   10-03-96  sjohn6      regen to pick up change in sgrp:
//                         XSDAS-PRINT-TABLE
//   10-17-96  oenfie      fixed DA0IP20-BUILD-MAP2: csp tbl
//                         search was not getting a hit before.
//   02-11-97  SJOHN6      switch recfl and shpfl on map
//   04-01-99  ECHAN       ADD NOHILITE IN DA0IP25-SET-ATTR-1
//                         CHG TRAMSACTION TO TRANSACTION IN
//                         DA0IP70-PROC-MAP-1
//                         CHECK DUPLICATE AND DEADLOCK IN
//                         DA0IP70-PROC-MAP-2
//                         DISPLAY STATUS IN DA0IM001
// *********************
Program DA0I type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DA0IW01 DA0IW01; // record
  DA0IW02 DA0IW02; // record
  DAWREC DAWREC; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XARTBL {deleteAfterUse = yes}; // table
  use XWHTBL {deleteAfterUse = yes}; // table
  use DA0IM.DA0IM001,  // forms
      DA0IM.DA0IM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DA0IP00-DRIVER: DA0IP00-DRIVER();
    exit program;
  end // end main
end // end DA0I

// DRIVER FOR APPLICATION
Function DA0IP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DA0IP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DA0IP20-BUILD-MAP2(); /* Build the second map*/
  else
    DA0IP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DA0IP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DA0IP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DA0IP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DA0IP00 *******************************/
  /* **************/
end // end DA0IP00-DRIVER

// INITIALIZE THE APPLICATION
Function DA0IP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DA0IW01 empty; /* Working storage record*/
  set DA0IW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DA0IM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0IM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0IM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DA0IM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DA0IM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DA0IM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DA0IM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DA0IP10 ******************************/
  /* **************/
end // end DA0IP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DA0IP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DA0IP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DA0IM001.DAW-CURR-PAGE)
    DA0IS-TS-READ-PAGE(); /* read temp storage page*/
    DA0IP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DA0IP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0IP20 **********************************/
  /* **************/
end // end DA0IP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DA0IP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DA0IM002.DA-CHNG-USER-ID = sysVar.userID;
  DA0IM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DA0IW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DA0IM002.DA-PLOG-TXT1 = " ";
    DA0IM002.DA-PLOG-TXT2 = " ";
    DA0IM002.XWHCD = " ";
    DA0IM002.XARCD = " ";
    DA0IM002.XARBLKFL = " ";
    DA0IM002.XARPRCFL = " ";
    DA0IM002.XARRCVFL = " ";
    DA0IM002.XARPKHFL = " ";
    DA0IM002.XARRTNFL = " ";
    DA0IM002.XARSHPFL = " ";
    DA0IM002.XARTPQTY = " ";
    DA0IM002.XARABRV = " ";
    DA0IM002.XARDESC = " ";
    DA0IM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DA0IW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DA0IW01.XARGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DA0IW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DA0IW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DA0IM002.XWHCD = DA0IW01.XWHCD; /* move to the map*/
      DA0IM002.XARCD = DA0IW01.XARCD; /* move to map*/
      DA0IM002.XARABRV = DA0IW01.XARABRV;
      DA0IM002.XARBLKFL = DA0IW01.XARBLKFL;
      DA0IM002.XARPRCFL = DA0IW01.XARPRCFL;
      DA0IM002.XARRCVFL = DA0IW01.XARRCVFL;
      DA0IM002.XARPKHFL = DA0IW01.XARPKHFL;
      DA0IM002.XARRTNFL = DA0IW01.XARRTNFL;
      DA0IM002.XARSHPFL = DA0IW01.XARSHPFL;
      DA0IM002.XARTPQTY = DA0IW01.XARTPQTY;
      DA0IM002.XARDESC = DA0IW01.XARDESC;
      DA0IM002.XXXSTAT = DA0IW01.XXXSTAT;
      DA0IM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DA0IM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DA0IM002.XWHCD = DA0IW02.XWHCD[DAW-ROW-NBR];
        DA0IM002.XARCD = DA0IW02.XARCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DA0IM002.XWHCD in XARTBL.XWHCD
         && DA0IM002.XARCD in XARTBL.XARCD)
          DA0IW01.XWHCD = XARTBL.XWHCD[sysVar.arrayIndex];
          DA0IW01.XARCD = XARTBL.XARCD[sysVar.arrayIndex];

          DA0IW01.XARABRV = XARTBL.XARABRV[sysVar.arrayIndex];
          DA0IW01.XARBLKFL = XARTBL.XARBLKFL[sysVar.arrayIndex];
          DA0IW01.XARPRCFL = XARTBL.XARPRCFL[sysVar.arrayIndex];
          DA0IW01.XARRCVFL = XARTBL.XARRCVFL[sysVar.arrayIndex];
          DA0IW01.XARPKHFL = XARTBL.XARPKHFL[sysVar.arrayIndex];
          DA0IW01.XARRTNFL = XARTBL.XARRTNFL[sysVar.arrayIndex];
          DA0IW01.XARSHPFL = XARTBL.XARSHPFL[sysVar.arrayIndex];
          DA0IW01.XARTPQTY = XARTBL.XARTPQTY[sysVar.arrayIndex];
          DA0IW01.XARDESC = XARTBL.XARDESC[sysVar.arrayIndex];
          DA0IW01.XXXSTAT = XARTBL.XXXSTAT[sysVar.arrayIndex];
          DA0IW01.ZZZSDT = XARTBL.ZZZSDT[sysVar.arrayIndex];
          DA0IW01.ZZZCHGTM = XARTBL.ZZZCHGTM[sysVar.arrayIndex];
          DA0IW01.ZZZCHGDT = XARTBL.ZZZCHGDT[sysVar.arrayIndex];
          DA0IW01.ZZZCHGCT = XARTBL.ZZZCHGCT[sysVar.arrayIndex];

          DA0IM002.XARABRV = XARTBL.XARABRV[sysVar.arrayIndex];
          DA0IM002.XARBLKFL = XARTBL.XARBLKFL[sysVar.arrayIndex];
          DA0IM002.XARPRCFL = XARTBL.XARPRCFL[sysVar.arrayIndex];
          DA0IM002.XARRCVFL = XARTBL.XARRCVFL[sysVar.arrayIndex];
          DA0IM002.XARPKHFL = XARTBL.XARPKHFL[sysVar.arrayIndex];
          DA0IM002.XARRTNFL = XARTBL.XARRTNFL[sysVar.arrayIndex];
          DA0IM002.XARSHPFL = XARTBL.XARSHPFL[sysVar.arrayIndex];
          DA0IM002.XARTPQTY = XARTBL.XARTPQTY[sysVar.arrayIndex];
          DA0IM002.XARDESC = XARTBL.XARDESC[sysVar.arrayIndex];
          DA0IM002.XXXSTAT = XARTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DA0IW01.XARGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DA0IW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DA0IW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DA0IM002.XWHCD = DA0IW01.XWHCD; /* move to the map*/
    DA0IM002.XARCD = DA0IW01.XARCD;
                                   /* move to the map*/
    DA0IM002.XARABRV = DA0IW01.XARABRV;
    DA0IM002.XARBLKFL = DA0IW01.XARBLKFL;
    DA0IM002.XARPRCFL = DA0IW01.XARPRCFL;
    DA0IM002.XARRCVFL = DA0IW01.XARRCVFL;
    DA0IM002.XARPKHFL = DA0IW01.XARPKHFL;
    DA0IM002.XARRTNFL = DA0IW01.XARRTNFL;
    DA0IM002.XARSHPFL = DA0IW01.XARSHPFL;
    DA0IM002.XARTPQTY = DA0IW01.XARTPQTY;
    DA0IM002.XARDESC = DA0IW01.XARDESC;
    DA0IM002.XXXSTAT = DA0IW01.XXXSTAT;
    DA0IM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DA0IM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DA0IM002.XXXSTAT == "A")
    DA0IM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DA0IM002.XXXSTAT == "I")
    DA0IM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DA0IM002.XXXSTAT == " ")
    DA0IM002.DAW-TBL-STATUS-DESC = " ";
  end

  DA0IP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DA0IP20 **********************************/
  /* **************/
end // end DA0IP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DA0IP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DA0IW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DA0IP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DA0IS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XARTBL.XWHCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DA0IP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DA0IS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DA0IS-TS-ADD-PAGE();
    end

    DA0IM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DA0IM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DA0IP22 **********************************/
  /* **************/
end // end DA0IP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DA0IP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DA0IW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DA0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DA0IW01.XARGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DA0IW02.XWHCD[DAW-ROW-NBR] = DA0IW01.XWHCD;
  DA0IW02.XARCD[DAW-ROW-NBR] = DA0IW01.XARCD;
  DA0IW02.XARABRV[DAW-ROW-NBR] = DA0IW01.XARABRV;
  DA0IW02.XARDESC[DAW-ROW-NBR] = DA0IW01.XARDESC;
  DA0IW02.XXXSTAT[DAW-ROW-NBR] = DA0IW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DA0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DA0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DA0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DA0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DA0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DA0IW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DA0IW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DA0IW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DA0IW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DA0IW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DA0IP221 ******************************/
  /* ***************/
end // end DA0IP23-DB2-TO-TSQ

