// SET MAP 1 FIELD ATTRIBUTES
Function DA0IP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DA0IM001.DAW-PAGE-LIT = "PAGE";
    set DA0IM001.DAW-CURR-PAGE initialAttributes;
    DA0IM001.DAW-OF-LIT = "OF";
    set DA0IM001.DAW-LAST-PAGE initialAttributes;
    DA0IM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DA0IM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DA0IM001.DAW-PAGE-LIT = " ";
    set DA0IM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DA0IM001.DAW-OF-LIT = " ";
    set DA0IM001.DAW-LAST-PAGE skip, invisible;
    DA0IM001.DAW-PF7-LIT = " ";
    DA0IM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DA0IM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DA0IM001.XWHCD[sysVar.arrayIndex] == " ")
      set DA0IM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DA0IM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DA0IP25 **************************/
  /* **************/
end // end DA0IP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DA0IP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DA0IM002.XWHCD initialAttributes;
    set DA0IM002.XARCD initialAttributes;
  else
    set DA0IM002.XWHCD skip;
    set DA0IM002.XARCD skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DA0IM002.XARABRV skip;
    set DA0IM002.XARBLKFL skip;
    set DA0IM002.XARPRCFL skip;
    set DA0IM002.XARRCVFL skip;
    set DA0IM002.XARPKHFL skip;
    set DA0IM002.XARRTNFL skip;
    set DA0IM002.XARSHPFL skip;
    set DA0IM002.XARTPQTY skip;
    set DA0IM002.XARDESC skip;
    set DA0IM002.XXXSTAT skip;
  else
    set DA0IM002.XARABRV initialAttributes;
    set DA0IM002.XARBLKFL initialAttributes;
    set DA0IM002.XARPRCFL initialAttributes;
    set DA0IM002.XARRCVFL initialAttributes;
    set DA0IM002.XARPKHFL initialAttributes;
    set DA0IM002.XARRTNFL initialAttributes;
    set DA0IM002.XARSHPFL initialAttributes;
    set DA0IM002.XARTPQTY initialAttributes;
    set DA0IM002.XARDESC initialAttributes;
    set DA0IM002.XXXSTAT initialAttributes;
  end

  DA0IM002.XWHABRV = " ";

  set DA0IM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DA0IM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DA0IM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
