   && DA0IW01.XARPKHFL == DA0IM002.XARPKHFL
   && DA0IW01.XARRTNFL == DA0IM002.XARRTNFL
   && DA0IW01.XARSHPFL == DA0IM002.XARSHPFL
   && DA0IW01.XARTPQTY == DA0IM002.XARTPQTY
   && DA0IW01.XARDESC == DA0IM002.XARDESC
   && DA0IW01.XXXSTAT == DA0IM002.XXXSTAT
