// Reset highlighted fields
Function AB0V135()
   /* ------------------------------------------------------------*/
   /* look at passed parms to decide whether to process nil or promo*/
   /* ------------------------------------------------------------*/


  if (ABPREC.PROCESS-TYP == "N")
    AB0V140();
  end

  if (ABPREC.PROCESS-TYP == "P")
    AB0V141();
  end


end // end AB0V135

// Process valid input request
Function AB0V140()
   /* ------------------------------------------------------------*/
   /* Check for validation errors*/
   /* ------------------------------------------------------------*/

  if (XP1REC.XP1MNO != 0 /* Validation errors are present*/
   && XP1REC.XP1MNO != 24) /* Not OK to submit*/
    exit stack; /* Redisplay map with message*/
  end


   /* ------------------------------------------------------------*/
   /* Initialize the work areas and indexes to be used.*/
   /* ------------------------------------------------------------*/

  set TA6REC empty; /* Subroutine work area*/

  TA6REC.TA6INDEX = 1; /* Initialize parameters index*/

  if (COMMAREA.XCTH-THEATER-RUNTYPE == "PROD") /* If CICS Production Region*/
    AB0VW01.AB0VWIXT = 1;
  else
    if ("/*EOF" in AB0VT01.TARJCL)
      AB0VW01.AB0VWIXT = sysVar.arrayIndex;
      AB0VW01.AB0VWIXT = AB0VW01.AB0VWIXT + 1;
    else
      AB0VW01.AB0VWIXT = 1;
    end
  end

   /* ------------------------------------------------------------*/
   /* Format JCL and parameters into the subroutine work area.*/
   /* ------------------------------------------------------------*/

  while (AB0VW01.AB0VWIXT > 0) /* Until end of table*/

    /* ------------------------------------------------------------*/
     /* Format the Job Control Language statement from the table.*/
    /* ------------------------------------------------------------*/

    TA6REC.TARJCL[TA6INDEX] = AB0VT01.TARJCL[AB0VWIXT]; /* JCL*/
    TA6REC.TARTYPE[TA6INDEX] = AB0VT01.TARTYPE[AB0VWIXT]; /* Type*/

    TA6REC.TA6INDEX = TA6REC.TA6INDEX + 1; /* Increment parms index*/
    /* ------------------------------------------------------------*/
     /* If not end of table, increment the table index.*/
    /* ------------------------------------------------------------*/

    if (AB0VT01.TARJCL[AB0VWIXT] == "/*EOF") /* End of table*/
      AB0VW01.AB0VWIXT = 0; /* Indicate end of table*/
    else
      AB0VW01.AB0VWIXT = AB0VW01.AB0VWIXT + 1; /* Increment table index*/
    end

  end


   /* ------------------------------------------------------------*/
   /* Submit the JCL and parameters for batch execution.*/
   /* ------------------------------------------------------------*/


  AB0V145(); /* Submit the JCL*/


end // end AB0V140

