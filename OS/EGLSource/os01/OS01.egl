// Display All Bulk Assignments
Function OS01S-DISPLAY-ALL()
  ;
  ;
    /* display all bulk assignment ids*/
  ;
  OS01M001.KUCGRPID = 0;
  OS01M001.KUKPCOID = 0;
  OS01M001.KUCCOID = 0;
  ;
  OSWWREC.KUCGRPID = 0;
