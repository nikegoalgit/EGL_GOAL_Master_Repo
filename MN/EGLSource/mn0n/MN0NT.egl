package mn0n;
//*** DATATABLE=MN0NT ****
// this table is used to validate menu option selection and
// translate the option selected to an application name
// ************************
DataTable MN0NT type basicTable  {
  shared = yes, resident = no
  }
  10 MN0NTOPT MN0NTOPT; 
  10 MN0NTAPP MN0NTAPP; 
   {contents = [
    [01, "MN0U"],
    [02, "MN0A"],
    [03, "GD00"],
    [04, "RD0M"],
    [05, "SY0A"],
    [06, "TA0F"]
  ]}
end // end MN0NT

