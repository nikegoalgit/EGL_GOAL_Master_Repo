// display summary screen
Function KY0JSO1()


   /* ************************************************************/
   /* *** This sets up the summary screen using the data      ****/
   /* *** stored in the tsq.                                  ****/
   /* ************************************************************/

  CYWREC.CYWIDX01 = 1;

  set KY0JM110.XMCYMSEL[CYWIDX01] cursor;

  while (CYWREC.CYWIDX01 <= CYWREC.CYWLINPP)
    if (KY0JWS0.KY0JWRTY[CYWIDX01] == "S")
      set KY0JM110.XMCYMSEL[CYWIDX01] initialAttributes;
      set KY0JM110.KY0JMREC[CYWIDX01] initialAttributes;
      /* *** format summary line ****/
      KY0JWB1.KY0JWSUM = KY0JWS0.KY0JWDTA[CYWIDX01];
      KY0JW01.KY0JWREC[CYWIDX01] = KY0JWB1.KY0JWREC;
      KY0J121(); /* <=== disp left sum line*/
    else
      KY0JW01.KY0JWREC[CYWIDX01] = " ";
      KY0JM110.KY0JMREC[CYWIDX01] = " ";
      set KY0JM110.XMCYMSEL[CYWIDX01] skip, invisible;
      set KY0JM110.KY0JMREC[CYWIDX01] skip, invisible;
      KY0J129(); /* <=== disp blank line*/
    end
    CYWREC.CYWIDX01 = CYWREC.CYWIDX01 + 1;
  end

   /* ************************************************************/
   /* *** if error: display the cleared screen                ****/
   /* ************************************************************/

  if (CYWREC.CYWERRSW == "Y")
    KY0JST1(); /* <=== reset screen (empty)*/
  end

   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/
end // end KY0JSO1


// display detail screen
Function KY0JSP1()

   /* ************************************************************/
   /* *** This sets up the detail screen using the data       ****/
   /* *** stored in the tsq.                                  ****/
   /* ************************************************************/

  CYWREC.CYWIDX01 = 1;

  while (CYWREC.CYWIDX01 <= CYWREC.CYWLINPP)
    if (KY0JWD0.KY0JWRTY[CYWIDX01] == "Z")
      set KY0JM110.XMCYMSEL[CYWIDX01] skip, invisible;
      set KY0JM110.KY0JMREC[CYWIDX01] skip, invisible;
      KY0J125(); /* <=== disp size title line*/
    else
      if (KY0JWD0.KY0JWRTY[CYWIDX01] == "D")
        set KY0JM110.XMCYMSEL[CYWIDX01] skip, invisible;
        set KY0JM110.KY0JMREC[CYWIDX01] skip, invisible;
        KY0JWL1.KY0JWLIN = KY0JWD0.KY0JWDTA[CYWIDX01];
        KY0J122(); /* <=== disp left det line*/
      else
        if (KY0JWD0.KY0JWRTY[CYWIDX01] == "S")
          set KY0JM110.XMCYMSEL[CYWIDX01] skip, invisible;
          set KY0JM110.KY0JMREC[CYWIDX01] initialAttributes;
          set KY0JM110.KY0JMREC[CYWIDX01] cursor;
          /* *** format summary line ****/
          KY0JWB1.KY0JWSUM = KY0JWD0.KY0JWDTA[CYWIDX01];
          KY0J121(); /* <=== disp left sum line*/
        else
          if (KY0JWD0.KY0JWRTY[CYWIDX01] == "B")
            set KY0JM110.XMCYMSEL[CYWIDX01] skip, invisible;
            set KY0JM110.KY0JMREC[CYWIDX01] skip, invisible;
            KY0J129(); /* <=== disp blank line*/
          else
            CYWREC.CYWERRSW = "Y";
            CYWREC.XXXLOCID = "SP1A";
            CYWREC.CYWMSGNO = 0001;
          end
        end
      end
    end
    CYWREC.CYWIDX01 = CYWREC.CYWIDX01 + 1;
  end

  KY0JW01.KY0JWREC[1] = KY0JM110.KY0JMREC[1]; /* save recon field*/

   /* ************************************************************/
   /* *** if error: display the cleared screen                ****/
   /* ************************************************************/

  if (CYWREC.CYWERRSW == "Y")
    KY0JST1(); /* <=== reset screen (empty)*/
  end

   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/
end // end KY0JSP1


// setup sum & det queues
Function KY0JSS1()


   /* ************************************************************/
   /* *** setup application specific ta1rec fields in case    ****/
   /* *** TA0020 is used for a severe DB error in this sgrp   ****/
   /* *** note: first time switch checked in sh1              ****/
   /* ************************************************************/

  TA1REC.TA1LOCAT[2] = "KY0JSS1 ";
  TA1REC.TA1MAP = CYWREC.CYWTAMAP;

   /* *** purge existing tsqs ****/

  XSCYSPQ(); /* perform size translation call*/

  if (CYWREC.CYWERRSW == "Y")
    return;
  end

   /* *** initialize fields ****/
  KY0JW01.KY0JWDPA = 1;
  KY0JW01.KY0JWSPA = 1;
  CYWREC.CYWSUMLX = 0;
  CYWREC.CYWFTMSW = "N"; /* set to no recs found*/

  KY0JS1F(); /* process complete 'in' adjustments*/

  if (CYWREC.CYWERRSW == "Y")
    return;
  end

  KY0JS1G(); /* process complete 'out' adjustments*/

  if (CYWREC.CYWERRSW == "Y")
    return;
  end

   /* *** find the beginning date for w.o. date range ****/
   /* *** needed in statement groups 1j,1k,1t,&1u     ****/

  set TA2REC empty;

  TA2REC.TA2DTOPT = "GREGGREG";
  TA2REC.TA2GRTYP = "G";
  TA2REC.TA2DYAOD = KY0JM110.KY0JMNBR * -1;
  TA2REC.TA2GRGGN = X2CREC.X2CCURDT;

  call "TA0040" (TA2REC) {isNoRefresh = yes, isExternal = yes};

  if (TA2REC.TA2RTNCD == 0)
    SLVREC.SLVSDT = TA2REC.TA2GRGGN;
    /* *** since we're setting up the start date let's setup ****/
    /* *** the end date also.                                ****/
    SLVREC.SLVEDT = X2CREC.X2CCURDT;
  else
    set TA1REC empty;

    TA1REC.TA1FUNC = "RETN";
    TA1REC.TA1TYPE = "APPL";
    TA1REC.TA1PGMNM = "KY0JSS1 ";
    TA1REC.TA1LOC = "TA0040 - GET DATE";
    SQLCA.SQLCAREC = TA2REC.TA2RTNCD;
    call "TA0020" (TA1REC, SQLCA) {isExternal = yes};
  end

  KY0JS1J(); /* process complete live w.o.'s going out of the location*/

  if (CYWREC.CYWERRSW == "Y")
    return;
  end

  KY0JS1T(); /* process complete hist w.o.'s going out of the location*/

  if (CYWREC.CYWERRSW == "Y")
    return;
  end

  KY0JS1K(); /* process incomplete live w.o.'s coming into the location*/

  if (CYWREC.CYWERRSW == "Y")
    return;
  end

  KY0JS1U(); /* process complete hist w.o.'s coming into the location*/

  if (CYWREC.CYWERRSW == "Y")
    return;
  end

   /* *** fill the last summary page with blank lines ****/

  while (CYWREC.CYWSUMLX < CYWREC.CYWLINPP)
    CYWSUMLX = CYWSUMLX + 1;
    KY0JWS0.KY0JWDPG[CYWSUMLX] = KY0JW01.KY0JWDPA;
    KY0JWS0.KY0JWRTY[CYWSUMLX] = "B";
  end

  TA5REC.TA5APPID = CYWREC.CYWSUMQU;
  TA5REC.TA5FNCCD = "A";
  call "TA0050" (KY0JWS0, TA5REC) {isNoRefresh = yes, isExternal = yes};
  if (TA5REC.TA5RTNCD != 0)
    CYWREC.CYWERRSW = "Y";
    CYWREC.XXXLOCID = "SS1A";
    CYWREC.CYWMSGNO = 0001; /* <--- severe tsq error msg*/
    return;
  end

  CYWREC.CYWDMAXR = KY0JW01.KY0JWDPA - 1; /* since it is always 1 ahead*/
  CYWREC.CYWSMAXR = KY0JW01.KY0JWSPA; /* max nbr of summary pages*/

   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/
end // end KY0JSS1


// reset screen (empty)
Function KY0JST1()


   /* ************************************************************/
