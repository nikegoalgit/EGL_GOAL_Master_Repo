// process history w.o. items
Function KY0JS1V()


   /* ************************************************************/
   /* *** After finding the work order move summary data to   ****/
   /* *** working storage. To gather detail data and summary  ****/
   /* *** quantities a select set of all items for that work  ****/
   /* *** order number is performed.                          ****/
   /* ************************************************************/

  TA1REC.TA1LOCAT[2] = "KY0JS1V ";
  TA1REC.TA1MAP = CYWREC.CYWTAMAP;

   /* ************************************************************/
   /* *** format all fields on summary line,initializing qty  ****/
   /* ************************************************************/

  KY0JWB1.KY0JWREC = KY0JW01.KY0JWSRC;

  if (CY4REC.YSACD == "CI")
    KY0JWB1.KY0JWWIO = "IN ";
  else
    KY0JWB1.KY0JWWIO = "OUT";
  end
  KY0JWB1.XSLCD = SLBREC.XSLCD;
  KY0JWB1.SL2CASE = SLBREC.SL2CASE;
  KY0JWB1.SL8WONBR = SLBREC.SL8WONBR;
  KY0JWB1.KY0JWSQY = 0;
  KY0JWB1.ZZZSDT = SLBREC.ZZZSDT;
  KY0JWB1.KY0JM-SL-WO-CRT-USER = SLBREC.SL8CRTBY;
  KY0JWB1.KY0JM-YWS-WO-STAT-CD = SLBREC.YWSCD;
  KY0JWB1.KY0JM-PK-PICK-SCHED-NBR = SLBREC.PK2SCHNO;

   /* ************************************************************/
   /* *** format key and then obtain w.o. history items       ****/
   /* ************************************************************/

  SLCREC.XWHCD = KY0JM110.XWHCD;
  SLCREC.SL8WONBR = SLBREC.SL8WONBR;

  XSSLCS1(); /* obtain set of all w.o. history items*/

  if (SQLCA.VAGen_SQLCODE == -911) /* <=== for any initial db access*/
    CYWREC.CYWSKYSW = "N";
    CYWREC.CYWERRSW = "Y";
    CYWREC.XXXLOCID = "S1VA";
    CYWREC.CYWMSGNO = 0031; /* <--- try again msg*/
    return;
  end
  if (SQLCA.VAGen_SQLCODE != 0)
    CYWREC.CYWERRSW = "Y"; /* <=== for all other neg. sql codes*/
    CYWREC.XXXLOCID = "S1VB";
    CYWREC.CYWMSGNO = 0001; /* <--- severe error msg*/
    KY0JST1(); /* <=== clear scrn (empty)*/
    return;
  end

  CYWREC.CYWIDX01 = 0; /* count nbr of sizes per line*/

  CYWREC.CYWDETLX = 0; /* count nbr of details*/

  while (SQLCA.VAGen_SQLCODE == +0)

    CYWIDX01 = CYWIDX01 + 1;

    KY0JW01.KY0JWSZ1 = SLCREC.SY5SZID;
   /* save original size*/
    KY0JWL1.SY5SZID[CYWIDX01] = SLCREC.SY5SZID;
    if (KY0JW01.KY0JWSZ1 == 0)
      CYWREC.CYWERRSW = "Y";
      CYWREC.XXXLOCID = "S1VC";
      CYWREC.CYWMSGNO = 0001; /* <--- severe error*/
      return;
    end
    if (SYPREC.SY4IDDSC[KY0JWSZ1] == "     ")
      KY0JWL1.SY4IDDSC[CYWIDX01] = "*****";
    else
      KY0JWL1.SY4IDDSC[CYWIDX01] = SYPREC.SY4IDDSC[KY0JWSZ1];
    end
    KY0JWL1.SL9WOQTY[CYWIDX01] = SLCREC.SL9WOQTY;
    KY0JWB1.KY0JWSQY = KY0JWB1.KY0JWSQY + SLCREC.SL9WOQTY;

    if (CYWREC.CYWIDX01 == 4)
       /* put detail line to detail buffer*/
      CYWREC.CYWDETLX = CYWREC.CYWDETLX + 1; /* increment det line ct.*/
      KY0JWB1.KY0JWDET[CYWDETLX] = KY0JWL1.KY0JWLIN;
       /* reset sizes per line counter*/
      CYWREC.CYWIDX01 = 0;
    end

    XSSLCF1(); /* fetch next work order item*/

    if (SQLCA.VAGen_SQLCODE == -911) /* <=== for any initial db access*/
      CYWREC.CYWSKYSW = "N";
      CYWREC.CYWERRSW = "Y";
      CYWREC.XXXLOCID = "S1VD";
      CYWREC.CYWMSGNO = 0031; /* <--- try again msg*/
      return;
    end
    if (SQLCA.VAGen_SQLCODE != 0
     && SQLCA.VAGen_SQLCODE != +100)
      CYWREC.CYWERRSW = "Y"; /* <=== for all other neg. sql codes*/
      CYWREC.XXXLOCID = "S1VE";
      CYWREC.CYWMSGNO = 0001; /* <--- severe error msg*/
      KY0JST1(); /* <=== clear scrn (empty)*/
      return;
    end

  end

   /* *** fill remaining sizes with spaces and put to det buffer*/

  if (SQLCA.VAGen_SQLCODE == +100)
    if (CYWREC.CYWIDX01 > 0)
      while (CYWREC.CYWIDX01 < 4)
        CYWREC.CYWIDX01 = CYWREC.CYWIDX01 + 1; /* increment work index*/
        KY0JWL1.SY4IDDSC[CYWIDX01] = "     ";
        KY0JWL1.SL9WOQTY[CYWIDX01] = 0;
        KY0JWL1.SY5SZID[CYWIDX01] = 0;
      end
      /* *** now put detail line to buffer area ****/
      CYWREC.CYWDETLX = CYWREC.CYWDETLX + 1; /* increment det line ct.*/
      KY0JWB1.KY0JWDET[CYWDETLX] = KY0JWL1.KY0JWLIN;
    end
  end

   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/
end // end KY0JS1V


// add adjustment record by size
Function KY0JS1W()

  TA1REC.TA1LOCAT[2] = "KY0JS1W ";

  CYWIDX05 = 1; /* work index to walk thru dets in*/
                                   /* buffer*/
  while (CYWIDX05 < 12) /* CYWREC.CYWDETLX;*/

    if (KY0JWD0.KY0JWRTY[CYWIDX05] == "D") /* detail rec*/
      KY0JWL1.KY0JWLIN = KY0JWD0.KY0JWDTA[CYWIDX05];
                                   /* move to w/s*/
      CYWIDX06 = 1; /* work index to fill the detail queue*/
      while (CYWIDX06 <= 4) /* 4 sizes per detail*/
        if (KY0JWL1.SY4IDDSC[CYWIDX06] > " ") /* size is there-*/
   /* create an adj rec*/
          CY4REC.SY5SZID = KY0JWL1.SY5SZID[CYWIDX06]; /* size to adj rec*/
          CY4REC.CY-CNT-ADJ-QTY = KY0JWL1.SL9WOQTY[CYWIDX06]; /* qty to adj rec*/

          if (KY0JWB1.KY0JWWIO == "IN ")
            CY4REC.YSACD = "CI";
          else
            CY4REC.YSACD = "CO";
          end
          CY4REC.SL2CASE = KY0JWB1.SL2CASE;
          CY4REC.CY4ADJNO = KY0JWB1.SL8WONBR;

          XSCY4A0(); /* add the adjustment record to db*/

          if (SQLCA.VAGen_SQLCODE == 0)
            /* ** next sentence ***/
          else
            CYWREC.CYWERRSW = "Y";
            if (SQLCA.VAGen_SQLCODE == -911)
              CYWREC.CYWSEXSW = "Y";
              CYWREC.XXXLOCID = "S1WA";
              CYWREC.CYWMSGNO = 0031; /* <--- try again msg*/
              return;
            end
            CYWREC.XXXLOCID = "S1WB";
            CYWREC.CYWMSGNO = 0001; /* <--- severe error msg*/
            return;
          end
        else
          CYWIDX06 = 4; /* no more sizes for this w.o.*/
          CYWIDX05 = 12; /* you're finished*/
        end
        CYWIDX06 = CYWIDX06 + 1; /* next size within detail*/
      end /* end 1st 'while'*/
    end
    CYWIDX05 = CYWIDX05 + 1; /* next detail*/
  end
   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/
end // end KY0JS1W


