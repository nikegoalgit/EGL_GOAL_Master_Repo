// tsq update w/last pfkey=pf6
Function KY0JS1N()

  if (KY0JM110.KY0JMREC[1] == KY0JW01.KY0JWREC[1])
    return; /* no change therefore no updates to*/
                                   /* tsqs*/
  end

   /* *** obtain detail tsq record and update the ****/
   /* *** reconcilliation field.                  ****/

  TA5REC.TA5APPID = CYWREC.CYWDETQU;
  TA5REC.TA5TSQIX = CYWREC.CYWDETIX;
  TA5REC.TA5FNCCD = "R";
  call "TA0050" (KY0JWD0, TA5REC) {isNoRefresh = yes, isExternal = yes};
  if (TA5REC.TA5RTNCD != 0)
    CYWREC.CYWERRSW = "Y";
    CYWREC.XXXLOCID = "S1NA";
    CYWREC.CYWMSGNO = 0001;
    return;
  end

  KY0JWB1.KY0JWSUM = KY0JWD0.KY0JWDTA[1];
  KY0JWB1.KY0JWREC = KY0JM110.KY0JMREC[1];

  KY0JWD0.KY0JWDTA[1] = KY0JWB1.KY0JWSUM;

  TA5REC.TA5FNCCD = "U";

  call "TA0050" (KY0JWD0, TA5REC) {isNoRefresh = yes, isExternal = yes};
  if (TA5REC.TA5RTNCD != 0)
    CYWREC.CYWERRSW = "Y";
    CYWREC.XXXLOCID = "S1NB";
    CYWREC.CYWMSGNO = 0001; /* <--- severe tsq error msg*/
    return;
  end

   /* *** now obtain the corresponding summary record ****/

  TA5REC.TA5APPID = CYWREC.CYWSUMQU;
  TA5REC.TA5TSQIX = KY0JWD0.KY0JWSPG[1];
  TA5REC.TA5FNCCD = "R";
  call "TA0050" (KY0JWS0, TA5REC) {isNoRefresh = yes, isExternal = yes};
  if (TA5REC.TA5RTNCD != 0)
    CYWREC.CYWERRSW = "Y";
    CYWREC.XXXLOCID = "S1NC";
    CYWREC.CYWMSGNO = 0001;
    return;
  end

   /* *** loop thru summary arrays until correct one is found ****/

  CYWIDX01 = 1;

  while (CYWIDX01 <= CYWREC.CYWLINPP)
    if (KY0JWS0.KY0JWDPG[CYWIDX01] == CYWREC.CYWDETIX)
      KY0JWB1.KY0JWSUM = KY0JWS0.KY0JWDTA[CYWIDX01];
      KY0JWB1.KY0JWREC = KY0JM110.KY0JMREC[1];

      /* *** now update summary tsq with new value ****/

      KY0JWS0.KY0JWDTA[CYWIDX01] = KY0JWB1.KY0JWSUM;

      TA5REC.TA5FNCCD = "U";

      call "TA0050" (KY0JWS0, TA5REC) {isNoRefresh = yes, isExternal = yes};
      if (TA5REC.TA5RTNCD != 0)
        CYWREC.CYWERRSW = "Y";
        CYWREC.XXXLOCID = "S1ND";
        CYWREC.CYWMSGNO = 0001; /* <--- severe tsq error msg*/
        return;
      end

    end
    CYWIDX01 = CYWIDX01 + 1;
  end

   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/
end // end KY0JS1N


