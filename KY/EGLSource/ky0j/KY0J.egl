      CYWREC.XXXLOCID = "SK1E";
      CYWREC.CYWMSGNO = 0001;
      return;
    end

    KY0JSP1(); /* <=== setup det scrn*/

  end

  if (CYWREC.CYWERRSW == "N")
    CYWREC.XXXLOCID = "SK1F";
    CYWREC.CYWMSGNO = 0013;
  end

   /* ************************************************************/
   /* *** end of STATEMENT GROUP                              ****/
   /* ************************************************************/
end // end KY0JSK1


// setup blank screen
Function KY0JSN1()

   /* ************************************************************/
   /* *** display a blank line until the page is full         ****/
   /* ************************************************************/

  CYWREC.CYWIDX01 = 1;

  while (CYWREC.CYWIDX01 <= CYWREC.CYWLINPP)
    set KY0JM110.XMCYMSEL[CYWIDX01] skip, invisible;
    set KY0JM110.KY0JMREC[CYWIDX01] skip, invisible;
    KY0J129(); /* <=== disp blank line*/
    CYWREC.CYWIDX01 = CYWREC.CYWIDX01 + 1;
