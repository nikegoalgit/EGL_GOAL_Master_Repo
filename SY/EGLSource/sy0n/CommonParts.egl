package sy0n;
// select application
DataItem SY0NTAPP char(4)  {
  align = left, fillCharacter = nullFill, inputRequired = no
}
end

// input option number
DataItem SY0NTOPT num(2)  {
  currency = no, numericSeparator = no, sign = none, align = right, 
  fillCharacter = " ", inputRequired = no
}
end

