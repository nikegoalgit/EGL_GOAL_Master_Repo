    OZ0KW00.OZ0KW-IXM = OZ0KW00.OZ0KW-IXM + 1; /* increment line number*/

  end


end // end OZ0K-15100

// Verify data in the line
Function OZ0K-15200()
  /* ------------------------------------------------------------*/
  /* edit map*/
  /* ------------------------------------------------------------*/
  OZ0KW00.OZ0KW-IXM = 1; /* initialize index*/

  while (OZ0KW00.OZ0KW-IXM < 16) /* until end of lines*/

    if (OZ0KM001.OZ0KMSEL[OZ0KW-IXM] != " ")

      if (OZ0KM001.OZ0KMSEL[OZ0KW-IXM] == "A"
       || OZ0KM001.OZ0KMSEL[OZ0KW-IXM] == "D"
       || OZ0KM001.OZ0KMSEL[OZ0KW-IXM] == "C")
           /* continue*/
      else
        OZWREC.XXXPRCNM = "15200"; /* Module identification*/
        set OZ0KM001.OZ0KMSEL[OZ0KW-IXM] cursor, bold; /* Highlight*/
        converseLib.validationFailed(58); /* Only 'd' or 'a' or 'c'*/
        OZ0KW00.OZ0KW-SWF = "N";
        return;
      end

      if (OZ0KM001.OZ0KMSEL[OZ0KW-IXM] == "A" || 
      OZ0KM001.OZ0KMSEL[OZ0KW-IXM] == "C")
        if (OZ0KM001.OZ0KM-CUST[OZ0KW-IXM] == "ALL")
          OZ0KW00.OZ0KW-SV-CUST = 0;
        else
          OZ0KW00.OZ0KW-WS-CUST = OZ0KM001.OZ0KM-CUST[OZ0KW-IXM];
          OZ0KW00.OZ0KW-SV-CUST = OZ0KW00.OZ0KW-WS-CUST-NUM;
          set KUCREC empty;
          KUCREC.KUCCOID = OZ0KW00.OZ0KW-SV-CUST;

          OZ0K-11510(); /* verify cust number*/
          if (SQLCA.VAGen_SQLCODE == 0)
             /* continue*/
          else
            OZWREC.XXXPRCNM = "15200"; /* Module identification*/
            set OZ0KM001.OZ0KM-CUST[OZ0KW-IXM] cursor, bold;
            OZ0KW00.OZ0KW-SWF = "N";
            converseLib.validationFailed(51); /* invalid customer*/
            return;
          end
        end

        if (OZ0KM001.OZ0KM-SEND-OCD == "N" || 
        OZ0KM001.OZ0KM-SEND-OCD == "Y")
           /* continue*/
        else
          OZWREC.XXXPRCNM = "15200"; /* Module identification*/
          set OZ0KM001.OZ0KM-XBDCD cursor, bold;
          OZ0KW00.OZ0KW-SWF = "N";
          converseLib.validationFailed(18); /* yes or no*/
          return;
        end

        if (OZ0KM001.OZ0KM-XBDCD > "  ")
          if (OZ0KM001.OZ0KM-XBDCD in XBDTBL.XBDCD)
             /* continue*/
          else
            OZWREC.XXXPRCNM = "15200"; /* Module identification*/
            set OZ0KM001.OZ0KM-XBDCD cursor, bold;
            OZ0KW00.OZ0KW-SWF = "N";
            converseLib.validationFailed(59); /* xbd cd invalid*/
            return;
          end
        else
          OZWREC.XXXPRCNM = "15200"; /* Module identification*/
          set OZ0KM001.OZ0KM-XBDCD cursor, bold;
          OZ0KW00.OZ0KW-SWF = "N";
          converseLib.validationFailed(59); /* xbd cd invalid*/
          return;
        end
      end

    end

    OZ0KW00.OZ0KW-IXM = OZ0KW00.OZ0KW-IXM + 1; /* increment line number*/

  end
end // end OZ0K-15200

// Process valid input request
Function OZ0K-16000()
  /* ------------------------------------------------------------*/
  /* If a scroll backward was requested, decrement the page*/
  /* number and return to the previous process.*/
  /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7) /* Scroll back requested*/

    OZ0KW00.XXXTSPAG = OZ0KW00.XXXTSPAG - 1; /* Decrement page number*/
    OZ0KW00.OZ0KW-SWF = "Y";

    return; /* Return to previous process*/

  end

  /* ------------------------------------------------------------*/
  /* If a scroll forward was requested, increment the page*/
  /* number and return to the previous process.*/
  /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf8) /* Scroll forward requested*/

    OZ0KW00.XXXTSPAG = OZ0KW00.XXXTSPAG + 1; /* Increment page number*/
    OZ0KW00.OZ0KW-SWF = "Y";

    return; /* Return to previous process*/

  end

  if (converseVar.eventKey not enter
   && converseVar.eventKey not pf7
   && converseVar.eventKey not pf8
   && converseVar.eventKey not pa2
   && converseVar.eventKey not pf1
   && converseVar.eventKey not pf4
  )
    OZWREC.XXXPRCNM = "16000"; /* module id*/
    converseLib.validationFailed(4); /* pa/pf key pressed is not valid*/
    exit stack; /* exit to display map*/

  end

  /* ------------------------------------------------------------*/
  /* enter pressed*/
  /* ------------------------------------------------------------*/
  OZ0KW00.OZ0KW-UPDATE-SW = "N";

  if (converseVar.eventKey is enter)
    OZ0KW00.OZ0KW-IXM = 1;

    while (OZ0KW-IXM < 16)

      if (OZ0KM001.OZ0KMSEL[OZ0KW-IXM] is modified || 
      OZ0KM001.OZ0KM-CUST[OZ0KW-IXM] is modified || 
      OZ0KM001.OZ0KM-SEND-OCD[OZ0KW-IXM] is modified || 
      OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM] is modified)

        if (OZ0KM001.OZ0KMSEL[OZ0KW-IXM] == "D")

    /* ------------------------------------------------------------*/
    /* delete the row*/
    /* ------------------------------------------------------------*/

          OZ0K-16100(); /* delete the row*/

        else

    /* ------------------------------------------------------------*/
    /* straight add from the screen*/
    /* ------------------------------------------------------------*/
          if (OZ0KM001.OZ0KMSEL[OZ0KW-IXM] == "A")

            set VOCECB01 empty; /* Clear row storage area*/

            VOCECB01.OP1CUOID = OZ0KW00.OZ0KW-SV-CUST;
            VOCECB01.XBDCD = OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM];
            VOCECB01.SOCD-CO-BULK-FL = OZ0KM001.OZ0KM-SEND-OCD[OZ0KW-IXM];

            set TA2REC empty;
            TA2REC.TA2DTOPT = "GREGGREG";
            TA2REC.TA2GRTYP = "E";
            TA2REC.TA2GRGE = VGVar.currentShortGregorianDate;

            call "TA0040" (TA2REC) {isNoRefresh = yes, isExternal = yes}; /* valid date pgm*/

            if (TA2REC.TA2RTNCD == 0)
              VOCECB01.ZZZSDT = TA2REC.TA2GRGGN;
            else
              OZWREC.XXXPRCNM = "15200"; /* Module identification*/
              converseLib.validationFailed(10); /* unexpected error*/
            end

            OZ0KW00.OZ0KW-TIME-BRK = VGVar.currentFormattedTime;
            OZ0KW00.OZ0KW-TM-HH = OZ0KW00.OZ0KW-TB-HH;
            OZ0KW00.OZ0KW-TM-MM = OZ0KW00.OZ0KW-TB-MM;
            VOCECB01.ZZZSDTM = OZ0KW00.OZ0KW-TIME-MK;

            OZ0K-16200(); /* add new row*/
            if (OZ0KW00.OZ0KW-UPDATE-SW == "N") /* ;error*/
              return;
            end
          else

    /* ------------------------------------------------------------*/
    /* change the row*/
    /* ------------------------------------------------------------*/
            if (OZ0KM001.OZ0KMSEL[OZ0KW-IXM] == "C")

              OZ0K-16300(); /* select row*/

              if (OZ0KW01.OZ0KW-ZZZCHGCT[OZ0KW-IXM] != VOCECB01.ZZZCHGCT)
                                   /* count*/
                                   /* changed*/

                OZWREC.XXXPRCNM = "16000"; /* module id*/
                converseLib.validationFailed(12); /* row was updated by another*/
                                   /* application*/
                OZ0KW00.OZ0KW-SWR = "Y"; /* request for data*/
                OZ0KW00.OZ0KW-SWF = "Y"; /* format a new map*/
                exit stack; /* exit to display map*/

              end

              VOCECB01.SOCD-CO-BULK-FL = OZ0KM001.OZ0KM-SEND-OCD[OZ0KW-IXM];

              set TA2REC empty;
              TA2REC.TA2DTOPT = "GREGGREG";
              TA2REC.TA2GRTYP = "E";
              TA2REC.TA2GRGE = VGVar.currentShortGregorianDate;

              call "TA0040" (TA2REC) {isNoRefresh = yes, isExternal = yes}; /* valid date pgm*/

              if (TA2REC.TA2RTNCD == 0)
                VOCECB01.ZZZCHGDT = TA2REC.TA2GRGGN;
              else
                OZWREC.XXXPRCNM = "15200"; /* Module identification*/
                converseLib.validationFailed(10); /* unexpected error*/
              end

              OZ0KW00.OZ0KW-TIME-BRK = VGVar.currentFormattedTime;
              OZ0KW00.OZ0KW-TM-HH = OZ0KW00.OZ0KW-TB-HH;
              OZ0KW00.OZ0KW-TM-MM = OZ0KW00.OZ0KW-TB-MM;
              VOCECB01.ZZZCHGTM = OZ0KW00.OZ0KW-TIME-MK;

    /* ------------------------------------------------------------*/
    /* check to make sure the xbdcd or cust hasn't changed - part of*/
    /* the key , if so, must add new row and delete the old one.*/
    /* ------------------------------------------------------------*/
              if (OZ0KW01.OZ0KW-CUST[OZ0KW-IXM] == OZ0KW00.OZ0KW-SV-CUST && 
              OZ0KW01.OZ0KW-XBDCD[OZ0KW-IXM] == 
              OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM])

                OZ0K-16400(); /* update table row*/

              else

                VOCECB01.OP1CUOID = OZ0KW00.OZ0KW-SV-CUST;
                VOCECB01.XBDCD = OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM];
                VOCECB01.ZZZSDT = TA2REC.TA2GRGGN;
                VOCECB01.ZZZSDTM = OZ0KW00.OZ0KW-TIME-MK;

                OZ0K-16200(); /* add new row*/

                if (SQLCA.VAGen_SQLCODE == 0)
                  OZ0K-16100(); /* delete old row*/
                end

              end
            end

          end

        end

      end

      OZ0KW-IXM = OZ0KW-IXM + 1;

    /* ------------------------------------------------------------*/
    /* Indicate that the table update was successfully completed.*/
    /* ------------------------------------------------------------*/
      if (OZ0KW00.OZ0KW-UPDATE-SW == "Y") /* ;*/
        OZWREC.XXXPRCNM = "16000"; /* show message*/
        OZWREC.XXXMSGNO = 1; /* Table update was completed*/
      end

    end /* end of while loop*/

  /* ------------------------------------------------------------*/
  /* Commit all changes made to protected resources.*/
  /* ------------------------------------------------------------*/

    VGVar.handleSysLibraryErrors = 0;
    sysLib.commit(); /* Commit all changes*/

  end
end // end OZ0K-16000

