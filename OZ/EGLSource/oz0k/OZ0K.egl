// Add page to temporary storage
Function OZ0K-11600()
  /* ------------------------------------------------------------*/
  /* Add the next page to temporary storage.*/
  /* ------------------------------------------------------------*/

  OZ0KW01.XXXTSLEN = OZ0KW00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5FNCCD = "A"; /* Function code*/

  try
    call "TA0050" (OZ0KW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

  /* ------------------------------------------------------------*/
  /* If successful, initialize the storage area and indicators*/
  /* to be used.  Return to the previous process.*/
  /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    set OZ0KW01 empty; /* M001 display storage*/

    OZ0KW00.TA5TSQIX = TA5REC.TA5TSQIX; /* Highest page number*/
    TA5REC.TA5TSQIX = 0; /* Current page number*/

    return; /* Return to previous process*/

  end

  /* ------------------------------------------------------------*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  OZWREC.XXXPRCNM = "11600"; /* Module identification*/
  converseLib.validationFailed(10); /* Unexpected error occurred*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "ADD PAGE TO TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = OZWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end OZ0K-11600

// Close selected set cursor
Function OZ0K-11700()

  if (OZ0KM001.OZ0KM-HDR-CUST > " ")
    try
      call "VOCECB01" ("C1", SQLCA, VOCECB01) {isNoRefresh = yes, isExternal = yes};
    end
                                   /* Close table set*/
  else
    try
      call "VOCECB01" ("C2", SQLCA, VOCECB01) {isNoRefresh = yes, isExternal = yes};
    end
                                   /* Close table set*/
  end

  /* ------------------------------------------------------------*/
  /* If successful, return to the previous process.*/
  /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && SQLCA.VAGen_SQLCODE == 0)      /* and normal return code*/

    return; /* Return to previous process*/

  end

  /* ------------------------------------------------------------*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/

  TA1REC.TA1MAP = sysVar.errorCode; /* CSP return code*/
  move SQLCA to ERRSQLCA withV60Compat;

  OZWREC.XXXPRCNM = "11700"; /* Module identification*/
  converseLib.validationFailed(10); /* Unexpected error occurred*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "DB2 "; /* Type of error*/
  TA1REC.TA1LOC = "CLOSE SELECTED SET OF TABLE ROWS";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = OZWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1DBASE = "DORDCD  "; /* Data base name*/
  TA1REC.TA1TBLVU = "VOCECB01"; /* Table view name*/

  call "TA0020" (TA1REC, ERRSQLCA) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end OZ0K-11700

// Format and converse the map
Function OZ0K-12000()
  /* ------------------------------------------------------------*/
  /* If an error was found, save the error message number.*/
  /* ------------------------------------------------------------*/

  if (converseVar.validationMsgNum != 0) /* Error was found*/
    OZWREC.XXXMSGNO = converseVar.validationMsgNum; /* Message number*/
  end

  /* ------------------------------------------------------------*/
  /* If obtain data failed or no error, format data to display.*/
  /* ------------------------------------------------------------*/

  if (OZ0KW00.OZ0KW-SWR == "Y" /* Obtain data failed*/
   || converseVar.validationMsgNum == 0) /* or no error found*/

    if (OZ0KW00.OZ0KW-SWF == "Y") /* Map must be formatted*/
      OZ0KW00.OZ0KW-HDR-CUST = OZ0KM001.OZ0KM-HDR-CUST;
      OZ0KW00.OZ0KW-HDR-CUST-NAME = OZ0KM001.OZ0KM-HDR-CUST-NAME;
      OZ0K-12100(); /* Format the data on the map*/
    end

  end

  /* ------------------------------------------------------------*/
  /* Format the current date and any FAST PATH information.*/
  /* ------------------------------------------------------------*/

  OZ0KM001.XXXCURDT = VGVar.currentShortGregorianDate; /* Current date*/

  OZ0KM001.CATOAP = COMMAREA.CATOAP; /* TO application*/
  OZ0KM001.CAITEM = COMMAREA.CAITEM; /* Application data*/

  /* ------------------------------------------------------------*/
  /* If a message to display, indicate sound audible alarm.*/
  /* ------------------------------------------------------------*/

  if (OZWREC.XXXMSGNO != 0 /* Message to display*/
   || COMMAREA.CAMSG != " ") /* or message text*/

    if (OZWREC.XXXMSGNO != 0) /* Message to display*/

      if (OZWREC.XXXMSGNO == 9999)
        converseLib.validationFailed();
      else
        converseLib.validationFailed(OZWREC.XXXMSGNO);
      end /* Message number*/
    else
      OZ0KM001.VAGen_EZEMSG = COMMAREA.CAMSG; /* Message text*/

    end

    OZ0KM001.XXXPRCNM = OZWREC.XXXPRCNM; /* Module identification*/

    set OZ0KM001 alarm; /* Indicate sound alarm*/

  end

  converse OZ0KM001 ;

  /* ------------------------------------------------------------*/
  /* Clear the message fields and save FAST PATH information.*/
  /* ------------------------------------------------------------*/

  OZWREC.XXXPRCNM = " "; /* Module identification*/
  OZWREC.XXXMSGNO = 0; /* Message number*/
  COMMAREA.CAMSG = " "; /* Message text*/
  OZ0KW00.OZ0KW-SFN = " "; /* reset function switch*/
   /* reset saved page*/
  OZ0KW00.OZ0KW-SAVE-PAGE = 0;
  COMMAREA.CATOAP = OZ0KM001.CATOAP; /* TO application*/
  COMMAREA.CAITEM = OZ0KM001.CAITEM; /* Application data*/
end // end OZ0K-12000

// Format the display data
Function OZ0K-12100()
  /* ------------------------------------------------------------*/
  /* Clear the fields in the map storage area to be formatted.*/
  /* ------------------------------------------------------------*/

  set OZ0KM001 initial; /* M001 input/output area*/

  OZ0KW00.OZ0KW-SWF = "N"; /* Format the map switch*/

  XX0XS01(); /* get system date and time*/

  /* ------------------------------------------------------------*/

  if (OZ0KW00.OZ0KW-SAVE-PAGE != 0) /* if page saved off*/

    OZ0KW00.XXXTSPAG = OZ0KW-SAVE-PAGE; /* use saved page*/

    OZ0K-12110(); /* Obtain temporary storage page*/

    /* ------------------------------------------------------------*/
    /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set OZ0KM001.XXXLITPG skip; /* "PAGE" literal*/
    OZ0KM001.XXXTSPAG = OZ0KW00.XXXTSPAG; /* Current page*/
    set OZ0KM001.XXXLITOF skip; /* "OF" literal*/
    OZ0KM001.TA5TSQIX = OZ0KW00.TA5TSQIX; /* Highest page*/

  /* ------------------------------------------------------------*/
  /* If the page in storage is not the page to be displayed,*/
  /* obtain the desired page from temporary storage.*/
  /* ------------------------------------------------------------*/

  else
    if (OZ0KW00.TA5TSQIX > 0) /* Pages were formatted*/

      if (OZ0KW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

        /* ------------------------------------------------------------*/
        /* If the current page has been changed, update the page in*/
        /* storage before obtaining the next one to display.*/
        /* ------------------------------------------------------------*/

        if (OZ0KW00.OZ0KW-SWU == "Y") /* Current page was changed*/

          OZ0K-12120(); /* Update temporary storage page*/

          OZ0KW00.OZ0KW-SWU = "N"; /* Storage update switch*/

        end

        /* ------------------------------------------------------------*/
        /* If the desired page number is greater than the highest page*/
        /* in storage, default to the highest page number.*/
        /* ------------------------------------------------------------*/

        if (OZ0KW00.XXXTSPAG > OZ0KW00.TA5TSQIX) /* Desired greater highest*/
          OZ0KW00.XXXTSPAG = OZ0KW00.TA5TSQIX; /* Default to highest*/
        end

        OZ0K-12110(); /* Obtain temporary storage page*/

      end

      /* ------------------------------------------------------------*/
      /* Display the page information on the screen.*/
      /* ------------------------------------------------------------*/

      set OZ0KM001.XXXLITPG skip; /* "PAGE" literal*/
      OZ0KM001.XXXTSPAG = OZ0KW00.XXXTSPAG; /* Current page*/
      set OZ0KM001.XXXLITOF skip; /* "OF" literal*/
      OZ0KM001.TA5TSQIX = OZ0KW00.TA5TSQIX; /* Highest page*/

    end

  end

  /* ------------------------------------------------------------*/
  /* Format the data in the map from the display storage.*/
  /* ------------------------------------------------------------*/

  OZ0KW00.OZ0KW-IXM = 1; /* Initialize map index*/
  while (OZ0KW00.OZ0KW-IXM <= 15
   && OZ0KW01.OZ0KW-XBDCD[OZ0KW-IXM] > " ")

    OZ0KM001.OZ0KMSEL[OZ0KW-IXM] = OZ0KW01.OZ0KW-SEL[OZ0KW-IXM];

    if (OZ0KW01.OZ0KW-CUST[OZ0KW-IXM] == 0)
      OZ0KM001.OZ0KM-CUST[OZ0KW-IXM] = "ALL";
      OZ0KM001.OZ0KM-CUST-NAME[OZ0KW-IXM] = "ALL CUSTOMERS";
    else
      OZ0KW00.OZ0KW-SV-CUST = OZ0KW01.OZ0KW-CUST[OZ0KW-IXM];
      OZ0KW00.OZ0KW-CUST-BLD = OZ0KW00.OZ0KW-SV-CUST;

      OZ0KW00.OZ0KW-IX1 = 1;
      while (OZ0KW00.OZ0KW-IX1 < 8 && 
      OZ0KW00.OZ0KW-CUST-BLD-1[OZ0KW-IX1] == "0")
        OZ0KW00.OZ0KW-CUST-BLD-1[OZ0KW-IX1] = " ";
        OZ0KW00.OZ0KW-IX1 = OZ0KW00.OZ0KW-IX1 + 1;
      end

      OZ0KM001.OZ0KM-CUST[OZ0KW-IXM] = OZ0KW00.OZ0KW-CUST-BLD;
      OZ0KM001.OZ0KM-CUST-NAME[OZ0KW-IXM] = OZ0KW01.OZ0KW-CUST-NAME[OZ0KW-IXM];
    end

    OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM] = OZ0KW01.OZ0KW-XBDCD[OZ0KW-IXM];
    OZ0KM001.OZ0KM-XBD-DESC[OZ0KW-IXM] = OZ0KW01.OZ0KW-XBD-DESC[OZ0KW-IXM];
    OZ0KM001.OZ0KM-SEND-OCD[OZ0KW-IXM] = OZ0KW01.OZ0KW-SEND-OCD[OZ0KW-IXM];

    OZ0KW00.OZ0KW-DATE-BRK = OZ0KW01.OZ0KW-ZZZSDT[OZ0KW-IXM];

    OZ0KW00.OZ0KW-DM-YY = OZ0KW00.OZ0KW-DB-YY;
    OZ0KW00.OZ0KW-DM-MM = OZ0KW00.OZ0KW-DB-MM;
    OZ0KW00.OZ0KW-DM-DD = OZ0KW00.OZ0KW-DB-DD;
    OZ0KW00.OZ0KW-DM-DASH1 = "-";
    OZ0KW00.OZ0KW-DM-DASH2 = "-";

    OZ0KM001.OZ0KM-SETUP-DATE[OZ0KW-IXM] = OZ0KW00.OZ0KW-DATE-MK;

    OZ0KW00.OZ0KW-IXM = OZ0KW00.OZ0KW-IXM + 1;
                                   /* Increment map index*/
  end

  OZ0KW00.OZ0KW-IXM = 1;
  while (OZ0KW00.OZ0KW-IXM < 16)
    if (OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM] == "  ")
      set OZ0KM001.OZ0KMSEL[OZ0KW-IXM] noHighLight, skip;
      set OZ0KM001.OZ0KM-CUST[OZ0KW-IXM] noHighLight, skip;
      set OZ0KM001.OZ0KM-SEND-OCD[OZ0KW-IXM] noHighLight, skip;
      set OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM] noHighLight, skip;
      set OZ0KM001.OZ0KM-XBD-DESC[OZ0KW-IXM] invisible, skip;
      set OZ0KM001.OZ0KM-CUST-NAME[OZ0KW-IXM] invisible, skip;
      set OZ0KM001.OZ0KM-SETUP-DATE[OZ0KW-IXM] invisible, skip;
    end
    OZ0KW00.OZ0KW-IXM = OZ0KW00.OZ0KW-IXM + 1;
  end

  if (COMMAREA.CACHGFL == "N")
    OZ0KW00.OZ0KW-IXM = 1;
    while (OZ0KW00.OZ0KW-IXM < 16)
      set OZ0KM001.OZ0KMSEL[OZ0KW-IXM] noHighLight, skip;
      set OZ0KM001.OZ0KM-CUST[OZ0KW-IXM] noHighLight, skip;
      set OZ0KM001.OZ0KM-SEND-OCD[OZ0KW-IXM] noHighLight, skip;
      set OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM] noHighLight, skip;
      OZ0KW00.OZ0KW-IXM = OZ0KW00.OZ0KW-IXM + 1;
    end
    set OZ0KM001.OZ0KM-HDR-CUST cursor;
  end

  OZ0KM001.OZ0KM-HDR-CUST = OZ0KW00.OZ0KW-HDR-CUST;
  OZ0KM001.OZ0KM-HDR-CUST-NAME = OZ0KW00.OZ0KW-HDR-CUST-NAME;

end // end OZ0K-12100

// Read temporary storage page
Function OZ0K-12110()
  /* ------------------------------------------------------------*/
  /* Obtain the next page of data to display.*/
  /* ------------------------------------------------------------*/

  OZ0KW01.XXXTSLEN = OZ0KW00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5TSQIX = OZ0KW00.XXXTSPAG; /* Page number*/
  TA5REC.TA5FNCCD = "R"; /* Function code*/

  try
    call "TA0050" (OZ0KW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

  /* ------------------------------------------------------------*/
  /* If successful, return to the previous process.*/
  /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    return; /* Return to previous process*/

  end

  /* ------------------------------------------------------------*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  OZWREC.XXXPRCNM = "12110"; /* Module identification*/
  converseLib.validationFailed(10); /* Unexpected error occurred*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "READ PAGE FROM TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = OZWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end OZ0K-12110

// Update temporary storage page
Function OZ0K-12120()
  /* ------------------------------------------------------------*/
  /* Update the page in temporary storage.*/
  /* ------------------------------------------------------------*/

  TA5REC.TA5FNCCD = "U"; /* Function code*/

  try
    call "TA0050" (OZ0KW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

  /* ------------------------------------------------------------*/
  /* If successful, return to the previous process.*/
  /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    return; /* Return to previous process*/

  end

  /* ------------------------------------------------------------*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  OZWREC.XXXPRCNM = "12120"; /* Module identification*/
  converseLib.validationFailed(10); /* Unexpected error occurred*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "UPDATE PAGE IN TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = OZWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end OZ0K-12120

// Check attention identifier
Function OZ0K-13000()
  /* -------------------------------------------------------------*/
  /* If CANCEL was requested, clear the map fields and exit.*/
  /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pa2) /* PA2 was pressed*/

    if (OZ0KW00.OZ0KW-SWR == "Y") /* Request for data*/
      set OZ0KW01 empty; /* M001 display storage*/
    end

    OZ0KW00.OZ0KW-SWF = "Y"; /* Format the map switch*/
    OZ0KW00.OZ0KW-SWR = "N"; /* Request for data switch*/

    COMMAREA.CATOAP = " "; /* TO application*/
    COMMAREA.CAITEM = " "; /* Application data*/

    exit stack; /* Exit to display map*/

  end

  /* ------------------------------------------------------------*/
  /* If FAST PATH was requested, transfer to the application.*/
  /* ------------------------------------------------------------*/
  if (OZ0KM001.CATOAP > " ") /* FAST PATH was requested*/

    if (converseVar.eventKey is enter) /* ENTER was pressed*/

      if (OZWREC.OZW-APPL[OZW-APPIDX] != "OZ0K")
        OZWREC.OZW-APPIDX = OZWREC.OZW-APPIDX + 1;
        OZWREC.OZW-APPL[OZW-APPIDX] = "OZ0K";
      end

      OZ0K-11100(); /* Purge temporary storage*/

      XSOZ-S0001(); /* Exit to another application*/

      set OZ0KM001.CATOAP cursor, bold; /* Highlight*/

    else

      OZWREC.XXXPRCNM = "13000"; /* Module identification*/
      converseLib.validationFailed(5); /* PA/PF key may not be used*/

    end

    exit stack; /* Exit to display map*/

  end

  /* ------------------------------------------------------------*/
  /* Determine whether a valid attention identifier was used to*/
  /* invoke the transaction.  Display an error message to the*/
  /* user if the PF key used is not supported.*/
  /* ------------------------------------------------------------*/
  if (converseVar.eventKey not enter /* ENTER was not pressed*/
   && converseVar.eventKey not pf1   
   && converseVar.eventKey not pf3   
   && converseVar.eventKey not pf4   
   && converseVar.eventKey not pf7   /* and PF7 was not pressed*/
   && converseVar.eventKey not pf8   /* and PF8 was not pressed*/
   && converseVar.eventKey not pf13) /* and PF13 was not pressed*/

    OZWREC.XXXPRCNM = "13000"; /* Module identification*/
    converseLib.validationFailed(4); /* PA/PF key pressed is not valid*/

    if (converseVar.eventKey is pakey) /* PA key was pressed*/
      converseLib.clearScreen();
       /* Clear screen before display*/
    end

    exit stack; /* Exit to display map*/

  else

    if (OZWREC.OZW-APPL[OZW-APPIDX] != "OZ0K")
      OZWREC.OZW-APPIDX = OZWREC.OZW-APPIDX + 1;
      OZWREC.OZW-APPL[OZW-APPIDX] = "OZ0K";
    end

  end

  /* ------------------------------------------------------------*/
  /* If HELP, exit to the previous level or exit to the Master*/
  /* Menu was requested, exit to another application.*/
  /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf1 /* PF1 was pressed*/
   || converseVar.eventKey is pf3 /* or PF3 was pressed*/
   || converseVar.eventKey is pf4) /* or PF4 was pressed*/

    if (converseVar.eventKey is pf3) /* PF3 was pressed*/

      if (OZWREC.OZW-APPIDX > 1)
        OZWREC.OZW-APPIDX = OZWREC.OZW-APPIDX - 1;

        if (OZWREC.OZW-APPL[OZW-APPIDX] > "    ")
          COMMAREA.CATOAP = OZWREC.OZW-APPL[OZW-APPIDX];
        else
          COMMAREA.CATOAP = "OZ0A";
        end

      else
        COMMAREA.CATOAP = COMMAREA.CAFROMAP;
      end

    end

    OZ0K-11100(); /* Purge temporary storage*/

    XSOZ-S0001(); /* Exit to another application*/

    exit stack; /* Exit to display map*/

  end

  /* ------------------------------------------------------------*/
  /* pf13 - add*/
  /* ------------------------------------------------------------*/
  if (converseVar.eventKey is pf13) /* PF13 was pressed*/
    if (COMMAREA.CACHGFL == "N")
      converseLib.validationFailed(74); /* no authority*/
      OZ0KW00.OZ0KW-SWR = "N";
      exit stack;
    else
      if ("A" in OZ0KM001.OZ0KMSEL)
        OZ0KW00.OZ0KW-SWR = "Y"; /* Request for data switch*/
        OZ0KW00.OZ0KW-SWF = "Y";
      else
        OZ0KM001.OZ0KMSEL[1] = "A";
        set OZ0KM001.OZ0KMSEL[1] cursor, initialAttributes;
        OZ0KM001.OZ0KM-CUST[1] = " ";
        set OZ0KM001.OZ0KM-CUST[1] initialAttributes;
        OZ0KM001.OZ0KM-CUST-NAME[1] = " ";
        OZ0KM001.OZ0KM-XBDCD[1] = "  ";
        set OZ0KM001.OZ0KM-XBDCD[1] initialAttributes;
        OZ0KM001.OZ0KM-XBD-DESC[1] = " ";
        OZ0KM001.OZ0KM-SEND-OCD[1] = " ";
        set OZ0KM001.OZ0KM-SEND-OCD[1] initialAttributes;
        OZ0KM001.OZ0KM-SETUP-DATE[1] = " ";
        OZ0KM001.VAGen_EZEMSG = " ";
        converseLib.validationFailed(0); /* PA/PF key pressed is not valid*/
        OZ0KW00.OZ0KW-SWR = "N"; /* Request for data switch*/
        exit stack; /* Exit to display map*/
      end
    end
  end

  /* ------------------------------------------------------------*/
  /* enter*/
  /* ------------------------------------------------------------*/
  if (converseVar.eventKey is enter)

    OZ0KW00.OZ0KW-SWR = "Y"; /* Request for data switch*/
    OZ0KW00.OZ0KW-SWF = "Y";

    if (OZ0KM001 not modified)
      OZ0KW00.XXXTSPAG = 1;
      exit stack;
    end

    if (OZ0KM001.OZ0KM-HDR-CUST == OZ0KW00.OZ0KW-HDR-CUST)
     /* continue*/
    else
      return;
    end

  end

end // end OZ0K-13000

// Reset highlighted fields
Function OZ0K-14000()
  /* ------------------------------------------------------------*/
  /* Reset all modifiable field attributes to normal intensity.*/
  /* ------------------------------------------------------------*/

  OZ0KW00.OZ0KW-IXM = 1; /* Initialize line index*/

  while (OZ0KW00.OZ0KW-IXM <= 15 /* * Until end of lines*/
   && COMMAREA.CACHGFL == "Y")

    set OZ0KM001.OZ0KMSEL[OZ0KW-IXM] normal; /* Reset select flag*/
    set OZ0KM001.OZ0KM-XBDCD[OZ0KW-IXM] normal;
    set OZ0KM001.OZ0KM-XBD-DESC[OZ0KW-IXM] normal;
    set OZ0KM001.OZ0KM-CUST[OZ0KW-IXM] normal;
    set OZ0KM001.OZ0KM-CUST-NAME[OZ0KW-IXM] normal;
    set OZ0KM001.OZ0KM-SEND-OCD[OZ0KW-IXM] normal;
    set OZ0KM001.OZ0KM-SETUP-DATE[OZ0KW-IXM] normal;

    OZ0KW00.OZ0KW-IXM = OZ0KW00.OZ0KW-IXM + 1; /* Increment line index*/

  end

  set OZ0KM001.CATOAP initialAttributes; /* TO application*/
  set OZ0KM001.CAITEM initialAttributes; /* Application data*/

end // end OZ0K-14000

// Verify data that was entered
Function OZ0K-15000()
  /* ------------------------------------------------------------*/
  /* Highlight any application data that was entered.*/
  /* ------------------------------------------------------------*/

  if (OZ0KM001.CAITEM > " ") /* Application data was entered*/

    OZWREC.XXXPRCNM = "15000"; /* Module identification*/
    set OZ0KM001.CAITEM cursor, bold; /* Highlight*/
    converseLib.validationFailed(7); /* Enter only with FAST PATH*/

  end


  /* ------------------------------------------------------------*/
  /* Determine whether a valid request to scroll was entered.*/
  /* ------------------------------------------------------------*/

  if (converseVar.eventKey is pf7 /* Scroll back request*/
   || converseVar.eventKey is pf8) /* or scroll forward request*/

    OZ0K-15100(); /* Verify scroll request*/

  else

    /* ------------------------------------------------------------*/
    /* Otherwise, verify data in the header.*/
    /* ------------------------------------------------------------*/

    if (converseVar.eventKey is enter && 
    OZ0KM001.OZ0KM-HDR-CUST > "  ")
      OZ0KW00.OZ0KW-CUST-BLD = OZ0KM001.OZ0KM-HDR-CUST;
      OZ0KW00.OZ0KW-IXM = 1; /* initialize index*/
      while (OZ0KW00.OZ0KW-IXM < 8) /* until end of chars*/
        if (OZ0KW00.OZ0KW-CUST-BLD-1[OZ0KW-IXM] == " ")
          /* continue*/
        else
          if (OZ0KW00.OZ0KW-CUST-BLD-1[OZ0KW-IXM] is numeric)
            /* continue*/
          else
            OZWREC.XXXPRCNM = "15200"; /* Module identification*/
            set OZ0KM001.OZ0KM-HDR-CUST cursor, bold; /* Highlight*/
            converseLib.validationFailed(51); /* invalid customer*/
