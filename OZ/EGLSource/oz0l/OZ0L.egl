// Format the display data
Function OZ0L-12100()
  /* ------------------------------------------------------------*/
  /* Clear the fields in the map storage area to be formatted.*/
  /* ------------------------------------------------------------*/

  set OZ0LM001 initial; /* M001 input/output area*/

  OZ0LW00.OZ0LW-SWF = "N"; /* Format the map switch*/

  XX0XS01(); /* get system date and time*/

  /* ------------------------------------------------------------*/

  if (OZ0LW00.OZ0LW-SAVE-PAGE != 0) /* if page saved off*/

    OZ0LW00.XXXTSPAG = OZ0LW-SAVE-PAGE; /* use saved page*/

    OZ0L-12110(); /* Obtain temporary storage page*/

    /* ------------------------------------------------------------*/
    /* Display the page information on the screen.*/
    /* ------------------------------------------------------------*/

    set OZ0LM001.XXXLITPG skip; /* "PAGE" literal*/
    OZ0LM001.XXXTSPAG = OZ0LW00.XXXTSPAG; /* Current page*/
    set OZ0LM001.XXXLITOF skip; /* "OF" literal*/
    OZ0LM001.TA5TSQIX = OZ0LW00.TA5TSQIX; /* Highest page*/

  /* ------------------------------------------------------------*/
  /* If the page in storage is not the page to be displayed,*/
  /* obtain the desired page from temporary storage.*/
  /* ------------------------------------------------------------*/

  else
    if (OZ0LW00.TA5TSQIX > 0) /* Pages were formatted*/

      if (OZ0LW00.XXXTSPAG != TA5REC.TA5TSQIX) /* Desired page not current*/

        /* ------------------------------------------------------------*/
        /* If the current page has been changed, update the page in*/
        /* storage before obtaining the next one to display.*/
        /* ------------------------------------------------------------*/

        if (OZ0LW00.OZ0LW-SWU == "Y") /* Current page was changed*/

          OZ0L-12120(); /* Update temporary storage page*/

          OZ0LW00.OZ0LW-SWU = "N"; /* Storage update switch*/

        end

        /* ------------------------------------------------------------*/
        /* If the desired page number is greater than the highest page*/
        /* in storage, default to the highest page number.*/
        /* ------------------------------------------------------------*/

        if (OZ0LW00.XXXTSPAG > OZ0LW00.TA5TSQIX) /* Desired greater highest*/
          OZ0LW00.XXXTSPAG = OZ0LW00.TA5TSQIX; /* Default to highest*/
        end

        OZ0L-12110(); /* Obtain temporary storage page*/

      end

      /* ------------------------------------------------------------*/
      /* Display the page information on the screen.*/
      /* ------------------------------------------------------------*/

      set OZ0LM001.XXXLITPG skip; /* "PAGE" literal*/
      OZ0LM001.XXXTSPAG = OZ0LW00.XXXTSPAG; /* Current page*/
      set OZ0LM001.XXXLITOF skip; /* "OF" literal*/
      OZ0LM001.TA5TSQIX = OZ0LW00.TA5TSQIX; /* Highest page*/

    end

  end

  /* ------------------------------------------------------------*/
  /* Format the data in the map from the display storage.*/
  /* ------------------------------------------------------------*/

  OZ0LW00.OZ0LW-IXM = 1; /* Initialize map index*/
  while (OZ0LW00.OZ0LW-IXM <= 15) /* Until screen is full*/

    OZ0LM001.OZ0LMSEL[OZ0LW-IXM] = OZ0LW01.OZ0LW-SEL[OZ0LW-IXM];
    OZ0LM001.OZ0LM-XOMCD[OZ0LW-IXM] = OZ0LW01.OZ0LW-XOMCD[OZ0LW-IXM];
    OZ0LM001.OZ0LM-XOM-ORD-MNTN-ABRV[OZ0LW-IXM] = OZ0LW01.OZ0LW-XOM-ORD-MNTN-ABRV[OZ0LW-IXM];
    OZ0LM001.OZ0LM-XXX-STAT[OZ0LW-IXM] = OZ0LW01.OZ0LW-XXX-STAT[OZ0LW-IXM];
    OZ0LM001.OZ0LM-SOCD-CUST-RPT-FL[OZ0LW-IXM] = OZ0LW01.OZ0LW-CUST-RPT-FL[OZ0LW-IXM];
    OZ0LM001.OZ0LM-SOCD-CHNG-RSN-DESC[OZ0LW-IXM] = OZ0LW01.OZ0LW-CHNG-RSN-DESC[OZ0LW-IXM];

    if (OZ0LM001.OZ0LM-XOMCD[OZ0LW-IXM] == "  "
     || COMMAREA.CACHGFL == "N")
      set OZ0LM001.OZ0LMSEL[OZ0LW-IXM] noHighLight, skip;
      set OZ0LM001.OZ0LM-XOMCD[OZ0LW-IXM] noHighLight, skip;
      set OZ0LM001.OZ0LM-XOM-ORD-MNTN-ABRV[OZ0LW-IXM] noHighLight, skip;
      set OZ0LM001.OZ0LM-XXX-STAT[OZ0LW-IXM] noHighLight, skip;
      set OZ0LM001.OZ0LM-SOCD-CUST-RPT-FL[OZ0LW-IXM] noHighLight, skip;
      set OZ0LM001.OZ0LM-SOCD-CHNG-RSN-DESC[OZ0LW-IXM] noHighLight, skip;
    end

    OZ0LW00.OZ0LW-IXM = OZ0LW00.OZ0LW-IXM + 1;
                                   /* Increment map index*/

  end

  if (COMMAREA.CACHGFL == "N")
    set OZ0LM001.CATOAP cursor;
  end

end // end OZ0L-12100

// Read temporary storage page
Function OZ0L-12110()
  /* ------------------------------------------------------------*/
  /* Obtain the next page of data to display.*/
  /* ------------------------------------------------------------*/

  OZ0LW01.XXXTSLEN = OZ0LW00.XXXTSLEN; /* Temporary storage length*/

  TA5REC.TA5APPID = COMMAREA.CACURRAP; /* Application identification*/
  TA5REC.TA5TSQIX = OZ0LW00.XXXTSPAG; /* Page number*/
  TA5REC.TA5FNCCD = "R"; /* Function code*/

  try
    call "TA0050" (OZ0LW01, TA5REC) {isNoRefresh = yes, isExternal = yes};
  end /* Temporary storage*/

  /* ------------------------------------------------------------*/
  /* If successful, return to the previous process.*/
  /* ------------------------------------------------------------*/

  if (sysVar.errorCode == "00000000" /* Call successful*/
   && TA5REC.TA5RTNCD == 0)          /* and normal return code*/

    return; /* Return to previous process*/

  end

  /* ------------------------------------------------------------*/
  /* Otherwise, indicate that an unexpected error has occurred.*/
  /* ------------------------------------------------------------*/

  TAEREC.TAEDESC1 = sysVar.errorCode; /* CSP return code*/
  TAEREC.TAEDESC2 = TA5REC.TA5DATA; /* Subroutine parameters*/

  OZWREC.XXXPRCNM = "12110"; /* Module identification*/
  converseLib.validationFailed(10); /* Unexpected error occurred*/

  TA1REC.TA1FUNC = "RETN"; /* Termination function*/
  TA1REC.TA1TYPE = "APPL"; /* Type of error*/
  TA1REC.TA1LOC = "READ PAGE FROM TEMPORARY STORAGE";
  TA1REC.TA1PGMAP = COMMAREA.CACURRAP; /* Program application*/
  TA1REC.TA1PGMNO = OZWREC.XXXPRCNM; /* Program number*/
  TA1REC.TA1MAPAP = COMMAREA.CACURRAP; /* Map application*/
  TA1REC.TA1MAPNO = "M001"; /* Map number*/

  call "TA0020" (TA1REC, TAEREC) {isExternal = yes}; /* Error diagnosis routine*/

  exit stack; /* Exit to display map*/


end // end OZ0L-12110

