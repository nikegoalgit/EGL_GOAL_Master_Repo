// EDIT MAP PAGE NUMBER
Function DT0BP62-EDIT-PAGE()
  /* ***********************************************************/
  /* Edit map current page number*/
  /* ***********************************************************/

  if (DT0BM001.DAW-CURR-PAGE == DAWREC.DAW-CURR-PAGE)
    return;
  end

  /* if select codes entered and page number changed*/
  /* then reset page number*/

  if (DAWREC.DAW-ADD-ROW-FL == "Y"
   || DAWREC.DAW-DELETE-ROW-FL == "Y"
   || DAWREC.DAW-SELECT-ROW-FL == "Y"
   || DAWREC.DAW-COPY-ROW-FL == "Y")
    set DT0BM001.DAW-CURR-PAGE bold;
    DT0BM001.VAGen_EZEMSG = "CANNOT MAINTAIN OR ADD A ROW AND CHANGE PAGE AT THE SAME TIME";
    DT0BM001.DAW-CURR-PAGE = DAWREC.DAW-CURR-PAGE;
    DT0BM001.DAW-INFO-MESSAGE = "PAGE NUMBER HAS BEEN RESET - HIT ENTER OR PF11 TO CONTINUE";
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
  end

  if (DT0BM001.DAW-CURR-PAGE > 1)
    if (DT0BM001.DAW-CURR-PAGE > DT0BM001.DAW-LAST-PAGE)
      set DT0BM001.DAW-CURR-PAGE cursor, bold;
      converseLib.validationFailed(006); /* page number entered is greater than*/
      DAWREC.DAW-EDIT-ERROR-FL = "Y"; /* last page available*/
    end
  end

  if (DT0BM001.DAW-CURR-PAGE == 0)
    set DT0BM001.DAW-CURR-PAGE cursor, bold;
    converseLib.validationFailed(007); /* page number entered is zero - page*/
    DAWREC.DAW-EDIT-ERROR-FL = "Y"; /* does not exist*/
  end

  /* **************/
  /* ****************** END DT0BP62 ****************************/
  /* **************/
end // end DT0BP62-EDIT-PAGE

