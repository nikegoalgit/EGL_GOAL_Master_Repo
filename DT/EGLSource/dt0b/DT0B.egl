// Close cursor based on search
Function DT0B-CLOSE-XCY()
  /* **************************************************************/
  /* Determine and close the correct XCY cursor*/
  /* **************************************************************/
  /* Use the C1 DB2 function if no search criteria*/
  if (DT0BM001.CNTY-CD-LO == " " && 
  DT0BM001.CNTY-CD-HI == " ")
    WDA1160.WDA1160-DB2-FUNCTION = "C1";
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* USE THE C2 DB2 function*/
  if ((DT0BM001.CNTY-CD-LO > " " && 
  DT0BM001.CNTY-CD-HI > " ")
   || (DT0BM001.CNTY-CD-LO == " " && 
  DT0BM001.CNTY-CD-HI > " "))
    WDA1160.WDA1160-DB2-FUNCTION = "C2";
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* USE THE CA DB2 function*/
  if (DT0BM001.CNTY-CD-LO > " " && 
  DT0BM001.CNTY-CD-HI == " ")
    WDA1160.WDA1160-DB2-FUNCTION = "CA";
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  if (SQLCA.VAGen_SQLCODE == 0) /* check for good rc*/
     /* next sentence*/
  else
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
    TA1REC.TA1TBLVU = "VXCY0001"; /* table view*/
    TA1REC.TA1LOCAT[1] = "CLOSE XCY";
    TA1REC.TA1LOCAT[2] = "WDA1160 ";
    XSDAS-DB2-ABEND-RB();
  end
end // end DT0B-CLOSE-XCY

// Fetch cursor based on search
Function DT0B-FETCH-XCY()
  /* **************************************************************/
  /* Determine and FETCH the correct XCY cursor*/
  /* **************************************************************/
  /* USE THE N1 DB2 function if no search criteria*/
  if (DT0BM001.CNTY-CD-LO == " " && 
  DT0BM001.CNTY-CD-HI == " ")
    WDA1160.WDA1160-DB2-FUNCTION = "N1";
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* USE THE N2 DB2 function*/
  if ((DT0BM001.CNTY-CD-LO > " " && 
  DT0BM001.CNTY-CD-HI > " ")
   || (DT0BM001.CNTY-CD-LO == " " && 
  DT0BM001.CNTY-CD-HI > " "))
    WDA1160.WDA1160-DB2-FUNCTION = "N2";
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* USE THE NA DB2 function*/
  if (DT0BM001.CNTY-CD-LO > " " && 
  DT0BM001.CNTY-CD-HI == " ")
    WDA1160.WDA1160-DB2-FUNCTION = "NA";
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  if (SQLCA.VAGen_SQLCODE == 0 /* check for good rc*/
   || SQLCA.VAGen_SQLCODE == 100) /* or not found*/
      /* next sentence*/
  else
    if (SQLCA.VAGen_SQLCODE == -911) /* database busy*/
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      DAWREC.DAW-INFO-MESSAGE = "DATABASE BUSY - PLEASE TRY AGAIN ";
    else
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
      TA1REC.TA1TBLVU = "VXCY0001"; /* table view*/
      TA1REC.TA1LOCAT[1] = "FETCH   ";
      XSDAS-DB2-ABEND-RB();
    end
  end
end // end DT0B-FETCH-XCY

// Open cursor based on search
Function DT0B-OPEN-XCY()
  /* **************************************************************/
  /* Determine and open the correct XCY cursor*/
  /* **************************************************************/
  set SQLCA empty;
  set VXCY0001 empty;
  WDA1160.WDA1160-CNTY-CD-LO = DT0BM001.CNTY-CD-LO;
  WDA1160.WDA1160-CNTY-CD-HI = DT0BM001.CNTY-CD-HI;

  /* Use the S1 DB2 function if no search criteria, get all rows*/
  if (DT0BM001.CNTY-CD-LO == " " && 
  DT0BM001.CNTY-CD-HI == " ")
    WDA1160.WDA1160-DB2-FUNCTION = "S1";
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* Use the S2 DB2 function*/
  if ((DT0BM001.CNTY-CD-LO > " " && 
  DT0BM001.CNTY-CD-HI > " ")
   || (DT0BM001.CNTY-CD-LO == " " && 
  DT0BM001.CNTY-CD-HI > " "))
    WDA1160.WDA1160-DB2-FUNCTION = "S2";
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* Use the SA DB2 function if search cnty-cd-lo only*/
  if (DT0BM001.CNTY-CD-LO > " " && 
  DT0BM001.CNTY-CD-HI == " ")
    WDA1160.WDA1160-DB2-FUNCTION = "SA";
    VXCY0001.XCYCD = DT0BM001.CNTY-CD-LO;
    call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};
  end

  if (SQLCA.VAGen_SQLCODE == 0 /* check for good rc*/
   || SQLCA.VAGen_SQLCODE == 100) /* or not found*/
     /* next sentence*/
  else
    if (SQLCA.VAGen_SQLCODE == -911) /* database busy*/
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      DAWREC.DAW-INFO-MESSAGE = "DATABASE BUSY - PLEASE TRY AGAIN ";
    else
      DAWREC.DAW-EDIT-ERROR-FL = "Y";

      TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
      TA1REC.TA1TBLVU = "VXCY0001"; /* table view*/
      TA1REC.TA1LOCAT[1] = "OPEN XCY";
      TA1REC.TA1LOCAT[2] = "WDA1160 ";
      XSDAS-DB2-ABEND-RB();
    end
  end
end // end DT0B-OPEN-XCY

// DRIVER FOR APPLICATION
Function DT0BP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DT0BP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DT0BP20-BUILD-MAP2(); /* Build the second map*/
  else
  /* Display first map for search*/
  /* ** PERFORM DT0BP20-BUILD-MAP1;    /* Build the first map*/
  end

  DT0BW01.DT0BW-CNTY-CD-LO = "999";
  DT0BW01.DT0BW-CNTY-CD-HI = "999";

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DT0BP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DT0BP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DT0BP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DT0BP00 *******************************/
  /* **************/
end // end DT0BP00-DRIVER

// INITIALIZE THE APPLICATION
Function DT0BP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DT0BW01 empty; /* Working storage record*/
  set DT0BW02 empty; /* Temp storage record*/
  DAWREC.DAW-CURR-PAGE = 0;

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DT0BM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DT0BM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DT0BM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DT0BM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DT0BM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DT0BM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DT0BM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DT0BP10 ******************************/
  /* **************/
end // end DT0BP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DT0BP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DT0BP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DT0BM001.DAW-CURR-PAGE
   || DT0BM001.DAW-CURR-PAGE == 1)
    DT0BS-TS-READ-PAGE(); /* read temp storage page*/
    DT0BP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DT0BP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0BP20 **********************************/
  /* **************/
end // end DT0BP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DT0BP20-BUILD-MAP2()
  /* *****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DT0BM002.DA-CHNG-USER-ID = sysVar.userID;
  DT0BM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DT0BW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DT0BM002.XSTDESC = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DT0BM002.DA-PLOG-TXT1 = " ";
    DT0BM002.DA-PLOG-TXT2 = " ";
    DT0BM002.XCYCD = " ";
    DT0BM002.XCYNM = " ";
    DT0BM002.XSTCD = " ";
    DT0BM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DT0BW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DT0BW01.XCYGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DT0BW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DT0BW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DT0BM002.XCYCD = DT0BW01.XCYCD; /* move to the map*/
      DT0BM002.XCYNM = DT0BW01.XCYNM;
      DT0BM002.XSTCD = DT0BW01.XSTCD;
      DT0BM002.XXXSTAT = DT0BW01.XXXSTAT;
      DT0BM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DT0BM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DT0BM002.XCYCD = DT0BW02.XCYCD[DAW-ROW-NBR];
        DT0BM002.XCYNM = DT0BW02.XCYNM[DAW-ROW-NBR];
        DT0BM002.XSTCD = DT0BW02.XSTCD[DAW-ROW-NBR];
                                   /* get the tbl row*/

        DT0BW01.DT0BW-FND-FL = "N";
        VXCY0001.XCYCD = DT0BM002.XCYCD;
        VXCY0001.XCYNM = DT0BM002.XCYNM;
        VXCY0001.XSTCD = DT0BM002.XSTCD;

  /* Singleton select against DCORP.XCY*/

        WDA1160.WDA1160-DB2-FUNCTION = "S ";
        call "DA1160" (WDA1160, SQLCA, VXCY0001) {isNoRefresh = yes, isExternal = yes};

        if (SQLCA.VAGen_SQLCODE == 100) /* IF not found*/
          /* next sentence*/
        else
          if (SQLCA.VAGen_SQLCODE == 0) /* IF found*/
            DT0BW01.DT0BW-FND-FL = "Y";
            DT0BM002.XXXSTAT = VXCY0001.XXXSTAT;
          else
            if (SQLCA.VAGen_SQLCODE == -911) /* database busy*/
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
              DAWREC.DAW-INFO-MESSAGE = "DATABASE BUSY - PLEASE TRY AGAIN ";
            else
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
              TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
              TA1REC.TA1TBLVU = "VXCY0001"; /* table view*/
              TA1REC.TA1LOCAT[1] = "SELECT  ";
              XSDAS-DB2-ABEND-RB();
            end
          end
        end
        DT0BW01.DT0BW-FND-FL = "Y";

        if (DT0BW01.DT0BW-FND-FL == "Y")
          DT0BW01.XCYCD = VXCY0001.XCYCD; /* Save for*/
          DT0BW01.XCYNM = VXCY0001.XCYNM; /* later*/
          DT0BW01.XSTCD = VXCY0001.XSTCD;
          DT0BW01.XXXSTAT = VXCY0001.XXXSTAT;
          DT0BW01.ZZZSDT = VXCY0001.ZZZSDT;
          DT0BW01.ZZZCHGDT = VXCY0001.ZZZCHGDT;
          DT0BW01.ZZZCHGTM = VXCY0001.ZZZCHGTM;
          DT0BW01.ZZZCHGCT = VXCY0001.ZZZCHGCT;
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DT0BW01.XCYGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DT0BW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DT0BW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DT0BM002.XCYCD = DT0BW01.XCYCD; /* move to the map*/
    DT0BM002.XCYNM = DT0BW01.XCYNM;
    DT0BM002.XSTCD = DT0BW01.XSTCD;
    DT0BM002.XXXSTAT = DT0BW01.XXXSTAT;
    DT0BM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DT0BM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DT0BM002.XXXSTAT == "A")
    DT0BM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DT0BM002.XXXSTAT == "I")
    DT0BM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DT0BM002.XXXSTAT == " ")
    DT0BM002.DAW-TBL-STATUS-DESC = " ";
  end

  DT0BP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0BP20 **********************************/
  /* **************/
end // end DT0BP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DT0BP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DT0BW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0BP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0BS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/

  /* Move the selected XCY rows to temp storage*/
  DT0B-OPEN-XCY();
  DT0B-FETCH-XCY(); /* fetch 1st row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0BW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TBL ";
    DT0BW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
    DT0BW02.XCYCD[DAW-ROW-NBR] = VXCY0001.XCYCD;
    DT0BW02.XCYNM[DAW-ROW-NBR] = VXCY0001.XCYNM;
    DT0BW02.XSTCD[DAW-ROW-NBR] = VXCY0001.XSTCD;
    DT0BW02.XXXSTAT[DAW-ROW-NBR] = VXCY0001.XXXSTAT;

    DT0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = " ";
    DT0BW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = " ";
    DT0BW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = " ";
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0BS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    DT0B-FETCH-XCY(); /* fetch next row*/
  end

  DT0B-CLOSE-XCY();

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DT0BS-TS-ADD-PAGE();
    end

    DT0BM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DT0BM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end
end // end DT0BP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DT0BP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DT0BW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DT0BW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DT0BW01.XCYGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DT0BW02.XCYCD[DAW-ROW-NBR] = DT0BW01.XCYCD;
  DT0BW02.XCYNM[DAW-ROW-NBR] = DT0BW01.XCYNM;
  DT0BW02.XSTCD[DAW-ROW-NBR] = DT0BW01.XSTCD;
  DT0BW02.XXXSTAT[DAW-ROW-NBR] = DT0BW01.XXXSTAT;
  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DT0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DT0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DT0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DT0BW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DT0BW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
