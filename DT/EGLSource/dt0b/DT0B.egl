// SET MAP 1 FIELD ATTRIBUTES
Function DT0BP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DT0BM001.DAW-PAGE-LIT = "PAGE";
    set DT0BM001.DAW-CURR-PAGE initialAttributes;
    DT0BM001.DAW-OF-LIT = "OF";
    set DT0BM001.DAW-LAST-PAGE initialAttributes;
    DT0BM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DT0BM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DT0BM001.DAW-PAGE-LIT = " ";
    set DT0BM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DT0BM001.DAW-OF-LIT = " ";
    set DT0BM001.DAW-LAST-PAGE skip, invisible;
    DT0BM001.DAW-PF7-LIT = " ";
    DT0BM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DT0BM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DT0BM001.XCYCD[sysVar.arrayIndex] == " ")
      set DT0BM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DT0BM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DT0BP25 **************************/
  /* **************/
end // end DT0BP25-SET-ATTR-1

