  sysVar.arrayIndex = 1;

  while (sysVar.arrayIndex <= 14)
    DT0CM001.DAW-SELECT-CD[sysVar.arrayIndex] = DT0CW02.DAW-SELECT-CD[sysVar.arrayIndex];
    DT0CM001.XPSCD[sysVar.arrayIndex] = DT0CW02.XPSCD[sysVar.arrayIndex];
    DT0CM001.XPSSCD[sysVar.arrayIndex] = DT0CW02.XPSSCD[sysVar.arrayIndex];
    DT0CM001.XPSDESC[sysVar.arrayIndex] = DT0CW02.XPSDESC[sysVar.arrayIndex];
    DT0CM001.XPSABRV[sysVar.arrayIndex] = DT0CW02.XPSABRV[sysVar.arrayIndex];
    DT0CM001.XXXSTAT[sysVar.arrayIndex] = DT0CW02.XXXSTAT[sysVar.arrayIndex];
    DT0CM001.DAW-STATUS-DESC[sysVar.arrayIndex] = DT0CW02.DAW-STATUS-DESC[sysVar.arrayIndex];
    DT0CM001.DA-CHNG-USER-ID[sysVar.arrayIndex] = DT0CW02.DA-CHNG-USER-ID[sysVar.arrayIndex];
    DT0CM001.DAW-CHNG-DT-MDY[sysVar.arrayIndex] = DT0CW02.DAW-CHNG-DT-MDY[sysVar.arrayIndex];
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  /* **************/
  /* ****************** END DT0CP71 *****************************/
  /* **************/
end // end DT0CP71-TSQ-TO-MAP

// BUILD NEW VDATRN01 RECORD
Function DT0CP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DT0CM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DT0CM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DT0CW01.DA-CCAT-KEY-TXT = " ";
  DT0CW01.DT0CW-KEY-XPSCD = DT0CM002.XPSCD;
  DT0CW01.DT0CW-KEY-XPSSCD = DT0CM002.XPSSCD;
  VDATRN01.DA-CCAT-KEY-TXT = DT0CW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DT0CW01.XPSCD = DT0CM002.XPSCD;
  DT0CW01.XPSSCD = DT0CM002.XPSSCD;
  DT0CW01.XPSDESC = DT0CM002.XPSDESC;
  DT0CW01.XPSSDESC = DT0CM002.XPSSDESC;
  DT0CW01.XPSABRV = DT0CM002.XPSABRV;
  DT0CW01.XXXSTAT = DT0CM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 55;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DT0CW01.XPSGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DT0CP72 **********************************/
  /* **************/
end // end DT0CP72-NEW-TRAN

// BUILD UPDATED VDATRN01 RECORD
Function DT0CP73-OLD-TRAN()
  /* *****************************************************************/
  /* Build an updated transaction row*/
  /* *****************************************************************/


  /* move the prolog information in case it changed*/
  /* The transaction type stays the same*/

  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DT0CM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DT0CM002.DA-PLOG-TXT2;

  /* Build the table row*/

  DT0CW01.XPSCD = DT0CM002.XPSCD;
  DT0CW01.XPSSCD = DT0CM002.XPSSCD;
  DT0CW01.XPSDESC = DT0CM002.XPSDESC;
  DT0CW01.XPSSDESC = DT0CM002.XPSSDESC;
  DT0CW01.XPSABRV = DT0CM002.XPSABRV;
  DT0CW01.XXXSTAT = DT0CM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 55;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DT0CW01.XPSGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y"
   || DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DT0CP73 **********************************/
  /* **************/
end // end DT0CP73-OLD-TRAN

// EXIT FROM THE APPLICATION
Function DT0CP99-TERMINATE()
  /* ************************************************************/
  /* Transfer out of the application*/
  /* ************************************************************/

  COMMAREA.CAUSERA = DAWREC.DAWGROUP;

  DT0CS-TS-PURGE-FIL(); /* purge temp storage*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y") /* return to DA01 application*/
    COMMAREA.CATOAP = "DA01";
    XSEXIT();
  end

  if (DAW-EXIT-OPTION == "RETN") /* return to prior application*/
    COMMAREA.CATOAP = COMMAREA.CAFROMAP;
    XSEXIT();
  end

  if (DAW-EXIT-OPTION == "MENU") /* return to main menu*/
    XSPF4();
  end

  if (DAW-EXIT-OPTION == "HELP") /* check for system help*/
    XSPF1(); /* help facility*/
  end

  /* Still here - then XSEXIT didn't allow the transfer so restart*/
  /* the current application and display the error message*/

  if (COMMAREA.CASECRC != 0) /* no message from xsexit*/
    XSDAS-XSEXIT-CAMSG();
  end

  COMMAREA.CAFROMAP = "DT0C";
  sysVar.transferName = "DT0C";
  transfer to program sysVar.transferName passing COMMAREA;

  /* **************/
  /* ****************** END DT0CP99 *****************************/
  /* **************/
