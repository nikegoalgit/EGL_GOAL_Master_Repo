// Close cursor based on search
Function DT0C-CLOSE-XPS()
  /* **************************************************************/
  /* Determine and close the correct XPS cursor*/
  /* **************************************************************/
  /* Use the C1 DB2 function if no search criteria*/
  if (DT0CM001.XPSCD-SRCH == " " && 
  DT0CM001.XPSSCD-SRCH == " ")
    call "DA1170" ("C1", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* USE THE C2 DB2 function*/
  if (DT0CM001.XPSCD-SRCH > " " && 
  DT0CM001.XPSSCD-SRCH == " ")
    call "DA1170" ("C2", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* USE THE CA DB2 function*/
  if (DT0CM001.XPSCD-SRCH == " " && 
  DT0CM001.XPSSCD-SRCH > " ")
    call "DA1170" ("CA", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  if (SQLCA.VAGen_SQLCODE == 0) /* check for good rc*/
     /* next sentence*/
  else
    DAWREC.DAW-EDIT-ERROR-FL = "Y";
    TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
    TA1REC.TA1TBLVU = "VXPS0001"; /* table view*/
    TA1REC.TA1LOCAT[1] = "CLOSE XPS";
    XSDAS-DB2-ABEND-RB();
  end
end // end DT0C-CLOSE-XPS

// Fetch cursor based on search
Function DT0C-FETCH-XPS()
  /* **************************************************************/
  /* Determine and FETCH the correct XPS cursor*/
  /* **************************************************************/
  /* USE THE N1 DB2 function if no search criteria*/
  if (DT0CM001.XPSCD-SRCH == " " && 
  DT0CM001.XPSSCD-SRCH == " ")
    call "DA1170" ("N1", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* USE THE N2 DB2 function*/
  if (DT0CM001.XPSCD-SRCH > " " && 
  DT0CM001.XPSSCD-SRCH == " ")
    call "DA1170" ("N2", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* USE THE NA DB2 function*/
  if (DT0CM001.XPSCD-SRCH == " " && 
  DT0CM001.XPSSCD-SRCH > " ")
    call "DA1170" ("NA", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  if (SQLCA.VAGen_SQLCODE == 0 /* check for good rc*/
   || SQLCA.VAGen_SQLCODE == 100) /* or not found*/
      /* next sentence*/
  else
    if (SQLCA.VAGen_SQLCODE == -911) /* database busy*/
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      DAWREC.DAW-INFO-MESSAGE = "DATABASE BUSY - PLEASE TRY AGAIN ";
    else
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
      TA1REC.TA1TBLVU = "VXPS0001"; /* table view*/
      TA1REC.TA1LOCAT[1] = "FETCH   ";
      XSDAS-DB2-ABEND-RB();
    end
  end
end // end DT0C-FETCH-XPS

// Open cursor based on search
Function DT0C-OPEN-XPS()
  /* **************************************************************/
  /* Determine and open the correct XPS cursor*/
  /* **************************************************************/
  set SQLCA empty;
  set VXPS0001 empty;
  VXPS0001.XPSCD = DT0CM001.XPSCD-SRCH;
  VXPS0001.XPSSCD = DT0CM001.XPSSCD-SRCH;

  /* Use the S1 DB2 function if no search criteria, get all rows*/
  if (DT0CM001.XPSCD-SRCH == " " && 
  DT0CM001.XPSSCD-SRCH == " ")
    call "DA1170" ("S1", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* Use the S2 DB2 function if search xpscd only*/
  if (DT0CM001.XPSCD-SRCH > " " && 
  DT0CM001.XPSSCD-SRCH == " ")
    call "DA1170" ("S2", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  /* Use the SA DB2 function if search xpsscd only*/
  if (DT0CM001.XPSCD-SRCH == " " && 
  DT0CM001.XPSSCD-SRCH > " ")
    call "DA1170" ("SA", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};
  end

  if (SQLCA.VAGen_SQLCODE == 0 /* check for good rc*/
   || SQLCA.VAGen_SQLCODE == 100) /* or not found*/
     /* next sentence*/
  else
    if (SQLCA.VAGen_SQLCODE == -911) /* database busy*/
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      DAWREC.DAW-INFO-MESSAGE = "DATABASE BUSY - PLEASE TRY AGAIN ";
    else
      DAWREC.DAW-EDIT-ERROR-FL = "Y";
      TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
      TA1REC.TA1TBLVU = "VXPS0001"; /* table view*/
      TA1REC.TA1LOCAT[1] = "OPEN XPS";
      XSDAS-DB2-ABEND-RB();
    end
  end
end // end DT0C-OPEN-XPS

// DRIVER FOR APPLICATION
Function DT0CP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DT0CP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DT0CP20-BUILD-MAP2(); /* Build the second map*/
  else
  /* Display first map for search*/
  /* ** PERFORM DT0CP20-BUILD-MAP1;    /* Build the first map*/
  end

  DT0CW01.DT0CW-XPSCD-SRCH = "ZZ";
  DT0CW01.DT0CW-XPSSCD-SRCH = "ZZ";

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DT0CP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DT0CP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DT0CP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DT0CP00 *******************************/
  /* **************/
end // end DT0CP00-DRIVER

// INITIALIZE THE APPLICATION
Function DT0CP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DT0CW01 empty; /* Working storage record*/
  set DT0CW02 empty; /* Temp storage record*/
  DAWREC.DAW-CURR-PAGE = 0;

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DT0CM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DT0CM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DT0CM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DT0CM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DT0CM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DT0CM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DT0CM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DT0CP10 ******************************/
  /* **************/
end // end DT0CP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DT0CP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DT0CP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DT0CM001.DAW-CURR-PAGE
   || DT0CM001.DAW-CURR-PAGE == 1)
    DT0CS-TS-READ-PAGE(); /* read temp storage page*/
    DT0CP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DT0CP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0CP20 **********************************/
  /* **************/
end // end DT0CP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DT0CP20-BUILD-MAP2()
  /* *****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DT0CM002.DA-CHNG-USER-ID = sysVar.userID;
  DT0CM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DT0CW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DT0CM002.DA-PLOG-TXT1 = " ";
    DT0CM002.DA-PLOG-TXT2 = " ";
    DT0CM002.XPSCD = " ";
    DT0CM002.XPSSCD = " ";
    DT0CM002.XPSDESC = " ";
    DT0CM002.XPSSDESC = " ";
    DT0CM002.XPSABRV = " ";
    DT0CM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DT0CW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DT0CW01.XPSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DT0CW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DT0CW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DT0CM002.XPSCD = DT0CW01.XPSCD; /* move to the map*/
      DT0CM002.XPSSCD = DT0CW01.XPSSCD;
      DT0CM002.XPSDESC = DT0CW01.XPSDESC;
      DT0CM002.XPSSDESC = DT0CW01.XPSSDESC;
      DT0CM002.XPSABRV = DT0CW01.XPSABRV;
      DT0CM002.XXXSTAT = DT0CW01.XXXSTAT;
      DT0CM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DT0CM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DT0CM002.XPSCD = DT0CW02.XPSCD[DAW-ROW-NBR];
        DT0CM002.XPSSCD = DT0CW02.XPSSCD[DAW-ROW-NBR];
                                   /* get the tbl row*/

        DT0CW01.DT0CW-FND-FL = "N";
        VXPS0001.XPSCD = DT0CM002.XPSCD;
        VXPS0001.XPSSCD = DT0CM002.XPSSCD;

  /* Singleton select against DCORP.XPS*/
        call "DA1170" ("S ", SQLCA, VXPS0001) {isNoRefresh = yes, isExternal = yes};

        if (SQLCA.VAGen_SQLCODE == 100) /* IF not found*/
          /* next sentence*/
        else
          if (SQLCA.VAGen_SQLCODE == 0) /* IF found*/
            DT0CW01.DT0CW-FND-FL = "Y";
            DT0CM002.XPSDESC = VXPS0001.XPSDESC;
            DT0CM002.XPSSDESC = VXPS0001.XPSSDESC;
            DT0CM002.XPSABRV = VXPS0001.XPSABRV;
            DT0CM002.XXXSTAT = VXPS0001.XXXSTAT;
          else
            if (SQLCA.VAGen_SQLCODE == -911) /* database busy*/
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
              DAWREC.DAW-INFO-MESSAGE = "DATABASE BUSY - PLEASE TRY AGAIN ";
            else
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
              TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
              TA1REC.TA1TBLVU = "VXPS0001"; /* table view*/
              TA1REC.TA1LOCAT[1] = "SELECT  ";
              XSDAS-DB2-ABEND-RB();
            end
          end
        end
        DT0CW01.DT0CW-FND-FL = "Y";

        if (DT0CW01.DT0CW-FND-FL == "Y")
          DT0CW01.XPSCD = VXPS0001.XPSCD; /* Save for*/
          DT0CW01.XPSSCD = VXPS0001.XPSSCD; /* later*/
          DT0CW01.XPSDESC = VXPS0001.XPSDESC;
          DT0CW01.XPSSDESC = VXPS0001.XPSSDESC;
          DT0CW01.XPSABRV = VXPS0001.XPSABRV;
          DT0CW01.XXXSTAT = VXPS0001.XXXSTAT;
          DT0CW01.ZZZSDT = VXPS0001.ZZZSDT;
          DT0CW01.ZZZCHGDT = VXPS0001.ZZZCHGDT;
          DT0CW01.ZZZCHGTM = VXPS0001.ZZZCHGTM;
          DT0CW01.ZZZCHGCT = VXPS0001.ZZZCHGCT;
        end
      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DT0CW01.XPSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DT0CW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DT0CW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DT0CM002.XPSCD = DT0CW01.XPSCD; /* move to the map*/
    DT0CM002.XPSSCD = DT0CW01.XPSSCD;
    DT0CM002.XPSDESC = DT0CW01.XPSDESC;
    DT0CM002.XPSSDESC = DT0CW01.XPSSDESC;
    DT0CM002.XPSABRV = DT0CW01.XPSABRV;
    DT0CM002.XXXSTAT = DT0CW01.XXXSTAT;
    DT0CM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DT0CM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DT0CM002.XXXSTAT == "A")
    DT0CM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DT0CM002.XXXSTAT == "I")
    DT0CM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DT0CM002.XXXSTAT == " ")
    DT0CM002.DAW-TBL-STATUS-DESC = " ";
  end

  DT0CP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0CP20 **********************************/
  /* **************/
end // end DT0CP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DT0CP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DT0CW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0CP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0CS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/

  /* Move the selected XPS rows to temp storage*/
  DT0C-OPEN-XPS();
  DT0C-FETCH-XPS(); /* fetch 1st row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0CW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TBL ";
    DT0CW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
    DT0CW02.XPSCD[DAW-ROW-NBR] = VXPS0001.XPSCD;
    DT0CW02.XPSSCD[DAW-ROW-NBR] = VXPS0001.XPSSCD;
    DT0CW02.XPSDESC[DAW-ROW-NBR] = VXPS0001.XPSDESC;
    DT0CW02.XPSABRV[DAW-ROW-NBR] = VXPS0001.XPSABRV;
    DT0CW02.XXXSTAT[DAW-ROW-NBR] = VXPS0001.XXXSTAT;

    DT0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = " ";
    DT0CW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = " ";
    DT0CW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = " ";
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0CS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    DT0C-FETCH-XPS(); /* fetch next row*/
  end

  DT0C-CLOSE-XPS();

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DT0CS-TS-ADD-PAGE();
    end

    DT0CM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DT0CM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end
end // end DT0CP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DT0CP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DT0CW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DT0CW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DT0CW01.XPSGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DT0CW02.XPSCD[DAW-ROW-NBR] = DT0CW01.XPSCD;
  DT0CW02.XPSSCD[DAW-ROW-NBR] = DT0CW01.XPSSCD;
  DT0CW02.XPSDESC[DAW-ROW-NBR] = DT0CW01.XPSDESC;
  DT0CW02.XPSABRV[DAW-ROW-NBR] = DT0CW01.XPSABRV;
  DT0CW02.XXXSTAT[DAW-ROW-NBR] = DT0CW01.XXXSTAT;
  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DT0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DT0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DT0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DT0CW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DT0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DT0CW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DT0CW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DT0CW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DT0CW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DT0CW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DT0CP23  ******************************/
  /* ***************/
