// SET MAP 1 FIELD ATTRIBUTES
Function DT0CP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DT0CM001.DAW-PAGE-LIT = "PAGE";
    set DT0CM001.DAW-CURR-PAGE initialAttributes;
    DT0CM001.DAW-OF-LIT = "OF";
    set DT0CM001.DAW-LAST-PAGE initialAttributes;
    DT0CM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DT0CM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DT0CM001.DAW-PAGE-LIT = " ";
    set DT0CM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DT0CM001.DAW-OF-LIT = " ";
    set DT0CM001.DAW-LAST-PAGE skip, invisible;
    DT0CM001.DAW-PF7-LIT = " ";
    DT0CM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DT0CM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DT0CM001.XPSCD[sysVar.arrayIndex] == " ")
      set DT0CM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DT0CM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DT0CP25 **************************/
  /* **************/
end // end DT0CP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DT0CP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DT0CM002.XPSCD initialAttributes;
    set DT0CM002.XPSSCD initialAttributes;
    set DT0CM002.XPSDESC initialAttributes;
    set DT0CM002.XPSSDESC initialAttributes;
    set DT0CM002.XPSABRV initialAttributes;
    set DT0CM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DT0CM002.XPSCD skip;
    set DT0CM002.XPSSCD skip;
    set DT0CM002.XPSDESC initialAttributes;
    set DT0CM002.XPSSDESC initialAttributes;
    set DT0CM002.XPSABRV initialAttributes;
    set DT0CM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DT0CM002.XPSCD skip;
    set DT0CM002.XPSSCD skip;
    set DT0CM002.XPSDESC skip;
    set DT0CM002.XPSSDESC skip;
    set DT0CM002.XPSABRV skip;
    set DT0CM002.XXXSTAT skip;
  end

  set DT0CM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DT0CM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DT0CM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DT0CM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DT0CM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DT0CM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DT0CM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DT0CM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DT0CP25 **************************/
  /* **************/
end // end DT0CP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DT0CP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DT0CP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DT0CM001.VAGen_EZEMSG = " ";
  DT0CM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DT0CP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DT0CP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DT0CP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DT0CP70-PROC-MAP-1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DT0CP30 *****************************/
  /* **************/
end // end DT0CP30-MAIN-MAP-1

