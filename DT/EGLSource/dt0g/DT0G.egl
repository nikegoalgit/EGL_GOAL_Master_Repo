// DRIVER FOR APPLICATION
Function DT0GP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DT0GP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DT0GP20-BUILD-MAP2(); /* Build the second map*/
  else
    DT0GP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DT0GP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DT0GP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DT0GP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DT0GP00 *******************************/
  /* **************/
end // end DT0GP00-DRIVER

// INITIALIZE THE APPLICATION
Function DT0GP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DT0GW01 empty; /* Working storage record*/
  set DT0GW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DT0GM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DT0GM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DT0GM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DT0GM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DT0GM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DT0GM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DT0GM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DT0GP10 ******************************/
  /* **************/
end // end DT0GP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DT0GP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DT0GP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DT0GM001.DAW-CURR-PAGE)
    DT0GS-TS-READ-PAGE(); /* read temp storage page*/
    DT0GP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DT0GP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0GP20 **********************************/
  /* **************/
end // end DT0GP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DT0GP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DT0GM002.DA-CHNG-USER-ID = sysVar.userID;
  DT0GM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DT0GW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  DT0GM002.XLESDESC = " ";

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DT0GM002.DA-PLOG-TXT1 = " ";
    DT0GM002.DA-PLOG-TXT2 = " ";
    DT0GM002.XBCCD = " ";
    DT0GM002.XBCDESC = " ";
    DT0GM002.XBCABRV = " ";
    DT0GM002.XLECD = " ";
    DT0GM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DT0GW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DT0GW01.XBCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DT0GW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DT0GW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DT0GM002.XBCCD = DT0GW01.XBCCD; /* move to the map*/
      DT0GM002.XBCDESC = DT0GW01.XBCDESC;
      DT0GM002.XBCABRV = DT0GW01.XBCABRV;
      DT0GM002.XLECD = DT0GW01.XLECD;
      DT0GM002.XXXSTAT = DT0GW01.XXXSTAT;
      DT0GM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DT0GM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DT0GM002.XBCCD = DT0GW02.XBCCD[DAW-ROW-NBR];
                                         /* get the tbl row*/
        if (DT0GM002.XBCCD in XBCTBL.XBCCD)
          DT0GW01.XBCCD = XBCTBL.XBCCD[sysVar.arrayIndex]; /* save for a*/
          DT0GW01.XBCDESC = XBCTBL.XBCDESC[sysVar.arrayIndex]; /* later compare*/
          DT0GW01.XBCABRV = XBCTBL.XBCABRV[sysVar.arrayIndex];
          DT0GW01.XLECD = XBCTBL.XLECD[sysVar.arrayIndex];
          DT0GW01.XXXSTAT = XBCTBL.XXXSTAT[sysVar.arrayIndex];
          DT0GW01.ZZZSDT = XBCTBL.ZZZSDT[sysVar.arrayIndex];
          DT0GW01.ZZZCHGDT = XBCTBL.ZZZCHGDT[sysVar.arrayIndex];
          DT0GW01.ZZZCHGTM = XBCTBL.ZZZCHGTM[sysVar.arrayIndex];
          DT0GW01.ZZZCHGCT = XBCTBL.ZZZCHGCT[sysVar.arrayIndex];

          DT0GM002.XBCDESC = XBCTBL.XBCDESC[sysVar.arrayIndex]; /* move to map*/
          DT0GM002.XBCABRV = XBCTBL.XBCABRV[sysVar.arrayIndex];
          DT0GM002.XLECD = XBCTBL.XLECD[sysVar.arrayIndex];
          DT0GM002.XXXSTAT = XBCTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DT0GW01.XBCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DT0GW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DT0GW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DT0GM002.XBCCD = DT0GW01.XBCCD; /* move to the map*/
    DT0GM002.XBCDESC = DT0GW01.XBCDESC;
    DT0GM002.XBCABRV = DT0GW01.XBCABRV;
    DT0GM002.XLECD = DT0GW01.XLECD;
    DT0GM002.XXXSTAT = DT0GW01.XXXSTAT;
    DT0GM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DT0GM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DT0GM002.XXXSTAT == "A")
    DT0GM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DT0GM002.XXXSTAT == "I")
    DT0GM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DT0GM002.XXXSTAT == " ")
    DT0GM002.DAW-TBL-STATUS-DESC = " ";
  end

  DT0GP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0GP20 **********************************/
  /* **************/
end // end DT0GP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DT0GP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DT0GW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0GP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0GS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XBCTBL.XBCCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0GP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0GS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DT0GS-TS-ADD-PAGE();
    end

    DT0GM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DT0GM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DT0GP22 **********************************/
  /* **************/
end // end DT0GP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DT0GP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DT0GW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DT0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DT0GW01.XBCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DT0GW02.XBCCD[DAW-ROW-NBR] = DT0GW01.XBCCD;
  DT0GW02.XBCDESC[DAW-ROW-NBR] = DT0GW01.XBCDESC;
  DT0GW02.XBCABRV[DAW-ROW-NBR] = DT0GW01.XBCABRV;
  DT0GW02.XLECD[DAW-ROW-NBR] = DT0GW01.XLECD;
  DT0GW02.XXXSTAT[DAW-ROW-NBR] = DT0GW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DT0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DT0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DT0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DT0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DT0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DT0GW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DT0GW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DT0GW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DT0GW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DT0GW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DT0GP221 ******************************/
  /* ***************/
end // end DT0GP23-DB2-TO-TSQ

