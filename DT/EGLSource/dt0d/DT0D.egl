// BUILD NEW VDATRN01 RECORD
Function DT0DP72-NEW-TRAN()
  /* *****************************************************************/
  /* Build a new transaction row*/
  /* *****************************************************************/

  set VDATRN01 empty;

  VDATRN01.XXX-TBL-PFX = DAWREC.XXX-TBL-PFX;

  /* Build the prolog information*/

  VDATRN01.ZZ-CHNG-DT = DAWREC.ZZ-CHNG-DT;
  VDATRN01.DA-CHNG-USER-ID = sysVar.userID;
  VDATRN01.DA-APVD-USER-ID = " ";
  VDATRN01.DA-PLOG-TXT1 = DT0DM002.DA-PLOG-TXT1;
  VDATRN01.DA-PLOG-TXT2 = DT0DM002.DA-PLOG-TXT2;

  /* Determine the transaction type*/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    VDATRN01.DA-ROW-EDIT-IND = "A";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    VDATRN01.DA-ROW-EDIT-IND = "C";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    VDATRN01.DA-ROW-EDIT-IND = "D";
  end

  /* Build the table key*/

  DT0DW01.DA-CCAT-KEY-TXT = " ";
  DT0DW01.DT0DW-KEY-TBLPF = DT0DM002.YSMTBLPF;
  DT0DW01.DT0DW-KEY-SYSPF = DT0DM002.YSMSYSPF;
  DT0DW01.DT0DW-KEY-SUMID = DT0DM002.YSMSUMID;
  DT0DW01.DT0DW-KEY-SUMCD = DT0DM002.YSMSUMCD;
  DT0DW01.DT0DW-KEY-SRCVL = DT0DM002.YSMSRCVL;
  VDATRN01.DA-CCAT-KEY-TXT = DT0DW01.DA-CCAT-KEY-TXT;

  /* Build the table row*/

  DT0DW01.YSMTBLPF = DT0DM002.YSMTBLPF;
  DT0DW01.YSMSYSPF = DT0DM002.YSMSYSPF;
  DT0DW01.YSMSUMID = DT0DM002.YSMSUMID;
  DT0DW01.YSMSUMCD = DT0DM002.YSMSUMCD;
  DT0DW01.YSMSRCVL = DT0DM002.YSMSRCVL;
  DT0DW01.YSMSMDSC = DT0DM002.YSMSMDSC;
  DT0DW01.XXXSTAT = DT0DM002.XXXSTAT;

  VDATRN01.DA-ROW-DATA-TXT-LEN = 51;
  VDATRN01.DA-ROW-DATA-TXT-TEXT = DT0DW01.YSMGROUP;

  /* Determine the status*/

  if (DAWREC.DAW-SET-PENDING-FL == "Y")
    VDATRN01.DA-CHNG-STAT-IND = "P";
  else
    VDATRN01.DA-CHNG-STAT-IND = "E";
  end

  /* **************/
  /* ***************** END DT0DP72 **********************************/
  /* **************/
end // end DT0DP72-NEW-TRAN

