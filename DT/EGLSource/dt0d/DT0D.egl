// DRIVER FOR APPLICATION
Function DT0DP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DT0DP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DT0DP20-BUILD-MAP2(); /* Build the second map*/
  else
  /* PERFORM DT0DP20-BUILD-MAP1;    /* Build the first map*/
  end

  DT0DW01.TBL-SRCH = "ZZZ";
  DT0DW01.SYS-SRCH = "ZZ";
  DT0DW01.SID-SRCH = 9999;
  DT0DW01.SCD-SRCH = "ZZ";
  DT0DW01.SVL-SRCH = "ZZZZ";

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DT0DP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DT0DP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DT0DP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DT0DP00 *******************************/
  /* **************/
end // end DT0DP00-DRIVER

