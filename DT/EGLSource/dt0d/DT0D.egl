// BUILD MAP 2 - TBLE ROW DETAILS
Function DT0DP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DT0DM002.DA-CHNG-USER-ID = sysVar.userID;
  DT0DM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DT0DW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DT0DM002.DA-PLOG-TXT1 = " ";
    DT0DM002.DA-PLOG-TXT2 = " ";
    DT0DM002.YSMTBLPF = " ";
    DT0DM002.YSMSYSPF = " ";
    DT0DM002.YSMSUMID = " ";
    DT0DM002.YSMSUMCD = " ";
    DT0DM002.YSMSRCVL = " ";
    DT0DM002.YSMSMDSC = " ";
    DT0DM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DT0DW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DT0DW01.YSMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DT0DW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DT0DW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DT0DM002.YSMTBLPF = DT0DW01.YSMTBLPF;
                                   /* move to the map*/
      DT0DM002.YSMSYSPF = DT0DW01.YSMSYSPF;
      DT0DM002.YSMSUMID = DT0DW01.YSMSUMID;
      DT0DM002.YSMSUMCD = DT0DW01.YSMSUMCD;
      DT0DM002.YSMSRCVL = DT0DW01.YSMSRCVL;
      DT0DM002.YSMSMDSC = DT0DW01.YSMSMDSC;
      DT0DM002.XXXSTAT = DT0DW01.XXXSTAT;
      DT0DM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DT0DM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/
                                   /* get the tbl row*/
        DT0DM002.YSMTBLPF = DT0DW02.YSMTBLPF[DAW-ROW-NBR];
        DT0DM002.YSMSYSPF = DT0DW02.YSMSYSPF[DAW-ROW-NBR];
        DT0DM002.YSMSUMID = DT0DW02.YSMSUMID[DAW-ROW-NBR];
        DT0DM002.YSMSUMCD = DT0DW02.YSMSUMCD[DAW-ROW-NBR];
        DT0DM002.YSMSRCVL = DT0DW02.YSMSRCVL[DAW-ROW-NBR];
        DT0DW01.DT0DW-FND-FL = "N";

    /* SKUMA3 CHANGE BEGINS*/

    /* Singleton lookup on DCORP.VSUMCD01*/

        VSUMCD01.YSMTBLPF = DT0DM002.YSMTBLPF;
        VSUMCD01.YSMSYSPF = DT0DM002.YSMSYSPF;
        VSUMCD01.YSMSUMID = DT0DM002.YSMSUMID;
        VSUMCD01.YSMSUMCD = DT0DM002.YSMSUMCD;
        VSUMCD01.YSMSRCVL = DT0DM002.YSMSRCVL;

        call "DA1180" ("S ", SQLCA, VSUMCD01) {isNoRefresh = yes, isExternal = yes};
        if (SQLCA.VAGen_SQLCODE == 100) /* IF not found*/
            /* next sentence*/
        else
          if (SQLCA.VAGen_SQLCODE == 0) /* IF found*/
            DT0DW01.DT0DW-FND-FL = "Y";
            DT0DM002.YSMSMDSC = VSUMCD01.YSMSMDSC;
            DT0DM002.XXXSTAT = VSUMCD01.XXXSTAT;
          else
            if (SQLCA.VAGen_SQLCODE == -911) /* database busy*/
              DAWREC.DAW-EDIT-ERROR-FL = "Y";
              DAWREC.DAW-INFO-MESSAGE = "DATABASE BUSY - PLEASE TRY AGAIN ";
            else
              DAWREC.DAW-EDIT-ERROR-FL = "Y";

              TA1REC.TA1DBASE = "DCORP   "; /* data base name*/
              TA1REC.TA1TBLVU = "VSUMCD01"; /* table view*/
              TA1REC.TA1LOCAT[1] = "SELECT  ";
              XSDAS-DB2-ABEND-RB();
            end
          end
        end
        DT0DW01.DT0DW-FND-FL = "Y";

        if (DT0DW01.DT0DW-FND-FL == "Y")
          DT0DW01.YSMTBLPF = VSUMCD01.YSMTBLPF; /* save for a*/
          DT0DW01.YSMSYSPF = VSUMCD01.YSMSYSPF; /* later compare*/
          DT0DW01.YSMSUMID = VSUMCD01.YSMSUMID;
          DT0DW01.YSMSUMCD = VSUMCD01.YSMSUMCD;
          DT0DW01.YSMSRCVL = VSUMCD01.YSMSRCVL;
          DT0DW01.YSMSMDSC = VSUMCD01.YSMSMDSC;
          DT0DW01.XXXSTAT = VSUMCD01.XXXSTAT;
          DT0DW01.ZZZSDT = VSUMCD01.ZZZSDT;
          DT0DW01.ZZZCHGDT = VSUMCD01.ZZZCHGDT;
          DT0DW01.ZZZCHGTM = VSUMCD01.ZZZCHGTM;
          DT0DW01.ZZZCHGCT = VSUMCD01.ZZZCHGCT;
        end
      end
    end
  end

  /* SKUMA3 CHANGE ENDS*/

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DT0DW01.YSMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DT0DW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DT0DW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DT0DM002.YSMTBLPF = DT0DW01.YSMTBLPF;
                                   /* move to the map*/
    DT0DM002.YSMSYSPF = DT0DW01.YSMSYSPF;
    DT0DM002.YSMSUMID = DT0DW01.YSMSUMID;
    DT0DM002.YSMSUMCD = DT0DW01.YSMSUMCD;
    DT0DM002.YSMSRCVL = DT0DW01.YSMSRCVL;
    DT0DM002.YSMSMDSC = DT0DW01.YSMSMDSC;
    DT0DM002.XXXSTAT = DT0DW01.XXXSTAT;
    DT0DM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DT0DM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DT0DM002.XXXSTAT == "A")
    DT0DM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DT0DM002.XXXSTAT == "I")
    DT0DM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DT0DM002.XXXSTAT == " ")
    DT0DM002.DAW-TBL-STATUS-DESC = " ";
  end

  DT0DP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0DP20 **********************************/
  /* **************/
end // end DT0DP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DT0DP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DT0DW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0DP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0DS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/

  /* Move the Selected SUMCD rows to temp storage*/

  DT0D-OPEN-YSM();
  DT0D-FETCH-YSM(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0DW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TBL ";
    DT0DW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";

    DT0DW02.YSMTBLPF[DAW-ROW-NBR] = VSUMCD01.YSMTBLPF;
    DT0DW02.YSMSYSPF[DAW-ROW-NBR] = VSUMCD01.YSMSYSPF;
    DT0DW02.YSMSUMID[DAW-ROW-NBR] = VSUMCD01.YSMSUMID;
    DT0DW02.YSMSUMCD[DAW-ROW-NBR] = VSUMCD01.YSMSUMCD;
    DT0DW02.YSMSRCVL[DAW-ROW-NBR] = VSUMCD01.YSMSRCVL;
    DT0DW02.XXXSTAT[DAW-ROW-NBR] = VSUMCD01.XXXSTAT;

    DT0DW02.DAW-STATUS-DESC[DAW-ROW-NBR] = " ";
    DT0DW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = " ";
    DT0DW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = " ";

    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0DS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    DT0D-FETCH-YSM(); /* fetch next row*/
  end

  DT0D-CLOSE-YSM();

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DT0DS-TS-ADD-PAGE();
    end

    DT0DM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DT0DM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DT0DP22 **********************************/
  /* **************/
end // end DT0DP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DT0DP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DT0DW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DT0DW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DT0DW01.YSMGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DT0DW02.YSMTBLPF[DAW-ROW-NBR] = DT0DW01.YSMTBLPF;
  DT0DW02.YSMSYSPF[DAW-ROW-NBR] = DT0DW01.YSMSYSPF;
  DT0DW02.YSMSUMID[DAW-ROW-NBR] = DT0DW01.YSMSUMID;
  DT0DW02.YSMSUMCD[DAW-ROW-NBR] = DT0DW01.YSMSUMCD;
  DT0DW02.YSMSRCVL[DAW-ROW-NBR] = DT0DW01.YSMSRCVL;
  DT0DW02.XXXSTAT[DAW-ROW-NBR] = DT0DW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DT0DW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DT0DW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DT0DW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DT0DW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DT0DW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DT0DW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DT0DW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DT0DW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DT0DW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DT0DW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DT0DP221 ******************************/
  /* ***************/
end // end DT0DP23-DB2-TO-TSQ

// SET MAP 1 FIELD ATTRIBUTES
Function DT0DP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DT0DM001.DAW-PAGE-LIT = "PAGE";
    set DT0DM001.DAW-CURR-PAGE initialAttributes;
    DT0DM001.DAW-OF-LIT = "OF";
    set DT0DM001.DAW-LAST-PAGE initialAttributes;
    DT0DM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DT0DM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DT0DM001.DAW-PAGE-LIT = " ";
    set DT0DM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DT0DM001.DAW-OF-LIT = " ";
    set DT0DM001.DAW-LAST-PAGE skip, invisible;
    DT0DM001.DAW-PF7-LIT = " ";
    DT0DM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DT0DM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*")
      set DT0DM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      if (DT0DM001.YSMTBLPF[sysVar.arrayIndex] == " ")
        set DT0DM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
        set DT0DM001.YSMSUMID[sysVar.arrayIndex] skip, invisible;
      else
        set DT0DM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
        set DT0DM001.YSMSUMID[sysVar.arrayIndex] initialAttributes;
      end
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DT0DP25 **************************/
  /* **************/
end // end DT0DP25-SET-ATTR-1

