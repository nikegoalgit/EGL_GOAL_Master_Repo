// SET MAP 1 FIELD ATTRIBUTES
Function DT0NP25-SET-ATTR-1()
  /* *********************************************************/
  /* Set MAP 001 field attributes - only show page numbers when*/
  /* there is more than 1 page. Autoskip,dark any rows with*/
  /* an '*' in the select. They can look at the data (for the*/
  /* status) but they can't select it.*/
  /* *********************************************************/

  if (DAW-LAST-PAGE > 1)
    DT0NM001.DAW-PAGE-LIT = "PAGE";
    set DT0NM001.DAW-CURR-PAGE initialAttributes;
    DT0NM001.DAW-OF-LIT = "OF";
    set DT0NM001.DAW-LAST-PAGE initialAttributes;
    DT0NM001.DAW-PF7-LIT = "PF7 = SCROLL BACK";
    DT0NM001.DAW-PF8-LIT = "PF8 = SCROLL FRWD";
  else
    DT0NM001.DAW-PAGE-LIT = " ";
    set DT0NM001.DAW-CURR-PAGE skip, noHighLight, invisible;
    DT0NM001.DAW-OF-LIT = " ";
    set DT0NM001.DAW-LAST-PAGE skip, invisible;
    DT0NM001.DAW-PF7-LIT = " ";
    DT0NM001.DAW-PF8-LIT = " ";
  end

  sysVar.arrayIndex = 14;
  while (sysVar.arrayIndex > 0)
    if (DT0NM001.DAW-SELECT-CD[sysVar.arrayIndex] == "*"
     || DT0NM001.XGCCD[sysVar.arrayIndex] == " ")
      set DT0NM001.DAW-SELECT-CD[sysVar.arrayIndex] skip, invisible;
    else
      set DT0NM001.DAW-SELECT-CD[sysVar.arrayIndex] cursor, initialAttributes;
    end
    sysVar.arrayIndex = sysVar.arrayIndex - 1;
  end

  /* **************/
  /* ****************** END DT0NP25 **************************/
  /* **************/
end // end DT0NP25-SET-ATTR-1

// SET FIELD ATTRIBUTES FOR MAP 2
Function DT0NP25-SET-ATTR-2()
  /* *********************************************************/
  /* Set MAP 002 field attributes - autoskip the DA-CODE when*/
  /* the function is 'upd'.*/
  /* *********************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    set DT0NM002.XGCCD initialAttributes;
    set DT0NM002.XGCDESC initialAttributes;
    set DT0NM002.XGCABRV initialAttributes;
    set DT0NM002.XXXSTAT initialAttributes;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    set DT0NM002.XGCCD skip;
    set DT0NM002.XGCDESC skip;
    set DT0NM002.XGCABRV skip;
    set DT0NM002.XXXSTAT skip;
  end

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    set DT0NM002.XGCCD skip;
    set DT0NM002.XGCDESC initialAttributes;
    set DT0NM002.XGCABRV initialAttributes;
    set DT0NM002.XXXSTAT initialAttributes;
  end

  set DT0NM002.DA-PLOG-TXT1 cursor, initialAttributes;
  set DT0NM002.DA-PLOG-TXT2 initialAttributes;

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DT0NM002.DAW-CURR-FUNCTION-DESC = "ADD ROW ";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "CPY")
    DT0NM002.DAW-CURR-FUNCTION-DESC = "COPY ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD")
    DT0NM002.DAW-CURR-FUNCTION-DESC = "UPDATE ROW";
  end
  if (DAWREC.DAW-CURR-FUNCTION-CD == "DEL")
    DT0NM002.DAW-CURR-FUNCTION-DESC = "DELETE ROW";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TRAN")
    DT0NM002.DAW-RECORD-TYPE-DESC = "TRANSACTION";
  end

  if (DAWREC.DAW-RECORD-TYPE == "TBL ")
    DT0NM002.DAW-RECORD-TYPE-DESC = "TABLE ROW  ";
  end


  /* **************/
  /* ****************** END DT0NP25 **************************/
  /* **************/
end // end DT0NP25-SET-ATTR-2

// MAIN PROCESS LOOP FOR MAP 1
Function DT0NP30-MAIN-MAP-1()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y'*/
  /* **********************************************************/

  DT0NP40-CONVERSE-1(); /* Display the map*/

  converseLib.validationFailed(0);
  DT0NM001.VAGen_EZEMSG = " ";
  DT0NM001.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DT0NP25-SET-ATTR-1(); /* Reset field attributes to defined*/

  DT0NP50-PF-KEYS-1(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N"
   && DAWREC.DAW-TERM-APPL-FL == "N") /* No errors and not ready to exit*/

    DT0NP60-EDIT-MAP-1(); /* Do all edits for map 1*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DT0NP70-PROC-MAP1(); /* Do any processing needed for map 1*/
    end

  end


  /* **************/
  /* ****************** END DT0NP30 *****************************/
  /* **************/
end // end DT0NP30-MAIN-MAP-1

// MAIN PROCESSING LOOP FOR MAP 2
Function DT0NP30-MAIN-MAP-2()
  /* **********************************************************/
  /* MAIN PROCESS - Program loops through this process until*/
  /* the user exits with a PF key or transfers to another*/
  /* application. Exit occurs when DA-TERM-APPL = 'Y' or*/
  /* DA-DONE-WITH-ROW-FL = 'Y'*/
  /* **********************************************************/

  DT0NP40-CONVERSE-2(); /* Display the map*/

  converseLib.validationFailed(0);
  DT0NM002.VAGen_EZEMSG = " ";
  DT0NM002.DAW-INFO-MESSAGE = " ";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  DT0NP25-SET-ATTR-2(); /* Reset field attributes to defined*/

  DT0NP50-PF-KEYS-2(); /* Edit pf keys*/

  if (DAWREC.DAW-EDIT-ERROR-FL == "N" /* No errors*/
   && DAWREC.DAW-TERM-APPL-FL == "N"  /* and not ready to exit to MENU*/
   && DAWREC.DAW-DONE-WITH-ROW-FL == "N") /* and not ready to exit to MAP1*/

    DT0NP60-EDIT-MAP-2(); /* Do all edits for MAP 2*/

    if (DAWREC.DAW-EDIT-ERROR-FL == "N")
      DT0NP70-PROC-MAP2(); /* Do any processing needed for MAP 2*/
    end

  end

  /* **************/
  /* ****************** END DT0NP30 *****************************/
  /* **************/
end // end DT0NP30-MAIN-MAP-2

