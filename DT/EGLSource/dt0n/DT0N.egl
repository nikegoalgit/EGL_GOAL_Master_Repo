Program DT0N type textUIProgram //VAGen Info - main text program
  {
  includeReferencedFunctions = yes, allowUnqualifiedItemReferences = yes, 
  throwNrfEofExceptions = yes, handleHardIOErrors = no, V60ExceptionCompatibility = yes, 
  I4GLItemsNullable = no, textLiteralDefaultIsString = no, localSQLScope = yes, 
  msgTablePrefix = "DA00", inputRecord = COMMAREA, segmented = yes
  }

  // Data Declarations
  COMMAREA COMMAREA ; // record
  DAWREC DAWREC; // record
  DT0NW01 DT0NW01; // record
  DT0NW02 DT0NW02; // record
  ERRSQLCA ERRSQLCA; // record
  SQLCA SQLCA; // record
  TA1REC TA1REC; // record
  TA5REC TA5REC; // record
  TAEREC TAEREC; // record
  VDATRN01 VDATRN01; // record
  WDA1140 WDA1140; // record
  XSDAW01 XSDAW01; // record
  XSDAW02 XSDAW02; // record

  // VAGen Info - items needed for migration
  VAGen_EZESYS char(8);
  VAGen_EZEREPLY num(1);
  VAGen_EZE_WAIT_TIME bin(9,2);
  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use XGCTBL {deleteAfterUse = yes}; // table
  use DT0NM.DT0NM001,  // forms
      DT0NM.DT0NM002
   {
    pfKeyEquate = no
  };

  function main()
    // VAGen Info - initialization needed for migration
    VAGen_EZESYS = VGLib.getVAGSysType();
    DT0NP00-DRIVER: DT0NP00-DRIVER();
    exit program;
  end // end main
end // end DT0N

// DRIVER FOR APPLICATION
Function DT0NP00-DRIVER()
  /* **************************************************************/
  /* DRIVER FOR APPLICATION*/
  /* **************************************************************/

  DT0NP10-INITIALIZE(); /* Initialize application*/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DT0NP20-BUILD-MAP2(); /* Build the second map*/
  else
    DT0NP20-BUILD-MAP1(); /* Build the first map*/
  end

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    while (DAWREC.DAW-DONE-WITH-ROW-FL == "N")
      DT0NP30-MAIN-MAP-2(); /* Process map until ready to exit*/
    end
  else
    while (DAWREC.DAW-TERM-APPL-FL == "N")
      DT0NP30-MAIN-MAP-1(); /* Process map until ready to exit*/
    end
  end

  DT0NP99-TERMINATE(); /* Terminate the application*/

  /* **************/
  /* ****************** END DT0NP00 *******************************/
  /* **************/
end // end DT0NP00-DRIVER

// INITIALIZE THE APPLICATION
Function DT0NP10-INITIALIZE()
  /* **********************************************************/
  /* This process initializes the program work area and map.*/
  /* **********************************************************/

  XSENTRY(); /* Check entry security*/

  XSSEGTR(); /* Set segmented transaction id*/

  /* Initialize records*/

  DAWREC.DAWGROUP = COMMAREA.CAUSERA; /* restore system record*/
  set DT0NW01 empty; /* Working storage record*/
  set DT0NW02 empty; /* Temp storage record*/

  DAWREC.DAW-TERM-APPL-FL = "N";
  DAWREC.DAW-EDIT-ERROR-FL = "N";

  /* Initialize and move standard information to the map*/

  set DT0NM001 initial;
  XSDAS-FMT-DB2-MDCY();
  DT0NM001.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DT0NM001.DAW-MAP-TIME = VGVar.currentFormattedTime;

  set DT0NM002 initial;
  XSDAS-FMT-DB2-MDCY();
  DT0NM002.DAW-MAP-DATE = DAWREC.DAW-DATE-MDCY;
  DT0NM002.DAW-MAP-TIME = VGVar.currentFormattedTime;

  if (COMMAREA.CAMSG != " "
   && COMMAREA.CAFRMSYS == "DA")
    DT0NM001.DAW-INFO-MESSAGE = COMMAREA.CAMSG; /* commarea message*/
  end

  /* **************/
  /* ****************** END DT0NP10 ******************************/
  /* **************/
end // end DT0NP10-INITIALIZE

// BUILD MAP 2 - LIST TABLE ROWS
Function DT0NP20-BUILD-MAP1()
  /* ****************************************************************/
  /* Build temp storage and move first page to MAP 001*/
  /* ****************************************************************/

  DT0NP22-BUILD-TSQ(); /* build temp storage file*/

  if (DAWREC.DAW-CURR-PAGE != DT0NM001.DAW-CURR-PAGE)
    DT0NS-TS-READ-PAGE(); /* read temp storage page*/
    DT0NP71-TSQ-TO-MAP(); /* move page to the map*/
  end

  DT0NP25-SET-ATTR-1(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0NP20 **********************************/
  /* **************/
end // end DT0NP20-BUILD-MAP1

// BUILD MAP 2 - TBLE ROW DETAILS
Function DT0NP20-BUILD-MAP2()
  /* ****************************************************************/
  /* Build MAP 002*/
  /* ****************************************************************/

  DT0NM002.DA-CHNG-USER-ID = sysVar.userID;
  DT0NM002.DAW-UPDATE-DATE = VGVar.currentShortGregorianDate;

  set DT0NW01 empty;
  set VDATRN01 empty;

  /* *****************************************************************/
  /* Add a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "ADD")
    DT0NM002.DA-PLOG-TXT1 = " ";
    DT0NM002.DA-PLOG-TXT2 = " ";
    DT0NM002.XGCCD = " ";
    DT0NM002.XGCDESC = " ";
    DT0NM002.XGCABRV = " ";
    DT0NM002.XXXSTAT = " ";
  end

  /* *****************************************************************/
  /* Update or copy a row*/
  /* *****************************************************************/

  if (DAWREC.DAW-CURR-FUNCTION-CD == "UPD"
   || DAWREC.DAW-CURR-FUNCTION-CD == "CPY"
   || DAWREC.DAW-CURR-FUNCTION-CD == "DEL")

    if (DAWREC.DAW-RECORD-TYPE == "TRAN") /* update, copy or delete*/
                                   /* a transaction*/

      VDATRN01.DAW-VDATRN01-KEY = DT0NW02.DAW-VDATRN01-KEY[DAW-ROW-NBR];
      XSDAS-DA1140-SEL(); /* select the transaction*/

      DT0NW01.XGCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
      DT0NW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
      DT0NW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

      DT0NM002.XGCCD = DT0NW01.XGCCD; /* move to the map*/
      DT0NM002.XGCDESC = DT0NW01.XGCDESC;
      DT0NM002.XGCABRV = DT0NW01.XGCABRV;
      DT0NM002.XXXSTAT = DT0NW01.XXXSTAT;
      DT0NM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
      DT0NM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

    else
      if (DAWREC.DAW-RECORD-TYPE == "TBL ") /* update or copy a table row*/

        DT0NM002.XGCCD = DT0NW02.XGCCD[DAW-ROW-NBR];
                                   /* get the tbl row*/
        if (DT0NM002.XGCCD in XGCTBL.XGCCD)
          DT0NW01.XGCCD = XGCTBL.XGCCD[sysVar.arrayIndex]; /* save for a*/
          DT0NW01.XGCDESC = XGCTBL.XGCDESC[sysVar.arrayIndex]; /* later compare*/
          DT0NW01.XGCABRV = XGCTBL.XGCABRV[sysVar.arrayIndex];
          DT0NW01.XXXSTAT = XGCTBL.XXXSTAT[sysVar.arrayIndex];
          DT0NW01.ZZZSDT = XGCTBL.ZZZSDT[sysVar.arrayIndex];
          DT0NW01.ZZZCHGDT = XGCTBL.ZZZCHGDT[sysVar.arrayIndex];
          DT0NW01.ZZZCHGTM = XGCTBL.ZZZCHGTM[sysVar.arrayIndex];
          DT0NW01.ZZZCHGCT = XGCTBL.ZZZCHGCT[sysVar.arrayIndex];

          DT0NM002.XGCDESC = XGCTBL.XGCDESC[sysVar.arrayIndex]; /* move to map*/
          DT0NM002.XGCABRV = XGCTBL.XGCABRV[sysVar.arrayIndex];
          DT0NM002.XXXSTAT = XGCTBL.XXXSTAT[sysVar.arrayIndex];
        end

      end
    end
  end

  /* *****************************************************************/
  /* inquire on a row selected in DA01*/
  /* *****************************************************************/

  if (DAWREC.DAW-RETURN-TO-DA01-FL == "Y")
    DAWREC.DAW-DONE-WITH-ROW-FL = "N";
    DAWREC.DAW-CURR-FUNCTION-CD = "UPD";
    DAWREC.DAW-RECORD-TYPE = "TRAN";

    VDATRN01.DAW-VDATRN01-KEY = DAWREC.DAW-VDATRN01-KEY;
    XSDAS-DA1140-SEL(); /* select the transaction*/

    DT0NW01.XGCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT; /* save data*/
    DT0NW01.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1; /* for a later*/
    DT0NW01.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2; /* compare*/

    DT0NM002.XGCCD = DT0NW01.XGCCD; /* move to the map*/
    DT0NM002.XGCDESC = DT0NW01.XGCDESC;
    DT0NM002.XGCABRV = DT0NW01.XGCABRV;
    DT0NM002.XXXSTAT = DT0NW01.XXXSTAT;
    DT0NM002.DA-PLOG-TXT1 = VDATRN01.DA-PLOG-TXT1;
    DT0NM002.DA-PLOG-TXT2 = VDATRN01.DA-PLOG-TXT2;

  end

  if (DT0NM002.XXXSTAT == "A")
    DT0NM002.DAW-TBL-STATUS-DESC = "ACTIVE  ";
  end
  if (DT0NM002.XXXSTAT == "I")
    DT0NM002.DAW-TBL-STATUS-DESC = "INACTIVE";
  end
  if (DT0NM002.XXXSTAT == " ")
    DT0NM002.DAW-TBL-STATUS-DESC = " ";
  end

  DT0NP25-SET-ATTR-2(); /* Set field attributes*/

  /* **************/
  /* ***************** END DT0NP20 **********************************/
  /* **************/
end // end DT0NP20-BUILD-MAP2

// BUILD TEMP STORAGE FILE
Function DT0NP22-BUILD-TSQ()
  /* ****************************************************************/
  /* Build temp storage from menu table to allow scrolling*/
  /* ****************************************************************/

  set DT0NW02 empty; /* temp storage record*/
  DAWREC.DAW-ROW-NBR = 0; /* temp storage row subscript*/
  DAWREC.DAW-CURR-PAGE = 0; /* temp storage page(record) number*/
  DAWREC.DAW-MAX-PAGE = 0; /* temp storage max page number*/

  /* Move the Maintenance records to temp storage*/

  XSDAS-DA1140-OPEN(); /* open cursor*/
  XSDAS-DA1140-FETCH(); /* fetch first row*/

  while (SQLCA.VAGen_SQLCODE == 0) /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0NP23-DB2-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0NS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    XSDAS-DA1140-FETCH(); /* fetch next row*/
  end

  XSDAS-DA1140-CLOSE(); /* close cursor*/


  /* Move the CSP table to temp storage*/

  sysVar.arrayIndex = 1;
  while (XGCTBL.XGCCD[sysVar.arrayIndex] != "99") /* until last row*/
    DAWREC.DAW-ROW-NBR = DAWREC.DAW-ROW-NBR + 1;
    DT0NP24-CSP-TO-TSQ(); /* move tbl row to temp storage row*/
    if (DAWREC.DAW-ROW-NBR == 14) /* maximum tsq rows per map*/
      DT0NS-TS-ADD-PAGE(); /* write temp storage record when full*/
    end
    sysVar.arrayIndex = sysVar.arrayIndex + 1;
  end

  if (DAWREC.DAW-ROW-NBR > 0 /* rows were moved*/
   || DAWREC.DAW-MAX-PAGE > 0) /* or tsq pages were written*/

    if (DAWREC.DAW-ROW-NBR < 14 /* last page not full*/
     && DAWREC.DAW-ROW-NBR != 0) /* rows exist*/
      DT0NS-TS-ADD-PAGE();
    end

    DT0NM001.DAW-CURR-PAGE = 1; /* page number to read*/
    DAWREC.DAW-CURR-PAGE = 0; /* last page read*/
    DT0NM001.DAW-LAST-PAGE = DAWREC.DAW-MAX-PAGE;

  end

  /* **************/
  /* ***************** END DT0NP22 **********************************/
  /* **************/
end // end DT0NP22-BUILD-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DT0NP23-DB2-TO-TSQ()
  /* **************************************************************/
  /* Move db2 table row to temp storage*/
  /* The key to the view is saved in temp storage to make it*/
  /* easier to retrieve and update/delete the row if necessary.*/
  /* **************************************************************/

  DT0NW02.DAW-RECORD-TYPE[DAW-ROW-NBR] = "TRAN ";
  DT0NW02.DAW-SELECT-CD[DAW-ROW-NBR] = " ";
  DT0NW01.XGCGROUP = VDATRN01.DA-ROW-DATA-TXT-TEXT;

  DT0NW02.XGCCD[DAW-ROW-NBR] = DT0NW01.XGCCD;
  DT0NW02.XGCDESC[DAW-ROW-NBR] = DT0NW01.XGCDESC;
  DT0NW02.XGCABRV[DAW-ROW-NBR] = DT0NW01.XGCABRV;
  DT0NW02.XXXSTAT[DAW-ROW-NBR] = DT0NW01.XXXSTAT;

  if (VDATRN01.DA-CHNG-STAT-IND == "E")
    DT0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "EDIT    ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "P")
    DT0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PENDING ";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "A")
    DT0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "APPROVED";
    DT0NW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "R")
    DT0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "REJECTED";
  end
  if (VDATRN01.DA-CHNG-STAT-IND == "I")
    DT0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = "PROCESS ";
    DT0NW02.DAW-SELECT-CD[DAW-ROW-NBR] = "*"; /* select not allowed*/
  end

  DT0NW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = VDATRN01.DA-CHNG-USER-ID;

  DAWREC.DAW-DATE-COMP = VDATRN01.ZZ-CHNG-DT;
  XSDAS-FMT-DB2-MDCY();
  DT0NW02.DAW-CHNG-DT-MDY[DAW-ROW-NBR] = DAWREC.DAW-DATE-MDCY;

  DT0NW02.DAW-VDATRN01-KEY[DAW-ROW-NBR] = VDATRN01.DAW-VDATRN01-KEY;


  /* ***************/
  /* ****************** END DT0NP221 ******************************/
  /* ***************/
end // end DT0NP23-DB2-TO-TSQ

// MOVE TO TEMP STORAGE PAGE
Function DT0NP24-CSP-TO-TSQ()
  /* **************************************************************/
