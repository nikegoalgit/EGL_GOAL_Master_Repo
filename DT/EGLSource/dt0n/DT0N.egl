  DT0NW02.XXXSTAT[DAW-ROW-NBR] = XGCTBL.XXXSTAT[sysVar.arrayIndex];

  DT0NW02.DAW-STATUS-DESC[DAW-ROW-NBR] = " ";
  DT0NW02.DA-CHNG-USER-ID[DAW-ROW-NBR] = " ";
