    ["//*                                                                             ", " "],
    ["//S0080  EXEC EP3101,                                                           ", " "],
    ["//       COND=(0,NE),                                                           ", " "],
    ["//       CTRL02=&CTRL02V                                                        ", " "],
    ["//*                                                                             ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
