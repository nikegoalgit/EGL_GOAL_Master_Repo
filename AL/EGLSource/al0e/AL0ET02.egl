    ["//*------------------------------------------------------------------*          ", " "],
    ["//*                                                                             ", " "],
    ["//S0100  EXEC EP1241,                                                           ", " "],
    ["//       COND=(0,NE),                                                           ", " "],
    ["//       CTRL01=&CTRL01V,                                                       ", " "],
    ["//       CTRL02=&CTRL02V                                                        ", " "],
    ["//*                                                                             ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
