    ["//* AGAINST SUMMARY OF TOTAL ITEMS QUANTITIES.                                  ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
    ["//*                                                                             ", " "],
    ["//S0020  EXEC EP1291,                                                           ", " "],
    ["//       COND=(0,NE),                                                           ", " "],
    ["//       CTRL01=&CTRL01V                                                        ", " "],
    ["//*                                                                             ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
    ["//* S0030 - S0090 STEPS CREATES THE ADD TRANSACTIONS                            ", " "],
    ["//*       THE PACKSLIP HEADER, DETAILS, INSTRUCTIONS                            ", " "],
    ["//* THIS STEP RUNS EP3000 WHICH CREATES VAS TICKETS FOR DCMS                    ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
    ["//*                                                                             ", " "],
    ["//S0030  EXEC EP3001,                                                           ", " "],
    ["//       COND=(0,NE),                                                           ", " "],
    ["//       CTRL01=&CTRL01V,                                                       ", " "],
    ["//       CTRL02=&CTRL02V                                                        ", " "],
    ["//*                                                                             ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
    ["//* THIS STEP RUNS EP3030 WHICH CREATES THE HEADER AND DETAIL RECORDS           ", " "],
    ["//* USED BY EP1240 AND ADDS THE VAS PACKAGING INFORMATION                       ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
    ["//*                                                                             ", " "],
    ["//S0040  EXEC EP3031,                                                           ", " "],
    ["//       COND=(0,NE),                                                           ", " "],
    ["//       CTRL01=&CTRL01V,                                                       ", " "],
    ["//       CTRL02=&CTRL02V                                                        ", " "],
    ["//*                                                                             ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
