    ["//* RECORDS                                                                     ", " "],
    ["//*------------------------------------------------------------------*          ", " "],
    ["//*                                                                             ", " "],
    ["//S0060  EXEC EP3051,                                                           ", " "],
    ["//       COND=(0,NE),                                                           ", " "],
    ["//       CTRL01=&CTRL01V,                                                       ", " "],
    ["//       CTRL02=&CTRL02V                                                        ", " "],
    ["//*                                                                             ", " "],
