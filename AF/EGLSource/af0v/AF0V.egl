  set AF0VM001.XPLCD cursor, bold; /* highlight field*/
  XP1REC.XP1MNO = 81; /* indicate error message*/
  XP1REC.XPLCD = " "; /* Space out code*/
  XP1REC.XPLDESC = " "; /* Space out save desc*/
  AF0VM001.XPLDESC = " "; /* Space out map desc*/
