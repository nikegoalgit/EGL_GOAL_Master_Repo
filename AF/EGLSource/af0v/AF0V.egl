  VAGen_EZE_ITEMLEN int;

  // Use Declarations
  use AF0VT02 {deleteAfterUse = yes}; // table
  use XDVTBL {deleteAfterUse = yes}; // table
  use XPRTBL {deleteAfterUse = yes}; // table
  use XGATBL {deleteAfterUse = yes}; // table
