    return; /* Return to calling process*/
  end
  ;
  ;
  AF0WW01.AF0WWFCT = AF0WM001.XXXUSER; /* Move to wk map storage*/
  ;
  ;
   /* *=============================================**/
   /* Clear the row storage area and format the key.*/
   /* *=============================================**/
  ;
