        set AF0WM001.XSECD cursor, bold; /* highlight field*/
        XP1REC.XP1MNO = 138; /* indicate error message*/
        XP1REC.XSNYR = 0; /* Reset working storage*/
      end
    else
      if (AF0WW01.AF0WWXSY > 2000)
        AFWREC.AFWPRCSN = "S07 "; /* Process number*/
        set AF0WM001.XSNYR cursor, bold; /* highlight field*/
        set AF0WM001.XSECD cursor, bold; /* highlight field*/
        XP1REC.XP1MNO = 138; /* indicate error message*/
        XP1REC.XSNYR = 0; /* Reset working storage*/
      end
    end
  end
