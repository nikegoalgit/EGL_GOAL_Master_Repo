// Edit season yr
Function AF0WS07()
  ;
  AF0WW01.AF0WWXSY = 0; /* initialize xsnyr wk area*/
  AF0WW01.AF0WWXSY = AF0WM001.XSNYR; /* move to wk area*/
   /* -----------------------  Y2K OUT BEGIN  ------------------------*/
   /*  */
   /* AF0WW01.AF0WWXSY = AF0WW01.AF0WWXSY + 1900;/* move century*/
   /*  */
